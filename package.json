{
  "name": "hsr-data",
  "author": "dvaji",
  "version": "0.5.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "index.d.ts"
  ],
  "engines": {
    "node": ">=13"
  },
  "scripts": {
    "clean:dist": "rimraf dist/",
    "build:types": "tsc src/index.ts --declarationDir dist/ --lib esnext --emitDeclarationOnly --declaration",
    "build:lib": "vite build",
    "build": "pnpm clean:dist && pnpm build:lib && pnpm build:types",
    "lint": "eslint src/ --ext .ts",
    "generate": "node ./scripts/generate.mjs",
    "format-data": "prettier --loglevel warn --write \"src/data/**/*.json\"",
    "prepublishOnly": "pnpm generate && pnpm build"
  },
  "devDependencies": {
    "@types/node": "^18.18.4",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
