{
  "name": "hsr-data",
  "author": "dvaji",
  "version": "3.1.1",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "index.d.ts"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean:dist": "rimraf dist/",
    "build:types": "tsc src/index.ts --declarationDir dist/ --lib esnext --emitDeclarationOnly --declaration",
    "build:lib": "vite build",
    "build": "pnpm clean:dist && pnpm build:lib && pnpm build:types",
    "lint": "eslint src/ --ext .ts",
    "generate": "node ./scripts/generate.mjs",
    "format-data": "prettier --log-level warn --write \"src/data/**/*.json\"",
    "prepublishOnly": "pnpm generate && pnpm build"
  },
  "devDependencies": {
    "@types/node": "^22.13.8",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "prettier": "^3.5.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.0"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
