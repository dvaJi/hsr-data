  "sections": [
    {
      "id": 1111700,
      "startingMessageId": [111170000],
      "messages": {
        "111170000": {
          "id": 111170000,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Herr Yang, bist du noch dran? Herr Yaaaang ...",
          "next": [111170003]
        },
        "111170003": {
          "id": 111170003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Ich bin schon die ganze Zeit hier. Was ist denn los, {NICKNAME}?",
          "next": [111170004, 111170005]
        },
        "111170004": {
          "id": 111170004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nimmt unser Express neue Mitglieder auf?",
          "next": [111170006]
        },
        "111170005": {
          "id": 111170005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich habe eine Freundin, die an Bord kommen möchte. Was meinst du, Herr Yang?",
          "next": [111170007]
        },
        "111170006": {
          "id": 111170006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Oh? Ich heiße sie natürlich gerne willkommen.",
          "next": [111170008]
        },
        "111170007": {
          "id": 111170007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Oh? Eine neue Freundschaft ist eine herrliche Sache. Ich heiße alle neuen Mitglieder gerne willkommen.",
          "next": [111170008]
        },
        "111170008": {
          "id": 111170008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Darf ich trotzdem etwas fragen?",
          "next": [111170009]
        },
        "111170009": {
          "id": 111170009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Ich bin neugierig, was für eine neue Freundin hast du denn kennengelernt, {NICKNAME}?",
          "next": [111170010, 111170011, 111170012]
        },
        "111170010": {
          "id": 111170010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sie ist ein Genie in Sachen Mechanik mit einem akribischen Verstand.",
          "next": [111170013]
        },
        "111170011": {
          "id": 111170011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sie ist ein Rockstar mit einem hemmungslosen Charakter.",
          "next": [111170014]
        },
        "111170012": {
          "id": 111170012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sie ist ... Ich weiß es auch nicht so recht.",
          "next": [111170015]
        },
        "111170013": {
          "id": 111170013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Dann hat sie wohl so einiges mit Himeko gemeinsam. Das wäre nicht schlecht.",
          "next": [111170016]
        },
        "111170014": {
          "id": 111170014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Oh? Kein übles Hobby. Ich bin auch durchaus interessiert.",
          "next": [111170016]
        },
        "111170015": {
          "id": 111170015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Das spielt keine Rolle. Ich vertraue ganz deinem Urteilsvermögen, was den Charakter einer Person betrifft.",
          "next": [111170016]
        },
        "111170016": {
          "id": 111170016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Jedenfalls möchte ich deine Freundin herzlich willkommen heißen.",
          "next": [111170017]
        },
        "111170017": {
          "id": 111170017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Allerdings liegt die Entscheidung nicht bei mir. Ich werde bei Himeko ein gutes Wort für dich einlegen.",
          "next": [111170018]
        },
        "111170018": {
          "id": 111170018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Keine Sorge. Wie ich sie kenne, sollte sie nichts dagegen haben.",
          "next": [111170019, 111170020]
        },
        "111170019": {
          "id": 111170019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Vielen Dank, Herr Yang.",
          "next": [111170021]
        },
        "111170020": {
          "id": 111170020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hurra, danke, Herr Yang!",
          "next": [111170021]
        },
        "111170021": {
          "id": 111170021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Du brauchst dich nicht zu bedanken. Das ist das Mindeste, was ich tun konnte.",
          "next": []
        }
      },
      "participatingContactsIds": [8001, 1004]
    }
  ]
}
