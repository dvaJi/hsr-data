        "130770010": {
          "id": 130770010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Hmm. Wenn ich 7. März nicht finden kann, werde ich mich an ihren Vormund wenden müssen.",
          "next": [
            130770011
          ]
        },
        "130770011": {
          "id": 130770011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Und das bist du!",
          "next": [
            130770012,
            130770015
          ]
        },
        "130770012": {
          "id": 130770012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Erstens bin ich nicht der Vormund von 7. März.",
