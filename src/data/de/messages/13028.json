  "sections": [
    {
      "id": 1302004,
      "startingMessageId": [130200401],
      "messages": {
        "130200401": {
          "id": 130200401,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ich und Huohuo waren heute bummeln und haben ein paar Bekannte von ihr getroffen.",
          "next": [130200402]
        },
        "130200402": {
          "id": 130200402,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Sie hat kaum ein Wort herausgebracht.",
          "next": [130200403]
        },
        "130200403": {
          "id": 130200403,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ihre Bekannten wussten auch nicht recht, was sie sagen sollten.",
          "next": [130200404]
        },
        "130200404": {
          "id": 130200404,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Also habe ich versucht, die unangenehme Situation zu überbrücken.",
          "next": [130200405]
        },
        "130200405": {
          "id": 130200405,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Und ich habe herausgefunden, dass sie alle sehr gesprächig sind.",
          "next": [130200406]
        },
        "130200406": {
          "id": 130200406,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Sushang hat sich am Schluss sogar mit ihnen zum Essen verabredet.",
          "next": [130200407]
        },
        "130200407": {
          "id": 130200407,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "[schockiert]",
          "next": [130200408]
        },
        "130200408": {
          "id": 130200408,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ich wusste, dass Sushangs sozialen Kompetenzen die zweitbesten nach meinen sind.",
          "next": [130200409, 130200410]
        },
        "130200409": {
          "id": 130200409,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich kann es mir schon vorstellen.",
          "next": [130200411]
        },
        "130200410": {
          "id": 130200410,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wie erwartet, Sushang hat ganz locker geschafft, was für Huohuo unmöglich war.",
          "next": [130200411]
        },
        "130200411": {
          "id": 130200411,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Sushang war fantastisch! Nicht so wie ich ...",
          "next": [130200412]
        },
        "130200412": {
          "id": 130200412,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Huohuo, mach dir nichts draus.",
          "next": [130200413]
        },
        "130200413": {
          "id": 130200413,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Geh in Zukunft öfter mit uns aus. Mit unserer Hilfe kannst du deine Sozialphobie bestimmt überwinden und das Gruppentier der Zehn-Fürsten-Kommission werden.",
          "next": [130200414]
        },
        "130200414": {
          "id": 130200414,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ähmmm ...",
          "next": [130200415, 130200417, 130200419]
        },
        "130200415": {
          "id": 130200415,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Huohuo als Gruppentier ... Was ist das für ein komischer Traum?",
          "next": [130200416]
        },
        "130200416": {
          "id": 130200416,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Das klingt nicht nach etwas Positivem.",
          "next": [130200422]
        },
        "130200417": {
          "id": 130200417,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wisst ihr, aus einer introvertierten Person eine extrovertierte zu machen ist ungefähr so, wie ein Sparschwein im Mechakranich-Kurierdienst anzustellen.",
          "next": [130200418]
        },
        "130200418": {
          "id": 130200418,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Kurz gesagt, ihr würdet Huohuo nur das Leben schwer machen.",
          "next": [130200422]
        },
        "130200419": {
          "id": 130200419,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Huohuo, für jeden Menschen kommt ein Zeitpunkt für Veränderungen, vielleicht ist jetzt deiner!",
          "next": [130200420]
        },
        "130200420": {
          "id": 130200420,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Jetzt?!",
          "next": [130200421]
        },
        "130200421": {
          "id": 130200421,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Nee nee ...",
          "next": [130200422]
        },
        "130200422": {
          "id": 130200422,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Danke, dass ihr euch um mich kümmert, aber ich möchte noch kein Gesellschaftstier sein ...",
          "next": [130200423]
        },
        "130200423": {
          "id": 130200423,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Aber ich möchte mit Menschen, mit denen ich vertraut bin, auf natürliche Weise ins Gespräch kommen.",
          "next": [130200424]
        },
        "130200424": {
          "id": 130200424,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Du schaffst das, Huohuo!",
          "next": [130200425]
        },
        "130200425": {
          "id": 130200425,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Also komm mit uns essen! Nur wir und die Leute, die wir vorhin getroffen haben. Ich helfe dir auch.",
          "next": [130200426]
        },
        "130200426": {
          "id": 130200426,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "?!",
          "next": [130200427]
        },
        "130200427": {
          "id": 130200427,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Der Benutzer, den Sie kontaktieren möchten, ist leider nicht erreichbar. Bitte versuchen Sie es später erneut.",
          "next": [130200428]
        },
        "130200428": {
          "id": 130200428,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ähmmm ...",
          "next": [130200429]
        },
        "130200429": {
          "id": 130200429,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ähmmm ...",
          "next": [130200430, 130200432]
        },
        "130200430": {
          "id": 130200430,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ähmmm ...",
          "next": [130200431]
        },
        "130200431": {
          "id": 130200431,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Das war zu viel für den Anfang.",
          "next": [130200434]
        },
        "130200432": {
          "id": 130200432,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sieht aus, als könnte Huohuo nicht kommen. Dann gehe ich jetzt essen.",
          "next": [130200433]
        },
        "130200433": {
          "id": 130200433,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "In dieser Gruppe sind 3 von 4 Leuten extrovertiert. Ich sag nicht, wer.",
          "next": [130200434]
        },
        "130200434": {
          "id": 130200434,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Anscheinend müssen wir es langsam angehen lassen.",
          "next": []
        }
      },
      "participatingContactsIds": [1206, 1217, 1210, 8001]
    }
  ]
}
