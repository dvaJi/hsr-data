      ],
      "messages": {
        "200510000": {
          "id": 200510000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "{NICKNAME}, SOS!",
          "next": [
            200510001
          ]
        },
        "200510001": {
          "id": 200510001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Es regnet und ich habe einen Wurf weißer Kätzchen ohne ihre Mutter gesehen. Was soll ich tun?",
          "next": [
            200510002,
            200510005,
            200510008
          ]
        },
        "200510002": {
          "id": 200510002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Vielleicht solltest du warten, bis ihre Mutter wiederkehrt? Sie könnte gerade nach Futter jagen.",
          "next": [
            200510003
          ]
        },
        "200510003": {
          "id": 200510003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Das dachte ich mir auch, deswegen habe ich einen Schirm aus geöltem Papier über sie gehalten.",
          "next": [
            200510004
          ]
        },
        "200510004": {
          "id": 200510004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Ich wage es nicht, sie woanders hinzubringen. Es wäre schade, wenn ihre Mutter wiederkehren und sie nicht finden würde.",
          "next": [
            200510011
          ]
        },
        "200510005": {
          "id": 200510005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bringe sie an einen Ort, an dem sie geschützt sind, sonst werden sie noch krank.",
          "next": [
            200510006
          ]
        },
        "200510006": {
          "id": 200510006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Ich wage es nicht, sie woanders hinzubringen. Es wäre schade, wenn ihre Mutter wiederkehren und sie nicht finden würde.",
          "next": [
            200510007
          ]
        },
        "200510007": {
          "id": 200510007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Zum Glück habe ich einen Papierschirm dabei. Ich kann ihn eine Weile über sie halten.",
          "next": [
            200510011
          ]
        },
        "200510008": {
          "id": 200510008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nimm sie mit nach Hause und werde ein Katzenvater.",
          "next": [
            200510009
          ]
        },
        "200510009": {
          "id": 200510009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Ich werde warten, bis ihre Mutter wiederkommt. Sie könnte gerade nach Futter jagen. Es wäre schade, wenn sie die Kätzchen nicht finden würde, wenn sie wiederkehrt.",
          "next": [
            200510010
          ]
        },
        "200510010": {
          "id": 200510010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Zum Glück habe ich einen Papierschirm dabei. Ich kann ihn eine Weile über sie halten.",
          "next": [
            200510011
          ]
        },
        "200510011": {
          "id": 200510011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Wenn ich sie mir so ansehe, seufz ...",
          "next": [
            200510012
          ]
        },
        "200510012": {
          "id": 200510012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Stimmt etwas nicht?",
          "next": [
            200510013
          ]
        },
        "200510013": {
          "id": 200510013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Ich habe nur gerade daran gedacht, dass ich meine eigentlichen Eltern nie gesehen habe. Ich weiß nicht einmal, ob sie mich einfach nicht wollten oder ob sie schon verstorben sind ...",
          "next": [
            200510014
          ]
        },
        "200510014": {
          "id": 200510014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Allerdings kann ich mich allgemein an nur wenige Dinge erinnern, also ... ist alles gut.",
          "next": [
            200510015
          ]
        },
        "200510015": {
          "id": 200510015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Das erste, an das ich mich erinnere, ist der Unterricht mit dem General.",
          "next": [
            200510016,
            200510018
          ]
        },
        "200510016": {
          "id": 200510016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das muss schwer für dich gewesen sein.",
          "next": [
            200510017
          ]
        },
        "200510017": {
          "id": 200510017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Ist schon gut. Ich glaube, für den General war es wohl noch schwerer.",
          "next": [
            200510020
          ]
        },
        "200510018": {
          "id": 200510018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Er hat ganz schön viel zu schaffen.",
          "next": [
            200510019
          ]
        },
        "200510019": {
          "id": 200510019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Ja, er muss sich so reingehängt haben.",
          "next": [
            200510020
          ]
        },
        "200510020": {
          "id": 200510020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Oh, die Katzenmutter ist wieder da.",
          "next": [
            200510021
          ]
        },
        "200510021": {
          "id": 200510021,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das ist gut!",
          "next": [
            200510022
          ]
        },
        "200510022": {
          "id": 200510022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Ich gehe jetzt, danke für’s Zuhören.",
          "next": [
            200510023
          ]
        },
        "200510023": {
          "id": 200510023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Denk dran, es geheim zu halten! Wir sind jetzt beide erwachsen, also sollte kein Schwur dafür nötig sein.",
          "next": []
        }
      },
