        },
        "200680016": {
          "id": 200680016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Vielleicht nicht schlecht, dass keine Nachrichten durchkommen. Endlich etwas Ruhe.",
