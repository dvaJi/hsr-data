        },
        "200680005": {
          "id": 200680005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Man kann wirklich auf das Jade-Tor hinauf?",
          "next": [200680006]
        },
        "200680006": {
          "id": 200680006,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[verwirrt]",
          "next": [200680007]
        },
        "200680007": {
          "id": 200680007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Na klar. In meinen jungen Jahren war ich oft dort oben.",
          "next": [200680011]
        },
        "200680008": {
          "id": 200680008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das Problem ist nur, wie kommt man wieder runter?",
          "next": [200680009]
        },
        "200680009": {
          "id": 200680009,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
