  "sections": [
    {
      "id": 2007000,
      "startingMessageId": [
        200700015
      ],
      "messages": {
        "200700002": {
          "id": 200700002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Unterhalte dich mit den Leuten.",
          "next": [
            200700003
          ]
        },
        "200700003": {
          "id": 200700003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Leider weiß ich nie, wie ich ein Gespräch anfangen soll ... Aber ich werde unweigerlich immer auf diesen Sarg angesprochen, den ich mit mir herumtrage.",
          "next": [
            200700008
          ]
        },
        "200700004": {
          "id": 200700004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mach, was dir gefällt. Spiel etwas, lies ein paar Bücher, genieße die Landschaft, was dir so einfällt.",
          "next": [
            200700005
          ]
        },
        "200700005": {
          "id": 200700005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Ich könnte die Bücher fertig lesen, die ich auf der Luofu ausgeborgt habe. Keine schlechte Idee.",
          "next": [
            200700008
          ]
        },
        "200700006": {
          "id": 200700006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Auf Reisen gibt es außer Schlafen nicht viel zu tun.",
          "next": [
            200700007
          ]
        },
        "200700007": {
          "id": 200700007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Was zählt, ist also das Ziel, nicht der Weg, was?",
          "next": [
            200700008
          ]
        },
        "200700008": {
          "id": 200700008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Verfolgst du diese Philosophie auch auf deinen Reisen, {NICKNAME}?",
          "next": [
            200700009,
            200700012
          ]
        },
        "200700009": {
          "id": 200700009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Natürlich. Ich mache das immer so.",
          "next": [
            200700010
          ]
        },
        "200700010": {
          "id": 200700010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Du hast offenbar Erfahrung auf dem Gebiet.",
          "next": [
            200700011
          ]
        },
        "200700011": {
          "id": 200700011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Danke für diesen aufrichtigen Rat.",
          "next": [
            200700014
          ]
        },
        "200700012": {
          "id": 200700012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Natürlich nicht. Ich würde das nie tun.",
          "next": [
            200700013
          ]
        },
        "200700013": {
          "id": 200700013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Du hast wohl gemeint, das würde eher zu mir passen?",
          "next": [
            200700014
          ]
        },
        "200700014": {
          "id": 200700014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Ich werde es versuchen.",
          "next": []
        },
        "200700015": {
          "id": 200700015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Hey {NICKNAME}!",
          "next": [
            200700016
          ]
        },
        "200700016": {
          "id": 200700016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Bitte entschuldige die Störung, aber ich wollte dich noch etwas fragen.",
          "next": [
            200700017
          ]
        },
        "200700017": {
          "id": 200700017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Wie ich bereits sagte, das Leben als wandernder Händler ist vor allem langwierig und langweilig ...",
          "next": [
            200700018
          ]
        },
        "200700018": {
          "id": 200700018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Hast du eine Idee, wie ich mir auf Reisen die Zeit vertreiben könnte, {NICKNAME}?",
          "next": [
            200700002,
            200700004,
            200700006
          ]
        }
      },
      "participatingContactsIds": [
        1203,
        8001
      ]
    }
  ]
}