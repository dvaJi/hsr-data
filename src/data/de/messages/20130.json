  ],
  "sections": [
    {
      "id": 2013000,
      "startingMessageId": [
        201300000
      ],
      "messages": {
        "201300000": {
          "id": 201300000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Ich habe ein neues Getränk namens „Spontane Verdampfung“. Hast du Lust, es zu probieren?",
          "next": [
            201300001,
            201300004
          ]
        },
        "201300001": {
          "id": 201300001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wenn ich es probiere, verdampfe ich dann spontan?",
          "next": [
            201300002
          ]
        },
        "201300002": {
          "id": 201300002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Ich kann unmöglich einen Menschen mit nur einem einzigen Getränk zum Verdampfen bringen.",
          "next": [
            201300003
          ]
        },
        "201300003": {
          "id": 201300003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Aber ich kann garantieren, dass du nicht enttäuscht sein wirst, was das Aroma und den Geschmack angeht.",
          "next": [
            201300009
          ]
        },
        "201300004": {
          "id": 201300004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "„Spontane Verdampfung“ ... Das klingt nach einem Getränk, das es nicht gibt.",
          "next": [
            201300005
          ]
        },
        "201300005": {
          "id": 201300005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Deine Beschreibung passt ganz gut zu diesem Getränk.",
          "next": [
            201300006
          ]
        },
        "201300006": {
          "id": 201300006,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "next": [
            201300007
          ]
        },
        "201300007": {
          "id": 201300007,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Also trinkt man Luft???",
          "next": [
            201300008
          ]
        },
        "201300008": {
          "id": 201300008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Kunden zu betrügen ist nicht mein Stil.",
          "next": [
            201300009
          ]
        },
        "201300009": {
          "id": 201300009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "An Orten wie Penacony findet man alle möglichen Getränkezutaten. Manchmal ist es schwer, den Geschmack zu beschreiben.",
          "next": [
            201300010
          ]
        },
        "201300010": {
          "id": 201300010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Vor der Kreation von Spontane Verdampfung habe ich schon was weiß ich wie viele „einzigartige“ Getränke gekostet.",
          "next": [
            201300011
          ]
        },
        "201300011": {
          "id": 201300011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bitte gibt mir ein Beispiel für den einzigartigen Geschmack eines Getränks.",
          "next": [
            201300012
          ]
        },
        "201300012": {
          "id": 201300012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Es gibt so viele Beispiele.",
          "next": [
            201300013
          ]
        },
        "201300013": {
          "id": 201300013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "In letzter Zeit haben die Getränke „Exil und Festmahl“ und „Verborgenes Talent“ einen wirklich tiefen Eindruck hinterlassen.",
          "next": [
            201300014,
            201300018
          ]
        },
        "201300014": {
          "id": 201300014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Erzähl mir von „Exil und Festmahl“.",
          "next": [
            201300015
          ]
        },
        "201300015": {
          "id": 201300015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Ich wollte zuerst einen extravaganten Geschmack erschaffen, aber ich hätte nicht gedacht, dass der industrielle Sirup so süß sein würde, dass er den Rest des Geschmacks überdeckt.",
          "next": [
            201300016
          ]
        },
        "201300016": {
          "id": 201300016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Das wollte ich dann wieder gutmachen, also fügte ich etwas bitteren Geschmack hinzu.",
          "next": [
            201300017
          ]
        },
        "201300017": {
          "id": 201300017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Ich hätte nicht gedacht, dass die Zutat so stark sein würde ... Nach einem Schluck ging mir die Bitterkeit bis in die Knochen.",
          "next": [
            201300022
          ]
        },
        "201300018": {
          "id": 201300018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Erzähl mir von „Verborgenes Talent“.",
          "next": [
            201300019
          ]
        },
        "201300019": {
          "id": 201300019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Das ursprüngliche Ziel mit diesem Getränk war es, die Gäste in den Eisstückchen nach einem Hauch von Süßsauer suchen zu lassen.",
          "next": [
            201300020
          ]
        },
        "201300020": {
          "id": 201300020,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Nach dem Motto, in Glassplittern nach Süßigkeiten suchen?",
          "next": [
            201300021
          ]
        },
        "201300021": {
          "id": 201300021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "So ungefähr, aber wenn ich ein Glas davon trinke, finde ich das Saure oft nicht. Stattdessen ist es so langweilig wie ein Glas Eiswasser.",
          "next": [
            201300022
          ]
        },
        "201300022": {
          "id": 201300022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Im Gegensatz dazu ist Spontane Verdampfung sehr auffällig.",
          "next": [
            201300023
          ]
        },
        "201300023": {
          "id": 201300023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Seine Basis ist SeelFroh, dazu kommen noch die Zutaten „Süße Luft“ und „Reminiszenz“.",
          "next": [
            201300024
          ]
        },
        "201300024": {
          "id": 201300024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Schönheit und Bitterkeit vermischen sich nach einem Schluck wie eine Kostprobe aus einem gewöhnlichen Leben.",
          "next": [
            201300025
          ]
        },
        "201300025": {
          "id": 201300025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Jedes Mal, wenn man versucht, sich an etwas zu erinnern und daran festzuhalten, bleibt nur ein Nachgeschmack von flüchtiger Leere.",
          "next": [
            201300026,
            201300028
          ]
        },
        "201300026": {
          "id": 201300026,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Okay, okay, okay, ich komme schon und koste das Leben!",
          "next": [
            201300027
          ]
        },
        "201300027": {
          "id": 201300027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Wir sehen uns in der Taverne.",
          "next": []
        },
        "201300028": {
          "id": 201300028,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das ist doch genau jetzt mein Leben! Muss ich zum Kosten extra kommen?",
          "next": [
            201300029
          ]
        },
        "201300029": {
          "id": 201300029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Vertrau mir, du wirst nicht enttäuscht sein.",
          "next": [
            201300030
          ]
        },
        "201300030": {
          "id": 201300030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "Wir sehen uns in der Taverne.",
          "next": []
        }
      },
      "participatingContactsIds": [
        1301,
        8001
      ]
    }
  ]
}