        },
        "200380008": {
          "id": 200380008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "Freu dich nicht zu früh.",
          "next": [200380009]
        },
        "200380009": {
          "id": 200380009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "Aber wenn du mitkommst, werde ich dir einen guten Platz reservieren.",
          "next": [200380010]
        },
        "200380010": {
          "id": 200380010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "Vergiss nicht, für uns Blumen zu besorgen. Du wirst vermutlich auf die Bühne gerufen.",
          "next": [200380011]
        },
        "200380011": {
          "id": 200380011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "?",
