  "sections": [
    {
      "id": 2016800,
      "startingMessageId": [
        201680000
      ],
      "messages": {
        "201680000": {
          "id": 201680000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Hey! Baseballschläger-Ninja!",
          "next": [
            201680001
          ]
        },
        "201680001": {
          "id": 201680001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Du magst über jede Menge Ningu verfügen, um gegen alle Arten des Bösen zu bestehen.",
          "next": [
            201680002
          ]
        },
        "201680002": {
          "id": 201680002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Aber fehlt es dir nicht an einer Möglichkeit, die Waffen schnell zu wechseln?",
          "next": [
            201680003
          ]
        },
        "201680003": {
          "id": 201680003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Ich habe dafür ein Ningu erschaffen und lade alle Ninjas ein, es zu testen.",
          "next": [
            201680004,
            201680006
          ]
        },
        "201680004": {
          "id": 201680004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich dachte nicht, dass du eine Bastlerin bist!",
          "next": [
            201680005
          ]
        },
        "201680005": {
          "id": 201680005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Keine Sorge, meine Werkzeuge sind von mir persönlich handgefertigt und ihr Einsatz stellt kein Risiko dar.",
          "next": [
            201680007
          ]
        },
        "201680006": {
          "id": 201680006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Baseballschläger-Ninja kommt!",
          "next": [
            201680007
          ]
        },
        "201680007": {
          "id": 201680007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Wenn ein Ninja durch die Welten reist, muss er wachsam seine Umgebung im Blick haben – Körper, Werkzeug, Wort und Wille müssen eine Einheit sein.",
          "next": [
            201680008
          ]
        },
        "201680008": {
          "id": 201680008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Ningu sollten auch durch Ninjakünste geschmiedet, durch den Ninjageist kanalisiert und mittels Ninjutsu eingesetzt werden.",
          "next": [
            201680009
          ]
        },
        "201680009": {
          "id": 201680009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Deine Hymne der Regelbrecher hat einen tiefen Eindruck hinterlassen.",
          "next": [
            201680010
          ]
        },
        "201680010": {
          "id": 201680010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Die Idee für die Entwicklung dieses Werkzeugs kam mir mitten in der Nacht.",
          "next": [
            201680011,
            201680013
          ]
        },
        "201680011": {
          "id": 201680011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Die Hymne der Regelbrecher? Du meinst „Regeln sind dazu da, gebrochen zu werden“?",
          "next": [
            201680012
          ]
        },
        "201680012": {
          "id": 201680012,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Warum habe ich dieses ungute Gefühl?",
          "next": [
            201680015
          ]
        },
        "201680013": {
          "id": 201680013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Regeln sind dazu da, gebrochen zu werden.",
          "next": [
            201680014
          ]
        },
        "201680014": {
          "id": 201680014,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Dieser Moment lässt nichts Gutes erahnen.",
          "next": [
            201680015
          ]
        },
        "201680015": {
          "id": 201680015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Die Baseballschläger-Spritze",
          "next": [
            201680016
          ]
        },
        "201680016": {
          "id": 201680016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "... Mehrzweck-Ningu!",
          "next": [
            201680017
          ]
        },
        "201680017": {
          "id": 201680017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Der Abzug ist im Schläger versteckt, einfach die hutförmige Visierkappe aufklappen.",
          "next": [
            201680018
          ]
        },
        "201680018": {
          "id": 201680018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "... Ninja-Zeremonienspiegel!",
          "next": [
            201680019
          ]
        },
        "201680019": {
          "id": 201680019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Beobachte im Spiegel die leuchtenden Bänder, halte die Zeit an und tauche ein in den Rhythmus des Kampfes.",
          "next": [
            201680020
          ]
        },
        "201680020": {
          "id": 201680020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "... Ninja-Fantasie!",
          "next": [
            201680021
          ]
        },
        "201680021": {
          "id": 201680021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "In der Spitze des Schlägers ist eine flammenförmige Vertiefung eingelassen, aus der Farben zu einem Regenbogen verspritzt werden können.",
          "next": [
            201680022
          ]
        },
        "201680022": {
          "id": 201680022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "... Ninja-Flammenpracht!",
          "next": [
            201680023
          ]
        },
        "201680023": {
          "id": 201680023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Nur ein einziger Schwung mit dieser Feuerlanze aus dem Schläger und schon entzünden sich lodernde Ninja-Talismane!",
          "next": [
            201680024,
            201680025
          ]
        },
        "201680024": {
          "id": 201680024,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Jetzt, wo wir für die richtige Stimmung gesorgt haben, kann das Ensemble endlich loslegen!",
          "next": [
            201680026
          ]
        },
        "201680025": {
          "id": 201680025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Damit kann ich gegen acht kämpfen!",
          "next": [
            201680026
          ]
        },
        "201680026": {
          "id": 201680026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Passend zu deiner Hymne habe ich eigens einen sprachgesteuerten Auslöser entwickelt.",
          "next": [
            201680027
          ]
        },
        "201680027": {
          "id": 201680027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Richte ihn auf einen feindlichen Ninja und singe deine besondere Hymne, um den Strahl auszulösen!",
          "next": [
            201680028
          ]
        },
        "201680028": {
          "id": 201680028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Abpraller-Ninjutsu: Sternen-Trailblaze, Feuerwind-Orchester: Kosmischer Jäger",
          "next": [
            201680029
          ]
        },
        "201680029": {
          "id": 201680029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Baseballschläger-Ninja, versuche zu schreien, um mehr Größe als Ninja zur Schau zu stellen.",
          "next": [
            201680030
          ]
        },
        "201680030": {
          "id": 201680030,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1317,
          "next": []
        }
      },
