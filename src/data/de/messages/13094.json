        "130940000": {
          "id": 130940000,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "„Nachricht“-Baby hat dir eine Nachrichtenanfrage geschickt.",
          "next": [
            130940001
          ]
        },
        "130940001": {
          "id": 130940001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Cool",
          "next": [
            130940002
          ]
        },
        "130940002": {
          "id": 130940002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Es fühlt sich so cool an, findest du nicht?",
          "next": [
            130940003,
            130940008,
            130940013
          ]
        },
        "130940003": {
          "id": 130940003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hmm? Noch mehr Bomben, die zu entschärfen sind?",
          "next": [
            130940004
          ]
        },
        "130940004": {
          "id": 130940004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Gib mir die Koordinaten, und ich gehe die Bombe entschärfen.",
          "next": [
            130940005
          ]
        },
        "130940005": {
          "id": 130940005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Es gibt hier nur 1.000 Bomben. Frau Sparkle hält immer ihr Versprechen!",
          "next": [
            130940006
          ]
        },
        "130940006": {
          "id": 130940006,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "(Bild) [Weinende Maske]",
          "next": [
            130940007
          ]
        },
        "130940007": {
          "id": 130940007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Ich bin nur hier, um Frau Sparkles Motiv zu verraten. Verstehst du?",
          "next": [
            130940017,
            130940018
          ]
        },
        "130940008": {
          "id": 130940008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hey!",
          "next": [
            130940009
          ]
        },
        "130940009": {
          "id": 130940009,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Schick mir keine Nachrichten mehr. Ich fürchte, Pom-Pom wird das falsch verstehen.",
          "next": [
            130940010
          ]
        },
        "130940010": {
          "id": 130940010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Willst du mich verärgern? Ich bin wie Kaugummi. Tritt auf mich und du wirst mich nie wieder los!",
          "next": [
            130940011
          ]
        },
        "130940011": {
          "id": 130940011,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "(Bild) [Freches Lachen]",
          "next": [
            130940012
          ]
        },
        "130940012": {
          "id": 130940012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Außerdem hat Frau Sparkle ihr kriminelles Motiv noch nicht verraten! Bist du nicht neugierig?",
          "next": [
            130940017,
            130940018
          ]
        },
        "130940013": {
          "id": 130940013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Feuerwerk! Toll!",
          "next": [
            130940014
          ]
        },
        "130940014": {
          "id": 130940014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Ja! Jetzt kapierst du es endlich, oder?",
          "next": [
            130940015
          ]
        },
        "130940015": {
          "id": 130940015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Bist du also neugierig, wofür Frau Sparkle das alles hier macht?",
          "next": [
            130940016
          ]
        },
        "130940016": {
          "id": 130940016,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "(Bild) [Feuerzeug]",
          "next": [
            130940017,
            130940018
          ]
        },
        "130940017": {
          "id": 130940017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Erklär es uns.",
          "next": [
            130940021
          ]
        },
        "130940018": {
          "id": 130940018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich gehe offline, wir reden später weiter.",
          "next": [
            130940019
          ]
        },
        "130940019": {
          "id": 130940019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Nein, geh nicht offline! Bitte! *schluchz*",
          "next": [
            130940020
          ]
        },
        "130940020": {
          "id": 130940020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Haha, Scherz ... Machen wir weiter!",
          "next": [
            130940021
          ]
        },
        "130940021": {
          "id": 130940021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Erinnerst du dich an die vorhergesagten „Drei Todesfälle“?",
          "next": [
            130940022
          ]
        },
        "130940022": {
          "id": 130940022,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Was kümmert dich das?",
          "next": [
            130940023
          ]
        },
        "130940023": {
          "id": 130940023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Wer weiß? Ich kann nur sagen, dass jemand ein Vermögen bezahlt hat, um Frau Sparkle als Regisseurin auf Penacony einzuladen.",
          "next": [
            130940024
          ]
        },
        "130940024": {
          "id": 130940024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Es scheint ein Freund zu sein, den sie im Videospiel kennengelernt hat. Diese Person hat Frau Sparkle aufdringlich angewiesen, die „Drei Todesfälle“ auf die möglichst sicherste Weise passieren zu lassen.",
          "next": [
            130940025
          ]
        },
        "130940025": {
          "id": 130940025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Im Gegenzug hat er auch sein „Skript“ mit Frau Sparkle geteilt.",
          "next": [
            130940026
          ]
        },
        "130940026": {
          "id": 130940026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Und was danach geschieht ... Du bist doch so schlau, sicher kannst du es erraten, Grauschopf!",
          "next": [
            130940027
          ]
        },
        "130940027": {
          "id": 130940027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Allerdings ist Frau Sparkle nicht die manipulative Art von Regisseurin. Sie hat beschlossen, dass die Schauspieler selbst die Richtung des Drehbuchs wählen sollen.",
          "next": [
            130940028
          ]
        },
        "130940028": {
          "id": 130940028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Überraschungsfrage! Hast du Frau Sparkles „Knopf für gegenseitig zugesicherte Zerstörung“ gedrückt?",
          "next": [
            130940029,
            130940031,
            130940034
          ]
        },
        "130940029": {
          "id": 130940029,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ja, ich habe ihn wie wild gedrückt.",
          "next": [
            130940030
          ]
        },
        "130940030": {
          "id": 130940030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Dann herzlichen Glückwunsch! Du hast den „dritten Todesfall“ mit deinen eigenen Händen neu geschrieben!",
          "next": [
            130940037
          ]
        },
        "130940031": {
          "id": 130940031,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Der war zu verdächtig! Ich habe ihn nicht gedrückt.",
          "next": [
            130940032
          ]
        },
        "130940032": {
          "id": 130940032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Hmm, du bist ziemlich vorsichtig.",
          "next": [
            130940033
          ]
        },
        "130940033": {
          "id": 130940033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Aber das ist in Ordnung. Weil mehr als die Hälfte der Schauspieler den Knopf gedrückt hat, verlief „der dritte Todesfall“ genau wie geplant!",
          "next": [
            130940037
          ]
        },
        "130940034": {
          "id": 130940034,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich habe den Knopf überhaupt nicht bekommen!",
          "next": [
            130940035
          ]
        },
        "130940035": {
          "id": 130940035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Wirklich? Dann hoffe ich, dass du es beim nächsten Mal schaffst.",
          "next": [
            130940036
          ]
        },
        "130940036": {
          "id": 130940036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Aber das ist in Ordnung. Weil mehr als die Hälfte der Schauspieler den Knopf gedrückt hat, verlief „der dritte Todesfall“ genau wie geplant!",
          "next": [
            130940037
          ]
        },
        "130940037": {
          "id": 130940037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Sie flog hoch in den Himmel und entzündete das größte Feuerwerk in der Geschichte von Penacony, um dem süßen Traum ein grandioses Finale zu geben.",
          "next": [
            130940038
          ]
        },
        "130940038": {
          "id": 130940038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Und sie selbst ist auch gesund und munter. Was für ein glückliches Ende!",
          "next": [
            130940039
          ]
        },
        "130940039": {
          "id": 130940039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Also, herzlichen Glückwunsch, Grauschopf. Schließlich bist du {F#diejenige, die}{M#derjenige, der} die Entscheidung getroffen und sogar Glühwürmchen gerettet hat.",
          "next": [
            130940040
          ]
        },
        "130940040": {
          "id": 130940040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Und das bedeutet, dass Frau Sparkle auch wie versprochen ihre Belohnung erhält!",
          "next": [
            130940041
          ]
        },
        "130940041": {
          "id": 130940041,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Oh.",
          "next": [
            130940042
          ]
        },
        "130940042": {
          "id": 130940042,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Übrigens, zur Feier des großen Finales hat Frau Sparkle beschlossen, dir ein weiteres Geheimnis zu verraten.",
          "next": [
            130940043,
            130940044
          ]
        },
        "130940043": {
          "id": 130940043,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was ist es?",
          "next": [
            130940045
          ]
        },
        "130940044": {
          "id": 130940044,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Okay, ich bin ganz Ohr!",
          "next": [
            130940045
          ]
        },
        "130940045": {
          "id": 130940045,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Weißt du ...",
          "next": [
            130940046
          ]
        },
        "130940046": {
          "id": 130940046,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Also eigentlich ...",
          "next": [
            130940047
          ]
        },
        "130940047": {
          "id": 130940047,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "... ist Dr. Primitiv ein großer Fan von Bananen.",
          "next": [
            130940048,
            130940049,
            130940050
          ]
        },
        "130940048": {
          "id": 130940048,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was?",
          "next": [
            130940051
          ]
        },
        "130940049": {
          "id": 130940049,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was meinst du?",
          "next": [
