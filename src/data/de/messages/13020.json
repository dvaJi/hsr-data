        },
        "130200335": {
          "id": 130200335,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich hab die Streams geguckt. Die kleine Gui hat so was nie gesagt.",
          "next": [130200336]
        },
        "130200336": {
          "id": 130200336,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Du bist der einzige echte Fan.",
