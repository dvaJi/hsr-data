        "130200337": {
          "id": 130200337,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Eigentlich würde das Thema doch auch gutes Stream-Material hergeben ...",
          "next": [130200338]
        },
        "130200338": {
          "id": 130200338,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Hmpf, in meinem Stream heute Abend werde ich diesen Gerüchten entschieden ein Ende setzen.",
