        },
        "130200327": {
          "id": 130200327,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ähm, genau.",
          "next": [130200328]
        },
        "130200328": {
          "id": 130200328,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Also, stimmt das?",
