        },
        "130200304": {
          "id": 130200304,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "[Akanbe]",
          "next": [130200305]
        },
        "130200305": {
          "id": 130200305,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ich bin heute zur Aurum-Allee gegangen und alle haben über die kleine Gui geredet.",
          "next": [130200306, 130200309, 130200311]
        },
        "130200306": {
          "id": 130200306,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich hatte schon gehört, dass sie berühmt ist, aber ich habe es nicht wirklich gespürt.",
          "next": [130200307]
        },
        "130200307": {
          "id": 130200307,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Sieht aus, als wäre es echt so.",
          "next": [130200308]
        },
        "130200308": {
          "id": 130200308,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ich verdonnere dich hiermit dazu, mehr Zeit im Internet zu verbringen.",
          "next": [130200314]
        },
        "130200309": {
          "id": 130200309,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sicher, dass du dich nicht verhört hast, Sushang? Vielleicht haben sie über etwas anderes geredet, zum Beispiel über „Gummireifen“?",
          "next": [130200310]
        },
        "130200310": {
          "id": 130200310,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Kann nicht sein, bestimmt haben sie über mich geredet!",
          "next": [130200314]
        },
        "130200311": {
          "id": 130200311,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Dann bist du jetzt also ein großer Name?",
          "next": [130200312]
        },
        "130200312": {
          "id": 130200312,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Pff, großer Name. Guinaifen ist einfach nur ein Name.",
          "next": [130200313]
        },
        "130200313": {
          "id": 130200313,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Rede so weiter und ich platze noch vor Stolz, hahaha ...",
          "next": [130200314]
        },
        "130200314": {
          "id": 130200314,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Was haben sie eigentlich so gesagt?",
          "next": [130200315]
        },
        "130200315": {
          "id": 130200315,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Sie haben gesagt, die kleine Gui hätte im Stream angekündigt, dass sie alle Aktivitäten einstellen will, und sie wollten den Grund erraten.",
          "next": [130200316]
        },
        "130200316": {
          "id": 130200316,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Hä?!",
          "next": [130200317]
        },
        "130200317": {
          "id": 130200317,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Hä?!",
          "next": [130200318]
        },
        "130200318": {
          "id": 130200318,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Hä?!",
          "next": [130200319, 130200321, 130200323]
        },
        "130200319": {
          "id": 130200319,
          "type": "Text",
          "sender": "Player",
