        },
        "130200323": {
          "id": 130200323,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Der Spruch gefällt mir: Hä?!",
          "next": [130200324]
        },
        "130200324": {
          "id": 130200324,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Du weißt also auch nichts davon.",
