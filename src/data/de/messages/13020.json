  "relatedMessages": [12182, 12195, 12201, 12206, 12207, 12208, 13020],
  "sections": [
    {
      "id": 1302000,
      "startingMessageId": [130200000],
      "messages": {
        "130200000": {
          "id": 130200000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ähm, heute gibt es nichts zu berichten.",
          "next": [130200001]
        },
        "130200001": {
          "id": 130200001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Es ist echt ruhig. In der Gruppe ist auch tote Hose, das kenn ich so gar nicht von anderen Gruppenchats.",
          "next": [130200002]
        },
        "130200002": {
          "id": 130200002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Hm, vielleicht sind alle in der Gruppe mit anderen Sachen beschäftigt.",
          "next": [130200003]
        },
        "130200003": {
          "id": 130200003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Oh, beschäftigt, alle beschäftigt, beschäftigt ist gut ...",
          "next": [130200004]
        },
        "130200004": {
          "id": 130200004,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "[weint]",
          "next": [130200005]
        },
        "130200005": {
          "id": 130200005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Kleine Gui ...",
          "next": [130200006]
        },
        "130200006": {
          "id": 130200006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "@{NICKNAME} @Huohuo Kümmert euch bitte mal schnell um unsere einsame kleine Gui.",
          "next": [130200007]
        },
        "130200007": {
          "id": 130200007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Was was was ist passiert?",
          "next": [130200008]
        },
        "130200008": {
          "id": 130200008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Die sind beide nur auf der Suche nach jemandem zum Chatten ...",
          "next": [130200009]
        },
        "130200009": {
          "id": 130200009,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "[okay]",
          "next": [130200010]
        },
        "130200010": {
          "id": 130200010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ach so.",
          "next": [130200011, 130200016, 130200026]
        },
        "130200011": {
          "id": 130200011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das ist der Geisterjägertrupp-Chat.",
          "next": [130200012]
        },
        "130200012": {
          "id": 130200012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Verstehe. Wir sollen also abhauen.",
          "next": [130200013]
        },
        "130200013": {
          "id": 130200013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Nein nein nein, hör nicht auf {F#sie}{M#ihn}, Shangshang!",
          "next": [130200014]
        },
        "130200014": {
          "id": 130200014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Es ist doch logisch, dass der Geisterjägertrupp nicht über das Geisterjagen spricht.",
          "next": [130200015]
        },
        "130200015": {
          "id": 130200015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Das ist logisch?!",
          "next": [130200030]
        },
        "130200016": {
          "id": 130200016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich glaube, das ist ein Verzweiflungsakt aus Langeweile. Beachte die beiden nicht, Huohuo.",
          "next": [130200017]
        },
        "130200017": {
          "id": 130200017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Okay, ich beachte sie nicht.",
          "next": [130200018]
        },
        "130200018": {
          "id": 130200018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Oh nein!",
          "next": [130200019]
        },
        "130200019": {
          "id": 130200019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Neinneinnein!",
          "next": [130200020]
        },
        "130200020": {
          "id": 130200020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Shangshang, was sollen wir machen?",
          "next": [130200021]
        },
        "130200021": {
          "id": 130200021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Hätte ich das vorher gewusst, hätte ich es verhindert (TдT)",
          "next": [130200022]
        },
        "130200022": {
          "id": 130200022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Die kleine Gui wollte mich unbedingt mit reinziehen (TдT)",
          "next": [130200023]
        },
        "130200023": {
          "id": 130200023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Es tut mir leid, dass ich sie nicht aufhalten konnte (TдT)",
          "next": [130200024]
        },
        "130200024": {
          "id": 130200024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Huohuo, bist du sauer? (TдT) Bestimmt, oder? (TдT)",
          "next": [130200025]
        },
        "130200025": {
          "id": 130200025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ich ... Ich bin nicht sauer! Mach dir doch nicht solche Sorgen!",
          "next": [130200030]
        },
        "130200026": {
          "id": 130200026,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich will auch chatten. Lasst uns mal so richtig chatten.",
          "next": [130200027]
        },
        "130200027": {
          "id": 130200027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Auf dich ist Verlass!",
          "next": [130200028]
        },
        "130200028": {
          "id": 130200028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Und Huohuo?",
          "next": [130200029]
        },
        "130200029": {
          "id": 130200029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ich hätte nicht gedacht, dass Frau Guinaifen extra nachfragen würde.",
          "next": [130200030]
        },
        "130200030": {
          "id": 130200030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Der Geisterjägertrupp soll ja auch keine seriöse Arbeitsgruppe sein.",
          "next": [130200031]
        },
        "130200031": {
          "id": 130200031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ihr könnt chatten, worüber ihr wollt.",
          "next": [130200032]
        },
        "130200032": {
          "id": 130200032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Yeah!",
          "next": [130200033]
        },
        "130200033": {
          "id": 130200033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Yeah!",
          "next": [130200034]
        },
        "130200034": {
          "id": 130200034,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Yeah!",
          "next": [130200035]
        },
        "130200035": {
          "id": 130200035,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[super]",
          "next": []
        }
      },
      "participatingContactsIds": [1210, 1206, 1217, 8001]
    },
    {
      "id": 1302001,
      "startingMessageId": [130200101],
      "messages": {
        "130200101": {
          "id": 130200101,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ich empfehle euch mal meine Entspannungsmethode.",
          "next": [130200102]
        },
        "130200102": {
          "id": 130200102,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Sucht euch am Hafen der Sternskiffe einen Platz, von dem aus ihr die Skiffe sehen könnt, und chillt!",
          "next": [130200103]
        },
        "130200103": {
          "id": 130200103,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "... Im Teehaus Schlafloser Graf geht das natürlich auch.",
          "next": [130200104]
        },
        "130200104": {
          "id": 130200104,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Die vorbeiziehenden Sternskiffe beobachten ...",
          "next": [130200105]
        },
        "130200105": {
          "id": 130200105,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Man merkt gar nicht, wie die Zeit vergeht ...",
          "next": [130200106]
        },
        "130200106": {
          "id": 130200106,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Das ist sehr wohltuend.",
          "next": [130200107]
        },
        "130200107": {
          "id": 130200107,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Versteh ich nicht.",
          "next": [130200108]
        },
        "130200108": {
          "id": 130200108,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Versteh ich nicht.",
          "next": [130200109, 130200112]
        },
        "130200109": {
          "id": 130200109,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Versteh ich nicht.",
          "next": [130200110]
        },
        "130200110": {
          "id": 130200110,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ich mache mir etwas Sorgen um Huohuos psychischen Zustand.",
          "next": [130200111]
        },
        "130200111": {
          "id": 130200111,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Wir probieren es mal. Aber Huohuo, wenn du etwas auf dem Herzen hast, kannst du es uns erzählen, ja?",
          "next": [130200117]
        },
        "130200112": {
          "id": 130200112,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wie dem Regen zuzuhören, um Stress abzubauen. Das ist eine ganz ähnliche Methode.",
          "next": [130200113]
        },
        "130200113": {
          "id": 130200113,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Das kann ich voll nachvollziehen.",
          "next": [130200114]
        },
        "130200114": {
          "id": 130200114,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Genau!",
          "next": [130200115]
        },
        "130200115": {
          "id": 130200115,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ich mache mir etwas Sorgen um den psychischen Zustand der beiden.",
          "next": [130200116]
        },
        "130200116": {
          "id": 130200116,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Wenn ihr etwas auf dem Herzen habt, könnt ihr es uns erzählen, ja?",
          "next": [130200117]
        },
        "130200117": {
          "id": 130200117,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ich sitze hier schon den halben Tag, es ist echt angenehm.",
          "next": [130200118]
        },
        "130200118": {
          "id": 130200118,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Oh, Moment!",
          "next": [130200119]
        },
        "130200119": {
          "id": 130200119,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Hm?",
          "next": [130200120]
        },
        "130200120": {
          "id": 130200120,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ist was passiert?",
          "next": [130200121]
        },
        "130200121": {
          "id": 130200121,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[verwirrt]",
          "next": [130200122]
        },
        "130200122": {
          "id": 130200122,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Bin wieder da.",
          "next": [130200123]
        },
        "130200123": {
          "id": 130200123,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Herr Xiyan ist vorbeigekommen, hat mir ohne was zu sagen eine Handvoll Süßigkeiten gegeben und ist wieder gegangen.",
          "next": [130200124]
        },
        "130200124": {
          "id": 130200124,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "W... Was ist denn da los?",
          "next": [130200125, 130200126]
        },
        "130200125": {
          "id": 130200125,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das zeigt, dass Huohuo sehr süß ist. Wenn ich in der Nähe wäre, würde ich dir auch Süßigkeiten geben.",
          "next": [130200128]
        },
        "130200126": {
          "id": 130200126,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wie du da die ganze Zeit den Sternskiffen hinterhergeschaut hast, sahst du wahrscheinlich echt armselig aus.",
          "next": [130200127]
        },
        "130200127": {
          "id": 130200127,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Jeder freundliche Mensch würde etwas für dich tun wollen.",
          "next": [130200128]
        },
        "130200128": {
          "id": 130200128,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "So wie man es nicht lassen kann, die Koifische im Teich zu füttern?",
          "next": [130200129]
        },
        "130200129": {
          "id": 130200129,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ist Füttern dort nicht verboten?",
          "next": [130200130]
        },
        "130200130": {
          "id": 130200130,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Huohuo füttern ist ja wohl erlaubt.",
          "next": [130200131]
        },
        "130200131": {
          "id": 130200131,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ach so.",
          "next": [130200132]
        },
        "130200132": {
          "id": 130200132,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "... Sind wir abgeschweift?",
          "next": [130200133]
        },
        "130200133": {
          "id": 130200133,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ich hab auch Süßigkeiten. Huohuo, wo bist du?",
          "next": [130200134]
        },
        "130200134": {
          "id": 130200134,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ich hab auch Süßigkeiten! Los geht’s!",
          "next": [130200135]
        },
        "130200135": {
          "id": 130200135,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[Los!]",
          "next": [130200136]
        },
        "130200136": {
          "id": 130200136,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ihr müsst nicht kommen, ich bin schon gegangen.",
          "next": [130200137]
        },
        "130200137": {
          "id": 130200137,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Aah ...",
          "next": []
        }
      },
      "participatingContactsIds": [1217, 1210, 1206, 8001]
    },
    {
      "id": 1302002,
      "startingMessageId": [130200201],
      "messages": {
        "130200201": {
          "id": 130200201,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Wer will Milchtee? Ich hab gerade noch zwei bestellt. Wer zuerst kommt, mahlt zuerst.",
          "next": [130200202]
        },
        "130200202": {
          "id": 130200202,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ich ich ich",
          "next": [130200203]
        },
        "130200203": {
          "id": 130200203,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ich ich ich",
          "next": [130200204]
        },
        "130200204": {
          "id": 130200204,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Okay, also ihr zwei.",
          "next": [130200205, 130200208, 130200212]
        },
        "130200205": {
          "id": 130200205,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich ich ich",
          "next": [130200206]
        },
        "130200206": {
          "id": 130200206,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Kann ich auch noch?",
          "next": [130200207]
        },
        "130200207": {
          "id": 130200207,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Nö.",
          "next": [130200216]
        },
        "130200208": {
          "id": 130200208,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "War ich so spät dran?",
          "next": [130200209]
        },
        "130200209": {
          "id": 130200209,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ihr starrt doch wohl nicht die ganze Zeit auf eure Jade-Abakusse?",
          "next": [130200210]
        },
        "130200210": {
          "id": 130200210,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ich doch nicht.",
          "next": [130200211]
        },
        "130200211": {
          "id": 130200211,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ich auch nicht.",
          "next": [130200216]
        },
        "130200212": {
          "id": 130200212,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kleine Gui ...",
          "next": [130200213]
        },
        "130200213": {
          "id": 130200213,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Du lässt drei Leute um zwei Milchtees kämpfen und bringst so unsere Freundschaft in Gefahr.",
          "next": [130200214]
        },
        "130200214": {
          "id": 130200214,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Wie willst du das wieder gutmachen?",
          "next": [130200215]
        },
        "130200215": {
          "id": 130200215,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "So dramatisch war das jetzt auch wieder nicht.",
          "next": [130200216]
        },
        "130200216": {
          "id": 130200216,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Tut mir leid, dass du diesmal keinen kriegst. Ich lade dich das nächste Mal ein.",
          "next": [130200217, 130200219]
        },
        "130200217": {
          "id": 130200217,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Oh, jetzt bist du dran. Ich hab es schon aufgeschrieben.",
          "next": [130200218]
        },
        "130200218": {
          "id": 130200218,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Sieh zu, was passiert, wenn du mich das nächste Mal nicht einlädst.",
          "next": [130200221]
        },
        "130200219": {
