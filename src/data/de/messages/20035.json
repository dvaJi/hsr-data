      "messages": {
        "200350000": {
          "id": 200350000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "{NICKNAME}, hattest du schon mal das Gefühl, dass ...",
          "next": [
            200350001
          ]
        },
        "200350001": {
          "id": 200350001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Alle vor dir Angst haben und dir nicht die Wahrheit sagen wollen?",
          "next": [
            200350002,
            200350004
          ]
        },
        "200350002": {
          "id": 200350002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich habe noch nie das Kommando übernommen.",
          "next": [
            200350003
          ]
        },
        "200350003": {
          "id": 200350003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Tut mir leid, ich weiß einfach nicht mehr, wen ich noch fragen kann.",
          "next": [
            200350006
          ]
        },
        "200350004": {
          "id": 200350004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du musst lernen, dich anzupassen.",
          "next": [
            200350005
          ]
        },
        "200350005": {
          "id": 200350005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Ich weiß, aber es ist schwer, das zu schaffen.",
          "next": [
            200350006
          ]
        },
        "200350006": {
          "id": 200350006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Meine Mutter hat gesagt, dass Wächter dazu verdammt sind, ein einsames Leben zu führen.",
          "next": [
            200350007
          ]
        },
        "200350007": {
          "id": 200350007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Ich nehme an, dass ich Zeit brauchen werde, um mich anzupassen.",
          "next": [
            200350008
          ]
        },
        "200350008": {
          "id": 200350008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Ich habe mich außerdem mit Seele unterhalten, aber sie hat mich erstaunt angesehen und gesagt, dass sie nicht versteht, was diese Leute denken.",
          "next": [
            200350009,
            200350013
          ]
        },
        "200350009": {
          "id": 200350009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ist es möglich, dass ...",
          "next": [
            200350010
          ]
        },
        "200350010": {
          "id": 200350010,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Seele ist anders, weil sie keine Angst vor dir hat?",
          "next": [
            200350011
          ]
        },
        "200350011": {
          "id": 200350011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Ach ja ...",
          "next": [
            200350012
          ]
        },
        "200350012": {
          "id": 200350012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Jetzt, wo ich mir das recht überlege, hat Seele vor niemandem Angst.",
          "next": [
            200350017
          ]
        },
        "200350013": {
          "id": 200350013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wie kann sie es wagen, dir das anzutun!",
          "next": [
            200350014
          ]
        },
        "200350014": {
          "id": 200350014,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Du bist die Oberste Wächterin.",
          "next": [
            200350015
          ]
        },
        "200350015": {
          "id": 200350015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Du meinst, sie hat keine Angst vor mir?",
          "next": [
            200350016
          ]
        },
        "200350016": {
          "id": 200350016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Ich glaube, du hast recht.",
          "next": [
            200350017
          ]
        },
        "200350017": {
          "id": 200350017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Vielleicht kann ich meinen eigenen Weg als Anführerin finden ...",
          "next": [
            200350018
          ]
        },
        "200350018": {
          "id": 200350018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Ich werde mein Bestes geben!",
          "next": []
        }
      },
