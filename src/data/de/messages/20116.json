  "relatedMessages": [20115, 20116, 20117],
  "sections": [
    {
      "id": 2011600,
      "startingMessageId": [201160000],
      "messages": {
        "201160000": {
          "id": 201160000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "{NICKNAME}, ich brauche deinen Rat.",
          "next": [201160001, 201160003]
        },
        "201160001": {
          "id": 201160001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sicher. Worum geht es?",
          "next": [201160002]
        },
        "201160002": {
          "id": 201160002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Vielen Dank.",
          "next": [201160005]
        },
        "201160003": {
          "id": 201160003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich bin mir nicht ganz sicher, was meinen Rat angeht. Betrachte ihn nur als Denkanstoß.",
          "next": [201160004]
        },
        "201160004": {
          "id": 201160004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Das reicht mir schon.",
          "next": [201160005]
        },
        "201160005": {
          "id": 201160005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Du kennst doch meine Schwester, Hanya, oder? Sie arbeitet in der Unterwelt und gibt immer ihr Bestes.",
          "next": [201160006]
        },
        "201160006": {
          "id": 201160006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Ich will ihr ein Geschenk schicken.",
          "next": [201160007]
        },
        "201160007": {
          "id": 201160007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Ich bin nicht gut darin, Geschenke auszusuchen, deshalb möchte ich dich um deinen Rat bitten.",
          "next": [201160008, 201160011, 201160014]
        },
        "201160008": {
          "id": 201160008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Richterin Hanya ist eine Schönheit. Ich bin sicher, mit ein paar netten Kleidern oder Kosmetikprodukten kannst du nichts falsch machen.",
          "next": [201160009]
        },
        "201160009": {
          "id": 201160009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Sie ist in der Tat eine Schönheit.",
          "next": [201160010]
        },
        "201160010": {
          "id": 201160010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Ich bin schlecht darin, solche Geschenke auszusuchen.",
          "next": [201160017]
        },
        "201160011": {
          "id": 201160011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Es muss nicht immer etwas Materielles sein. Manchmal reicht ein einfaches Danke.",
          "next": [201160012]
        },
        "201160012": {
          "id": 201160012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Das habe ich ihr schon sehr oft gesagt.",
          "next": [201160013]
        },
        "201160013": {
          "id": 201160013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Das reicht nicht ...",
          "next": [201160017]
        },
        "201160014": {
          "id": 201160014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bevor man einer Person etwas schenkt, sollte man ihre Vorlieben kennen. Was mag Richterin Hanya denn so?",
          "next": [201160015]
        },
        "201160015": {
          "id": 201160015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Sie mag es, Romane zu schreiben.",
          "next": [201160016]
        },
        "201160016": {
          "id": 201160016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Aber sie braucht keine weiteren Schreibwaren oder Bücher.",
          "next": [201160017]
        },
        "201160017": {
          "id": 201160017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Wenn nichts Besonderes an dem Geschenk ist, wird sie es bestimmt einfach zur Seite legen.",
          "next": [201160018]
        },
        "201160018": {
          "id": 201160018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Gibt es ein passenderes Geschenk für jemanden wie meine Schwester?",
          "next": [201160019, 201160020]
        },
        "201160019": {
          "id": 201160019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Es klingt vielleicht etwas abstrakt, aber ich glaube, was Richterin Hanya wirklich braucht, ist Zeit.",
          "next": [201160021]
        },
        "201160020": {
          "id": 201160020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Es klingt vielleicht etwas abstrakt, aber ich glaube, was Richterin Hanya wirklich braucht, ist etwas Gesellschaft.",
          "next": [201160021]
        },
        "201160021": {
          "id": 201160021,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Du könntest mit ihr einkaufen oder eine Immersia schauen gehen. Dein Geschenk ist es, Zeit mit ihr zu verbringen.",
          "next": [201160022]
        },
        "201160022": {
          "id": 201160022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Daran habe ich noch nie gedacht ...",
          "next": [201160023]
        },
        "201160023": {
          "id": 201160023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Ich werde sie fragen.",
          "next": [201160024]
        },
        "201160024": {
          "id": 201160024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Vielen Dank, {NICKNAME}.",
          "next": [201160025]
        },
        "201160025": {
          "id": 201160025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Huohuo hat recht, ich brauche wirklich Rat von jemandem aus der sterblichen Welt.",
          "next": [201160026]
        },
        "201160026": {
          "id": 201160026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Hm. Ich habe mich falsch ausgedrückt.",
          "next": [201160027]
        },
        "201160027": {
          "id": 201160027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Ich meine jemanden, der in der sterblichen Welt arbeitet.",
          "next": [201160028]
        },
        "201160028": {
          "id": 201160028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Bis zum nächsten Mal.",
          "next": []
        }
      },
      "participatingContactsIds": [1214, 8001]
    }
  ]
}
