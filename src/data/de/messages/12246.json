  "sections": [
    {
      "id": 1224600,
      "startingMessageId": [
        122460001
      ],
      "messages": {
        "122460001": {
          "id": 122460001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "{NICKNAME}, hast du Zeit, dich ein wenig zu unterhalten?",
          "next": [
            122460002,
            122460005
          ]
        },
        "122460002": {
          "id": 122460002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich habe gerade nichts zu tun, wir können gerne reden.",
          "next": [
            122460003
          ]
        },
        "122460003": {
          "id": 122460003,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1209,
          "next": [
            122460004
          ]
        },
        "122460004": {
          "id": 122460004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Super, danke!",
          "next": [
            122460008
          ]
        },
        "122460005": {
          "id": 122460005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wir können reden, wenn es nicht zu lange dauert.",
          "next": [
            122460006
          ]
        },
        "122460006": {
          "id": 122460006,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1209,
          "next": [
            122460007
          ]
        },
        "122460007": {
          "id": 122460007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Das wird es nicht.",
          "next": [
            122460008
          ]
        },
        "122460008": {
          "id": 122460008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Ich weiß nicht, ob es an dem Druck der Kampfkunstzeremonie liegt oder an etwas anderem ...",
          "next": [
            122460009
          ]
        },
        "122460009": {
          "id": 122460009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Ich träume immer wieder von Duellen gegen verschiedene Gegner ...",
          "next": [
            122460010
          ]
        },
        "122460010": {
          "id": 122460010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "... Ich erinnere mich nach dem Aufwachen nicht an viel, aber ich glaube ... ich habe oft verloren.",
          "next": [
            122460011,
            122460013,
            122460015
          ]
        },
        "122460011": {
          "id": 122460011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Armes Kind, selbst im Traum kannst du nicht gewinnen?",
          "next": [
            122460012
          ]
        },
        "122460012": {
          "id": 122460012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Wie die Kämpfe im Traum ausgehen, ist mir eigentlich egal.",
          "next": [
            122460018
          ]
        },
        "122460013": {
          "id": 122460013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das ist eine Manifestation des großen mentalen Drucks.",
          "next": [
            122460014
          ]
        },
        "122460014": {
          "id": 122460014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Verstehe.",
          "next": [
            122460018
          ]
        },
        "122460015": {
          "id": 122460015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Yanqing, du kennst Penacony, oder? Geh dorthin und träume, dass du deine Gegner besiegst.",
          "next": [
            122460016
          ]
        },
        "122460016": {
          "id": 122460016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Ich glaube nicht, dass es unbedingt ein süßer Traum ist, seine Gegner zu besiegen.",
          "next": [
            122460017
          ]
        },
        "122460017": {
          "id": 122460017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Aber danke für den Tipp, {NICKNAME}.",
          "next": [
            122460018
          ]
        },
        "122460018": {
          "id": 122460018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Ich habe zwar geträumt, aber ich traf jedes Mal auf unterschiedliche Gegner.",
          "next": [
            122460019
          ]
        },
        "122460019": {
          "id": 122460019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Soweit ich mich erinnere, gab es eine Speer-Probe, eine Degen-Probe, eine Großschwert-Probe, eine Todesgleven-Probe ... und sogar mich selbst.",
          "next": [
            122460020
          ]
        },
        "122460020": {
          "id": 122460020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Aber egal, bei welcher Probe, ich hatte immer das Gefühl, dass es eine vage Verbindung zu dir gibt, {NICKNAME}.",
          "next": [
            122460021
          ]
        },
        "122460021": {
          "id": 122460021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Es war, als ob du diese Gegner zu mir gebracht hättest ...",
          "next": [
            122460022,
            122460025,
            122460028
          ]
        },
        "122460022": {
          "id": 122460022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was soll das heißen? Willst du dich an mir rächen?",
          "next": [
            122460023
          ]
        },
        "122460023": {
          "id": 122460023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Nein, nein.",
          "next": [
            122460024
          ]
        },
        "122460024": {
          "id": 122460024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "So meinte ich das nicht!",
          "next": [
            122460031
          ]
        },
        "122460025": {
          "id": 122460025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich gebe es zu, ich habe all diese Gegner in deinen Träumen zu dir geschickt.",
          "next": [
            122460026
          ]
        },
        "122460026": {
          "id": 122460026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "{NICKNAME}, du machst immer noch so gerne Witze.",
          "next": [
            122460027
          ]
        },
        "122460027": {
          "id": 122460027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Es waren nur verschwommene Träume.",
          "next": [
            122460031
          ]
        },
        "122460028": {
          "id": 122460028,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich ... glaube nicht, dass ich schon dazu in der Lage bin, die Träume anderer Leute zu beeinflussen.",
          "next": [
            122460029
          ]
        },
        "122460029": {
          "id": 122460029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Ich glaube nicht, dass du das warst, {NICKNAME}.",
          "next": [
            122460030
          ]
        },
        "122460030": {
          "id": 122460030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Es waren nur verschwommene Träume.",
          "next": [
            122460031
          ]
        },
        "122460031": {
          "id": 122460031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Woran man am Tag denkt, davon träumt man in der Nacht.",
          "next": [
            122460032
          ]
        },
        "122460032": {
          "id": 122460032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Diese Träume sind bestimmt eine Projektion meines Wunsches, mich mit dir zu messen, {NICKNAME}.",
          "next": [
            122460033
          ]
        },
        "122460033": {
          "id": 122460033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Daher dachte ich ... {NICKNAME}, vielleicht könntest du ...",
          "next": [
            122460034,
            122460037
          ]
        },
        "122460034": {
          "id": 122460034,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich verstehe vollkommen. Du willst, dass ich dir ein paar Tipps gebe.",
          "next": [
            122460035
          ]
        },
        "122460035": {
          "id": 122460035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Genau.",
          "next": [
            122460036
          ]
        },
        "122460036": {
          "id": 122460036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Bring mir etwas bei, wenn du Zeit hast.",
          "next": [
            122460039
          ]
        },
        "122460037": {
          "id": 122460037,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nur nicht so hastig. Versuchen wir es noch einmal, wenn du dich verbessert hast.",
          "next": [
            122460038
          ]
        },
        "122460038": {
          "id": 122460038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Ich verstehe.",
          "next": [
            122460039
          ]
        },
        "122460039": {
          "id": 122460039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Es tut so gut, meine Gedanken auszusprechen. Danke, ich melde mich ein anderes Mal wieder bei dir.",
          "next": [
            122460040
          ]
        },
        "122460040": {
          "id": 122460040,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Alles klar, bis dann.",
          "next": []
        }
      },
      "participatingContactsIds": [
        1209,
        8001
      ]
    }
  ]
}