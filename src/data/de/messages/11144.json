      "messages": {
        "111440000": {
          "id": 111440000,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "(Einen altmodischen Sticker schicken)",
          "next": [
            111440001
          ]
        },
        "111440001": {
          "id": 111440001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Wie ich höre, bist du wieder in Belobog, {NICKNAME}. Wie geht es dir?",
          "next": [
            111440002,
            111440004
          ]
        },
        "111440002": {
          "id": 111440002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Die Bewohner in Belobog empfangen mich mit offenen Armen, Oberste Wächterin.",
          "next": [
            111440003
          ]
        },
        "111440003": {
          "id": 111440003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Kein Grund zur Förmlichkeit, {NICKNAME}.",
          "next": [
            111440006
          ]
        },
        "111440004": {
          "id": 111440004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich habe derzeit viel zu tun, aber ... es ist sehr erfüllend.",
          "next": [
            111440005
          ]
        },
        "111440005": {
          "id": 111440005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Davon habe ich gehört. Danke, dass du dir die Zeit nimmst, uns zu helfen.",
          "next": [
            111440006
          ]
        },
        "111440006": {
          "id": 111440006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Pela hat sich bereits bei mir gemeldet, was die Inbetriebnahme des Museums und die Wiederbeschaffung der Ausstellungsstücke betrifft. Wir haben es nur dir zu verdanken, dass dieses Projekt vorangehen kann.",
          "next": [
            111440007
          ]
        },
        "111440007": {
          "id": 111440007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Allerdings ist dieser Artefaktdieb immer noch auf freiem Fuß. Wir müssen ihn dingfest machen!",
          "next": [
            111440008
          ]
        },
        "111440008": {
          "id": 111440008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Wenn du Zeit hast, könntest du zur Festung Qlipoth kommen? Ich möchte die Planung der weiteren Ermittlungen besprechen.",
          "next": [
            111440009
          ]
        },
        "111440009": {
          "id": 111440009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich bin gerade mit Pela unterwegs. Wir kommen gemeinsam vorbei.",
          "next": [
            111440010
          ]
        },
        "111440010": {
          "id": 111440010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Pela ist bei dir? Großartig.",
          "next": [
            111440011
          ]
        },
        "111440011": {
          "id": 111440011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Ich bin in meinem Büro.",
          "next": []
        }
      },
