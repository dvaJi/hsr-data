      ],
      "messages": {
        "201670000": {
          "id": 201670000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Baseballschläger-Ninja! Ich bitte um Verzeihung!",
          "next": [
            201670001
          ]
        },
        "201670001": {
          "id": 201670001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Ich habe die Sterne bereist und viele Wunder gesehen.",
          "next": [
            201670002
          ]
        },
        "201670002": {
          "id": 201670002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Daher möchte ich dich etwas fragen.",
          "next": [
            201670003
          ]
        },
        "201670003": {
          "id": 201670003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Wie soll man das Weltvernichtungs-Ninja-Mantra sprechen?",
          "next": [
            201670004,
            201670007
          ]
        },
        "201670004": {
          "id": 201670004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Eine Zerstörung der Welt, die der von Nanook in nichts nachsteht!",
          "next": [
            201670005
          ]
        },
        "201670005": {
          "id": 201670005,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Dann wäre dein Ninja-Mantra unaufhaltsam.",
          "next": [
            201670006
          ]
        },
        "201670006": {
          "id": 201670006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Das würde gegen meinen Ninja-Grundsatz der Gerechtigkeit verstoßen.",
          "next": [
            201670009
          ]
        },
        "201670007": {
          "id": 201670007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Drei Tage und drei Nächte lang Training bis zur Dämmerung. Das ist alles.",
          "next": [
            201670008
          ]
        },
        "201670008": {
          "id": 201670008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Verstehe. Ich kann diese Kunst erst erlernen, nachdem ich drei Tage und drei Nächte lang gesungen habe ...",
          "next": [
            201670009
          ]
        },
        "201670009": {
          "id": 201670009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Um ehrlich zu sein, habe ich gerade erst an einer Ninja-Mantra-Studie mit einhundert Personen teilgenommen.",
          "next": [
            201670010
          ]
        },
        "201670010": {
          "id": 201670010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Alle Ninjas stehen bereit und bilden einen Kreis",
          "next": [
            201670011
          ]
        },
        "201670011": {
          "id": 201670011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Rauschende Dezibel entfachen die Asche des toten Samurais",
          "next": [
            201670012
          ]
        },
        "201670012": {
          "id": 201670012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Bescheiden im Sieg, großzügig in der Niederlage",
          "next": [
            201670013
          ]
        },
        "201670013": {
          "id": 201670013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Der Ninjalehrer hört weder Prahlerei noch Klage",
          "next": [
            201670014,
            201670017,
            201670019
          ]
        },
        "201670014": {
          "id": 201670014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das ist also das neue Mantra des Universums.",
          "next": [
            201670015
          ]
        },
        "201670015": {
          "id": 201670015,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Mit deinen Fähigkeiten bestehst du die Prüfung ohne großes Pensum!",
          "next": [
            201670016
          ]
        },
        "201670016": {
          "id": 201670016,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ich hatte auch einen Reim.",
          "next": [
            201670021
          ]
        },
        "201670017": {
          "id": 201670017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Deine Reime gehen direkt ins Ohr rein so wie die Juroren dir auf den Leim.",
          "next": [
            201670018
          ]
        },
        "201670018": {
          "id": 201670018,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ich hatte auch einen Reim.",
          "next": [
            201670021
          ]
        },
        "201670019": {
          "id": 201670019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du bist ein Held und keiner, der sich an den Schwachen vergreift, wann es ihm gefällt!",
          "next": [
            201670020
          ]
        },
        "201670020": {
          "id": 201670020,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ich hatte auch einen Reim.",
          "next": [
            201670021
          ]
        },
        "201670021": {
          "id": 201670021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Ich zweifle zwar nicht an mir, aber es gibt noch größere Herausforderungen als das hier.",
          "next": [
            201670022
          ]
        },
        "201670022": {
          "id": 201670022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Ich habe die Halskette tatsächlich bekommen, aber der Ninja-Lehrer hat sie mir wieder genommen.",
          "next": [
            201670023
          ]
        },
        "201670023": {
          "id": 201670023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Hätte ich die Chance gehabt, ins Finale zu kommen?",
          "next": [
            201670024
          ]
        },
        "201670024": {
          "id": 201670024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Das Rätsel des Ninja-Mantras bleibt mir unbenommen",
          "next": [
            201670025,
            201670026,
            201670027
          ]
        },
        "201670025": {
          "id": 201670025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Endlich ist die Blendende Ninja sprachlos!",
          "next": [
            201670028
          ]
        },
        "201670026": {
          "id": 201670026,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich zweifle am Gegner, er scheint absichtlich banal!",
          "next": [
            201670028
          ]
        },
        "201670027": {
          "id": 201670027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hör auf! Noch ein Reim von dir und ich falle in Ohnmacht.",
          "next": [
            201670028
          ]
        },
        "201670028": {
          "id": 201670028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Egal, ich scheue keine Hürden",
          "next": [
            201670029
          ]
        },
        "201670029": {
          "id": 201670029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Als Ninja trägt man stets seine Bürden",
          "next": [
            201670030
          ]
        },
        "201670030": {
          "id": 201670030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Ein letzter Reim!",
          "next": [
            201670031
          ]
        },
