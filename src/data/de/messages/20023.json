  "sections": [
    {
      "id": 2002300,
      "startingMessageId": [
        200230000
      ],
      "messages": {
        "200230000": {
          "id": 200230000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Hast du hiermit irgendwelche Erfahrung?",
          "next": [
            200230001
          ]
        },
        "200230001": {
          "id": 200230001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Streiten",
          "next": [
            200230002
          ]
        },
        "200230002": {
          "id": 200230002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Streiten in Textform",
          "next": [
            200230003
          ]
        },
        "200230003": {
          "id": 200230003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Warum fragst du?",
          "next": [
            200230032
          ]
        },
        "200230004": {
          "id": 200230004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Zwei Mitglieder von Wildfeuer sind in eine Art Konflikt geraten.",
          "next": [
            200230005
          ]
        },
        "200230005": {
          "id": 200230005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Im Moment wollen sie sich nicht treffen.",
          "next": [
            200230006
          ]
        },
        "200230006": {
          "id": 200230006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Aber ich habe jeweils nach beiden geschaut,",
          "next": [
            200230007
          ]
        },
        "200230007": {
          "id": 200230007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Und sie haben sich immer noch gegenseitig Nachrichten geschrieben.",
          "next": [
            200230008
          ]
        },
        "200230008": {
          "id": 200230008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Haben sie sich denn vertragen, oder streiten sie noch?",
          "next": [
            200230009,
            200230013,
            200230018
          ]
        },
        "200230009": {
          "id": 200230009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sie haben sich vertragen.",
          "next": [
            200230010
          ]
        },
        "200230010": {
          "id": 200230010,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Es gibt nur ein paar Dinge, die sie sich nicht persönlich sagen können.",
          "next": [
            200230011
          ]
        },
        "200230011": {
          "id": 200230011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Warum?",
          "next": [
            200230012
          ]
        },
        "200230012": {
          "id": 200230012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Wäre es nicht besser, persönlich miteinander zu reden?",
          "next": [
            200230022
          ]
        },
        "200230013": {
          "id": 200230013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sie streiten noch.",
          "next": [
            200230014
          ]
        },
        "200230014": {
          "id": 200230014,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Sie streiten sich wohl in Textform.",
          "next": [
            200230015
          ]
        },
        "200230015": {
          "id": 200230015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Das ist nicht besonders erfüllend, oder?",
          "next": [
            200230016
          ]
        },
        "200230016": {
          "id": 200230016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Da kann man sich auch gleich wieder persönlich streiten.",
          "next": [
            200230017
          ]
        },
        "200230017": {
          "id": 200230017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Einfach schnell die Dinge aus dem Weg räumen und den Konflikt beenden.",
          "next": [
            200230022
          ]
        },
        "200230018": {
          "id": 200230018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich weiß nicht.",
          "next": [
            200230019
          ]
        },
        "200230019": {
          "id": 200230019,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Jede Situation ist anders.",
          "next": [
            200230020
          ]
        },
        "200230020": {
          "id": 200230020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Es ist keine große Sache. Ich will mich nicht einmischen.",
          "next": [
            200230021
          ]
        },
        "200230021": {
          "id": 200230021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Es geht nur die beiden etwas an, sie werden sich schon einigen.",
          "next": [
            200230022
          ]
        },
        "200230022": {
          "id": 200230022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Was ich nur nicht verstanden habe:",
          "next": [
            200230023
          ]
        },
        "200230023": {
          "id": 200230023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Wenn ich mit jemandem in einen Konflikt geraten bin, als ich klein war,",
          "next": [
            200230024
          ]
        },
        "200230024": {
          "id": 200230024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Dann haben wir es immer durch Kämpfen gelöst.",
          "next": [
            200230025,
            200230026,
            200230027
          ]
        },
        "200230025": {
          "id": 200230025,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Schockiert]",
          "next": [
            200230028
          ]
        },
        "200230026": {
          "id": 200230026,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Verwirrt]",
          "next": [
            200230028
          ]
        },
        "200230027": {
          "id": 200230027,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Daumen hoch]",
          "next": [
            200230028
          ]
        },
        "200230028": {
          "id": 200230028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Wenn zwischen uns irgendeine Uneinigkeit auftritt,",
          "next": [
            200230029
          ]
        },
        "200230029": {
          "id": 200230029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Dann ist es mir egal, ob wir streiten oder kämpfen,",
          "next": [
            200230030
          ]
        },
        "200230030": {
          "id": 200230030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Aber du musst mich persönlich konfrontieren.",
          "next": [
            200230031
          ]
        },
        "200230031": {
          "id": 200230031,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "... Okay.",
          "next": []
        },
        "200230032": {
          "id": 200230032,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Verwirrt]",
          "next": [
            200230004
          ]
        }
      },
