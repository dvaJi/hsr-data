        "130320002": {
          "id": 130320002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sind heutzutage alle Betrugsnachrichten so offensichtlich?",
          "next": [
            130320003
          ]
        },
        "130320003": {
          "id": 130320003,
          "type": "Text",
