      "messages": {
        "101190101": {
          "id": 101190101,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Hallo, {NICKNAME}.",
          "next": [
            101190102
          ]
        },
        "101190102": {
          "id": 101190102,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Hast du Zeit? Ich habe dir immer noch nicht für deine Hilfe gedankt, als wir uns das erste Mal getroffen haben.",
          "next": [
            101190103,
            101190106,
            101190109
          ]
        },
        "101190103": {
          "id": 101190103,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das erste Mal getroffen ... Was meinst du?",
          "next": [
            101190104
          ]
        },
        "101190104": {
          "id": 101190104,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Ihr habt mich verwundet im Überwachungsraum gefunden und mich in Sicherheit gebracht.",
          "next": [
            101190105
          ]
        },
        "101190105": {
          "id": 101190105,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Danke ... Wenn ihr nicht gewesen wärt, hätte ich nicht überlebt.",
          "next": [
            101190112
          ]
        },
        "101190106": {
          "id": 101190106,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du übertreibst. Das war doch nichts.",
          "next": [
            101190107
          ]
        },
        "101190107": {
          "id": 101190107,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Wenn ihr mich nicht gefunden hättet, hätte ich nicht überlebt.",
          "next": [
            101190108
          ]
        },
        "101190108": {
          "id": 101190108,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Ich danke euch, dass ihr mich im Überwachungsraum gerettet und mich in Sicherheit gebracht habt.",
          "next": [
            101190112
          ]
        },
        "101190109": {
          "id": 101190109,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du solltest Dan Heng danken. Er war derjenige, der dich gefunden hat.",
          "next": [
            101190110
          ]
        },
        "101190110": {
          "id": 101190110,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Ihr habt beide mein Leben gerettet. Ich hätte sonst nicht überlebt.",
          "next": [
            101190111
          ]
        },
        "101190111": {
          "id": 101190111,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Ich danke euch, dass ihr mich gerettet und in Sicherheit gebracht habt.",
          "next": [
            101190112
          ]
        },
        "101190112": {
          "id": 101190112,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Hast du Zeit zur Basiszone zu kommen? Ich habe ein Geschenk, dass ich dir geben will.",
          "next": [
            101190113,
            101190115,
            101190118
          ]
        },
        "101190113": {
          "id": 101190113,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sicher. Ich bin gleich da.",
          "next": [
            101190114
          ]
        },
        "101190114": {
          "id": 101190114,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Ich warte auf dich.",
          "next": []
        },
        "101190115": {
          "id": 101190115,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ein Geschenk? Ich habe gar nichts für dich.",
          "next": [
            101190116
          ]
        },
        "101190116": {
          "id": 101190116,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Keine Sorge. Es ist nur eine Kleinigkeit.",
          "next": [
            101190117
          ]
        },
        "101190117": {
          "id": 101190117,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Ich warte auf dich.",
          "next": []
        },
        "101190118": {
          "id": 101190118,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kannst du es mir per Post zusenden?",
          "next": [
            101190119
          ]
        },
        "101190119": {
          "id": 101190119,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Ich möchte es dir persönlich geben. Das ist höflicher.",
          "next": [
            101190120
          ]
        },
