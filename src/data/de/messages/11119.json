        },
        "111190001": {
          "id": 111190001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Ich möchte über Pascal reden ...",
          "next": [111190002, 111190003]
        },
        "111190002": {
          "id": 111190002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bist du immer noch traurig ...?",
          "next": [111190004]
        },
        "111190003": {
          "id": 111190003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Okay. Worüber möchtest du reden?",
          "next": [111190005]
        },
        "111190004": {
          "id": 111190004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Nein ... ich habe mich beruhigt.",
          "next": [111190005]
        },
        "111190005": {
          "id": 111190005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Tatsächlich habe ich viel darüber nachgedacht.",
          "next": [111190006]
        },
        "111190006": {
          "id": 111190006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Ich verstehe nicht ... Was ist Pascal?",
          "next": [111190007]
        },
        "111190007": {
          "id": 111190007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Andere Roboter halten Pascal für eine Schöpfung des Fragmentums.",
          "next": [111190008]
        },
        "111190008": {
          "id": 111190008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Herr Svarog hält Pascal für einen normalen Roboter.",
          "next": [111190009]
        },
        "111190009": {
          "id": 111190009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Nicht einmal Pascal selbst weiß, was er ist.",
          "next": [111190010]
        },
        "111190010": {
          "id": 111190010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Ich glaube, Pascal könnte sogar ein Mensch sein.",
          "next": [111190011]
        },
        "111190011": {
          "id": 111190011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "{NICKNAME}, was meinst du?",
          "next": [111190012, 111190013, 111190014]
        },
        "111190012": {
          "id": 111190012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich glaube, Pascal ist nur ein Roboter.",
          "next": [111190015]
        },
        "111190013": {
          "id": 111190013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Pascal könnte eine Fragmentum-Kreatur sein ...",
          "next": [111190017]
        },
        "111190014": {
          "id": 111190014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Pascal ist ein lebendiger Mensch.",
          "next": [111190019]
        },
        "111190015": {
          "id": 111190015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Findest du, {NICKNAME}?",
          "next": [111190016]
        },
        "111190016": {
          "id": 111190016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Pascal sieht zwar aus wie ein normaler Roboter, aber ...",
          "next": [111190021]
        },
        "111190017": {
          "id": 111190017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Findest du, {NICKNAME}?",
          "next": [111190018]
        },
        "111190018": {
          "id": 111190018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Aber sieht Pascal nicht wie ein normaler kleiner Roboter aus? Außerdem ...",
          "next": [111190021]
        },
        "111190019": {
          "id": 111190019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Findest du, {NICKNAME}?",
          "next": [111190020]
        },
        "111190020": {
          "id": 111190020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Das denke ich auch, aber ...",
          "next": [111190021]
        },
        "111190021": {
          "id": 111190021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Fragmentum-Kreatur, Roboter, Mensch.",
          "next": [111190022]
        },
        "111190022": {
          "id": 111190022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Worin unterscheiden sie sich?",
          "next": [111190023]
        },
        "111190023": {
          "id": 111190023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Einige Fragmentum-Kreaturen können sprechen und sich wie Menschen verhalten.",
          "next": [111190024]
        },
        "111190024": {
          "id": 111190024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Herr Svarog sieht zwar aus wie ein Roboter, aber er kann auch sprechen, denken und sogar „Gefühle“ ausdrücken wie wir.",
          "next": [111190025]
        },
        "111190025": {
          "id": 111190025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Wie können wir also definieren, was „wir“ und was „sie“ sind?",
          "next": [111190026, 111190027, 111190028]
        },
        "111190026": {
          "id": 111190026,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nur Menschen haben die Fähigkeit, sich mit anderen Lebewesen zu vergleichen und sich in sie hineinzuversetzen.",
          "next": [111190029]
        },
        "111190027": {
          "id": 111190027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Einfach gesagt, unser biologischer Aufbau ist völlig anders.",
          "next": [111190032]
        },
        "111190028": {
          "id": 111190028,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Es gibt keinen Unterschied zwischen Menschen und anderen Lebewesen auf der Welt.",
          "next": [111190034, 111190035]
        },
        "111190029": {
          "id": 111190029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "So ist das also.",
          "next": [111190030]
        },
        "111190030": {
          "id": 111190030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Also, Pascal und Herr Svarog ...",
          "next": [111190031]
        },
        "111190031": {
          "id": 111190031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Vielleicht sind sie auch alle lebende, atmende Menschen.",
          "next": [111190038]
        },
        "111190032": {
          "id": 111190032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "So ist das also.",
          "next": [111190033]
        },
        "111190033": {
          "id": 111190033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Das ist wahrscheinlich der einzige Weg ... um eine klare Unterscheidung zwischen den anderen und uns vornehmen zu können.",
          "next": [111190038]
        },
        "111190034": {
          "id": 111190034,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wir sind aus demselben Universum hervorgegangen und wir steuern auf dasselbe Ende zu.",
          "next": [111190036]
        },
        "111190035": {
          "id": 111190035,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wir sind alle nur verschiedene Erscheinungsformen der gleichen Elementarteilchen.",
          "next": [111190036]
        },
        "111190036": {
          "id": 111190036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "So ist das also.",
          "next": [111190037]
        },
        "111190037": {
          "id": 111190037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Das klingt kompliziert, aber ich werde versuchen, es zu verstehen.",
          "next": [111190038]
        },
        "111190038": {
          "id": 111190038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Ich fühle mich nicht mehr so verloren.",
          "next": [111190039]
        },
        "111190039": {
          "id": 111190039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Danke, {NICKNAME}!",
          "next": [111190040]
        },
