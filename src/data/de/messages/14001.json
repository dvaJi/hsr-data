        "140010000": {
          "id": 140010000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1408,
          "text": "Habt ihr beiden euch gut ausgeruht?",
          "next": [
            140010001,
            140010004,
            140010007
          ]
        },
        "140010001": {
          "id": 140010001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bereit, etwas Großes in Angriff zu nehmen!",
          "next": [
            140010002
          ]
        },
        "140010002": {
          "id": 140010002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1408,
          "text": "Schön, ihr sprüht vor Energie.",
          "next": [
            140010003
          ]
        },
        "140010003": {
          "id": 140010003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Die Zimmer sind großartig, so dass es sich hier angenehm ausruhen lässt. Gibt es etwas, das du mit uns besprechen möchtest?",
          "next": [
            140010010
          ]
        },
        "140010004": {
          "id": 140010004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich habe nicht so gut geschlafen.",
          "next": [
            140010005
          ]
        },
        "140010005": {
          "id": 140010005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1408,
          "text": "Das ist nicht gut. Gibt es ein Problem mit dem Badezimmer?",
          "next": [
            140010006
          ]
        },
        "140010006": {
          "id": 140010006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Das Zimmer ist perfekt. Ich habe mich gestern einfach zu sehr angestrengt. Gibt es etwas, worüber du sprechen möchtest?",
          "next": [
            140010010
          ]
        },
        "140010007": {
          "id": 140010007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Oh, du bist es. Ich habe jemand anderen erwartet ...",
          "next": [
            140010008
          ]
        },
        "140010008": {
          "id": 140010008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1408,
          "text": "Wen hast du erwartet? Ist mir jemand zuvorgekommen?",
          "next": [
            140010009
          ]
        },
        "140010009": {
          "id": 140010009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Nichts Wichtiges. Ich frage mich nur, ob du irgendetwas von uns brauchst?",
          "next": [
            140010010
          ]
        },
        "140010010": {
          "id": 140010010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1408,
          "text": "Ich habe gute Neuigkeiten!",
          "next": [
            140010011
          ]
        },
        "140010011": {
          "id": 140010011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1408,
          "text": "Trianne hat Castrum Kremnos gefunden. Beeilt euch zum Bad der Erben von Chrysos im Marmorpalast und macht euch bereit, es mit Nikador aufzunehmen!",
          "next": [
            140010012
          ]
        },
        "140010012": {
          "id": 140010012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1408,
          "text": "Ah, ihr könnt euch noch ein bisschen ausruhen. Keine Eile.",
          "next": [
            140010013
          ]
        },
        "140010013": {
          "id": 140010013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1408,
          "text": "Nichts Dringendes.",
          "next": []
        }
