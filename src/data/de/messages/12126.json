  "sections": [
    {
      "id": 1212601,
      "startingMessageId": [121260101],
      "messages": {
        "121260101": {
          "id": 121260101,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "{NICKNAME}, ist alles in Ordnung?",
          "next": [121260102, 121260104]
        },
        "121260102": {
          "id": 121260102,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ja. Sogar unglaublich in Ordnung.",
          "next": [121260103]
        },
        "121260103": {
          "id": 121260103,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "Das ist gut. Ich habe immer an deine Fähigkeiten geglaubt.",
          "next": [121260107]
        },
        "121260104": {
          "id": 121260104,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sehr gut. Ich bin jetzt ein Mitglied der Jünger des Sanctus Medicus.",
          "next": [121260103]
        },
        "121260107": {
          "id": 121260107,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "Jemand hat gerade Bericht erstattet ... Hast du einen Wolkenritter gerettet?",
          "next": [121260108]
        },
        "121260108": {
          "id": 121260108,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das war nicht der Rede wert.",
          "next": [121260109]
        },
        "121260109": {
          "id": 121260109,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ich würde es nicht als Rettung bezeichnen. Wir hatten eine erfolgreiche Zusammenarbeit.",
          "next": [121260110]
        },
        "121260110": {
          "id": 121260110,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "Vielen Dank für deine Hilfe. Dieser Wolkenritter hat alle seine Informationen an den Thron der göttlichen Vorsehung weitergeleitet.",
          "next": [121260111]
        },
        "121260111": {
          "id": 121260111,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "Ohne deine Hilfe wäre er da nicht weggekommen.",
          "next": [121260113]
        },
        "121260113": {
          "id": 121260113,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "Nachdem du bereits die Jünger des Sanctus Medicus infiltriert hast, bitte beobachte weiter ihre Aktivitäten.",
          "next": [121260118]
        },
        "121260114": {
          "id": 121260114,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "Wie schon erwähnt, ist vor ein paar Monaten ein Wolkenritter namens Zhixin verschwunden.",
          "next": [121260115]
        },
        "121260115": {
          "id": 121260115,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "Ich möchte wissen, was mit ihm passiert ist. Ob er noch lebt oder tot ist ... damit ich seiner Familie eine Antwort geben kann.",
          "next": [121260116]
        },
        "121260116": {
          "id": 121260116,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "Wir sollten es von nun an vermeiden, in Kontakt zu treten. Wenn du dabei beobachtet wirst, den Thron der göttlichen Vorsehung zu betreten und zu verlassen, dann werden diese Jünger sicher argwönisch.",
          "next": [121260117]
        },
        "121260117": {
          "id": 121260117,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "Bitte komm unversehrt zurück. Sonst wird der General sehr wütend auf mich sein.",
          "next": []
        },
        "121260118": {
          "id": 121260118,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Gibt es etwas, auf das ich mich konzentrieren sollte?",
          "next": [121260114]
        }
      },
      "participatingContactsIds": [60, 8001]
    }
  ]
}
