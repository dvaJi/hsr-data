  ],
  "sections": [
    {
      "id": 2007100,
      "startingMessageId": [
        200710000
      ],
      "messages": {
        "200710000": {
          "id": 200710000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Sag, {NICKNAME}, wo hast du übernachtet, als du auf die Luofu gekommen bist? Wie war es dort?",
          "next": [
            200710001
          ]
        },
        "200710001": {
          "id": 200710001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Moment, lass mich raten.",
          "next": [
            200710002
          ]
        },
        "200710002": {
          "id": 200710002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Im Gasthaus Petrichor?",
          "next": [
            200710003,
            200710007
          ]
        },
        "200710003": {
          "id": 200710003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Richtig. Ich habe im Gasthaus Petrichor übernachtet.",
          "next": [
            200710004
          ]
        },
        "200710004": {
          "id": 200710004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "In einer Premium-Suite, war nicht übel.",
          "next": [
            200710005
          ]
        },
        "200710005": {
          "id": 200710005,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Daumen hoch]",
          "next": [
            200710006
          ]
        },
        "200710006": {
          "id": 200710006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "An die Premium-Suite kommt man nicht so einfach. Die meisten Leute zieht es dort nur wegen des Ausblicks auf die Sternenguckerallee bei Nacht hin.",
          "next": [
            200710012
          ]
        },
        "200710007": {
          "id": 200710007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Richtig. Ich habe im Gasthaus Petrichor übernachtet.",
          "next": [
            200710008
          ]
        },
        "200710008": {
          "id": 200710008,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "War nicht gerade das Nonplusultra.",
          "next": [
            200710009
          ]
        },
        "200710009": {
          "id": 200710009,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ich kann meine Hotelaufenthalte nie genießen, weil ich immer darüber nachdenke, was als Nächstes schiefgehen wird.",
          "next": [
            200710010
          ]
        },
        "200710010": {
          "id": 200710010,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Weinen]",
          "next": [
            200710011
          ]
        },
        "200710011": {
          "id": 200710011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Klingt, als hättest du eine schwere Zeit hinter dir.",
          "next": [
            200710012
          ]
        },
        "200710012": {
          "id": 200710012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Ich übernachte oft im Gasthaus Petrichor. Es ist sehr zentral gelegen.",
          "next": [
            200710013
          ]
        },
        "200710013": {
          "id": 200710013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Als wandernder Händler bin ich auf meinen Reisen eher sparsam, aber nach einiger Zeit wird mir langweilig.",
          "next": [
            200710014
          ]
        },
        "200710014": {
          "id": 200710014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Es gibt ein paar Wohnungen in der Nachbarschaft der Himmelspavillons, die man für kurze Zeit mieten kann, also bin ich dorthin gegangen.",
          "next": [
            200710015
          ]
        },
        "200710015": {
          "id": 200710015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Dort leben vor allem Einheimische.",
          "next": [
            200710016,
            200710020
          ]
        },
        "200710016": {
          "id": 200710016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ja, es war nicht so praktisch. Man fühlt sich dort irgendwie beobachtet.",
          "next": [
            200710017
          ]
        },
        "200710017": {
          "id": 200710017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Kommt auf deine Perspektive an.",
          "next": [
            200710018
          ]
        },
        "200710018": {
          "id": 200710018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "In Xianzhou verkriechen sich die Leute nur selten zu Hause. Sie sind ein freundliches Völkchen, also beobachte ich sie gerne in ihrem Alltag.",
          "next": [
            200710019
          ]
        },
        "200710019": {
          "id": 200710019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Gut, das stimmt nicht ganz. Meistens bin ich es, der beobachtet wird.",
          "next": [
            200710023
          ]
        },
        "200710020": {
          "id": 200710020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du bist selbst zu einem Einheimischen von Xianzhou geworden.",
          "next": [
            200710021
          ]
        },
        "200710021": {
          "id": 200710021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Hehe, du kennst mich.",
          "next": [
            200710022
          ]
        },
        "200710022": {
          "id": 200710022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Ja, ich habe mich der Bevölkerung auf der Luofu schon angepasst.",
          "next": [
            200710023
          ]
        },
        "200710023": {
          "id": 200710023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Ich habe kein Zuhause, bin praktisch Nomade. Wenn ich in der Nachbarschaft der Himmelspavillons wohne – auch nur für kurze Zeit – vergesse ich schnell, dass ich ein wandernder Händler bin.",
          "next": [
            200710024
          ]
        },
        "200710024": {
          "id": 200710024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Also gefällt es mir dort ganz gut.",
          "next": [
            200710025,
            200710027
          ]
        },
        "200710025": {
          "id": 200710025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich werde es beim nächsten Mal auch versuchen.",
          "next": [
            200710026
          ]
        },
        "200710026": {
          "id": 200710026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Schön, vielleicht haben wir Glück und werden Nachbarn.",
          "next": []
        },
        "200710027": {
          "id": 200710027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich kann dort wahrscheinlich nicht bleiben.",
          "next": [
            200710028
          ]
        },
        "200710028": {
          "id": 200710028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Darf ich fragen, warum?",
          "next": [
            200710029,
            200710030,
            200710031
          ]
        },
        "200710029": {
          "id": 200710029,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich habe Angst!",
          "next": [
            200710032
          ]
        },
        "200710030": {
          "id": 200710030,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich bin zu sehr an mein eigenes Bett gewöhnt!",
          "next": [
            200710032
          ]
        },
        "200710031": {
          "id": 200710031,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich bin zu penibel, was Sauberkeit angeht!",
          "next": [
            200710032
          ]
        },
        "200710032": {
          "id": 200710032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "So ist das also.",
          "next": [
            200710033
          ]
        },
        "200710033": {
          "id": 200710033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Verstehe. Ich respektiere deine Entscheidung.",
          "next": [
            200710034
          ]
        },
        "200710034": {
          "id": 200710034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Wir können darüber sprechen, wenn du es dir irgendwann anders überlegst.",
          "next": []
        }
      },
      "participatingContactsIds": [
        1203,
        8001
      ]
    }
  ]
}