  "sections": [
    {
      "id": 2017100,
      "startingMessageId": [
        201710000
      ],
      "messages": {
        "201710000": {
          "id": 201710000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "{F#Wohltäterin}{M#Wohltäter}, ich will nicht stören.",
          "next": [
            201710001
          ]
        },
        "201710001": {
          "id": 201710001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Aber ich habe den Planeten Thalassa besucht und dort frisch gefangene einheimische Leuchtfische erworben.",
          "next": [
            201710002
          ]
        },
        "201710002": {
          "id": 201710002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Sie sind eine seltene Zutat mit einem charakteristischen süßen Geschmack und werden in tiefen Riffen gefangen. Sie können als Nahrung, aber auch als Medizin verwendet werden. Hier hast du ein bisschen was zum Probieren.",
          "next": [
            201710003
          ]
        },
        "201710003": {
          "id": 201710003,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1202,
          "next": [
            201710004,
            201710006
          ]
        },
        "201710004": {
          "id": 201710004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wie großzügig von dir!",
          "next": [
            201710005
          ]
        },
        "201710005": {
          "id": 201710005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Du solltest wissen, mir geht es um mehr als eine reine Geschäftsbeziehung.",
          "next": [
            201710008
          ]
        },
        "201710006": {
          "id": 201710006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich nehme dein Angebot gerne an. Das wird ein Festschmaus!",
          "next": [
            201710007
          ]
        },
        "201710007": {
          "id": 201710007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Es ist mehr als genug für den gesamten Express da. Lasst es euch schmecken.",
          "next": [
            201710008
          ]
        },
        "201710008": {
          "id": 201710008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Als ich nach Thalassa zurückkehrte, wurde ich von den Leuten dort genauso behandelt wie früher.",
          "next": [
            201710009
          ]
        },
        "201710009": {
          "id": 201710009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Wahrscheinlich haben sie noch nichts von mir gehört, was ein wahrer Segen sein könnte.",
          "next": [
            201710010
          ]
        },
        "201710010": {
          "id": 201710010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Was werden wohl meine alten Freunde sagen, wenn ich zur Luofu zurückkehre ...",
          "next": [
            201710011,
            201710013,
            201710016
          ]
        },
        "201710011": {
          "id": 201710011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Yanming wird wahrscheinlich rufen: „Gut gemacht, meine Liebe!“",
          "next": [
            201710012
          ]
        },
        "201710012": {
          "id": 201710012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Ich frage mich, wie es der Himmlischen Handelsgilde so geht. Ich habe so viele Fragen rund ums Geschäftliche, die ich gerne mit ihnen besprechen würde.",
          "next": [
            201710019
          ]
        },
        "201710013": {
          "id": 201710013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mengming wird dich bestimmt auf eine Tasse Tee einladen.",
          "next": [
            201710014
          ]
        },
        "201710014": {
          "id": 201710014,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Sie hat auf deinen Vorschlag hin einen neuen Tee zubereitet und ihn „Tingyuns Mischung“ genannt.",
          "next": [
            201710015
          ]
        },
        "201710015": {
          "id": 201710015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Schon der Name verrät den süßen und intensiven Geschmack des Tees.",
          "next": [
            201710019
          ]
        },
        "201710016": {
          "id": 201710016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Yancui hat das Messer erhalten, das du für sie besorgt hast, und sie hat versprochen, einen Teller Rinderinnereien-Eintopf mit Chiliöl für dich zuzubereiten.",
          "next": [
            201710017
          ]
        },
        "201710017": {
          "id": 201710017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Dieses Gericht habe ich schon ewig nicht mehr gegessen. Mir läuft das Wasser im Mund zusammen!",
          "next": [
            201710018
          ]
        },
        "201710018": {
          "id": 201710018,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1202,
          "next": [
            201710019
          ]
        },
        "201710019": {
          "id": 201710019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Madame Yukong hat auch erwähnt, dass du die Geschenke ausgeliefert hast, die ich vorbereitet habe.",
          "next": [
            201710020
          ]
        },
        "201710020": {
          "id": 201710020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Danke, {F#Wohltäterin}{M#Wohltäter}.",
          "next": [
            201710021
          ]
        },
        "201710021": {
          "id": 201710021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Jetzt kann ich endlich unbesorgt meine Reise fortsetzen.",
          "next": [
            201710022
          ]
        },
        "201710022": {
          "id": 201710022,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1202,
          "next": []
        }
      },
