  "sections": [
    {
      "id": 2015400,
      "startingMessageId": [
        201540000
      ],
      "messages": {
        "201540000": {
          "id": 201540000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Großvater hat gesagt, dass der Astralexpress schon überall war.",
          "next": [
            201540001
          ]
        },
        "201540001": {
          "id": 201540001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Hast du in letzter Zeit irgendwelche merkwürdigen Gestalten gesehen?",
          "next": [
            201540002
          ]
        },
        "201540002": {
          "id": 201540002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Wenn du jemanden siehst, der sich merkwürdig verhält, trägt er vielleicht ein verfluchtes Schwert bei sich.",
          "next": [
            201540003,
            201540007,
            201540012
          ]
        },
        "201540003": {
          "id": 201540003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich bin eine merkwürdige Gestalt. Was willst du?",
          "next": [
            201540004
          ]
        },
        "201540004": {
          "id": 201540004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "?",
          "next": [
            201540005
          ]
        },
        "201540005": {
          "id": 201540005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Ich finde, du bist ganz normal drauf.",
          "next": [
            201540006
          ]
        },
        "201540006": {
          "id": 201540006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Und ich kann Leute ganz gut einschätzen.",
          "next": [
            201540015
          ]
        },
        "201540007": {
          "id": 201540007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nein, für mich sieht alles normal aus.",
          "next": [
            201540008
          ]
        },
        "201540008": {
          "id": 201540008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Vielleicht bist du ja die merkwürdige Gestalt hier?",
          "next": [
            201540009
          ]
        },
        "201540009": {
          "id": 201540009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "War nur ein Witz.",
          "next": [
            201540010
          ]
        },
        "201540010": {
          "id": 201540010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Ich weiß, dass du kein verfluchtes Schwert hast.",
          "next": [
            201540011
          ]
        },
        "201540011": {
          "id": 201540011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Und ich kann Leute ganz gut einschätzen.",
          "next": [
            201540015
          ]
        },
        "201540012": {
          "id": 201540012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Von welcher Art „merkwürdig“ reden wir hier? Ich kenne jede Menge merkwürdige Gestalten. Lass mich dir eine holen, ihr könnt euch unterhalten.",
          "next": [
            201540013
          ]
        },
        "201540013": {
          "id": 201540013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "...",
          "next": [
            201540014
          ]
        },
        "201540014": {
          "id": 201540014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Ich glaube, du irrst dich.",
          "next": [
            201540015
          ]
        },
        "201540015": {
          "id": 201540015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Schon gut, ich wollte nur fragen.",
          "next": [
            201540016
          ]
        },
        "201540016": {
          "id": 201540016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Es wäre schneller, dich gleich zu fragen, wo das verfluchte Schwert ist.",
          "next": [
            201540017
          ]
        },
        "201540017": {
          "id": 201540017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ist das dringend?",
          "next": [
            201540018
          ]
        },
        "201540018": {
          "id": 201540018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Schnell!",
          "next": [
            201540019
          ]
        },
        "201540019": {
          "id": 201540019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Sehr dringend.",
          "next": [
            201540020
          ]
        },
        "201540020": {
          "id": 201540020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Hanguang hat insgesamt 1.382 verfluchte Schwerter geschmiedet.",
          "next": [
            201540021
          ]
        },
        "201540021": {
          "id": 201540021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Bisher habe ich erst 300 von ihnen eingeschmolzen!",
          "next": [
            201540022
          ]
        },
        "201540022": {
          "id": 201540022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Ein verfluchtes Schwert dürstet nach Blut und liebt es, zu töten. Wer es führt, wird zu einer bloßen Marionette, die nur noch morden kann.",
          "next": [
            201540023
          ]
        },
        "201540023": {
          "id": 201540023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Wenn ich mich nicht beeile, wird es bald noch mehr Geister und vergossenes Blut geben.",
          "next": [
            201540024
          ]
        },
        "201540024": {
          "id": 201540024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Ich habe Großvater versprochen, eine Zeit lang zu warten, aber ich glaube, jetzt warte ich schon lang genug.",
          "next": [
            201540025,
            201540028
          ]
        },
        "201540025": {
          "id": 201540025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du hast gesagt, du wartest schon lang genug? Wie hat dein Großvater darauf reagiert?",
          "next": [
            201540026
          ]
        },
        "201540026": {
          "id": 201540026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Das ist nicht wichtig.",
          "next": [
            201540027
          ]
        },
        "201540027": {
          "id": 201540027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Wichtig ist nur, dass ich sie jagen will! Alle! Verfluchten! Schwerter!",
          "next": [
            201540031
          ]
        },
        "201540028": {
          "id": 201540028,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Setz dich nicht zu sehr unter Druck.",
          "next": [
            201540029
          ]
        },
        "201540029": {
          "id": 201540029,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Du bist immer noch jung.",
          "next": [
            201540030
          ]
        },
        "201540030": {
          "id": 201540030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Ja, und zwar zu jung, um zu schlafen!",
          "next": [
            201540031
          ]
        },
        "201540031": {
          "id": 201540031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Wie dem auch sei.",
          "next": [
            201540032
          ]
        },
        "201540032": {
          "id": 201540032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Ihr habt schon viel gesehen. Bitte sagt es mir, wenn ihr eines findet.",
          "next": [
            201540033
          ]
        },
        "201540033": {
          "id": 201540033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Ich werde kein einziges verfluchtes Schwert entkommen lassen.",
          "next": []
        }
      },
      "participatingContactsIds": [
        1221,
        8001
      ]
    }
  ]
}