      "messages": {
        "122230000": {
          "id": 122230000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "Hast du gehört?",
          "next": [122230001]
        },
        "122230001": {
          "id": 122230001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 25,
          "text": "Mhm.",
          "next": [122230002]
        },
        "122230002": {
          "id": 122230002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 25,
          "text": "Auch Forschungsleiterin Asta wurde angegriffen.",
          "next": [122230003]
        },
        "122230003": {
          "id": 122230003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Dieser Erzherzog Inferno ...",
          "next": [122230004]
        },
        "122230004": {
          "id": 122230004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Arglistig! Hinterhältig!",
          "next": [122230005]
        },
        "122230005": {
          "id": 122230005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 25,
          "text": "Psychologische Angriffe waren schon immer die schrecklichste Art von Angriffen.",
          "next": [122230006]
        },
        "122230006": {
          "id": 122230006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "In der Tat. Es scheint, dass die Vernichtungs-Gang mehr ist als die blinde Gewalt und die vulgären Schlächter, die in den Gerüchten beschrieben werden.",
          "next": [122230007]
        },
        "122230007": {
          "id": 122230007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "Wenn die Forschungsleiterin am Ende nicht gerettet würde, wären wir alle in großen, großen Schwierigkeiten.",
          "next": [122230008]
        },
        "122230008": {
          "id": 122230008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "Nicht unbedingt. Vielleicht ist sie diejenige, die das alles inszeniert hat.",
          "next": [122230009]
        },
        "122230009": {
          "id": 122230009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "Sie führt eine komplexe List durch, indem sie sich absichtlich schadet, um das Blatt zu wenden.",
          "next": [122230010]
        },
        "122230010": {
          "id": 122230010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Diese Gedankengänge haben etwas für sich ...",
          "next": [122230011]
        },
        "122230011": {
          "id": 122230011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 25,
          "text": "Oh, das reicht. Das ist lächerlich.",
          "next": [122230012]
        },
        "122230012": {
          "id": 122230012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 25,
          "text": "Ist es das wirklich wert? All das nur für ein paar Internet-Tastaturkrieger?",
          "next": [122230013]
        },
        "122230013": {
          "id": 122230013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Das stimmt.",
          "next": [122230014]
        },
        "122230014": {
          "id": 122230014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "Wo wir gerade dabei sind, wo ist der allererste Hetzer, Rich_Besserwisser?",
          "next": [122230015]
        },
        "122230015": {
          "id": 122230015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 25,
          "text": "Oh, richtig. Er klang wirklich rechtschaffen, als er mich in die Gruppe einlud.",
          "next": [122230016]
        },
        "122230016": {
          "id": 122230016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "Mal sehen.",
          "next": [122230017]
        },
        "122230017": {
          "id": 122230017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "Tss, der Typ hat die Gruppe verlassen, ohne ein Geräusch zu machen ...",
          "next": []
        }
      },
      "participatingContactsIds": [20, 25, 17, 18]
    }
  ]
}
