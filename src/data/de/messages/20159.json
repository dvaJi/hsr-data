          ]
        },
        "201590009": {
          "id": 201590009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das Astralexpress-Team ist hier, um zu siegen!",
          "next": [
            201590010
          ]
        },
        "201590010": {
          "id": 201590010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1220,
          "text": "Das wird ein tolles Kräftemessen!",
