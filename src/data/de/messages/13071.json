      ],
      "messages": {
        "130710000": {
          "id": 130710000,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wir haben ein Problem, Freunde.",
          "next": [
            130710001
          ]
        },
        "130710001": {
          "id": 130710001,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "An Bord des Schiffs befindet sich eine imaginäre Neutronenbombe.",
          "next": [
            130710002
          ]
        },
        "130710002": {
          "id": 130710002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "?",
          "next": [
            130710003
          ]
        },
        "130710003": {
          "id": 130710003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Was geschieht hier?",
          "next": [
            130710004
          ]
        },
        "130710004": {
          "id": 130710004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das haben wir alles der Maskierten Närrin zu verdanken.",
          "next": [
            130710005
          ]
        },
        "130710005": {
          "id": 130710005,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Von_wo_schaut_man_das_Feuerwerk ist dem Chat beigetreten.",
          "next": [
            130710006
          ]
        },
        "130710006": {
          "id": 130710006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 144,
          "text": "Sparkle wird die Situation persönlich erklären!",
          "next": [
            130710007
          ]
        },
        "130710007": {
          "id": 130710007,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 144,
          "text": "(Schelmische-Dame-Sticker)",
          "next": [
            130710008,
            130710010,
            130710012
          ]
        },
        "130710008": {
          "id": 130710008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "??? Ich habe dich nicht in die Gruppe eingeladen.",
          "next": [
            130710009
          ]
        },
        "130710009": {
          "id": 130710009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 144,
          "text": "So ist Sparkle nun mal. Sie braucht keine Einladung.",
          "next": [
            130710015
          ]
        },
        "130710010": {
          "id": 130710010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Na gut, dann raus damit!",
          "next": [
            130710011
          ]
        },
        "130710011": {
          "id": 130710011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 144,
          "text": "Du hast eine gute Menschenkenntnis. Das mag ich!",
          "next": [
            130710015
          ]
        },
        "130710012": {
          "id": 130710012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wie kannst du es wagen, dich in dieser Gruppe zu zeigen???",
          "next": [
            130710013
          ]
        },
        "130710013": {
          "id": 130710013,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Zeit, dass wir dir eine Lektion erteilen!",
          "next": [
            130710014
          ]
        },
        "130710014": {
          "id": 130710014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 144,
          "text": "Klar! Dann komm doch durchs Internet und versuch mich zu fangen!",
          "next": [
            130710015
          ]
        },
        "130710015": {
          "id": 130710015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 144,
          "text": "Na schön, kommen wir mal zur Sache!",
          "next": [
            130710016
          ]
        },
        "130710016": {
          "id": 130710016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 144,
          "text": "Sind euch schon allen die Kinnladen runtergeklappt? Sparkles Knöpfe wurden alle wiedergefunden ... Warum ist die Zeitschaltuhr der Bombe noch aktiv?",
          "next": [
            130710017
          ]
        },
        "130710017": {
          "id": 130710017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 144,
          "text": "Das haben wir alles unseren mutigen Hunden zu verdanken! Da sie alle Knöpfe sorgfältig wieder eingesammelt haben, konnte Sparkle alle nacheinander drücken!",
          "next": [
            130710018
          ]
        },
        "130710018": {
          "id": 130710018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 144,
          "text": "Sparkle hat sich buchstäblich die Finger wund getippt ... Einhunderttausend Knöpfe! Sie hat ganze achtundvierzig Systemstunden lang Knöpfe gedrückt!",
          "next": [
            130710019
          ]
        },
        "130710019": {
          "id": 130710019,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 144,
          "text": "(Weinende-Maske-Sticker)",
          "next": [
            130710020
          ]
        },
        "130710020": {
          "id": 130710020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "...",
          "next": [
            130710021
          ]
        },
        "130710021": {
          "id": 130710021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "...",
          "next": [
            130710022
          ]
        },
        "130710022": {
          "id": 130710022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1315,
          "text": "Was zum Kuckuck ist nur los mit dir???",
          "next": [
            130710023
          ]
        },
        "130710023": {
          "id": 130710023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Sei nicht unhöflich, Silbercowboy!",
          "next": [
            130710024,
            130710025
          ]
        },
        "130710024": {
          "id": 130710024,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "...",
          "next": [
            130710026
          ]
        },
        "130710025": {
          "id": 130710025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "All die anderen Dinge, die du mit dieser Geduld nur hättest anstellen können ...",
          "next": [
            130710026
          ]
        },
        "130710026": {
          "id": 130710026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 144,
          "text": "Danke für deine Wertschätzung! Natürlich kam es letztlich auch auf Sparkles Glück an",
          "next": [
            130710027
          ]
        },
        "130710027": {
          "id": 130710027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 144,
          "text": "Ich weiß nicht warum, aber es gab insgesamt 16.537 geschätzte Traumgäste, die freiwillig Sparkles Bürde mittragen wollten ... Mädchen, die gerne lächeln, haben immer Glück ...",
          "next": [
            130710028
          ]
        },
        "130710028": {
          "id": 130710028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 144,
          "text": "Wie auch immer. Die Bombe, die Sparkle auf dem Kreuzfahrtschiff platziert hat, ist in 999 Puppen versteckt.",
          "next": [
            130710029
          ]
        },
        "130710029": {
          "id": 130710029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 144,
          "text": "Ihr aufgeweckten Leute findet sicher heraus, wo die echte Bombe versteckt ist. Viel Glück!",
          "next": [
            130710030
          ]
        },
        "130710030": {
          "id": 130710030,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Von_wo_schaut_man_das_Feuerwerk hat den Chat verlassen.",
          "next": [
            130710031
          ]
        },
        "130710031": {
          "id": 130710031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Wie viele???",
          "next": [
            130710032
          ]
        },
        "130710032": {
          "id": 130710032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Keine Panik. Wir kümmern uns zusammen darum",
          "next": [
            130710033
          ]
        },
        "130710033": {
          "id": 130710033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1309,
          "text": "Leute, bitte verursacht keine unnötige Panik.",
          "next": [
            130710034
          ]
        },
        "130710034": {
          "id": 130710034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1309,
          "text": "Ich veranlasse die Familienmitglieder, sich auf die Evakuierung vorzubereiten. Die Bombe überlasse ich euch.",
          "next": [
            130710035
          ]
        },
        "130710035": {
          "id": 130710035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1309,
          "text": "Die Familie Eichenblatt hat sich immer auf die Stärke der Ordnung verlassen, um zu garantieren, dass „in Träumen niemals der Tod eintritt“.",
          "next": [
            130710036
          ]
        },
        "130710036": {
          "id": 130710036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1309,
          "text": "Aber der Segen der Ordnung ist nun verloren ... Die Konsequenzen sind unvorstellbar schlecht.",
          "next": [
            130710037
          ]
        },
        "130710037": {
          "id": 130710037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Da stimme ich zu. Leute, bitte tauscht euch mit den anderen in dieser Gruppe aus, falls ihr etwas findet.",
          "next": [
            130710038
          ]
        },
        "130710038": {
          "id": 130710038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Die Schönheit wird ihre Pflicht erfüllen!",
          "next": [
            130710039
          ]
        },
        "130710039": {
          "id": 130710039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1112,
          "text": "Wir von der IFK helfen auch. Keine Sorge, Leute.",
          "next": [
            130710040
          ]
        },
        "130710040": {
          "id": 130710040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Wollt ihr wetten, wer das hier überleben wird?",
          "next": [
            130710041
          ]
        },
        "130710041": {
          "id": 130710041,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "k",
          "next": [
            130710042
          ]
        },
        "130710042": {
          "id": 130710042,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Dr. Wahrheit hat den Chat verlassen.",
          "next": [
            130710043
          ]
        },
        "130710043": {
          "id": 130710043,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1315,
          "text": "Aaangriff!",
          "next": []
        }
