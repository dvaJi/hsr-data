  ],
  "sections": [
    {
      "id": 2013400,
      "startingMessageId": [
        201340000
      ],
      "messages": {
        "201340000": {
          "id": 201340000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Tut mir leid, ich habe deine Nachricht gerade erst gesehen.",
          "next": [
            201340001
          ]
        },
        "201340001": {
          "id": 201340001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Ich habe meine Mission in Penacony beendet. Jetzt kann ich mein Handy frei benutzen.",
          "next": [
            201340002
          ]
        },
        "201340002": {
          "id": 201340002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Das war wie im Gefängnis ...",
          "next": [
            201340003,
            201340005,
            201340007
          ]
        },
        "201340003": {
          "id": 201340003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich bin froh, dass es dir gut geht.",
          "next": [
            201340004
          ]
        },
        "201340004": {
          "id": 201340004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Haha, du bist einfach zu nett.",
          "next": [
            201340009
          ]
        },
        "201340005": {
          "id": 201340005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wo warst du?",
          "next": [
            201340006
          ]
        },
        "201340006": {
          "id": 201340006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Das ist eine lange Geschichte, ich erzähle es dir ein anderes Mal.",
          "next": [
            201340009
          ]
        },
        "201340007": {
          "id": 201340007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mach’s gut, es ist vorbei.",
          "next": [
            201340008
          ]
        },
        "201340008": {
          "id": 201340008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Haha, ganz {F#die Alte}{M#der Alte} ...",
          "next": [
            201340009
          ]
        },
        "201340009": {
          "id": 201340009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Da wir sonst nichts zu tun haben, sollen wir ein einfaches Würfelspiel spielen, damit uns nicht langweilig wird?",
          "next": [
            201340010
          ]
        },
        "201340010": {
          "id": 201340010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Wenn du eine Zahl würfelst, die gleich hoch wie meine oder höher ist, erzähle ich dir etwas darüber, was nach dem Spektakel im Themenpark passiert ist.",
          "next": [
            201340011,
            201340054
          ]
        },
        "201340011": {
          "id": 201340011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Okay, es kann losgehen.",
          "next": [
            201340012
          ]
        },
        "201340012": {
          "id": 201340012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Dann fangen wir an.",
          "next": [
            201340013
          ]
        },
        "201340013": {
          "id": 201340013,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Aventurin würfelt eine 3",
          "next": [
            201340014
          ]
        },
        "201340014": {
          "id": 201340014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Du bist dran. Du hast gute Chancen!",
          "next": [
            201340015,
            201340022
          ]
        },
        "201340015": {
          "id": 201340015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Na dann ...",
          "next": [
            201340016
          ]
        },
        "201340016": {
          "id": 201340016,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "{NICKNAME} würfelt eine 2",
          "next": [
            201340017
          ]
        },
        "201340017": {
          "id": 201340017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Sollen wir noch einmal würfeln? Die Regeln besagen nicht, dass das nicht geht.",
          "next": [
            201340018
          ]
        },
        "201340018": {
          "id": 201340018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Dann versuche ich es noch einmal.",
          "next": [
            201340019
          ]
        },
        "201340019": {
          "id": 201340019,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "{NICKNAME} würfelt eine 6",
          "next": [
            201340020
          ]
        },
        "201340020": {
          "id": 201340020,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Raus damit.",
          "next": [
            201340021
          ]
        },
        "201340021": {
          "id": 201340021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Das Glück ist dir hold ...",
          "next": [
            201340025
          ]
        },
        "201340022": {
          "id": 201340022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Eine hohe Zahl!",
          "next": [
            201340023
          ]
        },
        "201340023": {
          "id": 201340023,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "{NICKNAME} würfelt eine 4",
          "next": [
            201340024
          ]
        },
        "201340024": {
          "id": 201340024,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ich hab mehr als du. Raus damit!",
          "next": [
            201340025
          ]
        },
        "201340025": {
          "id": 201340025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Nachdem dem letzten Schwerthieb der Emanatorin trat ich in einen Fluss, der so breit war, wie ich es noch nie in meinem Leben gesehen hatte.",
          "next": [
            201340026
          ]
        },
        "201340026": {
          "id": 201340026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Ich dachte, ich sei am Ende meines Lebens angelangt.",
          "next": [
            201340027
          ]
        },
        "201340027": {
          "id": 201340027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Aber ... wie du siehst, lebe ich glücklicherweise noch.",
          "next": [
            201340028
          ]
        },
        "201340028": {
          "id": 201340028,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Aventurin würfelt eine 1",
          "next": [
            201340029
          ]
        },
        "201340029": {
          "id": 201340029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Du bist dran.",
          "next": [
            201340030,
            201340033
          ]
        },
        "201340030": {
          "id": 201340030,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Glück gehabt ...",
          "next": [
            201340031
          ]
        },
        "201340031": {
          "id": 201340031,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "{NICKNAME} würfelt eine 5",
          "next": [
            201340032
          ]
        },
        "201340032": {
          "id": 201340032,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Und dann?",
          "next": [
            201340036
          ]
        },
        "201340033": {
          "id": 201340033,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "So kann ich gar nicht verlieren!",
          "next": [
            201340034
          ]
        },
        "201340034": {
          "id": 201340034,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "{NICKNAME} würfelt eine 6",
          "next": [
            201340035
          ]
        },
        "201340035": {
          "id": 201340035,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Und danach?",
          "next": [
            201340036
          ]
        },
        "201340036": {
          "id": 201340036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Als ihr das Geheimnis unter der Wasseroberfläche von Penacony berührtet, fiel ich tatsächlich auch in die Urtraumlandschaft. Der Kernstein der Bewahrung schützte mich vor dem Einfluss der Nichtigkeit, doch er wurde dadurch auch völlig zertrümmert.",
          "next": [
            201340037
          ]
        },
        "201340037": {
          "id": 201340037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Zum Glück traf ich einen Ritter der Schönheit. Mit seiner Hilfe konnte ich schließlich dem Tod entkommen.",
          "next": [
            201340038
          ]
        },
        "201340038": {
          "id": 201340038,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Aventurin würfelt eine 1",
          "next": [
            201340039
          ]
        },
        "201340039": {
          "id": 201340039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Du bist dran. Der Ablauf muss trotzdem eingehalten werden.",
          "next": [
            201340040,
            201340043
          ]
        },
        "201340040": {
          "id": 201340040,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "So schnell gewonnen ...",
          "next": [
            201340041
          ]
        },
        "201340041": {
          "id": 201340041,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "{NICKNAME} würfelt eine 2",
          "next": [
            201340042
          ]
        },
        "201340042": {
          "id": 201340042,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Du hast wirklich Glück!",
          "next": [
            201340046
          ]
        },
        "201340043": {
          "id": 201340043,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was ist dann passiert?",
          "next": [
            201340044
          ]
        },
        "201340044": {
          "id": 201340044,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "{NICKNAME} würfelt eine 6",
          "next": [
            201340045
          ]
        },
        "201340045": {
          "id": 201340045,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Ein sicherer Sieg, {F#meine Liebe}{M#mein Lieber}.",
          "next": [
            201340046
          ]
        },
        "201340046": {
          "id": 201340046,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Und dann kam dank euch die ganze Wahrheit ans Licht. Topaz und Jade übernahmen die Verantwortung für Penacony, und ich konnte meinen Urlaub genießen.",
          "next": [
            201340047
          ]
        },
        "201340047": {
          "id": 201340047,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "*seufz* Ich habe in letzter Zeit ständig Alpträume und wache jedes Mal schweißgebadet auf.",
          "next": [
            201340048
          ]
        },
        "201340048": {
          "id": 201340048,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Dank der Vermittlung der IFK an einen Doktor des Chaos, der mich gründlich untersucht hat, wird es nun besser.",
          "next": [
            201340049
          ]
        },
        "201340049": {
          "id": 201340049,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Das ist mehr oder weniger die Lage. Du hast wirklich Glück, ich musste tatsächlich alles erzählen.",
          "next": [
            201340050,
            201340051
          ]
        },
        "201340050": {
          "id": 201340050,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich hab schon gemerkt, dass du absichtlich verloren hast. Als ob du so viel Pech hast.",
          "next": [
            201340052
          ]
        },
        "201340051": {
          "id": 201340051,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du bist glücklich, verloren zu haben? Du hast mich schon wieder reingelegt.",
          "next": [
            201340052
          ]
        },
        "201340052": {
          "id": 201340052,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Die Traumlandschaft ist zwar sehr lebhaft, aber es gibt dort nur wenige „Freunde“. Da musste ich mir doch selbst etwas einfallen lassen, oder?",
          "next": [
            201340053
          ]
        },
        "201340053": {
          "id": 201340053,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Danke, dass du mit mir diese schwere Zeit verbracht hast. Ich hoffe, ich habe dich nicht gestört.",
          "next": [
            201340057
          ]
        },
        "201340054": {
          "id": 201340054,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich lass es.",
          "next": [
            201340055
          ]
        },
        "201340055": {
          "id": 201340055,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Oh, wie schade. Ich dachte, es würde dir gefallen.",
          "next": [
            201340056
          ]
        },
        "201340056": {
          "id": 201340056,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Nun ja, es ist ja nicht so wichtig.",
          "next": [
            201340057
          ]
        },
        "201340057": {
          "id": 201340057,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Dann werde ich mich woanders amüsieren. Bis dann!",
          "next": []
        }
      },
      "participatingContactsIds": [
        1304,
        8001
      ]
    }
  ]
}