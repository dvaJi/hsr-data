  "relatedMessages": [11134, 11152, 20023, 20045, 20046],
  "sections": [
    {
      "id": 2004600,
      "startingMessageId": [200460000],
      "messages": {
        "200460000": {
          "id": 200460000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Bist du da? Ich habe eine Frage.",
          "next": [200460001]
        },
        "200460001": {
          "id": 200460001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Liest du",
          "next": [200460002]
        },
        "200460002": {
          "id": 200460002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "normalerweise gerne?",
          "next": [200460003, 200460006, 200460009]
        },
        "200460003": {
          "id": 200460003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Niemals.",
          "next": [200460004]
        },
        "200460004": {
          "id": 200460004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Ach ja?",
          "next": [200460005]
        },
        "200460005": {
          "id": 200460005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Ich glaube, da sind wir uns ähnlich.",
          "next": [200460012]
        },
        "200460006": {
          "id": 200460006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Manchmal lese ich Webromane von anderen Planeten.",
          "next": [200460007]
        },
        "200460007": {
          "id": 200460007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Webromane? Sind die interessant?",
          "next": [200460008]
        },
        "200460008": {
          "id": 200460008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Seufz, ich lese nicht gerne.",
          "next": [200460012]
        },
        "200460009": {
          "id": 200460009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich lese alles.",
          "next": [200460010]
        },
        "200460010": {
          "id": 200460010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Das ist beeindruckend.",
          "next": [200460011]
        },
        "200460011": {
          "id": 200460011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Seufz, ich lese nicht gerne.",
          "next": [200460012]
        },
        "200460012": {
          "id": 200460012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Lesen ist ermüdend und strengt mein Gehirn an.",
          "next": [200460013]
        },
        "200460013": {
          "id": 200460013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Sobald ich anfange, eine neue Seite zu lesen, habe ich bereits vergessen, was auf der vorherigen stand.",
          "next": [200460014]
        },
        "200460014": {
          "id": 200460014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Ich weiß nicht, wie Bronya es schafft, so viel zu lesen.",
          "next": [200460015, 200460018]
        },
        "200460015": {
          "id": 200460015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sie hat das schon von Kindheit an so gelernt.",
          "next": [200460016]
        },
        "200460016": {
          "id": 200460016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Hmpf, stimmt.",
          "next": [200460017]
        },
        "200460017": {
          "id": 200460017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Dann ist es also normal, dass ich nicht viel lesen kann, oder?",
          "next": [200460021]
        },
        "200460018": {
          "id": 200460018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Der erste Schritt ist immer der schwerste.",
          "next": [200460019]
        },
        "200460019": {
          "id": 200460019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Es ist sehr schwierig.",
          "next": [200460020]
        },
        "200460020": {
          "id": 200460020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Als ich klein war, habe ich einfach nur versucht, auf den Straßen zu überleben. Wie hätte ich da noch Zeit gehabt, um zu lesen?",
          "next": [200460021]
        },
        "200460021": {
          "id": 200460021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Ha, Bronya hat mir eine ellenlange Liste mit Buchempfehlungen gegeben.",
          "next": [200460022]
        },
        "200460022": {
          "id": 200460022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Ich habe ein paar davon ausprobiert und kam nicht über die ersten Seiten hinaus.",
          "next": [200460023]
        },
        "200460023": {
          "id": 200460023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Ich kann sie nicht lesen, ohne müde zu werden.",
          "next": [200460024]
        },
        "200460024": {
          "id": 200460024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Oh, aber ein Buch ist ziemlich interessant.",
          "next": [200460025]
        },
        "200460025": {
          "id": 200460025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "<i>Geschichte von den Winterlanden</i>. Ich habe nur ein paar Seiten gelesen, aber es ist ziemlich spannend.",
          "next": [200460026]
        },
        "200460026": {
          "id": 200460026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Das ist alles. Ich wollte nur etwas Luft ablassen.",
          "next": [200460027]
        },
        "200460027": {
          "id": 200460027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Aber ich werde weiterlesen und mich anstrengen, um das Buch fertigzulesen.",
          "next": []
        }
      },
      "participatingContactsIds": [1102, 8001]
    }
  ]
}
