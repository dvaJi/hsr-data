      "messages": {
        "200060000": {
          "id": 200060000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Ich habe in den Datenbanken ein altes Express-Kommunikationsprotokoll gefunden, und eine der Kategorisierungen lautet „Papawitze“.",
          "next": [
            200060001
          ]
        },
        "200060001": {
          "id": 200060001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Ich verstehe es nicht ganz, aber mein Instinkt sagt mir, dass du dazu in der Lage sein solltest.",
          "next": [
            200060002,
            200060004
          ]
        },
        "200060002": {
          "id": 200060002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "So stellst du dir also meine Person vor?",
          "next": [
            200060003
          ]
        },
        "200060003": {
          "id": 200060003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Ja.",
          "next": [
            200060006
          ]
        },
        "200060004": {
          "id": 200060004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das verstehe ich nicht.",
          "next": [
            200060005
          ]
        },
        "200060005": {
          "id": 200060005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Ich glaube, du hast das Protokoll noch nicht gesehen ...",
          "next": [
            200060006
          ]
        },
        "200060006": {
          "id": 200060006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Das steht dort.",
          "next": [
            200060007
          ]
        },
        "200060007": {
          "id": 200060007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "„Ich habe Wachdienst mit Himeko. Sie redet im Schlaf.“ Pom-Pom",
          "next": [
            200060008
          ]
        },
        "200060008": {
          "id": 200060008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Eine Stunde später gibt es einen weiteren Eintrag.",
          "next": [
            200060009
          ]
        },
        "200060009": {
          "id": 200060009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "„Auf Wachdienst mit Pom-Pom. Redet schon seit fast einer Stunde im Schlaf.“ Himeko",
          "next": [
            200060010
          ]
        },
        "200060010": {
          "id": 200060010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Was hältst du davon, {NICKNAME}?",
          "next": [
            200060011,
            200060013
          ]
        },
        "200060011": {
          "id": 200060011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "...",
          "next": [
            200060012
          ]
        },
        "200060012": {
          "id": 200060012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Okay, sieht so aus, als könnten diese Kategorisierungen vorerst so bleiben.",
          "next": [
            200060015
          ]
        },
        "200060013": {
          "id": 200060013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das ist eindeutig eine Horrorgeschichte.",
          "next": [
            200060014
          ]
        },
        "200060014": {
          "id": 200060014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Ist dem so? Ich habe es nicht verstanden, aber es hört sich so an, als bräuchten wir eine zusätzliche Markierung dafür.",
          "next": [
            200060015
          ]
        },
        "200060015": {
          "id": 200060015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Das Protokoll endet hier. Ich habe Himeko und Pom-Pom gefragt. Keiner von ihnen konnte sich an den Vorfall erinnern.",
          "next": [
            200060016
          ]
        },
        "200060016": {
          "id": 200060016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "„Wer ist derjenige, der im Schlaf spricht?“. Es sieht so aus, als könnten wir uns nur auf andere Einträge in der Datenbank verlassen, um diese Informationen zu überprüfen.",
          "next": [
            200060018
          ]
        },
        "200060017": {
          "id": 200060017,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Daumen hoch]",
          "next": []
        },
        "200060018": {
          "id": 200060018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich neige zu der Annahme, dass die Datenbank allmächtig ist.",
          "next": [
            200060017
          ]
        }
      },
