        },
        "201610019": {
          "id": 201610019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1222,
          "text": "Es ist in der Tat ein wichtiges Thema, wie du schnell die wichtigsten Tinkturen und Heilmittel für äußere Verletzungen herstellen kannst.",
          "next": [
            201610022
          ]
        },
        "201610020": {
          "id": 201610020,
          "type": "Text",
          "sender": "Player",
