        "200960001": {
          "id": 200960001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1112,
          "text": "Ich bin gerade am Überlegen und bräuchte deine Meinung.",
          "next": [
            200960002
          ]
        },
        "200960002": {
          "id": 200960002,
          "type": "Sticker",
