      ],
      "messages": {
        "130410000": {
          "id": 130410000,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Schwarzer Schwan hat dir eine Nachrichtenanfrage geschickt.",
          "next": [
            130410001
          ]
        },
        "130410001": {
          "id": 130410001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "{NICKNAME}, ich nehme an, du hast die Traumblase schon gesehen.",
          "next": [
            130410002
          ]
        },
        "130410002": {
          "id": 130410002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Entspann dich, ich will nur wissen, ob sich schon jemand nach dir erkundigt hat.",
          "next": [
            130410003,
            130410006
          ]
        },
        "130410003": {
          "id": 130410003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du bist Schwarzer Schwan?",
          "next": [
            130410004
          ]
        },
        "130410004": {
          "id": 130410004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "In der Tat, die Wahrerin der Erinnerungen, wie du weißt.",
          "next": [
            130410005
          ]
        },
        "130410005": {
          "id": 130410005,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1307,
          "next": [
            130410007
          ]
        },
        "130410006": {
          "id": 130410006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Stimmt etwas nicht?",
          "next": [
            130410007
          ]
        },
        "130410007": {
          "id": 130410007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Ich werde es kurz machen: Die Närrin namens „Sparkle“ sucht derzeit überall nach dieser Traumblase.",
          "next": [
            130410008
          ]
        },
        "130410008": {
          "id": 130410008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Ich weiß nicht, welche Absichten sie hat, aber wenn ich zulasse, dass sie ihr in die Hände fällt, wird das nur noch mehr Verwirrung stiften.",
          "next": [
            130410009
          ]
        },
        "130410009": {
          "id": 130410009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "{NICKNAME}, könntest du mir einen Gefallen tun und die Traumblase zurückholen?",
          "next": [
            130410010,
            130410012
          ]
        },
        "130410010": {
          "id": 130410010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Natürlich!",
          "next": [
            130410011
          ]
        },
        "130410011": {
          "id": 130410011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Deine Einstellung gefällt mir. Ich werde mit dir gehen.",
          "next": [
            130410014
          ]
        },
        "130410012": {
          "id": 130410012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Warum machst du es nicht selbst?",
          "next": [
            130410013
          ]
        },
        "130410013": {
          "id": 130410013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Ich habe nicht erwartet, dass du mich abweisen würdest ... Gut, ich könnte selbst gehen.",
          "next": [
            130410014
          ]
        },
        "130410014": {
          "id": 130410014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Aber ich kann mich nicht mehr erinnern, wo wir die Traumblase zurückgelassen haben.",
          "next": [
            130410015
          ]
        },
        "130410015": {
          "id": 130410015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Kannst du mir auf die Sprünge helfen?",
          "next": [
            130410016,
            130410022,
            130410028
          ]
        },
        "130410016": {
          "id": 130410016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Im Traumlandschaft-Shop, Dr. Edward hat sie.",
          "next": [
            130410017
          ]
        },
        "130410017": {
          "id": 130410017,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Du erinnerst dich nicht mehr?",
          "next": [
            130410018
          ]
        },
        "130410018": {
          "id": 130410018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Hehe!",
          "next": [
            130410019
          ]
        },
        "130410019": {
          "id": 130410019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Na ja, mein Gedächtnis ist nichts im Vergleich zu der echten Wahrerin der Erinnerungen.",
          "next": [
            130410020
          ]
        },
        "130410020": {
          "id": 130410020,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1307,
          "next": [
            130410021
          ]
        },
        "130410021": {
          "id": 130410021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Ah, ich kann nicht glauben, dass ich die Informationen so leicht bekommen habe. Ich war wirklich besorgt, dass ich auffliegen würde!",
          "next": [
            130410034
          ]
        },
        "130410022": {
          "id": 130410022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich habe sie gestohlen, in eine Flasche SeelFroh gesteckt und dann weggeworfen.",
          "next": [
            130410023
          ]
        },
        "130410023": {
          "id": 130410023,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Sie schwimmt wahrscheinlich immer noch im Traum irgendeiner Mülltonne herum.",
          "next": [
            130410024
          ]
        },
        "130410024": {
          "id": 130410024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Magst du Mülltonnen wirklich so sehr?",
          "next": [
            130410025
          ]
        },
        "130410025": {
          "id": 130410025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Ich sagte doch, das Mülltonnen-Königreich war ein Riesenspaß! Keiner hat es mir je geglaubt.",
          "next": [
            130410026
          ]
        },
        "130410026": {
          "id": 130410026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Offenbar hat es sich gelohnt, mich als Sampo zu verkleiden.",
          "next": [
            130410027
          ]
        },
        "130410027": {
          "id": 130410027,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1307,
          "next": [
            130410034
          ]
        },
        "130410028": {
          "id": 130410028,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich frage mich ... Wie kann eine Wahrerin der Erinnerungen eine Erinnerung vergessen?",
          "next": [
            130410029
          ]
        },
        "130410029": {
          "id": 130410029,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Bist du wirklich Schwarzer Schwan?",
          "next": [
            130410030
          ]
        },
        "130410030": {
          "id": 130410030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Hm? Du hast mich durchschaut?",
          "next": [
            130410031
          ]
        },
        "130410031": {
          "id": 130410031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Na ja, ich bin doch keine echte Wahrerin der Erinnerungen.",
          "next": [
            130410032
          ]
        },
        "130410032": {
          "id": 130410032,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1307,
          "next": [
            130410033
          ]
        },
        "130410033": {
          "id": 130410033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Ich hätte nicht gedacht, dass du mich so schnell durchschaut hast, wie blöd.",
          "next": [
            130410034
          ]
        },
        "130410034": {
          "id": 130410034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Bei Streichen geht es immer um das Hin und Her! Du hast ja erlebt, wie viel Spaß ich mit der Wahrerin der Erinnerungen habe ...",
          "next": [
            130410035
          ]
        },
        "130410035": {
          "id": 130410035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Du kannst von ihr lernen, {F#junge Namenlose}{M#junger Namenloser}.",
          "next": [
            130410036
          ]
        },
        "130410036": {
          "id": 130410036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Das ist erst der Anfang, ich komme wieder!",
          "next": [
            130410037
          ]
        },
        "130410037": {
          "id": 130410037,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1307,
          "next": [
            130410038,
            130410039
          ]
        },
        "130410038": {
          "id": 130410038,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nur zu. Wir werden sehen, wer wen beim nächsten Mal austrickst!",
          "next": [
            130410040
          ]
        },
        "130410039": {
          "id": 130410039,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bitte komm nicht wieder! Es kommt nie etwas Gutes dabei heraus, wenn man mit einer Närrin abhängt ...",
          "next": [
            130410040
          ]
        },
        "130410040": {
          "id": 130410040,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Nachricht wurde nicht gesendet.",
          "next": [
            130410041
          ]
        },
        "130410041": {
          "id": 130410041,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Dieser Benutzer existiert nicht.",
          "next": []
        }
      },
