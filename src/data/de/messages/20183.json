  "sections": [
    {
      "id": 2018300,
      "startingMessageId": [
        201830000
      ],
      "messages": {
        "201830000": {
          "id": 201830000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Grauhaar, willst du mit *uns* ein Rätselspiel spielen?",
          "next": [
            201830001
          ]
        },
        "201830001": {
          "id": 201830001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Was ist morgens klein, mittags groß und abends wieder klein, und am nächsten Tag geht es von vorne los?",
          "next": [
            201830002,
            201830003,
            201830004
          ]
        },
        "201830002": {
          "id": 201830002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Es sind Tribbie und die anderen Lehrerinnen!",
          "next": [
            201830005
          ]
        },
        "201830003": {
          "id": 201830003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Es sind Samen!",
          "next": [
            201830005
          ]
        },
        "201830004": {
          "id": 201830004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Es ist mein Kampfeswille!",
          "next": [
            201830005
          ]
        },
        "201830005": {
          "id": 201830005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Ganz nah dran, die Antwort ist – das Leben.",
          "next": [
            201830006
          ]
        },
        "201830006": {
          "id": 201830006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "So wie Samen, so wie *wir* – das Leben ist ein ewiger Kreislauf, ohne Ende.",
          "next": [
            201830007
          ]
        },
        "201830007": {
          "id": 201830007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Nochmal!",
          "next": [
            201830008
          ]
        },
        "201830008": {
          "id": 201830008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Was kann alles verschlingen? Es verschlingt Vögel und wilde Tiere, Blumen und Bäume. Es zerbeißt Eisen, zerfrisst Stahl und zermalmt sogar harte Steine zu Staub ... und am Ende vertreibt es die Dunkelheit?",
          "next": [
            201830009
          ]
        },
        "201830009": {
          "id": 201830009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Es ist „die Zeit“, oder?",
          "next": [
            201830010
          ]
        },
        "201830010": {
          "id": 201830010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Früher hat Trianne gesagt, dass kaum jemand diese beiden Rätsel lösen kann. Sie meinte, Grauhaar muss unbedingt mal versuchen.",
          "next": [
            201830011
          ]
        },
        "201830011": {
          "id": 201830011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Eigentlich ist das auch das, was *wir* dir sagen wollen.",
          "next": [
            201830012,
            201830013
          ]
        },
        "201830012": {
          "id": 201830012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Frau Trianne ...",
          "next": [
            201830015
          ]
        },
        "201830013": {
          "id": 201830013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das Leben und die Zeit ...",
          "next": [
            201830014
          ]
        },
        "201830014": {
          "id": 201830014,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ich verstehe.",
          "next": [
            201830015
          ]
        },
        "201830015": {
          "id": 201830015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Trianne hat den Samen der Hoffnung hinterlassen und die Zeit wird die Blume als Antwort hervorbringen.",
          "next": [
            201830016
          ]
        },
        "201830016": {
          "id": 201830016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "*Wir* werden uns ganz sicher am Ende des Westwinds wiedersehen.",
          "next": []
        }
      },
      "participatingContactsIds": [
        1403,
        8001
      ]
    }
  ]
}