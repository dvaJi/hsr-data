  "sections": [
    {
      "id": 2009800,
      "startingMessageId": [
        200980000
      ],
      "messages": {
        "200980000": {
          "id": 200980000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Übrigens, wie gefällt es dir so auf der Xianzhou? Gibt es irgendwelche Schwierigkeiten?",
          "next": [
            200980001,
            200980003,
            200980005
          ]
        },
        "200980001": {
          "id": 200980001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nein, alles läuft bestens.",
          "next": [
            200980002
          ]
        },
        "200980002": {
          "id": 200980002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Fantastisch. Das freut mich.",
          "next": [
            200980008
          ]
        },
        "200980003": {
          "id": 200980003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Um ehrlich zu sein, musste ich dort schon ein paar Schwierigkeiten in Kauf nehmen.",
          "next": [
            200980004
          ]
        },
        "200980004": {
          "id": 200980004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ja, verstehe. Das kenne ich nur zu gut.",
          "next": [
            200980008
          ]
        },
        "200980005": {
          "id": 200980005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "So habe ich darüber eigentlich nie nachgedacht. Ich werde sowieso nicht für immer hierbleiben.",
          "next": [
            200980006
          ]
        },
        "200980006": {
          "id": 200980006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Hm, da hast du wohl recht ...",
          "next": [
            200980007
          ]
        },
        "200980007": {
          "id": 200980007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Stimmt schon, aber als einer der Namenlosen wirst du trotzdem ziemlich oft mit der Xianzhou zu tun haben.",
          "next": [
            200980008
          ]
        },
        "200980008": {
          "id": 200980008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Das alles ist nicht weiter wichtig. Ich war nur gerade in Gedanken versunken.",
          "next": [
            200980009
          ]
        },
        "200980009": {
          "id": 200980009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Als ich hier das erste Mal auftauchte, war ich ziemlich ratlos.",
          "next": [
            200980010
          ]
        },
        "200980010": {
          "id": 200980010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ein Kind in einer fremden Umgebung und in einer Gesellschaft, über die es nichts wusste.",
          "next": [
            200980011
          ]
        },
        "200980011": {
          "id": 200980011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Wenn ich an die Angst und Beklemmung von damals zurückdenke, erinnert mich das irgendwie an dich.",
          "next": [
            200980012
          ]
        },
        "200980012": {
          "id": 200980012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ich machte mir Sorgen, dass es dir auch so gehen könnte.",
          "next": [
            200980013,
            200980015
          ]
        },
        "200980013": {
          "id": 200980013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Danke, dass du dir solche Gedanken machst, kleine Gui. Aber ich hab mich schon daran gewöhnt.",
          "next": [
            200980014
          ]
        },
        "200980014": {
          "id": 200980014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ja, das stimmt wohl. Du hast schon viel mehr durchgemacht als ich.",
          "next": [
            200980017
          ]
        },
        "200980015": {
          "id": 200980015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ah, das war damals eine harte Zeit für dich. Ich kann das gut nachempfinden.",
          "next": [
            200980016
          ]
        },
        "200980016": {
          "id": 200980016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Hah, mach dir keine Sorgen um mich. Kümmere dich lieber um dich selbst! Ernsthaft, du musst zwischendurch immer wieder eine Verschnaufpause einlegen, egal wie beschäftigt du bist.",
          "next": [
            200980017
          ]
        },
        "200980017": {
          "id": 200980017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Nun, lass es mich noch einmal sagen.",
          "next": [
            200980018
          ]
        },
        "200980018": {
          "id": 200980018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Wenn du jemals Hilfe brauchst, kannst du immer auf deine kleine Gui zählen.",
          "next": [
            200980019
          ]
        },
        "200980019": {
          "id": 200980019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Und falls ich nichts ausrichten kann, dann kenne ich Leute bei den Wolkenrittern und der Zehn-Fürsten-Kommission!",
          "next": [
            200980020
          ]
        },
        "200980020": {
          "id": 200980020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Heehee!",
          "next": []
        }
      },
      "participatingContactsIds": [
        1210,
        8001
      ]
    }
  ]
}