  "sections": [
    {
      "id": 2009700,
      "startingMessageId": [200970000],
      "messages": {
        "200970000": {
          "id": 200970000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Erinnerst du dich an mich, {NICKNAME}?",
          "next": [200970001, 200970003, 200970006]
        },
        "200970001": {
          "id": 200970001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Na klar, die kleine Gui!",
          "next": [200970002]
        },
        "200970002": {
          "id": 200970002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ha, sogar du nennst mich schon kleine Gui.",
          "next": [200970010]
        },
        "200970003": {
          "id": 200970003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tut mir leid, ich habe in letzter Zeit zu viele neue Leute kennengelernt. Wer bist du noch mal?",
          "next": [200970004]
        },
        "200970004": {
          "id": 200970004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Autsch, und dabei dachte ich, mich könnte man nicht vergessen.",
          "next": [200970005]
        },
        "200970005": {
          "id": 200970005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ätheriumkrieg! Das Interastrale Turnierfestival! Guinaifen! Erinnerst du dich jetzt?",
          "next": [200970010]
        },
        "200970006": {
          "id": 200970006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tut mir leid, ich bin nicht an Teeblättern, einer Versicherung oder sonst was interessiert.",
          "next": [200970007]
        },
        "200970007": {
          "id": 200970007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Argh!",
          "next": [200970008]
        },
        "200970008": {
          "id": 200970008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Wovon redest du überhaupt?",
          "next": [200970009]
        },
        "200970009": {
          "id": 200970009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ätheriumkrieg! Das Interastrale Turnierfestival! Guinaifen! Erinnerst du dich jetzt?",
          "next": [200970010]
        },
        "200970010": {
          "id": 200970010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ein Rivale ist nur ein ehrgeiziger neuer Freund, also sind wir jetzt Freunde.",
          "next": [200970011]
        },
        "200970011": {
          "id": 200970011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Wo liegt deine Heimatwelt? Ich hatte nicht die Gelegenheit, dich danach zu fragen.",
          "next": [200970012, 200970015, 200970018]
        },
        "200970012": {
          "id": 200970012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich bin nur {F#eine Namenlose}{M#ein Namenloser} vom Astralexpress.",
          "next": [200970013]
        },
        "200970013": {
          "id": 200970013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Auch {F#eine Namenlose}{M#ein Namenloser} kommt von irgendwoher.",
          "next": [200970014]
        },
        "200970014": {
          "id": 200970014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Schon in Ordnung, wenn du es nicht sagen willst. In der Ferne ist jeder fremd.",
          "next": [200970021]
        },
        "200970015": {
          "id": 200970015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Meine Heimat ist gegenwärtig die Weite des Alls.",
          "next": [200970016]
        },
        "200970016": {
          "id": 200970016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Auch jemand, der zwischen den Sternen lebt, kommt von irgendwoher.",
          "next": [200970017]
        },
        "200970017": {
          "id": 200970017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Schon in Ordnung, wenn du es nicht sagen willst. In der Ferne ist jeder fremd.",
          "next": [200970021]
        },
        "200970018": {
          "id": 200970018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich kann mich nicht erinnern, woher ich komme.",
          "next": [200970019]
        },
        "200970019": {
          "id": 200970019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Oh.",
          "next": [200970020]
        },
        "200970020": {
          "id": 200970020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Da habe ich wohl einen wunden Punkt getroffen. Tut mir leid.",
          "next": [200970021]
        },
        "200970021": {
          "id": 200970021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Seufz, meine Heimatwelt wurde schon von der Legion zerstört, also genau genommen habe ich gar keine mehr.",
          "next": [200970022]
        },
        "200970022": {
          "id": 200970022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Wir sind wohl beide Außenweltler, die vorübergehend die Xianzhou ihr Zuhause nennen, was?",
          "next": [200970023]
        },
        "200970023": {
          "id": 200970023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Wenn du irgendwelche Probleme hast oder mit dem Leben auf der Xianzhou nicht klarkommst, kannst du jederzeit zu mir kommen.",
          "next": [200970024]
        },
        "200970024": {
          "id": 200970024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ich treibe mich meistens in der Aurum-Allee herum und führe dort meine Straßenkunst auf, aber manchmal streame ich auch ein bisschen.",
          "next": [200970025]
        },
        "200970025": {
          "id": 200970025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wenn du mal in der Klemme steckst oder von irgendwelchen zwielichtigen Gestalten verfolgt wirst, musst du es mir nur sagen.",
          "next": [200970026]
        },
        "200970026": {
          "id": 200970026,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ich bin stark.",
          "next": [200970027]
        },
        "200970027": {
          "id": 200970027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Verstanden, {F#meine verlässliche neue Freundin}{M#mein verlässlicher neuer Freund}!",
          "next": [200970028]
        },
        "200970028": {
          "id": 200970028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Lass uns irgendwann mal wieder gegeneinander kämpfen. Man sieht sich!",
          "next": []
        }
      },
      "participatingContactsIds": [1210, 8001]
    }
  ]
}
