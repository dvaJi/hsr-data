  "sections": [
    {
      "id": 2016500,
      "startingMessageId": [
        201650000
      ],
      "messages": {
        "201650000": {
          "id": 201650000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Hast du schon mal versucht, einen deiner Vorgesetzten im Astralexpress umzubringen?",
          "next": [
            201650001,
            201650006
          ]
        },
        "201650001": {
          "id": 201650001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wer sind denn meine Vorgesetzten?",
          "next": [
            201650002
          ]
        },
        "201650002": {
          "id": 201650002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Ich weiß es nicht.",
          "next": [
            201650003
          ]
        },
        "201650003": {
          "id": 201650003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wir haben hier flache Hierarchien.",
          "next": [
            201650004
          ]
        },
        "201650004": {
          "id": 201650004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Wir haben einen Schaffner, aber keinen Anführer.",
          "next": [
            201650005
          ]
        },
        "201650005": {
          "id": 201650005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Oh.",
          "next": [
            201650009
          ]
        },
        "201650006": {
          "id": 201650006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Umbringen?! Warum sollte ich den Schaffner umbringen?",
          "next": [
            201650007
          ]
        },
        "201650007": {
          "id": 201650007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Als Herausforderung.",
          "next": [
            201650008
          ]
        },
        "201650008": {
          "id": 201650008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Na gut.",
          "next": [
            201650009
          ]
        },
        "201650009": {
          "id": 201650009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Scheinbar steht ihr in einem sehr einfachen Verhältnis zueinander.",
          "next": [
            201650010
          ]
        },
        "201650010": {
          "id": 201650010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Das ist anders als die Arbeit, die ich gewohnt bin. Bei uns gibt es Groll und Dankesschuld, Vorgesetzte und Untergebene, und Freunde, mit denen man die Mittagspause verbringt.",
          "next": [
            201650011
          ]
        },
        "201650011": {
          "id": 201650011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das ist zu kompliziert.",
          "next": [
            201650012
          ]
        },
        "201650012": {
          "id": 201650012,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Je weniger ich weiß, desto besser.",
          "next": [
            201650013
          ]
        },
        "201650013": {
          "id": 201650013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Was Rache angeht ... Generalin Feixiao gestattet es mir, sie jederzeit herauszufordern.",
          "next": [
            201650014
          ]
        },
        "201650014": {
          "id": 201650014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Ich habe es schon mit heimlichen Angriffen versucht, mit Fallen und öffentlichen Duellen, aber nichts hat funktioniert.",
          "next": [
            201650015
          ]
        },
        "201650015": {
          "id": 201650015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Wenn du keine Erfahrung hast, kann ich das so weitergeben.",
          "next": [
            201650016
          ]
        },
        "201650016": {
          "id": 201650016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Weitergeben?",
          "next": [
            201650017
          ]
        },
        "201650017": {
          "id": 201650017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "An Generalin Feixiao weitergeben, dass du keine Erfahrung im Umbringen deiner Vorgesetzten hast.",
          "next": [
            201650018
          ]
        },
        "201650018": {
          "id": 201650018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Ich bin heute wieder daran gescheitert, die Generalin umzubringen. Die Generalin hat vorgeschlagen, dass ich mit anderen sprechen sollte, um auf neue Ideen zu kommen.",
          "next": [
            201650019,
            201650020
          ]
        },
        "201650019": {
          "id": 201650019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ein gewagter Vorschlag! Dafür hat sie Respekt verdient.",
          "next": [
            201650021
          ]
        },
        "201650020": {
          "id": 201650020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das ist doch verrückt! Warum solltest du dich auf so was einlassen?",
          "next": [
            201650021
          ]
        },
        "201650021": {
          "id": 201650021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Ich glaube, es ist immer gut, dazuzulernen.",
          "next": [
            201650022
          ]
        },
        "201650022": {
          "id": 201650022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Bei Attentaten geht es um die Präzision und nicht die Quantität.",
          "next": [
            201650023
          ]
        },
        "201650023": {
          "id": 201650023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Selbst wenn ich deinen Rat annehmen würde, müsste ich Umsetzung noch perfektionieren.",
          "next": [
            201650024
          ]
        },
        "201650024": {
          "id": 201650024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Danke für deine Antwort. Ich werde jemand anderen fragen.",
          "next": []
        }
      },
      "participatingContactsIds": [
        1223,
        8001
      ]
    }
  ]
}