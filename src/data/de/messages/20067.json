  "relatedMessages": [20067, 20068, 20069],
  "sections": [
    {
      "id": 2006700,
      "startingMessageId": [200670000],
      "messages": {
        "200670000": {
          "id": 200670000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Ich hoffe, ich kann dich das fragen, {NICKNAME}.",
          "next": [200670001]
        },
        "200670001": {
          "id": 200670001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Machst du jetzt das, was du auch machen willst? Entspricht es deinen Träumen?",
          "next": [200670002, 200670003, 200670004]
        },
        "200670002": {
          "id": 200670002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ja, tut es. Ich bin mit meiner jetzigen Situation zufrieden.",
          "next": [200670005]
        },
        "200670003": {
          "id": 200670003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Es ist nicht ganz, was ich mir vorgestellt habe, aber es ist nicht übel.",
          "next": [200670005]
        },
        "200670004": {
          "id": 200670004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich weiß nicht, ich kann mich nicht so gut erinnern.",
          "next": [200670005]
        },
        "200670005": {
          "id": 200670005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Ganz ehrlich wäre es mir nie in den Sinn gekommen, nicht als Pilotin zu arbeiten.",
          "next": [200670006]
        },
        "200670006": {
          "id": 200670006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Wenn mein Vater mich jetzt sehen könnte, wäre er vielleicht wütend, weil ich dem Schlachtfeld wie ein Feigling den Rücken gekehrt habe.",
          "next": [200670007, 200670008]
        },
        "200670007": {
          "id": 200670007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Auch ein Bürojob ist ein Schlachtfeld. Es kann nicht einfach gewesen sein, es so weit zu bringen.",
          "next": [200670009]
        },
        "200670008": {
          "id": 200670008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Dein Leben, deine Entscheidung. Solange die Arbeit erfüllend ist.",
          "next": [200670009]
        },
        "200670009": {
          "id": 200670009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Ja ... Ich bin nicht wie mein Vater ...",
          "next": [200670010]
        },
        "200670010": {
          "id": 200670010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Ich wünsche meiner Tochter nur eines ...",
          "next": [200670011]
        },
        "200670011": {
          "id": 200670011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Soll sie ruhig ein gewöhnliches Leben führen. Ich hoffe nur, dass es ihr gut geht.",
          "next": [200670012, 200670015]
        },
        "200670012": {
          "id": 200670012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich habe keine Kinder, also kann ich dazu nichts sagen ...",
          "next": [200670013]
        },
        "200670013": {
          "id": 200670013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Stimmt, vielleicht ist es etwas zu früh, um mit dir darüber zu reden.",
          "next": [200670014]
        },
        "200670014": {
          "id": 200670014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Immerhin bist du noch jung.",
          "next": [200670017]
        },
        "200670015": {
          "id": 200670015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich kann verstehen, dass Eltern gewisse Vorstellungen haben, aber ihre Kinder sollten auch ein Mitspracherecht haben.",
          "next": [200670016]
        },
        "200670016": {
          "id": 200670016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Immerhin sind sie noch jung.",
          "next": [200670017]
        },
        "200670017": {
          "id": 200670017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Manche Dinge kann man erst verstehen, wenn man sie selbst erlebt hat.",
          "next": [200670018]
        },
        "200670018": {
          "id": 200670018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Es sind so viele Jahre vergangen ...",
          "next": []
        }
      },
      "participatingContactsIds": [1207, 8001]
    }
  ]
}
