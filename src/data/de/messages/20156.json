  "sections": [
    {
      "id": 2015600,
      "startingMessageId": [
        201560000
      ],
      "messages": {
        "201560000": {
          "id": 201560000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Möchtest du den neuesten Klatsch hören?",
          "next": [
            201560001
          ]
        },
        "201560001": {
          "id": 201560001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Irgendjemand hat den Meister mit leckerem Essen bestochen, damit er ihm das Kämpfen beibringt.",
          "next": [
            201560002
          ]
        },
        "201560002": {
          "id": 201560002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Aber warte.",
          "next": [
            201560003
          ]
        },
        "201560003": {
          "id": 201560003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Da kommt noch etwas.",
          "next": [
            201560004
          ]
        },
        "201560004": {
          "id": 201560004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du hast schon begonnen.",
          "next": [
            201560005
          ]
        },
        "201560005": {
          "id": 201560005,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Welchen brandheißen Klatsch hast du für mich?",
          "next": [
            201560006
          ]
        },
        "201560006": {
          "id": 201560006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Momentan belausche ich zwei Schwerter, die gerade aneinanderrasseln.",
          "next": [
            201560007
          ]
        },
        "201560007": {
          "id": 201560007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Sie sind ziemlich laut.",
          "next": [
            201560008
          ]
        },
        "201560008": {
          "id": 201560008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Also von Lauschen kann man hier eigentlich nicht reden.",
          "next": [
            201560009
          ]
        },
        "201560009": {
          "id": 201560009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Das Breitschwert sagt, sein Meister ist ein dummer Junge, der nichts auf die Reihe bekommt.",
          "next": [
            201560010
          ]
        },
        "201560010": {
          "id": 201560010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Das Kurzschwert sagt, seine Meisterin ist ein kluges Mädchen, das ganz toll kochen kann.",
          "next": [
            201560011
          ]
        },
        "201560011": {
          "id": 201560011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Vor einiger Zeit sind das Mädchen und der dumme Junge einem Schwertmeister begegnet, als sie gerade draußen am Üben waren. Sie wollten von ihm lernen, aber der Meister hat sich geweigert.",
          "next": [
            201560012
          ]
        },
        "201560012": {
          "id": 201560012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Es sei denn, sie würden ihm zehn Tage lang die köstlichsten Speisen servieren, und keine davon zweimal.",
          "next": [
            201560013
          ]
        },
        "201560013": {
          "id": 201560013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Und? Haben sie es geschafft?",
          "next": [
            201560014
          ]
        },
        "201560014": {
          "id": 201560014,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Haben sie etwas lernen können?",
          "next": [
            201560015
          ]
        },
        "201560015": {
          "id": 201560015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Ich höre gerade noch zu.",
          "next": [
            201560016
          ]
        },
        "201560016": {
          "id": 201560016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Sie sagen, die beiden haben sich dann zusammengetan und dem Meister Bettlerhuhn aufgetischt.",
          "next": [
            201560017
          ]
        },
        "201560017": {
          "id": 201560017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Oh, und auch gedämpften Tofu",
          "next": [
            201560018
          ]
        },
        "201560018": {
          "id": 201560018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Gebratene Entenfüße",
          "next": [
            201560019
          ]
        },
        "201560019": {
          "id": 201560019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Hühnerzungensuppe",
          "next": [
            201560020
          ]
        },
        "201560020": {
          "id": 201560020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Gebratene Rindersehnen nach Yin-Yang-Art",
          "next": [
            201560021
          ]
        },
        "201560021": {
          "id": 201560021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Teigtaschen mit Lotusschinken",
          "next": [
            201560022
          ]
        },
        "201560022": {
          "id": 201560022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Ruyi-Rollen",
          "next": [
            201560023
          ]
        },
        "201560023": {
          "id": 201560023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Schweinehirnsuppe",
          "next": [
            201560024
          ]
        },
        "201560024": {
          "id": 201560024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Mir knurrt der Magen!",
          "next": [
            201560025
          ]
        },
        "201560025": {
          "id": 201560025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Ich gehe mal eben ein paar Snacks kaufen, dann komme ich wieder und lausche weiter.",
          "next": [
            201560026
          ]
        },
        "201560026": {
          "id": 201560026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "... Lausche den Herzen der Schwerter.",
          "next": [
            201560027
          ]
        },
        "201560027": {
          "id": 201560027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich bin vom Zuhören auch schon hungrig! Besorg auch ein paar für mich!",
          "next": [
            201560028
          ]
        },
        "201560028": {
          "id": 201560028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Kein Problem",
          "next": []
        }
      },
