      "messages": {
        "111490000": {
          "id": 111490000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Hallo!",
          "next": [111490001]
        },
        "111490001": {
          "id": 111490001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Bist du das, {NICKNAME}?",
          "next": [111490002]
        },
        "111490002": {
          "id": 111490002,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "[Grüßen]",
          "next": [111490003, 111490005]
        },
        "111490003": {
          "id": 111490003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ja, ich bin es. Und wer bist du?",
          "next": [111490004]
        },
        "111490004": {
          "id": 111490004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Das ist gut.",
          "next": [111490007]
        },
        "111490005": {
          "id": 111490005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tut mir leid, da liegt eine Verwechslung vor.",
          "next": [111490006]
        },
        "111490006": {
          "id": 111490006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Verwechslung? Die Nummer hier sagt aber etwas anderes.",
          "next": [111490007]
        },
        "111490007": {
          "id": 111490007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Bitte erlaube mir zuerst, mich vorzustellen.",
          "next": [111490008]
        },
        "111490008": {
          "id": 111490008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Ich bin Wilder von der Abteilung für strategische Investitionen der Interastralen Friedenskorporation.",
          "next": [111490009]
        },
        "111490009": {
          "id": 111490009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Ich habe dich kontaktiert, weil ich dir ein Videospiel empfehlen möchte.",
          "next": [111490010, 111490012, 111490015]
        },
        "111490010": {
          "id": 111490010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ein Spiel? Super! Was für ein Spiel?",
          "next": [111490011]
        },
        "111490011": {
          "id": 111490011,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Daumen hoch]",
          "next": [111490018]
        },
        "111490012": {
          "id": 111490012,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Verwirrt]",
          "next": [111490013]
        },
        "111490013": {
          "id": 111490013,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Entschuldige, können wir uns später unterhalten? Ich bin gerade mit etwas anderem beschäftigt.",
          "next": [111490014]
        },
        "111490014": {
          "id": 111490014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Hey, wimmle mich nicht einfach so ab! Vielleicht bist du interessiert, wenn ich dir mehr über dieses Spiel erzähle!",
          "next": [111490018]
        },
        "111490015": {
          "id": 111490015,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Verwirrt]",
          "next": [111490016]
        },
        "111490016": {
          "id": 111490016,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Kann dein Unternehmen nicht auf andere Art Werbung machen?",
          "next": [111490017]
        },
        "111490017": {
          "id": 111490017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Haha, selbst bei der heutigen fortgeschrittenen Technik sind Textnachrichten immer noch der beste Weg, um Leute zu erreichen.",
          "next": [111490018]
        },
        "111490018": {
          "id": 111490018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Erlaube mir bitte, zur Einführung zurückzukehren.",
          "next": [111490019]
        },
        "111490019": {
          "id": 111490019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Dieses Spiel heißt Ätheriumkrieg. Es ist eine übergalaktische, erstklassige Triple-A-Kulturmarke, die von der Abteilung für Marketingentwicklung der IFK in Zusammenarbeit mit dem Interastralen Netzwerk und den Interastralen Friedensmedien entwickelt wurde.",
          "next": [111490020, 111490022]
        },
        "111490020": {
          "id": 111490020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sollte mich das beeindrucken?",
          "next": [111490021]
        },
        "111490021": {
          "id": 111490021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Hahaha, immerhin ist es einer unserer absoluten Kracher.",
          "next": [111490024]
        },
        "111490022": {
          "id": 111490022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Komm bitte zur Sache ...",
          "next": [111490023]
        },
        "111490023": {
          "id": 111490023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Keine Sorge, ich bin gleich fertig.",
          "next": [111490024]
        },
        "111490024": {
          "id": 111490024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Ätheriumkrieg ist ein Kartensammelspiel, allerdings ist es ganz anders als die gewöhnlichen Kartentauschspiele, wie du sie dir vorstellst!",
          "next": [111490025]
        },
        "111490025": {
          "id": 111490025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Hier kannst du die hochmoderne Ätherkopiertechnologie der IFK nutzen, um jede Kreatur, die du jemals gesehen hast, in einer Äthermünze abzuspeichern.",
          "next": [111490026]
        },
        "111490026": {
          "id": 111490026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Und dann kannst du sie ausdrucken und mit ihnen gegen andere Spieler antreten! Lass deine Äthergeister für deinen Ruhm kämpfen!",
          "next": [111490027]
        },
        "111490027": {
          "id": 111490027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Na, wie klingt das? Ziemlich cool, oder?",
          "next": [111490028, 111490031, 111490034]
        },
        "111490028": {
          "id": 111490028,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Daumen hoch]",
          "next": [111490029]
        },
        "111490029": {
          "id": 111490029,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Hört sich spaßig an. Wo kann ich dieses Spiel spielen?",
          "next": [111490030]
        },
        "111490030": {
          "id": 111490030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Haha, fantastisch! Endlich habe ich einen neuen Spieler für mein Lieblingsspiel gefunden!",
          "next": [111490037]
        },
        "111490031": {
          "id": 111490031,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Verwirrt]",
          "next": [111490032]
        },
        "111490032": {
          "id": 111490032,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Das klingt aber nicht so unterhaltsam wie „Kosmische Kriegsführung“ ...",
          "next": [111490033]
        },
        "111490033": {
          "id": 111490033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Kosmische Kriegsführung? Im Ernst?! Dieser Müll ist nicht annähernd so spannend wie Ätheriumkrieg!",
          "next": [111490037]
        },
        "111490034": {
          "id": 111490034,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Schockiert]",
          "next": [111490035]
        },
        "111490035": {
          "id": 111490035,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Warum bist du plötzlich so aufgeregt?",
          "next": [111490036]
        },
        "111490036": {
          "id": 111490036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Nun, um ehrlich zu sein, bin ich selbst ein großer Fan von Ätheriumkrieg.",
          "next": [111490037]
        },
        "111490037": {
          "id": 111490037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Wie auch immer, falls du interessiert bist, komm gern zum Geschäftsviertel in Nietstadt auf Jarilo-VI, um es auszuprobieren!",
          "next": [111490038]
        },
        "111490038": {
          "id": 111490038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Wir veranstalten dort das Interastrale Turnierfestival, ein toller Offline-Rummel für alle Fans von Ätheriumkrieg! Komm vorbei und amüsiere dich!",
          "next": [111490039, 111490041]
        },
        "111490039": {
          "id": 111490039,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was für ein Zufall! Ich bin gerade auf Jarilo-VI.",
          "next": [111490040]
        },
        "111490040": {
          "id": 111490040,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Bleibt wo ihr seid, ich bin sofort da.",
          "next": [111490043]
        },
        "111490041": {
          "id": 111490041,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ähm ...",
          "next": [111490042]
        },
        "111490042": {
          "id": 111490042,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ich schaue vorbei, wenn ich Zeit habe.",
          "next": [111490043]
        },
        "111490043": {
          "id": 111490043,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Kein Problem! Ich werde hier auf dich warten!",
          "next": []
        }
      },
