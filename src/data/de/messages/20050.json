  "sections": [
    {
      "id": 2005000,
      "startingMessageId": [
        200500000
      ],
      "messages": {
        "200500000": {
          "id": 200500000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "{NICKNAME}.",
          "next": [
            200500001
          ]
        },
        "200500001": {
          "id": 200500001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Ich bin ein bisschen neugierig.",
          "next": [
            200500002
          ]
        },
        "200500002": {
          "id": 200500002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Bist du auf deinen Reisen je Gegnern begegnet, die bei dir einen dauerhaften Eindruck hinterlassen haben?",
          "next": [
            200500003,
            200500009,
            200500016
          ]
        },
        "200500003": {
          "id": 200500003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich habe in der Herta-Raumstation die Bestie des Jüngsten Gerichts der Antimaterie-Legion besiegt.",
          "next": [
            200500004
          ]
        },
        "200500004": {
          "id": 200500004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Die Bestie des Jüngsten Gerichts? Ich glaube, ich habe schon mal in der Wolkenritter-Kampflehre von ihr gelesen, aber ich kann mich nicht mehr genau erinnern ...",
          "next": [
            200500005
          ]
        },
        "200500005": {
          "id": 200500005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Immerhin hatte Xianzhou schon lange nicht mehr mit der Antimaterie-Legion zu kämpfen.",
          "next": [
            200500006
          ]
        },
        "200500006": {
          "id": 200500006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Stell dir einfach eine riesige Echse mit Flügeln vor.",
          "next": [
            200500007
          ]
        },
        "200500007": {
          "id": 200500007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Ich glaube, jetzt verstehe ich, was du meinst ...",
          "next": [
            200500008
          ]
        },
        "200500008": {
          "id": 200500008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Ich bin so eifersüchtig auf dich. Außerhalb der Xianzhou gibt es so viele Monster, an denen sich Schwertkünste erproben lassen.",
          "next": [
            200500019
          ]
        },
        "200500009": {
          "id": 200500009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich habe auf Jarilo-VI gegen einen sehr starken Hauptmann gekämpft.",
          "next": [
            200500010
          ]
        },
        "200500010": {
          "id": 200500010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Gegen einen Hauptmann? Was ist das?",
          "next": [
            200500011
          ]
        },
        "200500011": {
          "id": 200500011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Er entspricht einem Kommandanten der Wolkenritter auf der Luofu.",
          "next": [
            200500012
          ]
        },
        "200500012": {
          "id": 200500012,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Seine Hiebe waren gletscherhart, seine Waffe unkonventionell und sein Schild bestand aus einem Instrumentenkoffer.",
          "next": [
            200500013
          ]
        },
        "200500013": {
          "id": 200500013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Einem Instrumentenkoffer!",
          "next": [
            200500014
          ]
        },
        "200500014": {
          "id": 200500014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "In der Tat. In der Hand der Mächtigen kann sogar ein Grashalm oder ein Stöckchen zu einer Waffe werden.",
          "next": [
            200500015
          ]
        },
        "200500015": {
          "id": 200500015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Was für ein interessanter Gegner. Gegen den würde ich gerne kämpfen!",
          "next": [
            200500019
          ]
        },
        "200500016": {
          "id": 200500016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "In der Tat, aber ich habe sie alle besiegt!",
          "next": [
            200500017
          ]
        },
        "200500017": {
          "id": 200500017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Haha, dir muss man wirklich Respekt zollen! Ich hoffe, wir sehen uns in auf der Bühne von Stellarwettkampf.",
          "next": [
            200500018
          ]
        },
        "200500018": {
          "id": 200500018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Aber wenn es so weit ist, werde ich auf alle Fälle den Titel des Schwertmeisters einheimsen!",
          "next": [
            200500019
          ]
        },
        "200500019": {
          "id": 200500019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Der General denkt, dass meine Schwertkunst noch verbesserungsbedürftig ist, daher werde ich weiterhin hart trainieren.",
          "next": [
            200500020
          ]
        },
        "200500020": {
          "id": 200500020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Würdest du ein paar Kämpfe mit mir absolvieren, wenn du Zeit hast?",
          "next": [
            200500021
          ]
        },
        "200500021": {
          "id": 200500021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Ich gehe jetzt, bis bald!",
          "next": []
        }
      },
      "participatingContactsIds": [
        1209,
        8001
      ]
    }
  ]
}