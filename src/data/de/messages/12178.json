        },
        "121780025": {
          "id": 121780025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Können wir dreifache Beuteraten haben? Oder zehnfache?",
          "next": [
            121780028
          ]
        },
        "121780026": {
          "id": 121780026,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
