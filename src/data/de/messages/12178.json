            121780024,
            121780025,
            121780026
          ]
        },
        "121780024": {
          "id": 121780024,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Können wir diese Funktion dauerhaft beibehalten?",
