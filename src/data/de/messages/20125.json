  "relatedMessages": [13041, 13052, 20124, 20125, 20126],
  "sections": [
    {
      "id": 2012500,
      "startingMessageId": [201250000],
      "messages": {
        "201250000": {
          "id": 201250000,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Pom-Pom hat dir eine Nachrichtenanfrage geschickt.",
          "next": [201250001]
        },
        "201250001": {
          "id": 201250001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "{NICKNAME}, Pom! Hilfe, Pom!",
          "next": [201250002]
        },
        "201250002": {
          "id": 201250002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Offenbar ist etwas Seltsames in den Express eingedrungen!!!",
          "next": [201250003]
        },
        "201250003": {
          "id": 201250003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Pom-Pom hat es bis in den Maschinenraum verfolgt, Pom!!!",
          "next": [201250004]
        },
        "201250004": {
          "id": 201250004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Komm und rette Pom-Pom, Pom!",
          "next": [201250005, 201250006]
        },
        "201250005": {
          "id": 201250005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wer bist du noch mal?",
          "next": [201250007]
        },
        "201250006": {
          "id": 201250006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sparkle? Schon wieder??",
          "next": [201250007]
        },
        "201250007": {
          "id": 201250007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Wovon redest du da? Hier ist Pom-Pom, dein Schaffner, Pom!",
          "next": [201250008]
        },
        "201250008": {
          "id": 201250008,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1000,
          "next": [201250009]
        },
        "201250009": {
          "id": 201250009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Bei einer Routine-Untersuchung wurde der Kommunikator beschädigt. Deswegen muss Pom-Pom ein temporäres Konto verwenden, Pom!!!",
          "next": [201250010, 201250022]
        },
        "201250010": {
          "id": 201250010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich vermute immer noch, dass du Sparkle bist.",
          "next": [201250011]
        },
        "201250011": {
          "id": 201250011,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Weil ...",
          "next": [201250012, 201250015, 201250018]
        },
        "201250012": {
          "id": 201250012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Pom-Pom würde niemals so viel Pom sagen!",
          "next": [201250013]
        },
        "201250013": {
          "id": 201250013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Oh ... Ich verstehe ...",
          "next": [201250014]
        },
        "201250014": {
          "id": 201250014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Mein Plan, der Figur treu zu bleiben, ist nach hinten losgegangen, mein Fehler ...",
          "next": [201250045]
        },
        "201250015": {
          "id": 201250015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Pom-Pom würde jeden bestrafen, der in den Maschinenraum einbricht, Pom!",
          "next": [201250016]
        },
        "201250016": {
          "id": 201250016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Wow.",
          "next": [201250017]
        },
        "201250017": {
          "id": 201250017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Ich wusste nicht, dass dieser kleine Plüsch so gefährlich ist. Das ist ziemlich cool ...",
          "next": [201250045]
        },
        "201250018": {
          "id": 201250018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Niemand kommt in den Express, außer den Namenlosen und Gästen, die ausdrücklich eingeladen wurden!",
          "next": [201250019]
        },
        "201250019": {
          "id": 201250019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Oh?",
          "next": [201250020]
        },
        "201250020": {
          "id": 201250020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Das glaube ich nicht. Es gibt eine ganze Reihe von Anhaltern, die es erfolgreich geschafft haben.",
          "next": [201250021]
        },
        "201250021": {
          "id": 201250021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Glaubst du, dass es unter ihnen keine Maskierte Narren gibt?",
          "next": [201250045]
        },
        "201250022": {
          "id": 201250022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du bist es! Der große Schaffner!",
          "next": [201250023]
        },
        "201250023": {
          "id": 201250023,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Jetzt, da du ein verdächtiges temporäres Konto verwendest, ist die Katze endlich aus dem Sack!",
          "next": [201250024]
        },
        "201250024": {
          "id": 201250024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "?",
          "next": [201250025]
        },
        "201250025": {
          "id": 201250025,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Tut mir leid ... Aber ich wusste schon immer über dein Geheimnis Bescheid ... das Geheimnis des Expresses ...",
          "next": [201250026]
        },
        "201250026": {
          "id": 201250026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "?",
          "next": [201250027, 201250032, 201250037]
        },
        "201250027": {
          "id": 201250027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was im Maschinenraum versteckt, ist eigentlich ...",
          "next": [201250028]
        },
        "201250028": {
          "id": 201250028,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Der ehemalige Schaffner des Expresses!",
          "next": [201250029]
        },
        "201250029": {
          "id": 201250029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "?",
          "next": [201250030]
        },
        "201250030": {
          "id": 201250030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Hm? Stimmt das wirklich?",
          "next": [201250031]
        },
        "201250031": {
          "id": 201250031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Würdet ihr Leute vom Astralexpress so weit gehen?",
          "next": [201250042]
        },
        "201250032": {
          "id": 201250032,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Der Schaffner des Expresses ist in Wirklichkeit ein Maskierter Narr!",
          "next": [201250033]
        },
        "201250033": {
          "id": 201250033,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Pom-Pom und Aha sind Familie!",
          "next": [201250034]
        },
        "201250034": {
          "id": 201250034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "?",
          "next": [201250035]
        },
        "201250035": {
          "id": 201250035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Hm? Wieso weiß ich das nicht?",
          "next": [201250036]
        },
        "201250036": {
          "id": 201250036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Würdet ihr Namenlosen wirklich so weit gehen?",
          "next": [201250042]
        },
        "201250037": {
          "id": 201250037,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Jeder im Express wurde vom Schaffner des Expresses erschaffen!",
          "next": [201250038]
        },
        "201250038": {
          "id": 201250038,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Sie sind alle Pom-Pom!",
          "next": [201250039]
        },
        "201250039": {
          "id": 201250039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "?",
          "next": [201250040]
        },
        "201250040": {
          "id": 201250040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Hm? Nicht einmal ich würde mir so etwas ausdenken.",
          "next": [201250041]
        },
        "201250041": {
          "id": 201250041,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Im Ernst, mein Freund, das geht zu weit!",
          "next": [201250042]
        },
        "201250042": {
          "id": 201250042,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sparkle, du hast dich gerade offenbart.",
          "next": [201250043]
        },
        "201250043": {
          "id": 201250043,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Ah, ich dachte wirklich, es wäre wahr ... Ich war so aufgeregt, dass ich darauf völlig vergessen habe.",
          "next": [201250044]
        },
        "201250044": {
          "id": 201250044,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Es ist ziemlich dreist, so eine Lüge zu erfinden.",
          "next": [201250045]
        },
        "201250045": {
          "id": 201250045,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "So schnell durchschaut zu werden, ist einfach peinlich ...",
          "next": [201250046]
        },
        "201250046": {
          "id": 201250046,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Ich muss mich auf die nächste Unterhaltung besser vorbereiten. Ich melde mich wieder, wenn mir etwas Gutes einfällt.",
          "next": [201250047, 201250048]
        },
        "201250047": {
          "id": 201250047,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nur zu. Wir werden sehen, wer wen beim nächsten Mal austrickst!",
          "next": [201250049]
        },
        "201250048": {
          "id": 201250048,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bitte komm nicht wieder! Es kommt nie etwas Gutes dabei heraus, wenn man mit einer Närrin abhängt ...",
          "next": [201250049]
        },
        "201250049": {
          "id": 201250049,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Aaah ...",
          "next": [201250050]
        },
        "201250050": {
          "id": 201250050,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Du klingst langsam wie eine kaputte Schallplatte.",
          "next": [201250051]
        },
        "201250051": {
          "id": 201250051,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Wie kann man Spaß haben als „Ärger machen“ bezeichnen?",
          "next": [201250052]
        },
        "201250052": {
          "id": 201250052,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Hast du noch nie den Spruch gehört: „Etwas Spaß am Tag hält dir den Maskierten Narren vom Leib“?",
          "next": [201250053]
        },
        "201250053": {
          "id": 201250053,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Maskierte Narren mischen gerne Dinge im Leben auf, die an sich langweilig sind, und machen sie so noch schöner!",
          "next": [201250054]
        },
        "201250054": {
          "id": 201250054,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Also gut, ich habe noch etwas zu erledigen. Wenn du mich nicht wiedersehen willst, komme ich nicht wieder.",
          "next": [201250055]
        },
        "201250055": {
          "id": 201250055,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Gib mir das nächste Mal Bescheid, wenn du etwas Spaßiges vorhast.",
          "next": [201250056]
        },
        "201250056": {
          "id": 201250056,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1000,
          "next": [201250057, 201250058]
        },
        "201250057": {
          "id": 201250057,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "next": [201250059]
        },
        "201250058": {
          "id": 201250058,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "next": [201250059]
        },
        "201250059": {
          "id": 201250059,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Nachricht wurde nicht gesendet.",
          "next": [201250060]
        },
        "201250060": {
          "id": 201250060,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Dieses Konto wurde gelöscht.",
          "next": []
        }
      },
      "participatingContactsIds": [8001, 1000]
    }
  ]
}
