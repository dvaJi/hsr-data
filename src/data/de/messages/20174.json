      "messages": {
        "201740000": {
          "id": 201740000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "{NICKNAME}, entschuldige die Unterbrechung.",
          "next": [
            201740001
          ]
        },
        "201740001": {
          "id": 201740001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Ich habe ein paar Desserts und Getränke aus meiner Heimatstadt mitgebracht, aber ich bin mir nicht sicher, ob sie dir schmecken werden.",
          "next": [
            201740002
          ]
        },
        "201740002": {
          "id": 201740002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Das hier sind einige beliebte einheimische Produkte und auch persönliche Empfehlungen von mir.",
          "next": [
            201740003
          ]
        },
        "201740003": {
          "id": 201740003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich hätte nicht gedacht, dass du sie magst!",
          "next": [
            201740004
          ]
        },
        "201740004": {
          "id": 201740004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Seitdem ich das Familienoberhaupt bin, hatte ich kaum Gelegenheit, so etwas zu essen.",
          "next": [
            201740005
          ]
        },
        "201740005": {
          "id": 201740005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Aber als ich neulich sah, wie Himeko Kaffee kochte, erinnerte mich das daran, dass es in Penacony einen Barkeeper gibt, der SeelFroh in den Kaffee mischt und ihn mit kleinen, dazu passenden Süßspeisen serviert. Das hat mich auf eine tolle Idee gebracht.",
          "next": [
            201740006
          ]
        },
        "201740006": {
          "id": 201740006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Ich kann für Himeko eine Tasse Kaffee zubereiten. Das wird ihr gefallen.",
          "next": [
            201740007,
            201740009
          ]
        },
        "201740007": {
          "id": 201740007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sie ist eine echte Feinschmeckerin.",
          "next": [
            201740008
          ]
        },
        "201740008": {
          "id": 201740008,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ich danke dir in ihrem Namen.",
          "next": [
            201740011
          ]
        },
        "201740009": {
          "id": 201740009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich kann den Tag kaum erwarten, an dem ihr beide gegeneinander antretet.",
          "next": [
            201740010
          ]
        },
        "201740010": {
          "id": 201740010,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ich werde eine Sitzbank und Popcorn vorbereiten.",
          "next": [
            201740011
          ]
        },
        "201740011": {
          "id": 201740011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "März soll eine richtige Naschkatze sein.",
          "next": [
            201740012
          ]
        },
        "201740012": {
          "id": 201740012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Ich habe von einer Puddingpastete gehört, die aus Xianzhou-Zutaten hergestellt wird und fast genauso wie Freude des Unsterblichen schmecken soll.",
          "next": [
            201740013
          ]
        },
        "201740013": {
          "id": 201740013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Azurfarbener Drache, Weißer Tiger, weniger Kohlenhydrate!",
          "next": [
            201740014
          ]
        },
        "201740014": {
          "id": 201740014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Ein Ernährungsberater empfahl mir einmal, drei Tage und Nächte lang Heilpflanzen in SeelFroh ziehen zu lassen und es dann zu trinken, um den Kopf freizubekommen.",
          "next": [
            201740015
          ]
        },
        "201740015": {
          "id": 201740015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Ich hab’s versucht. Es schmeckte wie ein erfrischender Kräutertee.",
          "next": [
            201740016
          ]
        },
        "201740016": {
          "id": 201740016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Herr Yang und Dan Heng hatten mit dem Express alle Hände voll zu tun. Vielleicht könnte ihnen das weiterhelfen.",
          "next": [
            201740017,
            201740019
          ]
        },
        "201740017": {
          "id": 201740017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sehr nett von dir.",
          "next": [
            201740018
          ]
        },
        "201740018": {
          "id": 201740018,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ich werde alle Leckereien verdrücken.",
          "next": [
            201740021
          ]
        },
        "201740019": {
          "id": 201740019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich kann es kaum erwarten, sie zu probieren.",
          "next": [
            201740020
          ]
        },
        "201740020": {
