  "sections": [
    {
      "id": 2014700,
      "startingMessageId": [
        201470000
      ],
      "messages": {
        "201470000": {
          "id": 201470000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Hättest du eine Minute, {NICKNAME}?",
          "next": [
            201470001
          ]
        },
        "201470001": {
          "id": 201470001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Ich wüsste gerne, was du über „Kinder“ denkst.",
          "next": [
            201470002,
            201470004
          ]
        },
        "201470002": {
          "id": 201470002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich habe keine Kinder, also bin ich vielleicht nicht {F#die beste Ansprechpartnerin}{M#der beste Ansprechpartner}.",
          "next": [
            201470003
          ]
        },
        "201470003": {
          "id": 201470003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Schon in Ordnung. Wir waren doch alle einmal Kinder. Deshalb interessiere ich mich auch für deine Meinung.",
          "next": [
            201470006
          ]
        },
        "201470004": {
          "id": 201470004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hier hast du ein Kind! Schieß los!",
          "next": [
            201470005
          ]
        },
        "201470005": {
          "id": 201470005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Das ist die richtige Einstellung! Ich bin schon sehr auf deine Meinung gespannt.",
          "next": [
            201470006
          ]
        },
        "201470006": {
          "id": 201470006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Was, glaubst du, stimmt ein Kind glücklich?",
          "next": [
            201470007,
            201470009,
            201470011
          ]
        },
        "201470007": {
          "id": 201470007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ausreichend Essen, genügend Anziehsachen und die Liebe seiner Familie. So wie bei der Besatzung!",
          "next": [
            201470008
          ]
        },
        "201470008": {
          "id": 201470008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Klingt, als stünde sich die Besatzung wirklich sehr nahe. Ich hoffe, euer Glück bleibt euch ewig treu.",
          "next": [
            201470013
          ]
        },
        "201470009": {
          "id": 201470009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kein Ärger und keine Sorgen!",
          "next": [
            201470010
          ]
        },
        "201470010": {
          "id": 201470010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Du hast recht. Ein Kind soll in einer unschuldigen, sorglosen Welt leben.",
          "next": [
            201470013
          ]
        },
        "201470011": {
          "id": 201470011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich weiß nicht.",
          "next": [
            201470012
          ]
        },
        "201470012": {
          "id": 201470012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Ja, manchmal lässt sich Glück nur schwer in Worte fassen.",
          "next": [
            201470013
          ]
        },
        "201470013": {
          "id": 201470013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Ich habe Topaz und Aventurin dieselbe Frage gestellt.",
          "next": [
            201470014
          ]
        },
        "201470014": {
          "id": 201470014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Ihre Definitionen von Glück haben sich mit der Zeit geändert und gehen jetzt auseinander, aber wenn es darum geht, was ein Kind glücklich macht, sind sie einer Meinung.",
          "next": [
            201470015
          ]
        },
        "201470015": {
          "id": 201470015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Die Kinder im Waisenhaus wünschen sich vermutlich dasselbe.",
          "next": [
            201470016,
            201470019
          ]
        },
        "201470016": {
          "id": 201470016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kinderwünsche? Kann ich dir da irgendwie helfen?",
          "next": [
            201470017
          ]
        },
        "201470017": {
          "id": 201470017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Danke für dein Angebot, aber ich komme schon zurecht.",
          "next": [
            201470018
          ]
        },
        "201470018": {
          "id": 201470018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Die Kinder vertrauen mir ohne Wenn und Aber, und dieses Vertrauen will ich von ganzem Herzen erwidern.",
          "next": [
            201470025
          ]
        },
        "201470019": {
          "id": 201470019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du besuchst Waisenhäuser?",
          "next": [
            201470020
          ]
        },
        "201470020": {
          "id": 201470020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Ja, ich bin sehr oft dort.",
          "next": [
            201470021
          ]
        },
        "201470021": {
          "id": 201470021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Du wirkst überrascht.",
          "next": [
            201470022
          ]
        },
        "201470022": {
          "id": 201470022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Na ja ... Ich habe schon einige Geschichten über das Bonajade-Pfandhaus gehört.",
          "next": [
            201470023
          ]
        },
        "201470023": {
          "id": 201470023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Verstehe ... Kein Grund zur Sorge. Bei Erwachsenen fallen die Entscheidungen natürlich strenger aus.",
          "next": [
            201470024
          ]
        },
        "201470024": {
          "id": 201470024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Die Kinder vertrauen mir ohne Wenn und Aber, und dieses Vertrauen will ich von ganzem Herzen erwidern.",
          "next": [
            201470025
          ]
        },
        "201470025": {
          "id": 201470025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1314,
          "text": "Jedenfalls muss ich jetzt langsam los.",
          "next": []
        }
      },
      "participatingContactsIds": [
        1314,
        8001
      ]
    }
  ]
}