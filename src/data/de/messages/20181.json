      "messages": {
        "201810000": {
          "id": 201810000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Grauhaar!",
          "next": [
            201810001
          ]
        },
        "201810001": {
          "id": 201810001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Willst du leckeres Popcorn, gerade frisch aus dem Raketenwerfer?",
          "next": [
            201810002,
            201810004,
            201810006
          ]
        },
        "201810002": {
          "id": 201810002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Komm gleich!",
          "next": [
            201810003
          ]
        },
        "201810003": {
          "id": 201810003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Komm schnell, du musst es heiß essen!",
          "next": [
            201810008
          ]
        },
        "201810004": {
          "id": 201810004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Popcorn?",
          "next": [
            201810005
          ]
        },
        "201810005": {
          "id": 201810005,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Hast du es mit dem Raketenwerfer gemacht?",
          "next": [
            201810008
          ]
        },
        "201810006": {
          "id": 201810006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich mache Diät, tut mir leid.",
          "next": [
            201810007
          ]
        },
        "201810007": {
          "id": 201810007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Nur ein Bissen, wird schon nichts passieren!",
          "next": [
            201810008
          ]
        },
        "201810008": {
          "id": 201810008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Als *wir* den Raketenwerfer erfunden haben, haben wir nicht daran gedacht, es so zu benutzen.",
          "next": [
            201810009
          ]
        },
        "201810009": {
          "id": 201810009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Wir wollten nur, dass die Leute mit dem Raketenwerfer in andere Städte reisen können und keine Angst vor der Schwarzen Flut haben müssen.",
          "next": [
            201810010
          ]
        },
        "201810010": {
          "id": 201810010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Und man könnte es auch verwenden, um bei Festen große Feuerwerke abzuschießen.",
          "next": [
            201810011
          ]
        },
        "201810011": {
          "id": 201810011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Wer hätte gedacht, dass Trianne so viele Maiskörner dabei hat, sie einfach in den Raketenwerfer wirft, wenn sie Hunger hat, und daraus süßes Popcorn macht.",
          "next": [
            201810012
          ]
        },
        "201810012": {
          "id": 201810012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Wer hätte gedacht, dass sich Trinnon nach der Mahlzeit in eine Decke wickelt und auf dem Raketenwerfer einschläft, ohne geweckt werden zu können.",
          "next": [
            201810013
          ]
        },
        "201810013": {
          "id": 201810013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Jetzt verstehe ich, das ist der Tribbie-Express!",
          "next": [
            201810014
          ]
        },
        "201810014": {
          "id": 201810014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Genau! Im Wagen von Grauhaar haltet ihr auch auf der Reise die Landschaften fest und könnt euch am Feuer ausruhen.",
          "next": [
            201810015
          ]
        },
        "201810015": {
          "id": 201810015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Allein der Gedanke macht mich schon glücklich!",
          "next": [
            201810016,
            201810017,
            201810019
          ]
        },
        "201810016": {
          "id": 201810016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kommt doch irgendwann mal im Express vorbei!",
          "next": [
            201810020
          ]
        },
        "201810017": {
          "id": 201810017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Im Express gibt es auch drei Kleine.",
          "next": [
            201810018
          ]
        },
        "201810018": {
          "id": 201810018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Hmm! Ich möchte gerne die Person mit den rosa Haaren kennenlernen!",
          "next": [
            201810020
          ]
        },
        "201810019": {
          "id": 201810019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Die Badewanne in dem großen Raum ist genauso gut wie das Goldene Bad, komm!",
          "next": [
            201810020
          ]
        },
        "201810020": {
          "id": 201810020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Lass uns zusammen durch den Himmel reisen, wenn der Wagen von Grauhaar wieder gestartet wird!",
          "next": [
            201810021
          ]
        },
        "201810021": {
          "id": 201810021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Ich freu mich schon!",
          "next": []
        }
      },
