      "messages": {
        "111330000": {
          "id": 111330000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Wie geht es dir, {NICKNAME}? Hast du Belobog schon verlassen?",
          "next": [
            111330001,
            111330003
          ]
        },
        "111330001": {
          "id": 111330001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich bleibe noch etwas länger.",
          "next": [
            111330002
          ]
        },
        "111330002": {
          "id": 111330002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Toll! Es gibt da nämlich etwas, wobei ich deine Hilfe benötige.",
          "next": [
            111330008
          ]
        },
        "111330003": {
          "id": 111330003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich wollte gerade abreisen.",
          "next": [
            111330004
          ]
        },
        "111330004": {
          "id": 111330004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Oh, tatsächlich? Dann werde ich dich nicht weiter behelligen.",
          "next": [
            111330005,
            111330006
          ]
        },
        "111330005": {
          "id": 111330005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Fragen kostet ja nichts.",
          "next": [
            111330007
          ]
        },
        "111330006": {
          "id": 111330006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Jetzt mach’s doch nicht so spannend ...",
          "next": [
            111330007
          ]
        },
        "111330007": {
          "id": 111330007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Um ehrlich zu sein, gibt es da etwas, wobei ich deine Hilfe benötige.",
          "next": [
            111330008
          ]
        },
        "111330008": {
          "id": 111330008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Ich bin erst seit Kurzem wieder in der Oberwelt. Ich hatte noch nicht einmal Zeit, mich umzusehen, und die Silbermähnen-Wachen wollen mich unbedingt zu ihrem Berater machen.",
          "next": [
            111330009
          ]
        },
        "111330009": {
          "id": 111330009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Du hast gerade erst die Sache mit dem Stellaron erledigt, und schon versuchen diese ungeduldigen Dummköpfe, einen Gegenangriff im Fragmentum zu starten.",
          "next": [
            111330010
          ]
        },
        "111330010": {
          "id": 111330010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Aber in Sachen Fragmentum verfügt wohl nur deine Leute und du über das nötige Fachwissen.",
          "next": [
            111330011,
            111330013
          ]
        },
        "111330011": {
          "id": 111330011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Noch ein Kampf?",
          "next": [
            111330012
          ]
        },
        "111330012": {
          "id": 111330012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Haha, in gewisser Weise schon.",
          "next": [
            111330015
          ]
        },
        "111330013": {
          "id": 111330013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich kenne das Fragmentum wie meine Westentasche.",
          "next": [
            111330014
          ]
        },
        "111330014": {
          "id": 111330014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Das ist die richtige Einstellung.",
          "next": [
            111330015
          ]
        },
        "111330015": {
          "id": 111330015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Wenn du dich der Operation anschließen möchtest, suche einen Verbindungsoffizier namens William im Sperrgebiet der Silbermähnen-Wache auf.",
          "next": [
            111330016
          ]
        },
        "111330016": {
          "id": 111330016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Das Fragmentum, das sie säubern wollen, heißt „Korridor der verhallenden Echos“. Ein ziemlich gruseliger Name, nicht wahr? Aber für dich ist das sicher ein Kinderspiel.",
          "next": []
        }
      },
      "participatingContactsIds": [
        12,
        8001
      ]
    }
  ]
}