  "sections": [
    {
      "id": 2009100,
      "startingMessageId": [200910000],
      "messages": {
        "200910000": {
          "id": 200910000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Ich habe von Bailu gelernt, wie man einige Kräuter verwendet, und war beeindruckt von der magischen Pharmakologie.",
          "next": [200910001, 200910005]
        },
        "200910001": {
          "id": 200910001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sind die auch Notfallmedikamente?",
          "next": [200910002]
        },
        "200910002": {
          "id": 200910002,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Es scheint, dass sie Pom-Pom schon mal was davon gegeben hat.",
          "next": [200910003]
        },
        "200910003": {
          "id": 200910003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Wirklich?",
          "next": [200910004]
        },
        "200910004": {
          "id": 200910004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Notfallmedikamente können direkt und einfach eingenommen werden.",
          "next": [200910007]
        },
        "200910005": {
          "id": 200910005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Gibt es Medikamente, die gut schmecken?",
          "next": [200910006]
        },
        "200910006": {
          "id": 200910006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "... Vielleicht. Ich werde es mir später ansehen.",
          "next": [200910007]
        },
        "200910007": {
          "id": 200910007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Sie hat von dem Konzept „Eine Person, ein Medikament“ gesprochen. Wenn ich das meistern könnte, würde ich in der Lage sein, verschiedene Medikamente für verschiedene Leute im Express herzustellen.",
          "next": [200910008]
        },
        "200910008": {
          "id": 200910008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Schade, dass ich die heilende Wolkensang-Magie nicht beherrsche.",
          "next": [200910009, 200910011, 200910013]
        },
        "200910009": {
          "id": 200910009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wollen wir mal die Omni-Synthesemaschine fragen?",
          "next": [200910010]
        },
        "200910010": {
          "id": 200910010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Vielleicht geht es noch schneller, wenn ich Bailu danach frage.",
          "next": [200910015]
        },
        "200910011": {
          "id": 200910011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich dachte, du weißt doch alles ...",
          "next": [200910012]
        },
        "200910012": {
          "id": 200910012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Ich muss noch sehr viel lernen.",
          "next": [200910015]
        },
        "200910013": {
          "id": 200910013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Erinnerst du dich an nichts?",
          "next": [200910014]
        },
        "200910014": {
          "id": 200910014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Ich besitze nicht die Kraft dafür.",
          "next": [200910015]
        },
        "200910015": {
          "id": 200910015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Obwohl die Kraft des Hohen Ältesten weiter vererbt wird, ist sie für mich eher eine Art „Wissen“ als eine „Erinnerung“.",
          "next": [200910016]
        },
        "200910016": {
          "id": 200910016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Die Ereignisse, die ich sehen kann ... Sie existierten, bevor ich mir meiner selbst bewusst wurde.",
          "next": [200910017, 200910023]
        },
        "200910017": {
          "id": 200910017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wenn es sich um „Wissen“ handelt, hängt die Verwendung davon von deiner Entscheidung ab.",
          "next": [200910018]
        },
        "200910018": {
          "id": 200910018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Wirklich?",
          "next": [200910019]
        },
        "200910019": {
          "id": 200910019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Ich hoffe, ich habe die richtige Entscheidung getroffen.",
          "next": [200910020]
        },
        "200910020": {
          "id": 200910020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du hast vielen geholfen.",
          "next": [200910021]
        },
        "200910021": {
          "id": 200910021,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Und ich bin auch keine Ausnahme.",
          "next": [200910022]
        },
        "200910022": {
          "id": 200910022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Das ist gut.",
          "next": []
        },
        "200910023": {
          "id": 200910023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich möchte auch alles verstehen.",
          "next": [200910024]
        },
        "200910024": {
          "id": 200910024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Es ist kein gutes Gefühl. Es gibt unzählige Bilder und Geräusche in deinem Kopf, aber du bist nie anwesend.",
          "next": [200910025]
        },
        "200910025": {
          "id": 200910025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Auch wenn ich es niemals verstehen kann – vielleicht fühlt es sich ähnlich wie die Stimme des Stellarons an?",
          "next": [200910026]
        },
        "200910026": {
          "id": 200910026,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Sie ist ... gewöhnungsbedürftig.",
          "next": [200910027]
        },
        "200910027": {
          "id": 200910027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Vielleicht ...",
          "next": [200910028]
        },
        "200910028": {
          "id": 200910028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Vielleicht komme ich eines Tages damit zurecht wie du.",
          "next": []
        }
      },
      "participatingContactsIds": [1002, 8001]
    }
  ]
}
