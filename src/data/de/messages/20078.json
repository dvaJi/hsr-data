      "messages": {
        "200780000": {
          "id": 200780000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Hallo, {NICKNAME}.",
          "next": [
            200780001,
            200780003
          ]
        },
        "200780001": {
          "id": 200780001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was ist los? Ist etwas passiert?",
          "next": [
            200780002
          ]
        },
        "200780002": {
          "id": 200780002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Nichts. Ich habe nur an dich gedacht.",
          "next": [
            200780005
          ]
        },
        "200780003": {
          "id": 200780003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du schon wieder.",
          "next": [
            200780004
          ]
        },
        "200780004": {
          "id": 200780004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Jep, störe ich gerade?",
          "next": [
            200780005
          ]
        },
        "200780005": {
          "id": 200780005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Ich hätte nicht erwartet, die einzige Person in diesem Kino zu sein.",
          "next": [
            200780006
          ]
        },
        "200780006": {
          "id": 200780006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Ich dachte, diesen Film würden mehr Leute mögen.",
          "next": [
            200780007
          ]
        },
        "200780007": {
          "id": 200780007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "„Aus der Vergangenheit“",
          "next": [
            200780008,
            200780011
          ]
        },
        "200780008": {
          "id": 200780008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Schaust du den Film allein?",
          "next": [
            200780009
          ]
        },
        "200780009": {
          "id": 200780009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Falls du die anderen Stellaron-Jäger meinst ...",
          "next": [
            200780010
          ]
        },
        "200780010": {
          "id": 200780010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Jeder hat nun mal andere Vorlieben, deshalb bestehe ich nicht darauf, dass sie mitkommen.",
          "next": [
            200780013
          ]
        },
        "200780011": {
          "id": 200780011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Lass uns diesen Film das nächste Mal zusammen anschauen.",
          "next": [
            200780012
          ]
        },
        "200780012": {
          "id": 200780012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Alles klar, ich nehme dich beim Wort.",
          "next": [
            200780013
          ]
        },
        "200780013": {
          "id": 200780013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Welche Art von Filmen magst du, {NICKNAME}?",
          "next": [
            200780014
          ]
        },
        "200780014": {
          "id": 200780014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Im Moment ...",
          "next": [
            200780015,
            200780019,
            200780022
          ]
        },
        "200780015": {
          "id": 200780015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Biografien.",
          "next": [
            200780016
          ]
        },
        "200780016": {
          "id": 200780016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Ich mag es auch, mir das Leben anderer anzuschauen oder darüber zu lesen.",
          "next": [
            200780017
          ]
        },
        "200780017": {
          "id": 200780017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Ich hoffe allerdings, dass niemand aus meinem Leben einen Film macht.",
          "next": [
            200780018
          ]
        },
        "200780018": {
          "id": 200780018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Mir gefällt der Gedanke nicht, dass irgendein Regisseur der Welt seine eigene Interpretation meines Lebens zeigt.",
          "next": [
            200780025
          ]
        },
        "200780019": {
          "id": 200780019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Actionfilme.",
          "next": [
            200780020
          ]
        },
        "200780020": {
          "id": 200780020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Ich finde mich oft in ähnlichen Situationen wie in solchen Filmen wieder.",
          "next": [
            200780021
          ]
        },
        "200780021": {
          "id": 200780021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Für andere Kinogänger mag das spannend sein, aber mir fehlt dabei die Immersion.",
          "next": [
            200780025
          ]
        },
        "200780022": {
          "id": 200780022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Science-Fiction.",
          "next": [
            200780023
          ]
        },
        "200780023": {
          "id": 200780023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Obwohl es sich um Geschichten handelt, die in der Zukunft spielen?",
          "next": [
            200780024
          ]
        },
        "200780024": {
          "id": 200780024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Dinge aus der Vergangenheit wiederholen sich oft in der Zukunft.",
          "next": [
            200780025
          ]
        },
        "200780025": {
          "id": 200780025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Okay, der Film fängt gleich an.",
          "next": [
            200780026
          ]
        },
        "200780026": {
          "id": 200780026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Wir sprechen uns bald, bis dann.",
          "next": [
            200780027,
            200780028
          ]
        },
        "200780027": {
          "id": 200780027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Viel Spaß beim Film.",
          "next": [
            200780029
          ]
        },
        "200780028": {
          "id": 200780028,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
