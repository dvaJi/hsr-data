        },
        "130870007": {
          "id": 130870007,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Wo ist mein Anteil?",
          "next": [
            130870008
          ]
        },
        "130870008": {
          "id": 130870008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 146,
          "text": "Es tut mir leid, aber gemäß den Nutzungsbedingungen des Themenparks hast du keinen Anspruch auf diese Erträge.",
          "next": [
            130870009
          ]
        },
        "130870009": {
          "id": 130870009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 146,
          "text": "Hast du nicht bereits genug Geschenke von uns erhalten?",
