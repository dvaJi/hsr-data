  "sections": [
    {
      "id": 2002600,
      "startingMessageId": [
        200260000
      ],
      "messages": {
        "200260000": {
          "id": 200260000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Weißt du, wo Peppy sich in letzter Zeit herumtreibt, {NICKNAME}?",
          "next": [
            200260001,
            200260002
          ]
        },
        "200260001": {
          "id": 200260001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tut mir leid, ich war vorhin beschäftigt und habe nicht wirklich auf Peppy geachtet.",
          "next": [
            200260003
          ]
        },
        "200260002": {
          "id": 200260002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Peppy? Ich nehme an, er stromert immer noch auf der Raumstation herum. Wo soll er sonst auch hin?",
          "next": [
            200260003
          ]
        },
        "200260003": {
          "id": 200260003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Stimmt etwas nicht?",
          "next": [
            200260004
          ]
        },
        "200260004": {
          "id": 200260004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Ich warte auf das Ergebnis von Peppys Untersuchung bei der Medizinabteilung.",
          "next": [
            200260005
          ]
        },
        "200260005": {
          "id": 200260005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Er scheint am Vorderbein zu hinken, aber es ist keine Verletzung zu sehen.",
          "next": [
            200260006
          ]
        },
        "200260006": {
          "id": 200260006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Ich habe mir die Überwachungsvideos der Raumstation angesehen, aber dort sieht man auch nicht, wie er sich am Bein verletzt hat.",
          "next": [
            200260007,
            200260009
          ]
        },
        "200260007": {
          "id": 200260007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Na ja, Haustiere sind wie ungezogene Kinder. Vielleicht tut er nur so?",
          "next": [
            200260008
          ]
        },
        "200260008": {
          "id": 200260008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Ich weiß nicht ... Peppy ist alles andere als ungezogen.",
          "next": [
            200260011
          ]
        },
        "200260009": {
          "id": 200260009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Deiner Beschreibung nach hat Peppy sich vielleicht auch an der Pfote verletzt, so wie Arlan?",
          "next": [
            200260010
          ]
        },
        "200260010": {
          "id": 200260010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Das ergibt keinen Sinn. Peppy hat keine sichtbaren Verletzungen.",
          "next": [
            200260011
          ]
        },
        "200260011": {
          "id": 200260011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "...",
          "next": [
            200260012
          ]
        },
        "200260012": {
          "id": 200260012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Ich habe Peppys Ergebnisse.",
          "next": [
            200260013
          ]
        },
        "200260013": {
          "id": 200260013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Hier steht, er ist bei bester Gesundheit.",
          "next": [
            200260014
          ]
        },
        "200260014": {
          "id": 200260014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Er ist also überhaupt nicht verletzt.",
          "next": [
            200260015
          ]
        },
        "200260015": {
          "id": 200260015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Es gibt keine Anzeichen von gebrochenen Knochen.",
          "next": [
            200260016
          ]
        },
        "200260016": {
          "id": 200260016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Also wieso würde Peppy so tun, als hätte er ein Hinkebein? Was hat er davon?",
          "next": [
            200260017
          ]
        },
        "200260017": {
          "id": 200260017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Ich habe nicht die geringste Ahnung.",
          "next": [
            200260018,
            200260022
          ]
        },
        "200260018": {
          "id": 200260018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du hast in letzter Zeit so viel zu tun. Du hast wahrscheinlich schon lange nicht mehr mit ihm gespielt.",
          "next": [
            200260019
          ]
        },
        "200260019": {
          "id": 200260019,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Also hat er getan, als wäre er verletzt, damit du mehr Zeit mit ihm verbringst.",
          "next": [
            200260020
          ]
        },
        "200260020": {
          "id": 200260020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Möglich. Ich bin schon länger nicht mehr mit ihm Gassi gegangen.",
          "next": [
            200260021
          ]
        },
        "200260021": {
          "id": 200260021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Ich muss auch wieder mehr von den Kaustäbchen kaufen, die er so gern mag.",
          "next": [
            200260026
          ]
        },
        "200260022": {
          "id": 200260022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Vielleicht imitiert er unterbewusst, wie Arlan ausgesehen hat.",
          "next": [
            200260023
          ]
        },
        "200260023": {
          "id": 200260023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Arlan hat sich während der Invasion der Legion an der Hand verletzt.",
          "next": [
            200260024
          ]
        },
        "200260024": {
          "id": 200260024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Peppy war damals die ganze Zeit bei ihm.",
          "next": [
            200260025
          ]
        },
        "200260025": {
          "id": 200260025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Du hast recht. Vielleicht imitiert er das Verhalten der Leute in seinem Umfeld ...",
          "next": [
            200260026
          ]
        },
        "200260026": {
          "id": 200260026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Auf jeden Fall gut, dass Peppy nicht verletzt ist.",
          "next": [
            200260027
          ]
        },
        "200260027": {
          "id": 200260027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Der Kleine ist ziemlich clever ...",
          "next": [
            200260028
          ]
        },
        "200260028": {
          "id": 200260028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Was soll ich sagen?",
          "next": [
            200260029
          ]
        },
        "200260029": {
          "id": 200260029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Wie der Besitzer, so das Haustier.",
          "next": []
        }
      },
      "participatingContactsIds": [
        1009,
        8001
      ]
    }
  ]
}