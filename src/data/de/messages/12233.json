      ],
      "messages": {
        "122330000": {
          "id": 122330000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Sind alle da?",
          "next": [
            122330001
          ]
        },
        "122330001": {
          "id": 122330001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Ich habe eine wichtige Ankündigung.",
          "next": [
            122330002,
            122330008,
            122330012
          ]
        },
        "122330002": {
          "id": 122330002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wir kriegen endlich ein Gehalt? Super!",
          "next": [
            122330003
          ]
        },
        "122330003": {
          "id": 122330003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Leider gibt es dafür noch keine Pläne.",
          "next": [
            122330004
          ]
        },
        "122330004": {
          "id": 122330004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Die Ausgaben der Namenlosen werden vorübergehend weiterhin von der Navigatorin verwaltet. Sollte einer unserer Passagiere etwas benötigen, könnt ihr bei mir zusätzliches Taschengeld beantragen.",
          "next": [
            122330005
          ]
        },
        "122330005": {
          "id": 122330005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Apropos, warum scheint sich Herr Yang eigentlich nie um Geld zu sorgen ...?",
          "next": [
            122330006
          ]
        },
        "122330006": {
          "id": 122330006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Herr Yang ist eine Ausnahme!",
          "next": [
            122330007
          ]
        },
        "122330007": {
          "id": 122330007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Seid neidisch, Jungspund.",
          "next": [
            122330017
          ]
        },
        "122330008": {
          "id": 122330008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Die Speisekarte im Imbisswagen wird aktualisiert? Endlich ist der Tag gekommen!",
          "next": [
            122330009
          ]
        },
        "122330009": {
          "id": 122330009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Ist {F#Passagierin}{M#Passagier} {NICKNAME} mit der aktuellen Speisekarte unzufrieden?",
          "next": [
            122330010
          ]
        },
        "122330010": {
          "id": 122330010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Ich schätze, {F#sie}{M#er} will sich wahrscheinlich mal wieder über die Fertiggerichte beschweren!",
          "next": [
            122330011
          ]
        },
        "122330011": {
          "id": 122330011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Dann gebe ich dir einen persönlichen Rat: Scrolle weniger durch Videos von Essens-Vloggern.",
          "next": [
            122330017
          ]
        },
        "122330012": {
          "id": 122330012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Okay, ich gebe es zu. Ich habe Dan Hengs Schulter-Nacken-Massagegerät ausgeliehen.",
          "next": [
            122330013
          ]
        },
        "122330013": {
          "id": 122330013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Du warst das also.",
          "next": [
            122330014
          ]
        },
        "122330014": {
          "id": 122330014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Es heißt übrigens nur „ausleihen“, wenn man vorher um Erlaubnis fragt. Wenn nicht, nennt man das „stehlen“.",
          "next": [
            122330015
          ]
        },
        "122330015": {
          "id": 122330015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Es ist nur ein Massagegerät. Wenn {NICKNAME} eins möchte, können wir eins bestellen.",
          "next": [
            122330016
          ]
        },
        "122330016": {
          "id": 122330016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Ich habe gerade nebenbei den Preis überprüft. Am besten überlassen wir die Rechnung {NICKNAME}.",
          "next": [
            122330017
          ]
        },
        "122330017": {
          "id": 122330017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Zurück zur Sache: Was ich euch mitteilen wollte, ist, dass der Astralexpress bald einer der Anteilseigner des Penacony-Konglomerats sein wird.",
          "next": [
            122330018
          ]
        },
        "122330018": {
          "id": 122330018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Die Familie und die IFK werden bald offizielle Verhandlungen einleiten, was das Projekt des Wiederaufbaus der Traumlandschaft sowie finanzielle Pläne für die Zukunft angeht.",
          "next": [
            122330019
          ]
        },
        "122330019": {
          "id": 122330019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Nach dieser Runde Finanzierungen wird die IFK 30 % der Aktien von Penacony halten.",
          "next": [
            122330020
          ]
        },
        "122330020": {
          "id": 122330020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Und der Abteilung für strategische Investitionen ist es gelungen, einen Vorteil für uns zu sichern: Wenn die Finanzierung abgeschlossen ist, wird die IFK 5 % der Aktien an den Astralexpress übertragen, um ihre Aufrichtigkeit bei einer langfristigen Partnerschaft mit den Namenlosen zum Ausdruck zu bringen.",
          "next": [
            122330021
          ]
        },
        "122330021": {
          "id": 122330021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Wow.",
          "next": [
            122330022
          ]
        },
        "122330022": {
          "id": 122330022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Ich weiß zwar nicht viel über Aktien oder Optionen ... aber das klingt doch toll!",
          "next": [
            122330023
          ]
        },
        "122330023": {
          "id": 122330023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Ich bin eher neugierig, wie die zukünftigen Dividenden berechnet werden?",
          "next": [
            122330024,
            122330026
          ]
        },
        "122330024": {
          "id": 122330024,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Also werden wir bald endlich finanziell unabhängig?",
          "next": [
            122330025
          ]
        },
        "122330025": {
          "id": 122330025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Ach, {NICKNAME}, hast du die ganze Zeit in Armut gelebt? Du hast es bestimmt nicht leicht gehabt.",
          "next": [
            122330029
          ]
        },
        "122330026": {
          "id": 122330026,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hey, nur 5 %",
          "next": [
            122330027
          ]
        },
        "122330027": {
          "id": 122330027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Offensichtlich ist deine Sensibilität für Geld auf demselben Niveau wie die von März.",
          "next": [
            122330028
          ]
        },
        "122330028": {
          "id": 122330028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Hallo!",
          "next": [
            122330029
          ]
        },
        "122330029": {
          "id": 122330029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Auf diese Weise haben wir als stets dem Trailblaze folgende Namenlose eine zusätzliche Verantwortung zu übernehmen.",
          "next": [
            122330030
          ]
        },
        "122330030": {
          "id": 122330030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Als Anteilseigner des Penacony-Konglomerats haben wir die Pflicht, Markenbotschafter zu sein und dafür zu sorgen, dass der Planet der Festlichkeiten nicht mehr vom wahren Pfad der Harmonie abweicht.",
          "next": [
            122330031,
            122330039
          ]
        },
        "122330031": {
          "id": 122330031,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Zeit, alle zu unterwerfen!",
          "next": [
            122330032
          ]
        },
        "122330032": {
          "id": 122330032,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ähm, nein ...",
          "next": [
            122330033
          ]
        },
        "122330033": {
          "id": 122330033,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Zeit, dass sich alle unsere Namen merken!",
          "next": [
            122330034
          ]
        },
        "122330034": {
          "id": 122330034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Oh, {NICKNAME} scheint voller Tatendrang zu sein.",
          "next": [
            122330035
          ]
        },
        "122330035": {
          "id": 122330035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "...",
          "next": [
            122330036
          ]
        },
        "122330036": {
          "id": 122330036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Hey, was machst du da?!",
          "next": [
            122330037
          ]
        },
        "122330037": {
          "id": 122330037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Lass es gut sein, März.",
          "next": [
            122330038
          ]
        },
        "122330038": {
          "id": 122330038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Ich kann {F#ihre}{M#seine} Entschlossenheit an {F#ihren}{M#seinen} Nachrichten sehen. Wir können {F#sie}{M#ihn} nicht aufhalten.",
          "next": []
        },
        "122330039": {
          "id": 122330039,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Zeit, alle zu unterwerfen!",
          "next": [
            122330032
          ]
        }
      },
