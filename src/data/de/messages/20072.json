  "relatedMessages": [20070, 20071, 20072],
  "sections": [
    {
      "id": 2007200,
      "startingMessageId": [200720000],
      "messages": {
        "200720000": {
          "id": 200720000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Ich habe letzte Nacht kein Auge zugetan, {NICKNAME}.",
          "next": [200720001]
        },
        "200720001": {
          "id": 200720001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Vielleicht bin ich zu naiv. Ohne Stammkunden lässt sich nachts kaum Geschäft machen.",
          "next": [200720004]
        },
        "200720004": {
          "id": 200720004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Ich war in der Aurum-Allee. Das ist eine alte Straße, die für ihren Nachtbasar bekannt ist.",
          "next": [200720005]
        },
        "200720005": {
          "id": 200720005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Als wandernder Händler musste ich mir das natürlich ansehen.",
          "next": [200720006, 200720010]
        },
        "200720006": {
          "id": 200720006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du warst als Verkäufer dort?",
          "next": [200720007]
        },
        "200720007": {
          "id": 200720007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Ja und nein. Kauf und Verkauf gehen dort Hand in Hand.",
          "next": [200720008]
        },
        "200720008": {
          "id": 200720008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Ich habe zwar ein paar Raritäten dabei, aber ich kann nicht immer jemanden finden, der sie mir abnimmt.",
          "next": [200720009]
        },
        "200720009": {
          "id": 200720009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Trotzdem lässt sich sagen, dass die Bewohner von Xianzhou sehr freundlich sind. Auch wenn sie nichts kaufen, unterhalten sie sich mit mir.",
          "next": [200720014]
        },
        "200720010": {
          "id": 200720010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du warst als Käufer dort?",
          "next": [200720011]
        },
        "200720011": {
          "id": 200720011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Ja und nein. Kauf und Verkauf gehen dort Hand in Hand.",
          "next": [200720012]
        },
        "200720012": {
          "id": 200720012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Viele Läden haben interessante Sachen im Sortiment, aber das heißt nicht, dass alles dort meinem persönlichen Geschmack entspricht.",
          "next": [200720013]
        },
        "200720013": {
          "id": 200720013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Trotzdem lässt sich sagen, dass die Bewohner von Xianzhou sehr freundlich sind. Auch wenn ich nichts kaufe, unterhalten sie sich mit mir.",
          "next": [200720014]
        },
        "200720014": {
          "id": 200720014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Ich hätte allerdings nicht gedacht, dass sich diese Unterhaltungen bis Ladenschluss ziehen würden.",
          "next": [200720015]
        },
        "200720015": {
          "id": 200720015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Die Nacht war lustig, aber wenn ich so darüber nachdenke, weiß ich nicht, ob sie mir etwas gebracht hat.",
          "next": [200720016, 200720019, 200720022]
        },
        "200720016": {
          "id": 200720016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sag so was nicht.",
          "next": [200720017]
        },
        "200720017": {
          "id": 200720017,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Du hast wichtige Informationen über die Industrie von Xianzhou ergattert!",
          "next": [200720018]
        },
        "200720018": {
          "id": 200720018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Haha, du siehst mich offenbar wirklich als fahrenden Händler.",
          "next": [200720025]
        },
        "200720019": {
          "id": 200720019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sag so was nicht.",
          "next": [200720020]
        },
        "200720020": {
          "id": 200720020,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Du hast die Freundschaft der Bewohner von Xianzhou gewonnen!",
          "next": [200720021]
        },
        "200720021": {
          "id": 200720021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Stimmt. Gibt es denn etwas Kostbareres?",
          "next": [200720025]
        },
        "200720022": {
          "id": 200720022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sag so was nicht.",
          "next": [200720023]
        },
        "200720023": {
          "id": 200720023,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Du hast dir ein paar Ringe unter den Augen geholt!",
          "next": [200720024]
        },
        "200720024": {
          "id": 200720024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Ach du Schande. Mir sieht man es eigentlich nicht an, wenn ich die ganze Nacht aufgeblieben bin.",
          "next": [200720025]
        },
        "200720025": {
          "id": 200720025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Wenn sich die Gelegenheit bietet, solltest du dir auch mal die Aurum-Allee ansehen.",
          "next": [200720026]
        },
        "200720026": {
          "id": 200720026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Und wenn ich dann noch in Xianzhou bin, begleite ich dich gerne.",
          "next": []
        }
      },
      "participatingContactsIds": [1203, 8001]
    }
  ]
}
