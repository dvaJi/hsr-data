        "201080003": {
          "id": 201080003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Was magst du am liebsten, {NICKNAME}? Weiches Brot, Krustenbrot oder vielleicht ein Butterbrot?",
