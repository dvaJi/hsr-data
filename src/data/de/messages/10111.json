      "messages": {
        "101110000": {
          "id": 101110000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Danke, dass du uns geholfen hast, den Verschwörer zu entlarven. Im Namen von Asta möchte ich dir danken. Den Rest kannst du mir überlassen.",
          "next": [
            101110001,
            101110003
          ]
        },
        "101110001": {
          "id": 101110001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Herta hat Asta nicht bestraft, oder?",
          "next": [
            101110002
          ]
        },
        "101110002": {
          "id": 101110002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Du kennst Frau Herta aber schlecht. Solche trivialen Angelegenheiten sind ihr völlig egal.",
          "next": [
            101110005
          ]
        },
        "101110003": {
          "id": 101110003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hat Asta sich all die Sachen wirklich von ihrem eigenen Geld gekauft?",
          "next": [
            101110004
          ]
        },
        "101110004": {
          "id": 101110004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Du hast also echt keine Ahnung ...",
          "next": [
            101110005
          ]
        },
        "101110005": {
          "id": 101110005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Diese Raumstation ist zwar Frau Hertas Eigentum, aber sie hat keinen Credit zu ihrem Bau beigesteuert.",
          "next": [
            101110006
          ]
        },
        "101110006": {
          "id": 101110006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Fräulein Astas Familie hat Verbindungen zur IFK, also ist sie ... Hm, „wohlhabend“ ist noch untertrieben. Die meisten Einrichtungen auf der Raumstation wurden von ihr bezahlt ...",
          "next": [
            101110007
          ]
        },
        "101110007": {
          "id": 101110007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Der Preis der Sachen, die sie sich gekauft hat ... war im Gegensatz dazu ein Klacks.",
          "next": [
            101110008,
            101110010,
            101110013
          ]
        },
        "101110008": {
          "id": 101110008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was genau ist die Interastrale Friedenskorporation?",
          "next": [
            101110009
          ]
        },
        "101110009": {
          "id": 101110009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Ahh ... Stell sie dir einfach als die reichste Organisation der Galaxie vor.",
          "next": [
            101110015
          ]
        },
        "101110010": {
          "id": 101110010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ist es zu spät, um mich bei Fräulein Asta einzuschmeicheln?",
          "next": [
            101110011
          ]
        },
        "101110011": {
          "id": 101110011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "... Mach keine Witze darüber. Ich nehme dich beim Wort.",
          "next": [
            101110012
          ]
        },
        "101110012": {
          "id": 101110012,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "...",
          "next": [
            101110015
          ]
        },
        "101110013": {
          "id": 101110013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hast du entschieden, was mit Richard passiert?",
          "next": [
            101110014
          ]
        },
        "101110014": {
          "id": 101110014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "... Ich glaube, die Details willst du gar nicht wissen.",
          "next": [
            101110015
          ]
        },
        "101110015": {
          "id": 101110015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Danke jedenfalls, dass du Fräulein Asta in dieser Angelegenheit geholfen hast.",
          "next": []
        }
