      "messages": {
        "200110000": {
          "id": 200110000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Ein weiterer neuer Tag, {NICKNAME}.",
          "next": [
            200110001
          ]
        },
        "200110001": {
          "id": 200110001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Sobald wir den Express betreten und in die Fußstapfen der Namenlosen treten, bedeutet das, dass wir uns mit außergewöhnlichen Dingen vertraut machen werden.",
          "next": [
            200110002
          ]
        },
        "200110002": {
          "id": 200110002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Ohne es zu merken überschreiten wir unsere eigenen Grenzen.",
          "next": [
            200110003
          ]
        },
        "200110003": {
          "id": 200110003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "{NICKNAME}, hast du so etwas schon einmal erlebt?",
          "next": [
            200110004,
            200110005
          ]
        },
        "200110004": {
          "id": 200110004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ja, tatsächlich. Ich habe auf einmal unzählige Nachrichten erhalten.",
          "next": [
            200110006
          ]
        },
        "200110005": {
          "id": 200110005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ja, tatsächlich. Ich habe auf einmal unzählige Aufträge erhalten.",
          "next": [
            200110009
          ]
        },
        "200110006": {
          "id": 200110006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Nachrichten repräsentieren die Beziehungen zwischen Menschen.",
          "next": [
            200110007
          ]
        },
        "200110007": {
          "id": 200110007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Wenn du jeden Tag Nachrichten erhältst, bedeutet das, dass die Beziehung zwischen dir und anderen Personen während deiner Trailblaze-Expedition vertieft wurde.",
          "next": [
            200110008
          ]
        },
        "200110008": {
          "id": 200110008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Aber Beziehungen müssen auch aufrecht erhalten werden. Teile deine Zeit weise ein.",
          "next": [
            200110012,
            200110013
          ]
        },
        "200110009": {
          "id": 200110009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Du kannst dich nicht als der Namenlosen würdig bezeichnen, wenn du nicht mehrere Aufträge auf einmal erledigen kannst.",
          "next": [
            200110010
          ]
        },
        "200110010": {
          "id": 200110010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Du solltest deine Route gut planen, damit du dich nicht irgendwann im Kreis drehst oder dir selbst unnötig viel Arbeit machst.",
          "next": [
            200110011
          ]
        },
        "200110011": {
          "id": 200110011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Ehe du dich versiehst, wirst du bereits das Muskelgedächtnis dafür entwickelt haben.",
          "next": [
            200110012,
            200110013
          ]
        },
        "200110012": {
          "id": 200110012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Klingt, als hättest du reichlich Erfahrung, Herr Yang!",
          "next": [
            200110014
          ]
        },
        "200110013": {
          "id": 200110013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wenn das der Preis ist, den der Trailblaze verlangt, bin ich bereit, ihn zu bezahlen!",
          "next": [
            200110016
          ]
        },
        "200110014": {
          "id": 200110014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Die Erfahrung der Namenlosen ist nichts als eine Lektion, die sie durch Vergießen von Schweiß und Blut gelernt haben.",
          "next": [
            200110015
          ]
        },
        "200110015": {
          "id": 200110015,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "[Okay]",
          "next": []
        },
        "200110016": {
          "id": 200110016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Beeindruckend, in so jungen Jahren bereits eine solches Bewusstsein entwickelt zu haben.",
          "next": [
            200110017
          ]
        },
        "200110017": {
          "id": 200110017,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "[Daumen hoch]",
          "next": []
        }
      },
