      "messages": {
        "111520000": {
          "id": 111520000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Wie ich höre, bist du wieder in Belobog, {NICKNAME}?",
          "next": [
            111520001
          ]
        },
        "111520001": {
          "id": 111520001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Sag mir Bescheid, wenn du einen Moment Zeit hast. Es ist dringend.",
          "next": [
            111520002
          ]
        },
        "111520002": {
          "id": 111520002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was ist los?",
          "next": [
            111520003
          ]
        },
        "111520003": {
          "id": 111520003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Eine Schar Gauner ist runter in die Unterwelt gekommen und hat die Große Mine in ihre Gewalt gebracht.",
          "next": [
            111520004
          ]
        },
        "111520004": {
          "id": 111520004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Ich habe Bronya gefragt, was wir tun sollen, und sie sagte, dass wir uns erst mal nicht mit ihnen anlegen sollen.",
          "next": [
            111520005,
            111520007
          ]
        },
        "111520005": {
          "id": 111520005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bronya hat recht. Wahrscheinlich ist es am besten, ihnen nicht in die Quere zu kommen.",
          "next": [
            111520006
          ]
        },
        "111520006": {
          "id": 111520006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Zu spät. Wildfeuer und die Silbermähnen-Wachen haben sie bereits angegriffen.",
          "next": [
            111520009
          ]
        },
        "111520007": {
          "id": 111520007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wir können nicht zulassen, dass diese Schufte die Bergarbeiter schikanieren! Wir müssen uns wehren.",
          "next": [
            111520008
          ]
        },
        "111520008": {
          "id": 111520008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Wir sind schon dabei. Wildfeuer und die Silbermähnen-Wachen haben sie bereits angegriffen.",
          "next": [
            111520009
          ]
        },
        "111520009": {
          "id": 111520009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Aber diese Schufte haben krasse, überlegene Superwaffen mit erhöhtem Schaden. Wir kommen nicht gegen sie an.",
          "next": [
            111520010
          ]
        },
        "111520010": {
          "id": 111520010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Ich sende dir einen Notruf, weil ich nicht mehr weiterweiß.",
          "next": [
            111520011
          ]
        },
        "111520011": {
          "id": 111520011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Haltet durch. Wir sind schon auf dem Weg.",
          "next": []
        }
      },
