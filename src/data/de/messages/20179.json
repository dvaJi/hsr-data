  "sections": [
    {
      "id": 2017900,
      "startingMessageId": [
        201790000
      ],
      "messages": {
        "201790000": {
          "id": 201790000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Ein neuer Modetrend hat Okhema erobert.",
          "next": [
            201790001
          ]
        },
        "201790001": {
          "id": 201790001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Jugendliche ahmen gelegentlich die legendären Helden von Amphoreus nach, indem sie sich in ähnliche Outfits gekleidet auf dem Platz versammeln.",
          "next": [
            201790002
          ]
        },
        "201790002": {
          "id": 201790002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "In letzter Zeit habe ich viele gesehen, die sich von deinem Kleidungsstil inspirieren lassen.",
          "next": [
            201790003,
            201790005,
            201790009
          ]
        },
        "201790003": {
          "id": 201790003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das gehört alles zum Cosplay.",
          "next": [
            201790004
          ]
        },
        "201790004": {
          "id": 201790004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Du scheinst viel darüber zu wissen.",
          "next": [
            201790012
          ]
        },
        "201790005": {
          "id": 201790005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wie kam es dazu, dass sie meine Kleidung tragen, ohne dass ich davon weiß?",
          "next": [
            201790006
          ]
        },
        "201790006": {
          "id": 201790006,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Hier geht es um Urheberrechte!",
          "next": [
            201790007
          ]
        },
        "201790007": {
          "id": 201790007,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "next": [
            201790008
          ]
        },
        "201790008": {
          "id": 201790008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Zweifellos hat deine Ankunft bei den Menschen in Okhema einen bleibenden Eindruck hinterlassen.",
          "next": [
            201790012
          ]
        },
        "201790009": {
          "id": 201790009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich mache auch mit! Ich spiele mich selbst.",
          "next": [
            201790010
          ]
        },
        "201790010": {
          "id": 201790010,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "next": [
            201790011
          ]
        },
        "201790011": {
          "id": 201790011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Die Bewohner von Okhema würden es sicherlich zu schätzen wissen.",
          "next": [
            201790012
          ]
        },
        "201790012": {
          "id": 201790012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Es ist wahrscheinlich eine berufliche Eigenart, aber eure Outfits fielen mir sofort auf, als wir uns das erste Mal trafen.",
          "next": [
            201790013
          ]
        },
        "201790013": {
          "id": 201790013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Die Schneiderkunst ist skulptural und weist eine ausgeprägt architektonische Ästhetik auf.",
          "next": [
            201790014
          ]
        },
        "201790014": {
          "id": 201790014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Die Nähte sind vielfältig und verwenden Techniken, die selbst mir unbekannt sind.",
          "next": [
            201790015
          ]
        },
        "201790015": {
          "id": 201790015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Die kräftige und dennoch geschmackvolle Farbauswahl passt gut zu jeder euer Persönlichkeiten.",
          "next": [
            201790016
          ]
        },
        "201790016": {
          "id": 201790016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Es gibt mir als Schneidermeisterin eine Menge neue Inspirationen.",
          "next": [
            201790017
          ]
        },
        "201790017": {
          "id": 201790017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Wenn ihr möchtet, kann ich euch einige lokale Outfits empfehlen.",
          "next": [
            201790018,
            201790021,
            201790023
          ]
        },
        "201790018": {
          "id": 201790018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was meinst du, würde zu mir passen?",
          "next": [
            201790019
          ]
        },
        "201790019": {
          "id": 201790019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Ich werde darüber nachdenken.",
          "next": [
            201790020
          ]
        },
        "201790020": {
          "id": 201790020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Vielleicht ein schwarzer Kapuzenmantel – ein zeitloser Klassiker.",
          "next": [
            201790027
          ]
        },
        "201790021": {
          "id": 201790021,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wie wäre es mit etwas, das {F#eine wahre Heldin}{M#ein wahrer Held} tragen würde?",
          "next": [
            201790022
          ]
        },
        "201790022": {
          "id": 201790022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Haha, trägst du das nicht schon?",
          "next": [
            201790027
          ]
        },
        "201790023": {
          "id": 201790023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich würde gerne so etwas wie deines anprobieren – es gefällt mir.",
          "next": [
            201790024
          ]
        },
        "201790024": {
          "id": 201790024,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "next": [
            201790025
          ]
        },
        "201790025": {
          "id": 201790025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Kleidung sollte maßgeschneidert sein. Jeder hat seinen eigenen Stil.",
          "next": [
            201790026
          ]
        },
        "201790026": {
          "id": 201790026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Heutzutage neige ich zu einfachen und eleganten Outfits, aber ich kann sie auch an deine Vorlieben anpassen.",
          "next": [
            201790027
          ]
        },
        "201790027": {
          "id": 201790027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Ich war eine leidenschaftliche Sammlerin schöner Kleidungsstücke und hatte sogar einen Kleiderschrank, der so groß war wie ein Haus.",
          "next": [
            201790028
          ]
        },
        "201790028": {
          "id": 201790028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Aber jetzt möchte ich, dass die Outfits nicht nur mir gehören, sondern jedem zur Verfügung stehen, der sie braucht.",
          "next": [
            201790029
          ]
        },
        "201790029": {
          "id": 201790029,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1402,
          "next": []
        }
      },
      "participatingContactsIds": [
        1402,
        8001
      ]
    }
  ]
}