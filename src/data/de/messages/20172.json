        "201720000": {
          "id": 201720000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Wie sieht ein typischer Tag auf dem Express aus?",
          "next": [
            201720001,
            201720003,
            201720006
          ]
        },
        "201720001": {
          "id": 201720001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wir essen drei Mahlzeiten am Tag.",
          "next": [
            201720002
          ]
        },
        "201720002": {
          "id": 201720002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Die Crew des Expresses scheint ähnliche Gewohnheiten wie die Bewohner von Penacony zu haben.",
          "next": [
            201720008
          ]
        },
        "201720003": {
          "id": 201720003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wir schlafen. Und vor dem Schlafengehen bleiben wir lange auf.",
          "next": [
            201720004
          ]
        },
        "201720004": {
          "id": 201720004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Meinen Beobachtungen nach scheinst nur du diese Angewohnheit zu haben.",
          "next": [
            201720005
          ]
        },
        "201720005": {
          "id": 201720005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Aber ich kann das verstehen.",
          "next": [
            201720008
          ]
        },
        "201720006": {
          "id": 201720006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wir massieren den Schaffner, ist doch klar.",
          "next": [
            201720007
          ]
        },
        "201720007": {
          "id": 201720007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Der Schaffner hat es nicht leicht und seine Rolle ist vergleichbar mit der eines Familienoberhaupts.",
          "next": [
            201720008
          ]
        },
        "201720008": {
          "id": 201720008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Als Passagier würde ich gerne im Tagesgeschäft mithelfen.",
          "next": [
            201720009
          ]
        },
        "201720009": {
          "id": 201720009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Ich weiß nicht, wo ich mit anpacken kann, also will ich dich um Rat fragen.",
          "next": [
            201720010,
            201720013,
            201720015
          ]
        },
        "201720010": {
          "id": 201720010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Spiel mir ein paar Schlaflieder, damit ich besser einschlafen kann.",
          "next": [
            201720011
          ]
        },
        "201720011": {
          "id": 201720011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Ich kann etwas Abendmusik oder ein Schlaflied spielen.",
          "next": [
            201720012
          ]
        },
        "201720012": {
          "id": 201720012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Natürlich kann ich jedem nur empfehlen, sich Robins Album anzuhören.",
          "next": [
            201720017
          ]
        },
        "201720013": {
          "id": 201720013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Auf dem Trailblaze passieren jeden Tag interessante Dinge. Ich könnte dir stundenlang davon erzählen.",
          "next": [
            201720014
          ]
        },
        "201720014": {
          "id": 201720014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Ich war früher Bronze-Melodia. Damit sollte ich also klarkommen.",
          "next": [
            201720017
          ]
        },
        "201720015": {
          "id": 201720015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Biete unserem Schaffner einfach eine Massage an.",
          "next": [
            201720016
          ]
        },
        "201720016": {
          "id": 201720016,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Wenn der Schaffner glücklich ist, sind alle glücklich.",
          "next": [
            201720017
          ]
        },
        "201720017": {
          "id": 201720017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Was für ein Zufall. Ich habe gerade den Schaffner gesehen.",
          "next": [
            201720018
          ]
        },
        "201720018": {
          "id": 201720018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Dann fange ich mit dieser kleinen Aufgabe an. Ich massiere Pom-Pom nachher.",
          "next": [
            201720019
          ]
        },
        "201720019": {
          "id": 201720019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1313,
          "text": "Danke für deinen Rat und Vorschlag.",
          "next": []
        }
      },
