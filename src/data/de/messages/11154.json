        "111540001": {
          "id": 111540001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Bronya! Wir sind gerade in der Unterwelt.",
          "next": [111540002]
        },
        "111540002": {
          "id": 111540002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Die Leute von der IFK haben die Große Mine übernommen. Wir und Seele haben sie gerade hinausgeworfen.",
          "next": [111540003]
        },
        "111540003": {
          "id": 111540003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Ihr seid bei Seele, verstehe.",
          "next": [111540004]
        },
        "111540004": {
          "id": 111540004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Wie stehen die Dinge? Seid ihr verletzt?",
          "next": [111540005]
        },
        "111540005": {
          "id": 111540005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Keine Sorge. Wir kommen klar!",
          "next": [111540006, 111540008]
        },
        "111540006": {
          "id": 111540006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tut mir leid. Wir konnten Topaz nicht überzeugen ...",
          "next": [111540007]
        },
        "111540007": {
          "id": 111540007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Keine Sorge, {NICKNAME}! Das wurde sowieso nicht von euch erwartet.",
          "next": [111540010]
        },
        "111540008": {
          "id": 111540008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich fürchte, die IFK wird noch weiter gehen.",
          "next": [111540009]
        },
        "111540009": {
          "id": 111540009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Verstehe. Danke, dass ihr versucht habt, mit Topaz zu verhandeln. Das wurde sowieso nicht von euch erwartet.",
          "next": [111540010]
        },
        "111540010": {
          "id": 111540010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Irgendetwas ist hier gerade passiert und ich kann es nicht genau in Textnachrichten beschreiben.",
