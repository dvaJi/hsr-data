      "messages": {
        "101090000": {
          "id": 101090000,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "„Du_kennst_mich_schon“ hat „Ultimate_ins_Gesicht“ in den Gruppenchat eingeladen und die letzten 10 Nachrichten geteilt.",
          "next": [
            101090001
          ]
        },
        "101090001": {
          "id": 101090001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 16,
          "text": "Sie war mir gleich verdächtig.",
          "next": [
            101090002
          ]
        },
        "101090002": {
          "id": 101090002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Gib uns Details.",
          "next": [
            101090003
          ]
        },
        "101090003": {
          "id": 101090003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "Gib uns Details.",
          "next": [
            101090004
          ]
        },
        "101090004": {
          "id": 101090004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "Strengt eure grauen Zellen an. Wie viel Profit kann man aus so einer großen Raumstation rausholen?",
          "next": [
            101090005
          ]
        },
        "101090005": {
          "id": 101090005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Das ist nicht witzig. Sie ist Frau Hertas Schülerin.",
          "next": [
            101090006
          ]
        },
        "101090006": {
          "id": 101090006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "Na und? Wir dürfen also nicht über sie reden, nur weil sie Hertas Schülerin ist?",
          "next": [
            101090007
          ]
        },
        "101090007": {
          "id": 101090007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Also ...",
          "next": [
            101090008
          ]
        },
        "101090008": {
          "id": 101090008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Es gibt eine Menge Gerüchte über sie.",
          "next": [
            101090009
          ]
        },
        "101090009": {
          "id": 101090009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Gib uns Details.",
          "next": [
            101090010
          ]
        },
        "101090010": {
          "id": 101090010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Es passt mir gerade nicht so gut.",
          "next": [
            101090011
          ]
        },
        "101090011": {
          "id": 101090011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "Von wegen „passt gerade nicht“!",
          "next": [
            101090012,
            101090014,
            101090018
          ]
        },
        "101090012": {
          "id": 101090012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was für ein Gruppenchat ist das denn?",
          "next": [
            101090013
          ]
        },
        "101090013": {
          "id": 101090013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "Sieh dir doch den Gruppennamen an, @Ultimate_ins_Gesicht",
          "next": [
            101090020
          ]
        },
        "101090014": {
          "id": 101090014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Von wem redest du?",
          "next": [
            101090015
          ]
        },
        "101090015": {
          "id": 101090015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Kennst du denn die Regeln nicht, Neuling?",
          "next": [
            101090016
          ]
        },
        "101090016": {
          "id": 101090016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Asta",
          "next": [
            101090017
          ]
        },
        "101090017": {
          "id": 101090017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "Diese Asta.",
          "next": [
            101090020
          ]
        },
        "101090018": {
          "id": 101090018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Eine Zusammenfassung bitte!",
          "next": [
            101090019
          ]
        },
        "101090019": {
          "id": 101090019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "Lies selbst!",
          "next": [
            101090020
          ]
        },
        "101090020": {
          "id": 101090020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Unterbrich nicht, @Richard der Schlauberger raus mit der Sprache!",
          "next": [
            101090021
          ]
        },
        "101090021": {
          "id": 101090021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Nö, vergiss es.",
          "next": [
            101090022
          ]
        },
        "101090022": {
          "id": 101090022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 16,
          "text": "Verarsch uns nicht. Das ist ein anonymer Chat, also red nicht um den heißen Brei herum. Spuck’s schon aus.",
          "next": [
            101090023
          ]
        },
        "101090023": {
          "id": 101090023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Es ist ja nicht so, als wüsstet ihr es nicht schon ...",
          "next": [
            101090024
          ]
        },
        "101090024": {
          "id": 101090024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "Komm zur Sache!",
          "next": [
            101090025
          ]
        },
        "101090025": {
          "id": 101090025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Ihr wisst schon, die Veruntreuungsgeschichte.",
          "next": [
            101090026
          ]
        },
        "101090026": {
          "id": 101090026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Habt ihr gesehen, wie viele Pakete sich an der Rezeption stapeln? Die sind alle an unsere junge Forschungsleiterin adressiert!",
          "next": [
            101090027
          ]
        },
        "101090027": {
          "id": 101090027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "Meine Güte! Da versickern also all unsere Gelder. Kein Wunder, dass das Budget so knapp ist.",
          "next": [
            101090028
          ]
        },
        "101090028": {
          "id": 101090028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "Nicht so voreilig! Wir sollten noch nicht mit Schuldzuweisungen um uns werfen.",
          "next": [
            101090029
          ]
        },
        "101090029": {
          "id": 101090029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Wir können uns nicht nur auf deine Behauptung verlassen. Wo sind die Beweise?",
          "next": [
            101090030
          ]
        },
        "101090030": {
          "id": 101090030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Beweise? Habt ihr gesehen, wie viele Pakete sich an der Rezeption stapeln? Die sind alle an unsere junge Forschungsleiterin adressiert!",
          "next": [
            101090031,
            101090033
          ]
        },
        "101090031": {
          "id": 101090031,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was ist in den Paketen?",
          "next": [
            101090032
          ]
        },
        "101090032": {
          "id": 101090032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Ich habe sie mir nur kurz angesehen und es sind alles superteure Sachen.",
          "next": [
            101090035
          ]
        },
        "101090033": {
          "id": 101090033,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kaufte sie diese auf ihren Namen?",
          "next": [
            101090034
          ]
        },
        "101090034": {
          "id": 101090034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Was denn sonst? Hätte sie die Raumstation als Empfänger angegeben, müsste sie die Sachen offiziell verbuchen. So unvorsichtig ist unsere Forschungsleiterin bestimmt nicht.",
          "next": [
            101090035
          ]
        },
        "101090035": {
          "id": 101090035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Vielleicht hat sie ihr eigenes Geld benutzt.",
          "next": [
            101090036
          ]
        },
        "101090036": {
          "id": 101090036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Wer außer Frau Herta könnte sich solche Sachen von seinem eigenen Geld leisten?",
          "next": [
            101090037
          ]
        },
        "101090037": {
          "id": 101090037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Kennt ihr diesen Arlan von der Sicherheitsabteilung? Der war an der Rezeption und hat sich sehr seltsam und verdächtig benommen. Hätte er nicht so schuldig dreingesehen, hätte ich es nie bemerkt.",
          "next": [
            101090038
          ]
        },
        "101090038": {
          "id": 101090038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Hmm, jetzt wo du es sagst ... Ich habe auch gesehen, wie Arlan Pakete abgeholt und sich komisch verhalten hat.",
          "next": [
            101090039
          ]
        },
        "101090039": {
          "id": 101090039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Sag ich doch.",
          "next": [
            101090040
          ]
        },
        "101090040": {
          "id": 101090040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "Jetzt ergibt alles Sinn.",
          "next": [
            101090041
          ]
        },
        "101090041": {
          "id": 101090041,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "Jetzt ergibt alles Sinn.",
          "next": [
            101090042,
            101090044,
            101090047
          ]
        },
        "101090042": {
          "id": 101090042,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Willst du damit sagen, dass Asta und Arlan gemeinsame Sache machen, um sich heimlich zu bereichern?",
          "next": [
            101090043
          ]
        },
        "101090043": {
          "id": 101090043,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "Weiß nicht, @Richard der Schlauberger hat es gesagt, nicht ich.",
          "next": [
            101090051
          ]
        },
        "101090044": {
          "id": 101090044,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ihr fantasiert nur doch nur, es gibt keine handfesten Beweise ...",
          "next": [
            101090045
          ]
        },
        "101090045": {
          "id": 101090045,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Mir egal, ob du mir glaubst.",
          "next": [
            101090046
          ]
        },
        "101090046": {
          "id": 101090046,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "LOL! Jetzt aber. Das kann keiner leugnen. Ist doch ein offenes Geheimnis, dass Arlan ein Spezi von dieser Asta ist und sie ihn hier eingeschleust hat.",
          "next": [
            101090051
          ]
        },
        "101090047": {
          "id": 101090047,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was dir alles einfällt ... Du solltest Eintritt für deine Geschichten verlangen.",
          "next": [
            101090048
          ]
        },
        "101090048": {
          "id": 101090048,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Ich möchte hiermit klarstellen, dass ich mit den zuvor genannten Informationen nichts zu tun habe!",
          "next": [
            101090049
          ]
        },
        "101090049": {
          "id": 101090049,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "Ich möchte hiermit klarstellen, dass ich mit den zuvor genannten Informationen nichts zu tun habe!",
          "next": [
            101090050
          ]
        },
        "101090050": {
          "id": 101090050,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Ich befolge aus freien Stücken alle Vorgaben und Anweisungen von Frau Herta!",
          "next": [
            101090051
          ]
        },
        "101090051": {
          "id": 101090051,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 16,
          "text": "... Ah, tss. Ich warte schon den ganzen Tag auf heiße Gerüchte. War das schon alles?",
          "next": [
            101090052
          ]
        },
        "101090052": {
          "id": 101090052,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Eben. Letztendlich ist das doch nur heiße Luft ohne Beweise.",
          "next": [
            101090053
          ]
        },
        "101090053": {
          "id": 101090053,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "@M-Roboter, aber du hast doch gerade selbst gesagt, dass Arlan sich verdächtig benommen hat.",
          "next": [
            101090054
          ]
        },
        "101090054": {
          "id": 101090054,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Mehr aber auch nicht!",
          "next": [
            101090055
          ]
        },
        "101090055": {
          "id": 101090055,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Ich stelle verdeckte Ermittlungen zu ihren Einnahmequellen an, aber es ist zu früh, um alles zu enthüllen.",
          "next": [
            101090056
          ]
        },
        "101090056": {
          "id": 101090056,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Wartet nur. In ein paar Tagen gibt es auf der Raumstation einen waschechten Skandal!",
          "next": [
            101090057
          ]
        },
        "101090057": {
          "id": 101090057,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 16,
          "text": "Wehe, wenn nicht.",
          "next": []
        }
      },
