{
  "id": 20041,
  "contacts": {
    "id": 1104,
    "name": "Gepard",
    "icon": "1104",
    "signature": "Im Dienst. Entschuldigung für die späte Antwort.",
    "typeId": 1,
    "type": "Figuren"
  },
  "relatedMessages": [11114, 11801, 20021, 20041, 20042],
  "sections": [
    {
      "id": 2004100,
      "startingMessageId": [200410000],
      "messages": {
        "200410000": {
          "id": 200410000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "{NICKNAME}, ich möchte gerne etwas mit dir besprechen.",
          "next": [200410001]
        },
        "200410001": {
          "id": 200410001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Hast du gerade Zeit?",
          "next": [200410002]
        },
        "200410002": {
          "id": 200410002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ja, worum geht es denn?",
          "next": [200410003]
        },
        "200410003": {
          "id": 200410003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Meine jüngste Schwester ist tatsächlich gerade von einer wissenschaftlichen Expedition nach Hause gekommen.",
          "next": [200410004]
        },
        "200410004": {
          "id": 200410004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Ich möchte ihr etwas schenken. Was wäre deiner Meinung nach als Geschenk geeignet?",
          "next": [200410005, 200410014, 200410023]
        },
        "200410005": {
          "id": 200410005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was mag sie denn gern?",
          "next": [200410006]
        },
        "200410006": {
          "id": 200410006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Ich fühle mich schlecht, das zu sagen, aber ich bin mir nicht sicher, was sie mag.",
          "next": [200410007]
        },
        "200410007": {
          "id": 200410007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Ich weiß nur, dass Ihr Wissenschaft und Feldstudien sehr wichtig sind.",
          "next": [200410008]
        },
        "200410008": {
          "id": 200410008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Oh, ich habe eine Idee. Wie wäre es, wenn ich ihr neue wissenschaftliche Expeditionsausrüstung schenke?",
          "next": [200410009]
        },
        "200410009": {
          "id": 200410009,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Daumen hoch]",
          "next": [200410010]
        },
        "200410010": {
          "id": 200410010,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Das ist keine schlechte Idee.",
          "next": [200410011]
        },
        "200410011": {
          "id": 200410011,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Hauptsache, es gefällt ihr.",
          "next": [200410012]
        },
        "200410012": {
          "id": 200410012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Du hast recht. Danke, dass du mir geholfen hast, das durchzudenken.",
          "next": [200410013]
        },
        "200410013": {
          "id": 200410013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Ich gehe schon mal vor, um nach Geschenken zu sehen. Bis bald.",
          "next": []
        },
        "200410014": {
          "id": 200410014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du kannst ihr Blumen besorgen.",
          "next": [200410015]
        },
        "200410015": {
          "id": 200410015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Ich habe darüber nachgedacht, ihr Blumen zu schenken, aber ich kann mich daran erinnern, dass sie gesagt hat, normale Pflanzen würden sie nicht interessieren.",
          "next": [200410016]
        },
        "200410016": {
          "id": 200410016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Sie mag die Pflanzen in der Schneewüste. Die, die in der Stadt ziemlich selten sind.",
          "next": [200410017]
        },
        "200410017": {
          "id": 200410017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Ich habe eine Idee. Wenn ich das nächste Mal in der Schneewüste patrouilliere, werde ich die Augen nach interessanten Pflanzen offen halten.",
          "next": [200410018]
        },
        "200410018": {
          "id": 200410018,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Daumen hoch]",
          "next": [200410019]
        },
        "200410019": {
          "id": 200410019,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Das ist keine schlechte Idee.",
          "next": [200410020]
        },
        "200410020": {
          "id": 200410020,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Aber pass auf dich auf.",
          "next": [200410021]
        },
        "200410021": {
          "id": 200410021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Haha, das werde ich, danke.",
          "next": [200410022]
        },
        "200410022": {
          "id": 200410022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Ich habe jetzt was zu erledigen, bis bald.",
          "next": []
        },
        "200410023": {
          "id": 200410023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du kannst ihr neue Klamotten kaufen.",
          "next": [200410024]
        },
        "200410024": {
          "id": 200410024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Darüber habe ich auch nachgedacht, aber ich habe keinen guten Modegeschmack, also habe ich mich nicht getraut, etwas für sie auszusuchen.",
          "next": [200410025]
        },
        "200410025": {
          "id": 200410025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Serval hat ihr wirklich schöne Klamotten gekauft.",
          "next": [200410026]
        },
        "200410026": {
          "id": 200410026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Aber ich will nicht, dass Serval davon erfährt.",
          "next": [200410027]
        },
        "200410027": {
          "id": 200410027,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Ablehnen]",
          "next": [200410028]
        },
        "200410028": {
          "id": 200410028,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Warum so schüchtern?",
          "next": [200410029]
        },
        "200410029": {
          "id": 200410029,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Du gehörst doch zur Familie. Geh einfach und frag nach.",
          "next": [200410030]
        },
        "200410030": {
          "id": 200410030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "...",
          "next": [200410031]
        },
        "200410031": {
          "id": 200410031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Du hast recht, ich sollte einfach Serval fragen, damit ich nicht das Falsche kaufe.",
          "next": [200410032]
        },
        "200410032": {
          "id": 200410032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Danke für deinen Vorschlag, ich werde jetzt Serval aufsuchen. Bis bald.",
          "next": []
        }
      },
      "participatingContactsIds": [1104, 8001]
    }
  ]
}
