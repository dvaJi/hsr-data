      "messages": {
        "130610000": {
          "id": 130610000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 141,
          "text": "{F#Meine Gute}{M#Mein Guter}, der Fall ist gelöst.",
          "next": [
            130610001
          ]
        },
        "130610001": {
          "id": 130610001,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Erzähl mir mehr.",
          "next": [
            130610002
          ]
        },
        "130610002": {
          "id": 130610002,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Das höre ich gerne.",
          "next": [
            130610003
          ]
        },
        "130610003": {
          "id": 130610003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 141,
          "text": "Diese Kassette ist nicht der Verstorbene, sondern der Mörder.",
          "next": [
            130610004
          ]
        },
        "130610004": {
          "id": 130610004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 141,
          "text": "Unter dem Vorwand, seinem Chef belastende Beweise zu liefern, bat er ihn um eine Verabredung und tötete ihn.",
          "next": [
            130610005
          ]
        },
        "130610005": {
          "id": 130610005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 141,
          "text": "Wegen der lauten Musik dachten alle Augenzeugen, er sei der Verstorbene.",
          "next": [
            130610006
          ]
        },
        "130610006": {
          "id": 130610006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 141,
          "text": "Schließlich sehen alle Kassetten in Penacony gleich aus, nur der Ton ist anders.",
          "next": [
            130610007
          ]
        },
        "130610007": {
          "id": 130610007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 141,
          "text": "Am Ende steckte er das Band des Chefs in seine eigene Hülle, stahl so seine Stimme und Identität, und stieg in den Zug.",
          "next": [
            130610008
          ]
        },
        "130610008": {
          "id": 130610008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 141,
          "text": "Der entscheidende Beweis ist, dass sich in seiner Hülle ein repariertes Band befand.",
          "next": [
            130610009,
            130610011
          ]
        },
        "130610009": {
          "id": 130610009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Am Ende wissen wir immer noch nicht, was sein Chef gegen ihn in der Hand hatte.",
          "next": [
            130610010
          ]
        },
        "130610010": {
          "id": 130610010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 141,
          "text": "Ach, mach dir über solche Sachen keine Gedanken.",
          "next": [
            130610013
          ]
        },
        "130610011": {
          "id": 130610011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Es ist nur eine Kassette, aber ihre Methoden sind so grausam.",
          "next": [
            130610012
          ]
        },
        "130610012": {
          "id": 130610012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 141,
          "text": "Genau deshalb werden wir gebraucht.",
          "next": [
            130610013
          ]
        },
        "130610013": {
          "id": 130610013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 141,
          "text": "Dank dir konnten wir die Wahrheit ans Licht bringen.",
          "next": [
            130610014
          ]
        },
        "130610014": {
          "id": 130610014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 141,
          "text": "Ich freue mich auf unsere nächste Zusammenarbeit.",
          "next": []
        }
      },
