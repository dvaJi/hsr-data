        },
        "101100005": {
          "id": 101100005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Er heißt also tatsächlich Richard?!",
          "next": [101100006]
        },
        "101100006": {
          "id": 101100006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 24,
          "text": "Ja, er ist groß und dünn, trägt eine schwarze Brille und hat einen schrägen Pony.",
