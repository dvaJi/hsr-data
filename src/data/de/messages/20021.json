        "200210000": {
          "id": 200210000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Danke für die Express-Limonaden, {NICKNAME}.",
          "next": [200210001]
        },
        "200210001": {
          "id": 200210001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Ganz besonders für die Dosenlimo. Sie schmeckt großartig.",
          "next": [200210002]
        },
        "200210002": {
          "id": 200210002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Ich möchte dir auch im Namen der anderen Wachen danken!",
          "next": [200210003]
        },
        "200210003": {
          "id": 200210003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Gibt es übrigens irgendwelche Getränke aus Belobog, die du magst?",
          "next": [200210004, 200210010, 200210014]
        },
        "200210004": {
          "id": 200210004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Jim Rogers Limonaden – der Geschmack von Brot zum Preis von Wasser.",
          "next": [200210005]
        },
        "200210005": {
          "id": 200210005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Seine Werbung bleibt wahrscheinlich länger im Gedächtnis als sein Geschmack.",
          "next": [200210006]
        },
        "200210006": {
          "id": 200210006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Es schmeckt sowieso nur nach Brot.",
          "next": [200210007]
        },
        "200210007": {
          "id": 200210007,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Das stimmt.",
          "next": [200210008]
        },
        "200210008": {
          "id": 200210008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Ich besorge dir beim nächsten Mal mehr davon.",
          "next": [200210009]
        },
        "200210009": {
          "id": 200210009,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Fingerherz]",
          "next": []
        },
        "200210010": {
          "id": 200210010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Muh-Muh-Milch, voll im Geschmack.",
          "next": [200210011]
        },
        "200210011": {
          "id": 200210011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Das erinnert mich daran, wie Serval mich als Kind zwang, jeden Tag Milch zu trinken ...",
          "next": [200210012]
        },
        "200210012": {
          "id": 200210012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Ihr Käse und Sauerrahm sind auch nicht schlecht. Ich besorge dir nächstes Mal was davon.",
          "next": [200210013]
        },
        "200210013": {
          "id": 200210013,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Fingerherz]",
          "next": []
        },
        "200210014": {
          "id": 200210014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Roggenbrot-Soda-Salat. Jeder schwärmt davon.",
          "next": [200210015]
        },
        "200210015": {
          "id": 200210015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Es ist immerhin das Star-Getränk des Hotels Goethe.",
          "next": [200210016]
        },
        "200210016": {
          "id": 200210016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Wusstest du, dass du Roggenbrot-Soda-Salat auch selbst machen kannst? Es ist ganz leicht.",
          "next": [200210017]
        },
        "200210017": {
          "id": 200210017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Wir können beim nächsten Mal zusammen versuchen, welchen zu machen.",
          "next": [200210018]
        },
        "200210018": {
          "id": 200210018,
          "type": "Sticker",
