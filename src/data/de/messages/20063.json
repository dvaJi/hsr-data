  "relatedMessages": [20061, 20062, 20063],
  "sections": [
    {
      "id": 2006300,
      "startingMessageId": [200630000],
      "messages": {
        "200630000": {
          "id": 200630000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Was für ein unangenehmer Tag ...",
          "next": [200630001]
        },
        "200630001": {
          "id": 200630001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ich wollte zum Schlaflosen Grafen gehen, um mir Geschichten anzuhören, und rate mal, was passiert ist.",
          "next": [200630003]
        },
        "200630002": {
          "id": 200630002,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Verwirrt]",
          "next": [200630004]
        },
        "200630003": {
          "id": 200630003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was?",
          "next": [200630002]
        },
        "200630004": {
          "id": 200630004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ich habe meinen alten ABC-Meister getroffen, der mich am Lyzeum unterrichtet hat!",
          "next": [200630005]
        },
        "200630005": {
          "id": 200630005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ich wusste nicht, dass er gerade auf der Luofu ist. So ein Pech aber auch ...",
          "next": [200630006]
        },
        "200630006": {
          "id": 200630006,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[Weinen]",
          "next": [200630007, 200630012]
        },
        "200630007": {
          "id": 200630007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich dachte, alte Lehrer zu treffen ist etwas Gutes?",
          "next": [200630008]
        },
        "200630008": {
          "id": 200630008,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Warum ist das Pech?",
          "next": [200630009]
        },
        "200630009": {
          "id": 200630009,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Verwirrt]",
          "next": [200630010]
        },
        "200630010": {
          "id": 200630010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Oh, stell dir nur vor!",
          "next": [200630011]
        },
        "200630011": {
          "id": 200630011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Als ich noch zur Schule ging, nahm er kaum Notiz von mir.",
          "next": [200630019]
        },
        "200630012": {
          "id": 200630012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was ist ein Lyzeum? Und was ist ein ABC-Meister?",
          "next": [200630013]
        },
        "200630013": {
          "id": 200630013,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ich bin nicht von hier, ich kenne diese Wörter nicht.",
          "next": [200630014]
        },
        "200630014": {
          "id": 200630014,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Verwirrt]",
          "next": [200630015]
        },
        "200630015": {
          "id": 200630015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ach, das ist nichts weiter. Wahrscheinlich weiß ich sowieso nicht viel mehr als du.",
          "next": [200630016]
        },
        "200630016": {
          "id": 200630016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Sekunde, lass mich kurz überlegen, wie ich dir das erkläre.",
          "next": [200630017]
        },
        "200630017": {
          "id": 200630017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ah, genau!",
          "next": [200630018]
        },
        "200630018": {
          "id": 200630018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ein Lyzeum ist eine Schule und ein ABC-Meister ist ein Lehrer.",
          "next": [200630019]
        },
        "200630019": {
          "id": 200630019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "*seufz* Daran wollte ich mich wirklich nicht erinnern. Es ist so unangenehm.",
          "next": [200630020]
        },
        "200630020": {
          "id": 200630020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Erzähl’s mir. Was ist denn so unangenehm?",
          "next": [200630021]
        },
        "200630021": {
          "id": 200630021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ich wuchs in einer Militärfamilie auf und wenn es darum geht, dass ich meine Kampffertigkeiten schule, ist meine Mutter sehr streng.",
          "next": [200630022]
        },
        "200630022": {
          "id": 200630022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Also musste ich vor Tagesanbruch aufstehen, um mit ihr Übungskämpfe durchzuführen, und danach zum Lyzeum gehen.",
          "next": [200630023]
        },
        "200630023": {
          "id": 200630023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Aber ich war immer erschöpft, als ich dort eintraf. Bevor ich auch nur ein Wort des ABC-Meisters gehört hatte, schlief ich schon ein.",
          "next": [200630024]
        },
        "200630024": {
          "id": 200630024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Als er mich gerade sah, rief er: „Hey, ist das nicht die Schläfrige Sushang?“",
          "next": [200630025]
        },
        "200630025": {
          "id": 200630025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Daraufhin starrten mich sofort alle auf der Straße an. Das war mir so peinlich.",
          "next": [200630026]
        },
        "200630026": {
          "id": 200630026,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[Weinen]",
          "next": [200630027]
        },
        "200630027": {
          "id": 200630027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das ist ziemlich peinlich.",
          "next": [200630028]
        },
        "200630028": {
          "id": 200630028,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Schockiert]",
          "next": [200630029]
        },
        "200630029": {
          "id": 200630029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "*seufz* Ich hoffe, ich begegne ihm nie wieder ...",
          "next": []
        }
      },
      "participatingContactsIds": [1206, 8001]
    }
  ]
}
