      "messages": {
        "111480000": {
          "id": 111480000,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ist da jemand?",
          "next": [111480001]
        },
        "111480001": {
          "id": 111480001,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ich wollte Gepard bitten, ehrenamtlich als Museumsführer zu arbeiten.",
          "next": [111480002]
        },
        "111480002": {
          "id": 111480002,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Aber er sagte, er hätte bereits eine Bewerbung eingereicht.",
          "next": [111480003]
        },
        "111480003": {
          "id": 111480003,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Weißt du irgendetwas darüber?",
          "next": [111480004]
        },
        "111480004": {
          "id": 111480004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "Moment. Lass mich nachsehen.",
          "next": [111480005]
        },
        "111480005": {
          "id": 111480005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "...",
          "next": [111480006]
        },
        "111480006": {
          "id": 111480006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "Ich habe die Bewerbung gefunden. Sie war zwischen irgendwelchen anderen Dokumenten eingequetscht.",
          "next": [111480007]
        },
        "111480007": {
          "id": 111480007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "Bisher habe ich sie mir noch nicht angesehen.",
          "next": [111480008]
        },
        "111480008": {
          "id": 111480008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "Bitte sag ihm, dass er sich auf das Bewerbungsgespräch vorbereiten soll.",
          "next": [111480009]
        },
        "111480009": {
          "id": 111480009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "Ach, übrigens, diesmal musst du dabei nicht anwesend sein.",
          "next": [111480010]
        },
        "111480010": {
          "id": 111480010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "Ruh dich gut aus.",
          "next": [111480011, 111480015]
        },
        "111480011": {
          "id": 111480011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "?",
          "next": [111480012]
        },
        "111480012": {
          "id": 111480012,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Juhu! Danke.",
          "next": [111480013]
        },
        "111480013": {
          "id": 111480013,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Daumen hoch]",
          "next": [111480014]
        },
        "111480014": {
          "id": 111480014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "Nicht der Rede wert.",
          "next": [111480023]
        },
        "111480015": {
          "id": 111480015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "?",
          "next": [111480016]
        },
        "111480016": {
          "id": 111480016,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Offenbar werde ich jetzt nicht mehr gebraucht.",
          "next": [111480017]
        },
        "111480017": {
          "id": 111480017,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Weinen]",
          "next": [111480018]
        },
        "111480018": {
          "id": 111480018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "...",
          "next": [111480019]
        },
        "111480019": {
          "id": 111480019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "Ach, komm!",
          "next": [111480020]
        },
        "111480020": {
          "id": 111480020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "Tu nicht so mitleidig! Diesmal bist du noch glimpflich davongekommen.",
          "next": [111480021]
        },
        "111480021": {
          "id": 111480021,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "[Ablehnen]",
          "next": [111480022]
        },
        "111480022": {
          "id": 111480022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "Ich will nur, dass du dich etwas ausruhst. Mehr wollte ich nicht andeuten.",
          "next": [111480023]
        },
        "111480023": {
          "id": 111480023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "Immerhin hast du schon die ganze Zeit hart für das Museum gearbeitet.",
          "next": [111480024]
        },
        "111480024": {
          "id": 111480024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "Ich muss jetzt los. Bitte gib das mit dem Bewerbungsgespräch an Gepard weiter.",
          "next": [111480025]
        },
        "111480025": {
          "id": 111480025,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1106,
          "text": "[Grüßen]",
