  "sections": [
    {
      "id": 2006100,
      "startingMessageId": [200610000],
      "messages": {
        "200610000": {
          "id": 200610000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "{NICKNAME}.",
          "next": [200610031]
        },
        "200610001": {
          "id": 200610001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ich ärgere mich so.",
          "next": [200610002]
        },
        "200610002": {
          "id": 200610002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wieso das?",
          "next": [200610003]
        },
        "200610003": {
          "id": 200610003,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Erzähl’s mir.",
          "next": [200610004]
        },
        "200610004": {
          "id": 200610004,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[Weinen]",
          "next": [200610005]
        },
        "200610005": {
          "id": 200610005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ich gehöre jetzt schon eine Weile zu den Wolkenrittern der Luofu.",
          "next": [200610006]
        },
        "200610006": {
          "id": 200610006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Bis auf die täglichen Patrouillen habe ich bisher eigentlich nichts Bemerkenswertes gemacht.",
          "next": [200610007]
        },
        "200610007": {
          "id": 200610007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Wann werde ich so weit sein, dass ich eine kompetente Wolkenritterin werden kann?",
          "next": [200610008, 200610011]
        },
        "200610008": {
          "id": 200610008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Patrouilliere einfach sorgfältig und schließe deine täglichen Aufgaben ab. Du bist kompetent genug.",
          "next": [200610009]
        },
        "200610009": {
          "id": 200610009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Das meine ich nicht. Jeder träumt davon, eine Legende zu sein, und wenn auch nur eine kleine.",
          "next": [200610010]
        },
        "200610010": {
          "id": 200610010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Seit ich klein war, hat mir meine Mutter ständig von den heroischen Taten der Wolkenritter erzählt. Von Größe zu träumen ist also Teil meiner DNA.",
          "next": [200610014]
        },
        "200610011": {
          "id": 200610011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Dass Frieden herrscht, ist eine gute Sache. Das bedeutet, dass deine Patrouillen Wirkung zeigen.",
          "next": [200610012]
        },
        "200610012": {
          "id": 200610012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ich weiß ...",
          "next": [200610013]
        },
        "200610013": {
          "id": 200610013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Seit ich klein war, hat mir meine Mutter ständig von den heroischen Taten der Wolkenritter erzählt. Von Größe zu träumen ist also Teil meiner DNA ...",
          "next": [200610014]
        },
        "200610014": {
          "id": 200610014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wenn du anderen so gerne helfen willst, warum schließt du dich nicht den Namenlosen an?",
          "next": [200610015]
        },
        "200610015": {
          "id": 200610015,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Bei ihnen gibt es jeden Tag Anfragen und Aufträge. Es ist beinahe unmöglich, sie alle abzuschließen.",
          "next": [200610016]
        },
        "200610016": {
          "id": 200610016,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[Schockiert]",
          "next": [200610017]
        },
        "200610017": {
          "id": 200610017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Wie aufregend!",
          "next": [200610018]
        },
        "200610018": {
          "id": 200610018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ich will eine Namenlose sein.",
          "next": [200610019]
        },
        "200610019": {
          "id": 200610019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Was für Anfragen sind das? Kann ich mal sehen?",
          "next": [200610020]
        },
        "200610020": {
          "id": 200610020,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Anlage „Ausrottung der Verderbtheit“ wurde gesendet.",
          "next": [200610021]
        },
        "200610021": {
          "id": 200610021,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Anlage „Ausrottung der Verderbtheit“ wurde empfangen.",
          "next": [200610022]
        },
        "200610022": {
          "id": 200610022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Was bedeutet „Verderbtheit“?",
          "next": [200610023]
        },
        "200610023": {
          "id": 200610023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Oh, gefunden.",
          "next": [200610024]
        },
        "200610024": {
          "id": 200610024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Warum klingt der Titel des Auftrags so klinisch?",
          "next": [200610025]
        },
        "200610025": {
          "id": 200610025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Weil er recht geschwollen ist ...",
          "next": [200610026]
        },
        "200610026": {
          "id": 200610026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Oh.",
          "next": [200610027]
        },
        "200610027": {
          "id": 200610027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Oh, ach so?",
          "next": [200610028]
        },
        "200610028": {
          "id": 200610028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Du weißt aber viel.",
          "next": [200610029]
        },
        "200610029": {
          "id": 200610029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Es ist schwer, ein Namenloser zu sein!",
          "next": [200610030]
        },
        "200610030": {
          "id": 200610030,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[Weinen]",
          "next": []
        },
        "200610031": {
          "id": 200610031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Endlich habe ich dich erwischt.",
          "next": [200610032]
        },
        "200610032": {
          "id": 200610032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ich habe gehört, dass du von jenseits der Allianz auf die Luofu gekommen bist?",
          "next": [200610033]
        },
        "200610033": {
          "id": 200610033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Kannst du mir helfen?",
          "next": [200610001]
        }
      },
      "participatingContactsIds": [1206, 8001]
    }
  ]
}
