      "messages": {
        "200580000": {
          "id": 200580000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Hey, hast du dich für die neue Immersia registriert, das vor Kurzem erschienen ist?",
          "next": [
            200580001
          ]
        },
        "200580001": {
          "id": 200580001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Komm und sei {M#mein Freund}{F#meine Freundin}.",
          "next": [
            200580002,
            200580005
          ]
        },
        "200580002": {
          "id": 200580002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "{M#Freund}{F#Freundin}?",
          "next": [
            200580003
          ]
        },
        "200580003": {
          "id": 200580003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Oh, ich bezeichne alle als Freunde, die mit uns Himmlische Jade spielen.",
          "next": [
            200580004
          ]
        },
        "200580004": {
          "id": 200580004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Aber ich sprach von der neuen foxianischen empathischen Immersia namens „Viridianische Seele“. Unsere Mitspieler dort nennen wir auch Freunde.",
          "next": [
            200580008
          ]
        },
        "200580005": {
          "id": 200580005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Immersia?",
          "next": [
            200580006
          ]
        },
        "200580006": {
          "id": 200580006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Oh, das ist nur eine empathische foxianische Immersia namens „Viridianische Seele“.",
          "next": [
            200580007
          ]
        },
        "200580007": {
          "id": 200580007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Speziell zum Spielen von Himmlische Jade, also nennen wir sie Freunde.",
          "next": [
            200580008
          ]
        },
        "200580008": {
          "id": 200580008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Solange du über einen Jade-Abakus verfügst, kannst du auch mitmachen. Es ist sehr leicht.",
          "next": [
            200580009
          ]
        },
        "200580009": {
          "id": 200580009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ah, ich verstehe. Das ist quasi ein Handyspiel.",
          "next": [
            200580010
          ]
        },
        "200580010": {
          "id": 200580010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "? Was ist ein Handyspiel? Das verstehe ich nicht.",
          "next": [
            200580011
          ]
        },
        "200580011": {
          "id": 200580011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Jedenfalls habe ich mich bereits registriert. Es ist kostenlos.",
          "next": [
            200580012
          ]
        },
        "200580012": {
          "id": 200580012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Jetzt kann ich es bei der Arbeit spielen.",
          "next": [
            200580013
          ]
        },
        "200580013": {
          "id": 200580013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Kommst du? Ich erstelle einen Freunderaum für uns.",
          "next": [
            200580015,
            200580020
          ]
        },
        "200580014": {
          "id": 200580014,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Weinen]",
          "next": [
            200580017
          ]
        },
        "200580015": {
          "id": 200580015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das würde ich gerne.",
          "next": [
            200580016
          ]
        },
        "200580016": {
          "id": 200580016,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Aber ich weiß nicht, wie man Himmlische Jade spielt.",
          "next": [
            200580014
          ]
        },
        "200580017": {
          "id": 200580017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Oh, das ist leicht.",
          "next": [
            200580018
          ]
        },
        "200580018": {
          "id": 200580018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Komm bei Gelegenheit zur Spielhalle, dann lernst du es im Nu.",
          "next": [
            200580023
          ]
        },
        "200580019": {
          "id": 200580019,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Verwirrt]",
          "next": [
            200580021
          ]
        },
        "200580020": {
          "id": 200580020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Spielst du Himmlische Jade auch bei der Arbeit?",
          "next": [
            200580019
          ]
        },
        "200580021": {
          "id": 200580021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Arbeit ist anstrengend, okay?",
          "next": [
            200580022
          ]
        },
        "200580022": {
          "id": 200580022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Dadurch baut man echt gut Stress ab. Versuch’s einfach mal.",
          "next": [
            200580023
          ]
        },
        "200580023": {
          "id": 200580023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wirklich?",
          "next": [
            200580024
          ]
        },
        "200580024": {
          "id": 200580024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Ganz im Ernst.",
          "next": [
            200580025
          ]
        },
        "200580025": {
          "id": 200580025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Bei so vielen Leuten im Express sollte es leicht sein, genügend Spieler zu finden.",
          "next": [
            200580026
          ]
        },
        "200580026": {
          "id": 200580026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Du kannst es allen beibringen, nachdem du es gelernt hast. Dann könnt ihr zusammen spielen.",
          "next": [
            200580027
          ]
        },
        "200580027": {
          "id": 200580027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Ich schicke dir später die Einladung.",
          "next": [
            200580028
          ]
        },
        "200580028": {
          "id": 200580028,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "[Daumen hoch]",
          "next": []
        }
