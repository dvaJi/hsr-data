        "201900001": {
          "id": 201900001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1015,
          "text": "Dein Zimmer war wirklich ein Chaos, wie ich schon lange nicht mehr gesehen habe.",
          "next": [
            201900002
          ]
        },
        "201900002": {
          "id": 201900002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1015,
          "text": "„Überwältigt von Glück“, das bezieht sich wahrscheinlich auf Menschen wie dich, oder?",
          "next": [
            201900003,
            201900005
          ]
        },
        "201900003": {
          "id": 201900003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich habe mit eigenen Augen gesehen, wie du verschwunden bist.",
          "next": [
            201900004
          ]
        },
        "201900004": {
          "id": 201900004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ist das die Stunde der Hexen?",
