  "sections": [
    {
      "id": 2016000,
      "startingMessageId": [
        201600000
      ],
      "messages": {
        "201600000": {
          "id": 201600000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1222,
          "text": "Nur kein Stress und keine Eile, treib’s auf der Arbeit nicht zu arg. Wer soll mich ersetzen, wenn ich nicht mehr kann und mag?",
          "next": [
            201600001
          ]
        },
        "201600001": {
          "id": 201600001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1222,
          "text": "Das sage ich jeden Tag auf!",
          "next": [
            201600002,
            201600003,
            201600004
          ]
        },
        "201600002": {
          "id": 201600002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Gut gesagt! Ein toller Rat!",
          "next": [
            201600006
          ]
        },
        "201600003": {
          "id": 201600003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Stimmt irgendetwas nicht?",
          "next": [
            201600006
          ]
        },
        "201600004": {
          "id": 201600004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bringt es wirklich etwas, das aufzusagen?",
          "next": [
            201600005
          ]
        },
        "201600005": {
          "id": 201600005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1222,
          "text": "Ach, ich versuche nur, mir selbst Mut zu machen.",
          "next": [
            201600006
          ]
        },
        "201600006": {
          "id": 201600006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1222,
          "text": "Bei der Kommission wurde früher zwölf Stunden am Tag und sechs Tage die Woche gearbeitet. Diejenigen, die da nicht mitziehen wollten, wurden als „schlechte Teamplayer“ abgestempelt und sogar zur Kündigung genötigt.",
          "next": [
            201600007
          ]
        },
        "201600007": {
          "id": 201600007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1222,
          "text": "Aber die Leute waren dadurch nicht motivierter, die Produktivität hat sogar nachgelassen.",
          "next": [
            201600008
          ]
        },
        "201600008": {
          "id": 201600008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1222,
          "text": "Deshalb lege ich mich jetzt wieder mit Leuten an, die dieses System unterstützen.",
          "next": [
            201600009,
            201600011
          ]
        },
        "201600009": {
          "id": 201600009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wow, ich wusste nicht, dass es so etwas gab ...",
          "next": [
            201600010
          ]
        },
        "201600010": {
          "id": 201600010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1222,
          "text": "Ja, kaum zu glauben, oder?",
          "next": [
            201600013
          ]
        },
        "201600011": {
          "id": 201600011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich habe wirklich Glück mit dem Astralexpress ...",
          "next": [
            201600012
          ]
        },
        "201600012": {
          "id": 201600012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1222,
          "text": "Ich bin auch ziemlich neidisch.",
          "next": [
            201600013
          ]
        },
        "201600013": {
          "id": 201600013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1222,
          "text": "{NICKNAME}, du bist weit gereist und hast viele Welten gesehen. Hast du irgendeinen Rat, den du mir geben kannst?",
          "next": [
            201600014,
            201600017,
            201600019
          ]
        },
        "201600014": {
          "id": 201600014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Greif immer frontal an und leg deine Karten auf den Tisch.",
          "next": [
            201600015
          ]
        },
        "201600015": {
          "id": 201600015,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ich weiß, dass viele Leute dich unterstützen werden!",
          "next": [
            201600016
          ]
        },
        "201600016": {
          "id": 201600016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1222,
          "text": "In der Tat, ich werde die hohen Erwartungen, die die Alchemisten an mich haben, nicht enttäuschen",
          "next": [
            201600021
          ]
        },
        "201600017": {
          "id": 201600017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hab immer das große Ganze im Auge und hüte dich vor Fallstricken.",
          "next": [
            201600018
          ]
        },
        "201600018": {
          "id": 201600018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1222,
          "text": "In der Tat. Das ist nichts, was ich mir über Nacht aneignen kann. Ich muss langfristiger denken.",
          "next": [
            201600021
          ]
        },
        "201600019": {
          "id": 201600019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Als {F#die Galaktische Baseballerin}{M#der Galaktische Baseballer} rate ich dir zu einer physischen Vorgehensweise! Schnell, direkt und effektiv!",
          "next": [
            201600020
          ]
        },
        "201600020": {
          "id": 201600020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1222,
          "text": "Ich habe kein Talent fürs Kämpfen. Ich fürchte, ein solches Vorgehen wird mir nichts nützen. Allerdings heißt es ja, dass besondere Zeiten besondere Maßnahmen erfordern ...",
          "next": [
            201600021
          ]
        },
        "201600021": {
          "id": 201600021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1222,
          "text": "Vielen Dank für deinen Rat. Nach der Unterhaltung mit dir bin ich gleich viel entspannter.",
          "next": [
            201600022
          ]
        },
        "201600022": {
          "id": 201600022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1222,
          "text": "Komm gerne wieder vorbei, wenn du mal was auf dem Herzen hast!",
          "next": []
        }
      },
      "participatingContactsIds": [
        1222,
        8001
      ]
    }
  ]
}