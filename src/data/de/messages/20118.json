  "sections": [
    {
      "id": 2011800,
      "startingMessageId": [
        201180000
      ],
      "messages": {
        "201180000": {
          "id": 201180000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "<u>Tippe, um dich für den [Rosetta-Rosenthal Cup] anzumelden. Wir freuen uns darauf, dich kennenzulernen.</u>",
          "next": [
            201180001
          ]
        },
        "201180001": {
          "id": 201180001,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Anhang „Thema der 1. Debattierrunde“ gesendet.",
          "next": [
            201180002
          ]
        },
        "201180002": {
          "id": 201180002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "{NICKNAME}, ich habe dir die Informationen zum Debattierwettbewerb geschickt, der vor Kurzem in der Gilde abgehalten wurde. Das Thema der Debatte ist „Sind Wissenschaft und Technologie neutral?“",
          "next": [
            201180003
          ]
        },
        "201180003": {
          "id": 201180003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Versteh mich nicht falsch. Ich leite nur den Post von jemand anderem weiter.",
          "next": [
            201180004
          ]
        },
        "201180004": {
          "id": 201180004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Das Thema ist für Anfänger geeignet.",
          "next": [
            201180005,
            201180008,
            201180010
          ]
        },
        "201180005": {
          "id": 201180005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich kann mich gut genug ausdrücken, um damit zurechtzukommen.",
          "next": [
            201180006
          ]
        },
        "201180006": {
          "id": 201180006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Das glaube ich nicht.",
          "next": [
            201180007
          ]
        },
        "201180007": {
          "id": 201180007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Die Kunst der Sprache kennt keine Grenzen.",
          "next": [
            201180012
          ]
        },
        "201180008": {
          "id": 201180008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aber ich habe keine Ahnung vom Debattieren.",
          "next": [
            201180009
          ]
        },
        "201180009": {
          "id": 201180009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Umso mehr Grund, es zu lernen.",
          "next": [
            201180012
          ]
        },
        "201180010": {
          "id": 201180010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tut mir leid, ich bin dafür zu schüchtern.",
          "next": [
            201180011
          ]
        },
        "201180011": {
          "id": 201180011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Du kennst dich selbst nicht gut genug.",
          "next": [
            201180012
          ]
        },
        "201180012": {
          "id": 201180012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Es ist egal, ob du beim Wettbewerb mitmachst oder nicht.",
          "next": [
            201180013
          ]
        },
        "201180013": {
          "id": 201180013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Aber ich würde gerne wissen, wie du das Debattieren als Aktivität wahrnimmst.",
          "next": [
            201180014,
            201180015,
            201180016
          ]
        },
        "201180014": {
          "id": 201180014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Beim Debattieren geht es darum, mit der anderen Seite Argumente auszutauschen.",
          "next": [
            201180017
          ]
        },
        "201180015": {
          "id": 201180015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Beim Debattieren geht es darum, die andere Seite wütend zu machen.",
          "next": [
            201180017
          ]
        },
        "201180016": {
          "id": 201180016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Beim Debattieren geht es darum, den anderen gegenüber bissig aufzutreten.",
          "next": [
            201180017
          ]
        },
        "201180017": {
          "id": 201180017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Lass es mich dir anhand des Themas der Debatte erklären.",
          "next": [
            201180018
          ]
        },
        "201180018": {
          "id": 201180018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Welche Seite wählst du?",
          "next": [
            201180019,
            201180025
          ]
        },
        "201180019": {
          "id": 201180019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Technologie ist neutral.",
          "next": [
            201180020
          ]
        },
        "201180020": {
          "id": 201180020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Dann übernehme ich die andere Seite. Du darfst zuerst sprechen.",
          "next": [
            201180021
          ]
        },
        "201180021": {
          "id": 201180021,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Technologie ist ein zweischneidiges Schwert, denn sie kann sowohl für gute als auch für böse Zwecke eingesetzt werden.",
          "next": [
            201180022
          ]
        },
        "201180022": {
          "id": 201180022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Hast du jemals von Dr. Primitivs atavistischem Experiment gehört?",
          "next": [
            201180023
          ]
        },
        "201180023": {
          "id": 201180023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Um bei seinen Testobjekten Stressreaktionen auszulösen, hat er eine Reihe von Technologien entwickelt, mit denen er sie foltern und ihnen Schmerzen zufügen konnte.",
          "next": [
            201180024
          ]
        },
        "201180024": {
          "id": 201180024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Solche Technologien sind verabscheuungswürdig.",
          "next": [
            201180030
          ]
        },
        "201180025": {
          "id": 201180025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Technologie ist nicht neutral.",
          "next": [
            201180026
          ]
        },
        "201180026": {
          "id": 201180026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Dann übernehme ich die andere Seite. Du darfst zuerst sprechen.",
          "next": [
            201180027
          ]
        },
        "201180027": {
          "id": 201180027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Einige Technologien sind grundsätzlich gut, wie zum Beispiel solche, die das Leben verlängern.",
          "next": [
            201180028
          ]
        },
        "201180028": {
          "id": 201180028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Fortschrittliche medizinische Technologien haben in der Tat viele Leben gerettet, aber medizinische Behandlungen können nicht immer jemandes Leben verlängern.",
          "next": [
            201180029
          ]
        },
        "201180029": {
          "id": 201180029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Warst du nicht schon einmal bei der Xianzhou-Allianz? Du solltest den Preis der Unsterblichkeit besser kennen als ich.",
          "next": [
            201180030
          ]
        },
        "201180030": {
          "id": 201180030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Das war’s.",
          "next": [
            201180031
          ]
        },
        "201180031": {
          "id": 201180031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Siehst du, das bestätigt mein Argument.",
          "next": [
            201180032,
            201180035
          ]
        },
        "201180032": {
          "id": 201180032,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich habe noch mehr zu bieten!",
          "next": [
            201180033
          ]
        },
        "201180033": {
          "id": 201180033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "In den meisten Fällen weist die Fähigkeit, schnell zu sprechen, auch auf einen schnellen Denker hin.",
          "next": [
            201180034
          ]
        },
        "201180034": {
          "id": 201180034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Aber es kann auch bedeuten, dass solche Leute überhaupt nicht nachdenken, bevor sie etwas sagen.",
          "next": [
            201180038
          ]
        },
        "201180035": {
          "id": 201180035,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du redest nur Unsinn.",
          "next": [
            201180036
          ]
        },
        "201180036": {
          "id": 201180036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Wenn du das sagst, verlierst du.",
          "next": [
            201180037
          ]
        },
        "201180037": {
          "id": 201180037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Aber du hast recht.",
          "next": [
            201180038
          ]
        },
        "201180038": {
          "id": 201180038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Also, es ist klar, dass niemand von uns den anderen überzeugen kann.",
          "next": [
            201180039
          ]
        },
        "201180039": {
          "id": 201180039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Verstehst du jetzt? Manchmal wird die Wahrheit durch eine Debatte klarer, manchmal auch nicht.",
          "next": [
            201180040
          ]
        },
        "201180040": {
          "id": 201180040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Die meisten Debatten sind nur Spiele mit Worten.",
          "next": [
            201180041
          ]
        },
        "201180041": {
          "id": 201180041,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Was die übrigen angeht ... Ich bin schon zu Debatten eingeladen worden, die über sprachliche Gefilde hinausgingen, ohne dass ich das überhaupt wollte.",
          "next": [
            201180042
          ]
        },
        "201180042": {
          "id": 201180042,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Über sprachliche Gefilde hinaus?",
          "next": [
            201180043
          ]
        },
        "201180043": {
          "id": 201180043,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Ein gesunder Körper ist immer unverzichtbar. Bitte denk daran, {NICKNAME}.",
          "next": []
        }
      },
      "participatingContactsIds": [
        1305,
        8001
      ]
    }
  ]
}