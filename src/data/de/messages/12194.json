  "sections": [
    {
      "id": 1219400,
      "startingMessageId": [121940000],
      "messages": {
        "121940000": {
          "id": 121940000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Ich glaube, hinter diesem Ifrit steckt noch mehr.",
          "next": [121940001]
        },
        "121940001": {
          "id": 121940001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Warum sagst du das?",
          "next": [121940002]
        },
        "121940002": {
          "id": 121940002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Die Feuerdämonen von Fetora sind von Natur aus wild und brutal, aber Ifrit ist dennoch ohne Schwierigkeiten zu einem Anführer geworden.",
          "next": [121940003]
        },
        "121940003": {
          "id": 121940003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Selbst unter den Feuerdämonen gehören seine Flammen zu einer besonderen Art.",
          "next": [121940004]
        },
        "121940004": {
          "id": 121940004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Was bedeutet das?",
          "next": [121940005]
        },
        "121940005": {
          "id": 121940005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Bei vielen brutalen Vorfällen, die von dem Ewigflammen-Anwesen angestiftet wurden, habe ich ähnliche Berichte von Augenzeugen gefunden, dass „Unruhestifter aus den Flammen stiegen“.",
          "next": [121940006]
        },
        "121940006": {
          "id": 121940006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Meine Vermutung ist, dass Ifrits Flammen vielleicht eine Fähigkeit erhalten haben, die über das Brennen hinausgeht und mit seinem Ursprung zusammenhängt.",
          "next": [121940007]
        },
        "121940007": {
          "id": 121940007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Ich habe von einer Art Feuer mit ähnlichen Eigenschaften gehört, das schwer zu fassen war, da es ständig zwischen Phasen wandelte.",
          "next": [121940008]
        },
        "121940008": {
          "id": 121940008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Es gibt nicht viele Daten dazu, nur ein Satz sticht ins Auge: „Brennt sich durch Welten und hinterlässt zahlreiche Flammenableger“.",
          "next": [121940009]
        },
        "121940009": {
          "id": 121940009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Ifrit ... ist vielleicht einer dieser Ableger.",
          "next": [121940010]
        },
        "121940010": {
          "id": 121940010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Zwischen Phasen wandeln ...",
          "next": [121940011]
        },
        "121940011": {
          "id": 121940011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Mehr habe ich nicht.",
          "next": [121940012]
        },
        "121940012": {
          "id": 121940012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Es gibt nur eine sehr begrenzte Anzahl von Aufzeichnungen dazu, ich weiß nicht, ob dir das geholfen hat.",
          "next": [121940013]
        },
        "121940013": {
          "id": 121940013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das reicht mir aus, danke.",
          "next": [121940014]
        },
        "121940014": {
          "id": 121940014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Dann ist gut.",
          "next": []
        }
      },
      "participatingContactsIds": [1002, 8001]
    }
  ]
}
