        "121640101": {
          "id": 121640101,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Mingxi hat Xiao Han dem Gruppenchat hinzugefügt.",
          "next": [121640102]
        },
        "121640102": {
          "id": 121640102,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 79,
          "text": "Ich habe den Kleinen Meister wieder hinzugefügt.",
          "next": [121640103]
        },
        "121640103": {
          "id": 121640103,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 80,
          "text": "Oh, hi!",
          "next": [121640120]
        },
        "121640105": {
          "id": 121640105,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 79,
          "text": "Die Handelsgilde wird weiterhin das Gewerbegebiet Aurum-Allee verwalten.",
          "next": [121640106]
        },
        "121640106": {
          "id": 121640106,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 80,
          "text": "Das hättest du alles sagen können, bevor du mich in die Gruppe aufgenommen hast.",
          "next": [121640107]
        },
        "121640107": {
          "id": 121640107,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 80,
          "text": "Wir haben gegen dich verloren.",
          "next": [121640108, 121640109]
        },
        "121640108": {
          "id": 121640108,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wir haben gewonnen? Unglaublich!",
          "next": [121640126]
        },
        "121640109": {
          "id": 121640109,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Richte Madame Yukong schnell unseren Dank aus!",
          "next": [121640110]
        },
        "121640110": {
          "id": 121640110,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Dies ist das Ergebnis der Bewertung der Himmelreisekommission, an dem ich nicht beteiligt war.",
          "next": [121640111]
        },
        "121640111": {
          "id": 121640111,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1207,
          "next": [121640112]
        },
        "121640112": {
          "id": 121640112,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Jippie!!!",
          "next": [121640113]
        },
        "121640113": {
          "id": 121640113,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Sei nicht traurig, Kleiner Meister.",
          "next": [121640114]
        },
        "121640114": {
          "id": 121640114,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 80,
          "text": "*schluchz*",
          "next": [121640115]
        },
        "121640115": {
          "id": 121640115,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 79,
          "text": "Und noch etwas: Die Abteilung Erhabenes Heiligtum der IFK wird der Gilde als Händler beitreten.",
          "next": [121640116, 121640117]
        },
        "121640116": {
          "id": 121640116,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Die IFK ist so ein großes Unternehmen und zählt trotzdem als Händler?",
          "next": [121640118]
        },
        "121640117": {
          "id": 121640117,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bedeutet das nicht, dass Xiao Han auch Teil der Gilde wird?",
          "next": [121640118]
        },
        "121640118": {
          "id": 121640118,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 80,
          "text": "Als Regionalleiter des Unternehmens hatte ich bereits Pläne, Verbindungen zur lokalen Geschäftswelt herzustellen.",
          "next": [121640119]
        },
        "121640119": {
          "id": 121640119,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 80,
          "text": "Für die Zukunft bitte ich euch um eure Unterstützung bei der Wiederbelebung der Aurum-Allee.",
          "next": [121640127]
        },
        "121640120": {
          "id": 121640120,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 79,
          "text": "Die Bietergebnisse sind veröffentlicht worden.",
          "next": [121640121]
        },
        "121640121": {
          "id": 121640121,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 79,
          "text": "Ich habe eine Nachricht von der Himmelreisekommission erhalten. Wir haben es geschafft.",
          "next": [121640122]
        },
        "121640122": {
          "id": 121640122,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Wow",
          "next": [121640123]
        },
        "121640123": {
