      "messages": {
        "201500000": {
          "id": 201500000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Übrigens habe ich neulich eine lustige Sache erlebt.",
          "next": [
            201500001
          ]
        },
        "201500001": {
          "id": 201500001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Als ich am Geschäft „Colors“ vorbeikam, bat mich eine Mitarbeiterin um ein Autogramm.",
          "next": [
            201500002,
            201500005,
            201500007
          ]
        },
        "201500002": {
          "id": 201500002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Natürlich bist du eine Berühmtheit, nach allem, was du vollbracht hast!",
          "next": [
            201500003
          ]
        },
        "201500003": {
          "id": 201500003,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "next": [
            201500004
          ]
        },
        "201500004": {
          "id": 201500004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Eigentlich war es ein Missverständnis.",
          "next": [
            201500010
          ]
        },
        "201500005": {
          "id": 201500005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "War es eine Art Missverständnis ...?",
          "next": [
            201500006
          ]
        },
        "201500006": {
          "id": 201500006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Genau.",
          "next": [
            201500010
          ]
        },
        "201500007": {
          "id": 201500007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ein Autogramm von dir? Ich will auch eins!",
          "next": [
            201500008
          ]
        },
        "201500008": {
          "id": 201500008,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "next": [
            201500009
          ]
        },
        "201500009": {
          "id": 201500009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Eigentlich war es ein Missverständnis.",
          "next": [
            201500010
          ]
        },
        "201500010": {
          "id": 201500010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Robins Makeup in der Werbung „Antlitz des Abends“ ähnelt dem der Mädchen aus meiner Heimat. Ich war eine Weile in Nostalgie versunken.",
          "next": [
            201500011
          ]
        },
        "201500011": {
          "id": 201500011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Und jetzt, wo ich darüber nachdenke, vermisse ich die Zeit ein bisschen. Wie die Mädchen sich darum sorgten, dass ihr Makeup nicht von einem der häufigen Stürme ruiniert werden würde.",
          "next": [
            201500012
          ]
        },
        "201500012": {
          "id": 201500012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Apropos Colors: ich habe dort einige richtig gute Produkte gesehen. Also habe ich sie gekauft, als Zeichen meiner Wertschätzung.",
          "next": [
            201500013
          ]
        },
        "201500013": {
          "id": 201500013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Ich glaube, Himeko wird diesen rosaroten Lippenstift mögen.",
          "next": [
            201500014
          ]
        },
        "201500014": {
          "id": 201500014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Und dieser Lidschatten der Serie „Frühlingsbilder“ passt sehr gut zu 7. März.",
          "next": [
            201500015
          ]
        },
        "201500015": {
          "id": 201500015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Und nicht nur das – Ich habe ein paar wasserspendende und reinigende Produkte mitgenommen, die von den Leuten in den höchsten Tönen gelobt werden.",
          "next": [
            201500016,
            201500018
          ]
        },
        "201500016": {
          "id": 201500016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Vielen Dank, ich werde es weitersagen!",
          "next": [
            201500017
          ]
        },
        "201500017": {
          "id": 201500017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Gern geschehen.",
          "next": [
            201500020
          ]
        },
        "201500018": {
          "id": 201500018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hätte nie gedacht, dass du so viel über Makeup weißt.",
          "next": [
            201500019
          ]
        },
        "201500019": {
          "id": 201500019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Überrascht es dich?",
          "next": [
            201500020
          ]
        },
        "201500020": {
          "id": 201500020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Ich schicke dir die Geschenke per Post, bevor ich es vergesse.",
          "next": [
            201500021
          ]
        },
        "201500021": {
          "id": 201500021,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Dein Kontakt ist offline.",
          "next": [
            201500022
          ]
        },
        "201500022": {
          "id": 201500022,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Dein Kontakt ist online.",
          "next": [
            201500023
          ]
        },
        "201500023": {
          "id": 201500023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Es tut mir leid.",
          "next": [
            201500024
          ]
        },
        "201500024": {
          "id": 201500024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Weißt du übrigens, wo die Post ist?",
          "next": [
            201500025,
            201500026
          ]
        },
        "201500025": {
          "id": 201500025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich wusste es ...",
          "next": [
            201500027
          ]
        },
        "201500026": {
          "id": 201500026,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Brauchst du meine Hilfe?",
          "next": [
            201500027
          ]
        },
        "201500027": {
          "id": 201500027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Oh, gefunden.",
          "next": [
            201500028
          ]
        },
        "201500028": {
          "id": 201500028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Sie ist direkt hinter mir.",
