  "sections": [
    {
      "id": 1302001,
      "startingMessageId": [130200101],
      "messages": {
        "130200101": {
          "id": 130200101,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ich empfehle euch mal meine Entspannungsmethode.",
          "next": [130200102]
        },
        "130200102": {
          "id": 130200102,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Sucht euch am Hafen der Sternskiffe einen Platz, von dem aus ihr die Skiffe sehen könnt, und chillt!",
          "next": [130200103]
        },
        "130200103": {
          "id": 130200103,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "... Im Teehaus Schlafloser Graf geht das natürlich auch.",
          "next": [130200104]
        },
        "130200104": {
          "id": 130200104,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Die vorbeiziehenden Sternskiffe beobachten ...",
          "next": [130200105]
        },
        "130200105": {
          "id": 130200105,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Man merkt gar nicht, wie die Zeit vergeht ...",
          "next": [130200106]
        },
        "130200106": {
          "id": 130200106,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Das ist sehr wohltuend.",
          "next": [130200107]
        },
        "130200107": {
          "id": 130200107,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Versteh ich nicht.",
          "next": [130200108]
        },
        "130200108": {
          "id": 130200108,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Versteh ich nicht.",
          "next": [130200109, 130200112]
        },
        "130200109": {
          "id": 130200109,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Versteh ich nicht.",
          "next": [130200110]
        },
        "130200110": {
          "id": 130200110,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ich mache mir etwas Sorgen um Huohuos psychischen Zustand.",
          "next": [130200111]
        },
        "130200111": {
          "id": 130200111,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Wir probieren es mal. Aber Huohuo, wenn du etwas auf dem Herzen hast, kannst du es uns erzählen, ja?",
          "next": [130200117]
        },
        "130200112": {
          "id": 130200112,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wie dem Regen zuzuhören, um Stress abzubauen. Das ist eine ganz ähnliche Methode.",
          "next": [130200113]
        },
        "130200113": {
          "id": 130200113,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Das kann ich voll nachvollziehen.",
          "next": [130200114]
        },
        "130200114": {
          "id": 130200114,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Genau!",
          "next": [130200115]
        },
        "130200115": {
          "id": 130200115,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ich mache mir etwas Sorgen um den psychischen Zustand der beiden.",
          "next": [130200116]
        },
        "130200116": {
          "id": 130200116,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Wenn ihr etwas auf dem Herzen habt, könnt ihr es uns erzählen, ja?",
          "next": [130200117]
        },
        "130200117": {
          "id": 130200117,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ich sitze hier schon den halben Tag, es ist echt angenehm.",
          "next": [130200118]
        },
        "130200118": {
          "id": 130200118,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Oh, Moment!",
          "next": [130200119]
        },
        "130200119": {
          "id": 130200119,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Hm?",
          "next": [130200120]
        },
        "130200120": {
          "id": 130200120,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ist was passiert?",
          "next": [130200121]
        },
        "130200121": {
          "id": 130200121,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[verwirrt]",
          "next": [130200122]
        },
        "130200122": {
          "id": 130200122,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Bin wieder da.",
          "next": [130200123]
        },
        "130200123": {
          "id": 130200123,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Herr Xiyan ist vorbeigekommen, hat mir ohne was zu sagen eine Handvoll Süßigkeiten gegeben und ist wieder gegangen.",
          "next": [130200124]
        },
        "130200124": {
          "id": 130200124,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "W... Was ist denn da los?",
          "next": [130200125, 130200126]
        },
        "130200125": {
          "id": 130200125,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das zeigt, dass Huohuo sehr süß ist. Wenn ich in der Nähe wäre, würde ich dir auch Süßigkeiten geben.",
          "next": [130200128]
        },
        "130200126": {
          "id": 130200126,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wie du da die ganze Zeit den Sternskiffen hinterhergeschaut hast, sahst du wahrscheinlich echt armselig aus.",
          "next": [130200127]
        },
        "130200127": {
          "id": 130200127,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Jeder freundliche Mensch würde etwas für dich tun wollen.",
          "next": [130200128]
        },
        "130200128": {
          "id": 130200128,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "So wie man es nicht lassen kann, die Koifische im Teich zu füttern?",
          "next": [130200129]
        },
        "130200129": {
          "id": 130200129,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ist Füttern dort nicht verboten?",
          "next": [130200130]
        },
        "130200130": {
          "id": 130200130,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Huohuo füttern ist ja wohl erlaubt.",
          "next": [130200131]
        },
        "130200131": {
          "id": 130200131,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ach so.",
          "next": [130200132]
        },
        "130200132": {
          "id": 130200132,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "... Sind wir abgeschweift?",
          "next": [130200133]
        },
        "130200133": {
          "id": 130200133,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ich hab auch Süßigkeiten. Huohuo, wo bist du?",
          "next": [130200134]
        },
        "130200134": {
          "id": 130200134,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ich hab auch Süßigkeiten! Los geht’s!",
          "next": [130200135]
        },
        "130200135": {
          "id": 130200135,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[Los!]",
          "next": [130200136]
        },
        "130200136": {
          "id": 130200136,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ihr müsst nicht kommen, ich bin schon gegangen.",
          "next": [130200137]
        },
        "130200137": {
          "id": 130200137,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Aah ...",
          "next": []
        }
      },
      "participatingContactsIds": [1217, 1210, 1206, 8001]
    }
  ]
}
