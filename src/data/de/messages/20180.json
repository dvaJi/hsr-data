      ],
      "messages": {
        "201800000": {
          "id": 201800000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "{NICKNAME}, möchtest du unsere einzigartigen Badehäuser ausprobieren?",
          "next": [
            201800001
          ]
        },
        "201800001": {
          "id": 201800001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Sie bieten oft einzigartige Bäder und Badetechniken, die bald im Marmorpalast vorgestellt werden.",
          "next": [
            201800002
          ]
        },
        "201800002": {
          "id": 201800002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Das Badeerlebnis in Okhema ist unvergesslich und ich kann dir nur empfehlen, es einmal auszuprobieren.",
          "next": [
            201800003
          ]
        },
        "201800003": {
          "id": 201800003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Probiere zum Beispiel ein Bad mit siebenfarbigen Badesalzen, die speziell zur Beruhigung müder Muskeln entwickelt wurden.",
          "next": [
            201800004
          ]
        },
        "201800004": {
          "id": 201800004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Während du eine lebhafte Debatte genießt, pressen unsere Bademeister frische Granatäpfel aus und übergießen dich mit dem luxuriösen Saft.",
          "next": [
            201800005
          ]
        },
        "201800005": {
          "id": 201800005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Anschließend massieren und klopfen sie deine Haut sanft, bis sie sich warm und seidig glatt anfühlt.",
          "next": [
            201800006
          ]
        },
        "201800006": {
          "id": 201800006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Erfreue dich nach deinem Bad endloser Möglichkeiten, wie unseren Gewinnspielen oder einer therapeutischen Rückenmassage durch ein Droma.",
          "next": [
            201800007,
            201800009
          ]
        },
        "201800007": {
          "id": 201800007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Warte, was? Ein Droma?",
          "next": [
            201800008
          ]
        },
        "201800008": {
          "id": 201800008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Keine Sorge. Diese Dromen sind speziell ausgebildet und wissen, wie sie ihre Kraft perfekt kontrollieren können.",
          "next": [
            201800012
          ]
        },
        "201800009": {
          "id": 201800009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wenn es kein feuriges Ölbad gibt, bin ich raus.",
          "next": [
            201800010
          ]
        },
        "201800010": {
          "id": 201800010,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Du solltest dich von meinem Zimmer inspirieren lassen.",
          "next": [
            201800011
          ]
        },
        "201800011": {
          "id": 201800011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Ich würde gerne irgendwann einmal zu Besuch kommen.",
          "next": [
            201800012
          ]
        },
        "201800012": {
          "id": 201800012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Hmm ... apropos, es ist Zeit für meinen täglichen Ausflug ins Badehaus.",
          "next": [
            201800013
          ]
        },
        "201800013": {
          "id": 201800013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Bis später.",
          "next": [
            201800014,
            201800016
          ]
        },
        "201800014": {
          "id": 201800014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du gehst jeden Tag baden?",
          "next": [
            201800015
          ]
        },
        "201800015": {
          "id": 201800015,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Hast du sonst nicht viel zu tun?",
          "next": [
            201800017
          ]
        },
        "201800016": {
          "id": 201800016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Machst du dir keine Sorgen, dass du schrumpelig wirst?",
          "next": [
            201800017
          ]
        },
        "201800017": {
          "id": 201800017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Ja, deshalb kann ich mir nicht dreimal am Tag ein Bad gönnen, sondern nur einmal.",
          "next": [
            201800018
          ]
        },
        "201800018": {
          "id": 201800018,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1402,
          "next": []
        }
      },
