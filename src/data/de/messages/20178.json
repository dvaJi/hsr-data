  "sections": [
    {
      "id": 2017800,
      "startingMessageId": [
        201780000
      ],
      "messages": {
        "201780000": {
          "id": 201780000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "{NICKNAME}, ich frage mich, ob du Theaterstücke magst.",
          "next": [
            201780001
          ]
        },
        "201780001": {
          "id": 201780001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Vor kurzem wurde eine Reihe neuer Stücke veröffentlicht und ich habe einige Plätze für unsere Gäste reserviert.",
          "next": [
            201780002,
            201780004,
            201780006
          ]
        },
        "201780002": {
          "id": 201780002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Erzähl mir davon.",
          "next": [
            201780003
          ]
        },
        "201780003": {
          "id": 201780003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Lass mich dir ein paar vorstellen ...",
          "next": [
            201780008
          ]
        },
        "201780004": {
          "id": 201780004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nicht interessiert.",
          "next": [
            201780005
          ]
        },
        "201780005": {
          "id": 201780005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Hör dir einfach meine Vorstellung an.",
          "next": [
            201780008
          ]
        },
        "201780006": {
          "id": 201780006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wird „Nikador im Kampf mit der Rosigen Himmelsmaid“ aufgeführt?",
          "next": [
            201780007
          ]
        },
        "201780007": {
          "id": 201780007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Ha, solche Werke gibt es nicht selten.",
          "next": [
            201780008
          ]
        },
        "201780008": {
          "id": 201780008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Die Theaterszene in Amphoreus ist lebhaft und bietet eine Reihe spektakulärer Stücke an.",
          "next": [
            201780009
          ]
        },
        "201780009": {
          "id": 201780009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Es gibt weltbekannte und gefeierte Klassiker wie „Die Aidonier“ und „Die Frauen von Okhema“ ...",
          "next": [
            201780010
          ]
        },
        "201780010": {
          "id": 201780010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Neben dramatischen Werken erfreuen sich auch einige heitere, humorvolle Stücke großer Beliebtheit.",
          "next": [
            201780011
          ]
        },
        "201780011": {
          "id": 201780011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Diese Saison beinhaltet beispielsweise Titel wie „Zagreus: Die Liebe eines Tyrannen“, „Liebling, ich bin als ein sprechendes Droma aufgewacht“ und „Festung Kremnos“ ...",
          "next": [
            201780012
          ]
        },
        "201780012": {
          "id": 201780012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Ich habe gehört, dass diese Stücke einen Stil haben, der sich deutlich von den Klassikern unterscheidet. Schade, dass ich sie noch nicht sehen konnte.",
          "next": [
            201780013,
            201780014,
            201780016
          ]
        },
        "201780013": {
          "id": 201780013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich liebe diese Art von Aufführungen!",
          "next": [
            201780017
          ]
        },
        "201780014": {
          "id": 201780014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wir haben eine universelle ästhetische Anziehungskraft im gesamten Kosmos!",
          "next": [
            201780015
          ]
        },
        "201780015": {
          "id": 201780015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Wirklich? Es könnte einfach ein natürlicher Aspekt der Evolution der Kunst sein.",
          "next": [
            201780017
          ]
        },
        "201780016": {
          "id": 201780016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Jeder braucht ab und zu eine Pause.",
          "next": [
            201780017
          ]
        },
        "201780017": {
          "id": 201780017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Diese Stücke hören sich sehr lustig an. Es ist keine Überraschung, dass sie so beliebt sind.",
          "next": [
            201780018
          ]
        },
        "201780018": {
          "id": 201780018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Lass uns unsere Gedanken austauschen, nachdem du sie gesehen hast.",
          "next": []
        }
      },
      "participatingContactsIds": [
        1402,
        8001
      ]
    }
  ]
}