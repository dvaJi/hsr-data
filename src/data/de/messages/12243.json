        "122430000": {
          "id": 122430000,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Du hast heimlich an der Kampfkunstzeremonie teilgenommen? Warum hast du mir nichts gesagt?",
          "next": [
            122430002
          ]
        },
        "122430002": {
          "id": 122430002,
          "type": "Image",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "image": "PhoneMessagePic_10046",
          "text": "[Bild]",
          "next": [
            122430003
          ]
        },
        "122430003": {
          "id": 122430003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1310,
          "text": "???",
          "next": [
            122430004
          ]
        },
        "122430004": {
          "id": 122430004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1310,
          "text": "Das bin ich nicht.",
          "next": [
            122430005
          ]
        },
        "122430005": {
          "id": 122430005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1310,
          "text": "Ich würde nie etwas so ... Unförmiges tragen.",
          "next": [
            122430006
          ]
        },
        "122430006": {
          "id": 122430006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1310,
          "text": "Moment mal! Silberwolf ist seit einigen Tagen auf der Luofu. Will sie uns einen Streich spielen?",
          "next": [
            122430007
          ]
        },
        "122430007": {
          "id": 122430007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1310,
          "text": "Ich ruf sie mal an und frag sie. Moment.",
          "next": [
            122430008
          ]
        },
        "122430008": {
          "id": 122430008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1310,
          "text": "Sie hat sich vor Lachen auf dem Boden gekugelt, als sie das gehört hat.",
          "next": [
            122430009
          ]
        },
        "122430009": {
          "id": 122430009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1310,
          "text": "Sie lacht immer noch. Moment.",
          "next": [
            122430010
          ]
        },
        "122430010": {
          "id": 122430010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1310,
          "text": "Sie kriegt kaum noch Luft vor lauter Lachen.",
          "next": [
            122430011
          ]
        },
        "122430011": {
          "id": 122430011,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1310,
          "text": "[sprachlos]",
          "next": [
            122430012
          ]
        },
        "122430012": {
          "id": 122430012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1310,
          "text": "Sie hat aufgehört und weist alles von sich.",
          "next": [
            122430013
          ]
        },
        "122430013": {
          "id": 122430013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1310,
          "text": "Anscheinend nimmt jemand unter meinem Namen an der Kampfkunstzeremonie teil.",
          "next": [
            122430014,
            122430016
          ]
        },
        "122430014": {
          "id": 122430014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich dachte, du hättest nur eine neue Ausrüstung an oder so. Dann hat sich also jemand als du ausgegeben!",
          "next": [
            122430015
          ]
        },
        "122430015": {
          "id": 122430015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1310,
          "text": "Wieso sollte ich so eine ... „neue Ausrüstung“ tragen?",
          "next": [
            122430018
          ]
        },
        "122430016": {
          "id": 122430016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das kam mir auch komisch vor! Ich wusste, dass sich irgendein Witzbold als du ausgegeben hat!",
          "next": [
            122430017
          ]
        },
        "122430017": {
          "id": 122430017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1310,
          "text": "Und das nicht einmal besonders gut!",
          "next": [
            122430018
          ]
        },
        "122430018": {
          "id": 122430018,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1310,
          "text": "[wütend]",
          "next": [
            122430019
          ]
        },
        "122430019": {
          "id": 122430019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Keine Sorge. Luka wird dir helfen, diesem Witzbold eine Lektion zu erteilen!",
          "next": [
            122430020
          ]
        },
        "122430020": {
          "id": 122430020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1310,
          "text": "Aber es ist ja nichts wirklich Schlimmes passiert. Also bitte nicht übertreiben.",
          "next": [
            122430021
          ]
        },
        "122430021": {
          "id": 122430021,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1310,
          "text": "[Aufbruch]",
          "next": []
        }
