  "sections": [
    {
      "id": 1111800,
      "startingMessageId": [111180000],
      "messages": {
        "111180000": {
          "id": 111180000,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Schaffner! Hiiiilfe!!!",
          "next": [111180002]
        },
        "111180002": {
          "id": 111180002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "[Automatische Antwort] Gerade im Dienst im Express. Bitte hinterlässt du eine Nachricht, wenn es dringend ist!",
          "next": [111180003, 111180004]
        },
        "111180003": {
          "id": 111180003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich kann dich tippen sehen ...",
          "next": [111180005]
        },
        "111180004": {
          "id": 111180004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Warum fährst du weg? Ich bin noch nicht im Express!",
          "next": [111180006]
        },
        "111180005": {
          "id": 111180005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Verflixt, Pom-Poms Tarnung ist aufgeflogen!",
          "next": [111180007]
        },
        "111180006": {
          "id": 111180006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Juhu, Pom-Pom hat dich gut erwischt, {NICKNAME}!",
          "next": [111180007]
        },
        "111180007": {
          "id": 111180007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Okay, ich höre auf, herumzualbern. Was ist los, {NICKNAME}?",
          "next": [111180008, 111180009]
        },
        "111180008": {
          "id": 111180008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nimmt unser Express neue Mitglieder auf?",
          "next": [111180010]
        },
        "111180009": {
          "id": 111180009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Also ... Ich habe eine Freundin ...",
          "next": [111180010]
        },
        "111180010": {
          "id": 111180010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Oh, du willst deine Freundin mit an Bord bringen?",
          "next": [111180011]
        },
        "111180011": {
          "id": 111180011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Dann sag Pom-Pom, wie ist sie?",
          "next": [111180012]
        },
        "111180012": {
          "id": 111180012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Bitte ordne deine Gedanken, bevor du antwortest. Pom-Pom will den Fall richtig einschätzen können!",
          "next": [111180013, 111180014, 111180015]
        },
        "111180013": {
          "id": 111180013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sie ist ein Genie in Sachen Mechanik mit einem akribischen Verstand.",
          "next": [111180016]
        },
        "111180014": {
          "id": 111180014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sie ist ein Rockstar mit einem hemmungslosen Charakter.",
          "next": [111180017]
        },
        "111180015": {
          "id": 111180015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sie ist ... Ich weiß es auch nicht so recht.",
          "next": [111180018]
        },
        "111180016": {
          "id": 111180016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Oh, Fräulein Himeko wird sicher begeistert sein!",
          "next": [111180019]
        },
        "111180017": {
          "id": 111180017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Rockstar? Kann Pom-Pom eine Kopie ihrer Musik haben?",
          "next": [111180019]
        },
        "111180018": {
          "id": 111180018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Dann vergiss es! Pom-Pom hat sowieso nur zum Spaß gefragt!",
          "next": [111180019]
        },
        "111180019": {
          "id": 111180019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Pom-Pom denkt jedenfalls, dass es kein Problem gibt. Aber ...",
          "next": [111180020]
        },
        "111180020": {
          "id": 111180020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Der Maschinenraum ist tabu!",
          "next": [111180021]
        },
        "111180021": {
          "id": 111180021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Mach nichts kaputt!",
          "next": [111180022]
        },
        "111180022": {
          "id": 111180022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Kann sie es versprechen?",
          "next": [111180023, 111180024, 111180025]
        },
        "111180023": {
          "id": 111180023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kein Problem!",
          "next": [111180026]
        },
        "111180024": {
          "id": 111180024,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich ... werde versuchen, sie zu überreden ...",
          "next": [111180026]
        },
        "111180025": {
          "id": 111180025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich glaube nicht, dass man sie kontrollieren kann.",
          "next": [111180026]
        },
        "111180026": {
          "id": 111180026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Wie dem auch sei, diese Dinge sind nicht erlaubt! Solange das befolgt wird, wird Pom-Pom keine Einwände haben!",
          "next": [111180027, 111180028]
        },
        "111180027": {
          "id": 111180027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Verstanden. Danke, Schaffner.",
          "next": [111180029]
        },
        "111180028": {
          "id": 111180028,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Super! Besser als Pom-Pom geht es nicht!",
          "next": [111180029]
        },
        "111180029": {
          "id": 111180029,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "(schickt einen Allzwecksticker)",
          "next": []
        }
      },
