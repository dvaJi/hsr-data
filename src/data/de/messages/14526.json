      "messages": {
        "145260000": {
          "id": 145260000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "An alle: Wir konnten immer noch keinen Kontakt zu dem Zugwagen aufnehmen, der in Amphoreus eingefahren ist.",
          "next": [
            145260001
          ]
        },
        "145260001": {
          "id": 145260001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Der Zustand von 7. März scheint sich ebenfalls zu verschlechtern.",
          "next": [
            145260002,
            145260003
          ]
        },
        "145260002": {
          "id": 145260002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Himeko! Welt! Seht ihr mich? Könnt ihr die Nachrichten lesen, die ich geschickt habe?!",
          "next": [
            145260011
          ]
        },
        "145260003": {
          "id": 145260003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Himeko! Welt! Rettet mich! Amphoreus ist verdammt!",
          "next": [
            145260011
          ]
        },
        "145260004": {
          "id": 145260004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Wie sieht es bei dir aus, Welt?",
          "next": [
            145260005
          ]
        },
        "145260005": {
          "id": 145260005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Bisher läuft alles gut. Herta hat Screwllum gebeten, uns zu helfen, und wir haben uns in der Isolationszone ausführlich unterhalten.",
          "next": [
            145260006
          ]
        },
        "145260006": {
          "id": 145260006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Beide Genies haben mit ihrem Wissen dazu beigetragen, dass wir der Lösung des Rätsels um Amphoreus jetzt näher gekommen sind.",
          "next": [
            145260007
          ]
        },
        "145260007": {
          "id": 145260007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Allerdings gibt es viele Informationen, mit denen wir nicht gerechnet haben. Es ist schwierig, alles in einer Textnachricht zu erklären.",
          "next": [
            145260008
          ]
        },
        "145260008": {
          "id": 145260008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Ich schicke dir die Besprechungsnotizen gleich als Video.",
          "next": [
            145260009
          ]
        },
        "145260009": {
          "id": 145260009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Alles klar.",
          "next": [
            145260010
          ]
        },
        "145260010": {
          "id": 145260010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hilf mir mal, Sternenanker. Ich möchte zur Raumstation und auch die Besprechungsnotizen überprüfen.",
          "next": [
            145260012
          ]
        },
