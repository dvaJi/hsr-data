        "201410009": {
          "id": 201410009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Das ist zu verdächtig. Lass mich das schnell erledigen!",
          "next": [
            201410010
          ]
        },
        "201410010": {
          "id": 201410010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1315,
          "text": "Schweiß, was zum Geier hab ich denn davon, wenn ich dir das Zeug gebe? Da komm ich nicht ganz mit. Hältst du mich für dämlich? Hahaha!",
