  "sections": [
    {
      "id": 2006200,
      "startingMessageId": [200620000],
      "messages": {
        "200620000": {
          "id": 200620000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "{NICKNAME}.",
          "next": [200620001]
        },
        "200620001": {
          "id": 200620001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Kennst du Yanqing?",
          "next": [200620002, 200620024, 200620006]
        },
        "200620002": {
          "id": 200620002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich habe seinen Namen schon mal gehört.",
          "next": [200620003]
        },
        "200620003": {
          "id": 200620003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Er ist der Goldjunge der Wolkenritter.",
          "next": [200620012]
        },
        "200620006": {
          "id": 200620006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wer ist Yanqing?",
          "next": [200620007]
        },
        "200620007": {
          "id": 200620007,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Verwirrt]",
          "next": [200620008]
        },
        "200620008": {
          "id": 200620008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Du musst ihn doch kennen.",
          "next": [200620009]
        },
        "200620009": {
          "id": 200620009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Das ist dieser Junge, der General Jing Yuan überallhin folgt.",
          "next": [200620010]
        },
        "200620010": {
          "id": 200620010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Der Wächter des Generals?",
          "next": [200620011]
        },
        "200620011": {
          "id": 200620011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Genau der. Er ist der Goldjunge der Wolkenritter!",
          "next": [200620012]
        },
        "200620012": {
          "id": 200620012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ich habe ihn heute bei seinen Schwertübungen gesehen. Ich kann seinen Bewegungen nicht mal mit den Augen folgen! Das war ein Schock.",
          "next": [200620013]
        },
        "200620013": {
          "id": 200620013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Er ist so jung! Wie kann er so talentiert sein?",
          "next": [200620014, 200620017]
        },
        "200620014": {
          "id": 200620014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Könnte er eins dieser Kampfgenies sein?",
          "next": [200620015]
        },
        "200620015": {
          "id": 200620015,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Vielleicht liegt ihm das Kämpfen von seiner Physis her.",
          "next": [200620016]
        },
        "200620016": {
          "id": 200620016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Du hast so recht!",
          "next": [200620020]
        },
        "200620017": {
          "id": 200620017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Könnte das daran liegen, dass General Jing Yuan ihn persönlich trainiert?",
          "next": [200620018]
        },
        "200620018": {
          "id": 200620018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Das ist es wohl, was mit „an der Startlinie gewinnen“ gemeint ist.",
          "next": [200620019]
        },
        "200620019": {
          "id": 200620019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Meine Mutter ist auch eine Wolkenritterin und sie hat mich von klein auf gezwungen, zu trainieren. Wieso bin ich dann nicht so gut wie er?",
          "next": [200620020]
        },
        "200620020": {
          "id": 200620020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Wenn ich so darüber nachdenke, erinnert mich die Art, wie er sein Schwert führt, irgendwie an meine Mutter.",
          "next": [200620021]
        },
        "200620021": {
          "id": 200620021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ich sollte ihn bei Gelegenheit um ein paar Tipps bitten.",
          "next": [200620022]
        },
        "200620022": {
          "id": 200620022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ich bin bereit, dich zu begleiten!",
          "next": [200620023]
        },
        "200620023": {
          "id": 200620023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Okay!",
          "next": []
        },
        "200620024": {
          "id": 200620024,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Zwischen uns ist es schon zu ein paar Schlagabtäuschen gekommen.",
          "next": [200620025]
        },
        "200620025": {
          "id": 200620025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Was? Du hast mit dem Goldjungen der Wolkenritter gekämpft?",
          "next": [200620026]
        },
        "200620026": {
          "id": 200620026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Das glaube ich dir nicht ...",
          "next": [200620012]
        }
      },
      "participatingContactsIds": [1206, 8001]
    }
  ]
}
