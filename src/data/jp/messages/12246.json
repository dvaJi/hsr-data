        "122460034": {
          "id": 122460034,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "何を言いたいかはわかってる。{F#私}{M#俺}と手合わせして、指導してほしいん{F#でしょ}{M#だろ}",
          "next": [
            122460035
          ]
        },
        "122460035": {
          "id": 122460035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "うん！",
          "next": [
            122460036
          ]
        },
        "122460036": {
          "id": 122460036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "時間がある時でいいから、僕と手合わせしてほしんだ",
