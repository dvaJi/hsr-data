      "messages": {
        "200410000": {
          "id": 200410000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "{NICKNAME}、1つ相談したいことがある",
          "next": [200410001]
        },
        "200410001": {
          "id": 200410001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "今、大丈夫だろうか？",
          "next": [200410002]
        },
        "200410002": {
          "id": 200410002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "時間ならあるよ、言って",
          "next": [200410003]
        },
        "200410003": {
          "id": 200410003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "実は、我らがランドゥー家の妹がフィールドワークから帰ってくるんだ",
          "next": [200410004]
        },
        "200410004": {
          "id": 200410004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "そこで、彼女にプレゼントをしたいと考えているんだが、何を贈ればいいと思う？",
          "next": [200410005, 200410014, 200410023]
        },
        "200410005": {
          "id": 200410005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "彼女は何が好き{F#なの}{M#なんだ}？",
          "next": [200410006]
        },
        "200410006": {
          "id": 200410006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "恥ずかしい話だが、彼女の趣味とかはよくわからないんだ",
          "next": [200410007]
        },
        "200410007": {
          "id": 200410007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "ただ、フィールドワークなどの現地調査にとても熱心な子で",
          "next": [200410008]
        },
        "200410008": {
          "id": 200410008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "ああ、そうだ。最新式の調査用の装備を贈ろう",
          "next": [200410009]
        },
        "200410009": {
          "id": 200410009,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "「いいね」",
          "next": [200410010]
        },
        "200410010": {
          "id": 200410010,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "いいじゃない{M#か}",
          "next": [200410011]
        },
        "200410011": {
          "id": 200410011,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "彼女が好きならそれでいいんだ{F#よ}",
          "next": [200410012]
        },
        "200410012": {
          "id": 200410012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "ああ。感謝する、方向性がわかった",
          "next": [200410013]
        },
        "200410013": {
          "id": 200410013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "プレゼントを選んでこようと思う。では",
          "next": []
        },
        "200410014": {
          "id": 200410014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "花束なんてのはどう{M#だ}？",
          "next": [200410015]
        },
        "200410015": {
          "id": 200410015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "それは考えた。だが、普通の植物には興味がないと言っていて",
          "next": [200410016]
        },
        "200410016": {
          "id": 200410016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "彼女が好きなのは雪原で育つ、都市の中では見かけない植物なんだ",
          "next": [200410017]
        },
        "200410017": {
          "id": 200410017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "そうだな。今度外で見回りする時に辺りに気にしてみよう",
          "next": [200410018]
        },
        "200410018": {
          "id": 200410018,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "「いいね」",
          "next": [200410019]
        },
        "200410019": {
          "id": 200410019,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "いいじゃない{M#か}",
          "next": [200410020]
        },
        "200410020": {
          "id": 200410020,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "でも安全には気を付けて",
          "next": [200410021]
        },
        "200410021": {
          "id": 200410021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "はは、ありがとう。気をつけるよ",
          "next": [200410022]
        },
        "200410022": {
          "id": 200410022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "では、この辺で失礼する",
          "next": []
        },
        "200410023": {
          "id": 200410023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "新しい服なんてのはどう{M#だ}？",
          "next": [200410024]
        },
        "200410024": {
          "id": 200410024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "それも考えたんだが、僕はセンスがないから、むやみに買えないんだ…",
          "next": [200410025]
        },
        "200410025": {
          "id": 200410025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "姉さんなら、よく彼女に綺麗な服を買ってあげているが",
          "next": [200410026]
        },
        "200410026": {
          "id": 200410026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "今回は姉さんに知られたくないんだ",
          "next": [200410027]
        },
        "200410027": {
          "id": 200410027,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "「いやだ」",
          "next": [200410028]
        },
        "200410028": {
          "id": 200410028,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "何をひねくれてる{F#の}{M#んだ}？",
          "next": [200410029]
        },
        "200410029": {
          "id": 200410029,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "家族なん{F#でしょ}{M#だろ}？直接聞けばいい{F#よ}{M#}",
          "next": [200410030]
        },
        "200410030": {
          "id": 200410030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "……",
          "next": [200410031]
        },
        "200410031": {
          "id": 200410031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "それもそうだな。やっぱり聞いてこよう。変な服を買ってしまうわけにはいかない",
          "next": [200410032]
        },
        "200410032": {
          "id": 200410032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "アドバイスありがとう。今から姉さんに聞きに行ってくる。それじゃ",
          "next": []
        }
