        "122350008": {
          "id": 122350008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "おお！すごいのう！よくできておるな！オレも欲しいぞ、オレの分はないのか？",
          "next": [
            122350009
          ]
        },
        "122350009": {
          "id": 122350009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "それは@{NICKNAME}に聞いてみないとね",
