        "122350005": {
          "id": 122350005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "{NICKNAME}、窓ガラスを破って中を見れたりしない？ウチらの人形とか入ってないかな～",
