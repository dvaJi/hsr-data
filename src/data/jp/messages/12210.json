        },
        "122100007": {
          "id": 122100007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 25,
          "text": "ちょっと無理がある気がするけど、もしこれが本当なら……",
          "next": [122100008]
        },
        "122100008": {
          "id": 122100008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "陰謀の匂いがする",
