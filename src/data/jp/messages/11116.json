      "messages": {
        "111160000": {
          "id": 111160000,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "姫子先生！聞きたいことがあるんだけど——",
          "next": [111160003]
        },
        "111160003": {
          "id": 111160003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "何でも聞いていいわよ、{NICKNAME}",
          "next": [111160004, 111160005, 111160006]
        },
        "111160004": {
          "id": 111160004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "星穹列車は新しい乗員を募集してる{M#か}？",
          "next": [111160007]
        },
        "111160005": {
          "id": 111160005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "その…列車に乗りたいって言ってる友達がいて…",
          "next": [111160008]
        },
        "111160006": {
          "id": 111160006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "姫子のコーヒーを飲みたがってる友達がいる{M#んだ}{F#の}！",
          "next": [111160009]
        },
        "111160007": {
          "id": 111160007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "新しい乗員？もちろん歓迎するわ。一緒に旅をしたいと思ってくれる人がいるのなら、列車のドアはその人のために開かれる",
          "next": [111160010]
        },
        "111160008": {
          "id": 111160008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "新しい乗員を紹介してくれるの？もちろん歓迎するわ。一緒に旅をしたいと思ってくれる人がいるのなら、列車のドアはその人のために開かれる",
          "next": [111160010]
        },
        "111160009": {
          "id": 111160009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "え？それは珍しいわね…歓迎しないわけがないわ",
          "next": [111160010]
        },
        "111160010": {
          "id": 111160010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "でも、まずはあんたの新しい友達について、もっと詳しく教えてちょうだい",
          "next": [111160011]
        },
        "111160011": {
          "id": 111160011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "あんたから見て、その友達はどんな人？",
          "next": [111160012, 111160013, 111160014]
        },
        "111160012": {
          "id": 111160012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "彼女は緻密な思考を持つ機械工{M#だ}{F#だよ}",
          "next": [111160015]
        },
        "111160013": {
          "id": 111160013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "彼女はさっぱりした性格のロックスター{M#だ}{F#だよ}",
          "next": [111160016]
        },
        "111160014": {
          "id": 111160014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "彼女は…なんて言えばいいかわからない",
          "next": [111160017]
        },
        "111160015": {
          "id": 111160015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "それはいいわね、私と話が合うかもしれないわ",
          "next": [111160018]
        },
        "111160016": {
          "id": 111160016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "それは若い人が好みそうな趣味ね、列車も賑やかになりそう",
          "next": [111160018]
        },
        "111160017": {
          "id": 111160017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "心配する必要はないわ。一緒に過ごしていれば、いろんなことが見えてくるはずだもの",
          "next": [111160018]
        },
        "111160018": {
          "id": 111160018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "とにかく、私はあんたの友達が列車に乗ることに賛成よ",
          "next": [111160019]
        },
        "111160019": {
          "id": 111160019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "焦らずに荷造りするように伝えてくれる？私たちの列車は、いつでも新しい乗員を歓迎するわ",
          "next": [111160020, 111160021]
        },
        "111160020": {
          "id": 111160020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "聞いてくれてありがとう、姫子",
          "next": [111160022]
        },
        "111160021": {
          "id": 111160021,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "わかった。ありがとう、姫子先生！",
          "next": [111160022]
        },
        "111160022": {
          "id": 111160022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "気にしないで。旅の途中で困ったことがあったら、いつでも私に言ってちょうだい",
          "next": []
        }
      },
      "participatingContactsIds": [8001, 1003]
    }
  ]
