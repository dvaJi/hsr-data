          ]
        },
        "130410038": {
          "id": 130410038,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "心配せずにまた来ればいい{F#よ}{M#}。次はどっちが騙す番になるだろう{F#ね}{M#な}",
          "next": [
            130410040
          ]
        },
