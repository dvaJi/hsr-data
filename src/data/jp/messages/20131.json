        "201310004": {
          "id": 201310004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "すまん、ペットが勝手にスマホを触ったんだ",
          "next": [
            201310005
          ]
        },
        "201310005": {
          "id": 201310005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1301,
          "text": "とん",
          "next": [
