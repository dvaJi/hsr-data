        },
        "201250039": {
          "id": 201250039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "？",
          "next": [
            201250040
          ]
        },
        "201250040": {
          "id": 201250040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
