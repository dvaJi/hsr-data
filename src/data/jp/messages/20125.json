        },
        "201250027": {
          "id": 201250027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "エンジンルームに隠されていたものは実は……",
          "next": [
            201250028
          ]
        },
        "201250028": {
          "id": 201250028,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "あなたの前任の車掌だということを！",
