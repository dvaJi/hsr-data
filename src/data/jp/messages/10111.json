            101110015
          ]
        },
        "101110013": {
          "id": 101110013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "リチャーたちのことはどうするつもり？",
          "next": [
            101110014
          ]
        },
        "101110014": {
          "id": 101110014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "…たぶん、貴様は知りたくないだろうな",
