        "130240004": {
          "id": 130240004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "さっき宇宙ステーションを通りかかった時にヘルタの広告を見たんだけど、なかなか{F#いいね}{M#いいな}",
          "next": [130240005]
        },
        "130240005": {
