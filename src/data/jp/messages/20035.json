      "messages": {
        "200350000": {
          "id": 200350000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "{NICKNAME}、こういう感覚ってある？",
          "next": [200350001]
        },
        "200350001": {
          "id": 200350001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "周りの人たちがあなたを怖がっていて、誰も本心を話してくれない",
          "next": [200350002, 200350004]
        },
        "200350002": {
          "id": 200350002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "国のリーダーになった経験はない",
          "next": [200350003]
        },
        "200350003": {
          "id": 200350003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "ごめんなさい、誰に聞くべきかわからなくって",
          "next": [200350006]
        },
        "200350004": {
          "id": 200350004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "集団に溶け込むんだ{F#よ}",
          "next": [200350005]
        },
        "200350005": {
          "id": 200350005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "理屈はわかってるけど、実際にやってみるとかなり難しくて",
          "next": [200350006]
        },
        "200350006": {
          "id": 200350006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "お母様が言ってたの。守護者の生活は孤独だって",
          "next": [200350007]
        },
        "200350007": {
          "id": 200350007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "やっぱり、時間をかけて慣れていくしかないみたい",
          "next": [200350008]
        },
        "200350008": {
          "id": 200350008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "ゼーレとも話したことがあるんだけど、彼女は困惑した顔で、そういう人たちの考えが理解できないって言ってた…",
          "next": [200350009, 200350013]
        },
        "200350009": {
          "id": 200350009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "もしかしたら",
          "next": [200350010]
        },
        "200350010": {
          "id": 200350010,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "ゼーレは{F#あんた}{M#お前}が怖くないからだと思う",
          "next": [200350011]
        },
        "200350011": {
          "id": 200350011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "そういうこと…",
          "next": [200350012]
        },
        "200350012": {
          "id": 200350012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "よく考えてみれば、ゼーレは誰が相手でも恐れたりしない",
          "next": [200350017]
        },
        "200350013": {
          "id": 200350013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "よく{F#あんた}{M#お前}にそんなことが言える{F#ね}{M#な}！",
          "next": [200350014]
        },
        "200350014": {
          "id": 200350014,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "{F#あんた}{M#お前}は大守護者{F#でしょ}{M#だろ}？",
          "next": [200350015]
        },
        "200350015": {
          "id": 200350015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "つまり、ゼーレは私が怖くないってこと？",
          "next": [200350016]
        },
        "200350016": {
          "id": 200350016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "よく考えると、確かにそうだと思う",
          "next": [200350017]
        },
        "200350017": {
          "id": 200350017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "やっぱり、私は自分の道を歩けるみたい…",
          "next": [200350018]
        },
        "200350018": {
          "id": 200350018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "頑張らなければ！",
          "next": []
        }
      },
