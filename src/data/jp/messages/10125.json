        },
        "101250007": {
          "id": 101250007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "{NICKNAME}、{F#貴女}{M#貴方}に伝えるべきかどうか、少し迷っていたことがありまして…",
          "next": [
            101250008
          ]
        },
        "101250008": {
          "id": 101250008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "{F#貴女}{M#貴方}たちが気づいているかわかりませんが",
          "next": [
            101250009
          ]
        },
        "101250009": {
          "id": 101250009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "列車の中には、まだ4匹目の虫がいます",
          "next": [
            101250010
          ]
        },
        "101250010": {
          "id": 101250010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "全然気づかなかった！",
          "next": [
            101250011
