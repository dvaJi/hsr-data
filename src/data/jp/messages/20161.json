        },
        "201610019": {
          "id": 201610019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1222,
          "text": "いかに迅速に汎用的な傷薬を作るかは、たしかに重要な問題ですね",
          "next": [
            201610022
          ]
        },
        "201610020": {
          "id": 201610020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "困難なんてない{F#かな}{M#な}",
