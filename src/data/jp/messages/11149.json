        "111490013": {
          "id": 111490013,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "今日はここまでにしよう。悪いけど他に用がある{F#から}{M#んだ}",
          "next": [
            111490014
          ]
        },
        "111490014": {
          "id": 111490014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "！！！そんなに急いでブロックしようとしないでください！話を聞けば興味が湧くかもしれないじゃないですか",
          "next": [
            111490018
          ]
        },
        "111490015": {
          "id": 111490015,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "「困惑」",
          "next": [
            111490016
