  "relatedMessages": [12180, 12202, 12221, 20097, 20098, 20099],
  "sections": [
    {
      "id": 2009800,
      "startingMessageId": [200980000],
      "messages": {
        "200980000": {
          "id": 200980000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "ところで、仙舟の暮らしには慣れた？何か困ってることはない？",
          "next": [200980001, 200980003, 200980005]
        },
        "200980001": {
          "id": 200980001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "大丈夫だよ。特に困っていることはないかな",
          "next": [200980002]
        },
        "200980002": {
          "id": 200980002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "それならよかった。そう言ってもらえて安心したよ",
          "next": [200980008]
        },
        "200980003": {
          "id": 200980003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "実は少し困ってることがある",
          "next": [200980004]
        },
        "200980004": {
          "id": 200980004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "うーん、よく分かるよ。あたしも最初はそうだった",
          "next": [200980008]
        },
        "200980005": {
          "id": 200980005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "気にしていない{F#よ}{M#}。ここに長くいるつもりはないから",
          "next": [200980006]
        },
        "200980006": {
          "id": 200980006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "そういう意味じゃなくて…",
          "next": [200980007]
        },
        "200980007": {
          "id": 200980007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "長く住むつもりはなくても、あんたたちナナシビトはよく仙舟に来るでしょ？",
          "next": [200980008]
        },
        "200980008": {
          "id": 200980008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "大したことじゃないの。急に思い出しただけ",
          "next": [200980009]
        },
        "200980009": {
          "id": 200980009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "あたしが初めて仙舟に来たとき、まだ子どもだったんだ",
          "next": [200980010]
        },
        "200980010": {
          "id": 200980010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "周りは知らない景色ばかりだし、まったく知らない世間や文化ばかりだった",
          "next": [200980011]
        },
        "200980011": {
          "id": 200980011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "あの頃の恐怖や不安を思い出して、ふとあんたのことが気になったの",
          "next": [200980012]
        },
        "200980012": {
          "id": 200980012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "あんたも同じような気持ちになってるんじゃないかと思って",
          "next": [200980013, 200980015]
        },
        "200980013": {
          "id": 200980013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "心配してくれてありがとう、けいちゃん。でも、もう慣れた{F#よ}{M#}",
          "next": [200980014]
        },
        "200980014": {
          "id": 200980014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "うん、そうみたいだね。あんたはあたしよりも大変な目に遭ってるし",
          "next": [200980017]
        },
        "200980015": {
          "id": 200980015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "子どもの頃のけいちゃんは大変だったんだね。聞いてると胸が痛む{F#よ}{M#}",
          "next": [200980016]
        },
        "200980016": {
          "id": 200980016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "あたしのことはいいから！自分のことを心配して！忙しくても体には気を付けてね",
          "next": [200980017]
        },
        "200980017": {
          "id": 200980017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "それから",
          "next": [200980018]
        },
        "200980018": {
          "id": 200980018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "何か困ったことがあれば、あたしに言ってね～",
          "next": [200980019]
        },
        "200980019": {
          "id": 200980019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "あたしに解決できなくても、雲騎軍や十王司、それにみんながいるでしょ",
          "next": [200980020]
        },
        "200980020": {
          "id": 200980020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "エヘヘ",
          "next": []
        }
      },
      "participatingContactsIds": [1210, 8001]
    }
  ]
}
