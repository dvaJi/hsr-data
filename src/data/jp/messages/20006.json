  "sections": [
    {
      "id": 2000600,
      "startingMessageId": [200060000],
      "messages": {
        "200060000": {
          "id": 200060000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "アーカイブで昔の列車の通信ログを見つけたんだが、その中に「寒いギャグ」というカテゴリーがあった",
          "next": [200060001]
        },
        "200060001": {
          "id": 200060001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "俺には理解できないが、直感的に{NICKNAME}なら理解できると思ったんだ",
          "next": [200060002, 200060004]
        },
        "200060002": {
          "id": 200060002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "{M#俺}{F#私}って、丹恒の目にはそんなふうに映ってる{M#のか}{F#の}？",
          "next": [200060003]
        },
        "200060003": {
          "id": 200060003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "ああ",
          "next": [200060006]
        },
        "200060004": {
          "id": 200060004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "{M#俺}{F#私}にも理解できない",
          "next": [200060005]
        },
        "200060005": {
          "id": 200060005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "まだログを見ていないだろう……",
          "next": [200060006]
        },
        "200060006": {
          "id": 200060006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "ログの内容はこうだ",
          "next": [200060007]
        },
        "200060007": {
          "id": 200060007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "「姫子と交代で見張りをすることになったが、ずっと寝言を言っておった」――パム",
          "next": [200060008]
        },
        "200060008": {
          "id": 200060008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "もう1つログが残されている。入力されたのは、このログの1時間後だ",
          "next": [200060009]
        },
        "200060009": {
          "id": 200060009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "「パムと交代で見張りをしているけど、もう1時間も寝言を言っているわ」――姫子",
          "next": [200060010]
        },
        "200060010": {
          "id": 200060010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "{NICKNAME}、どう思う？",
          "next": [200060011, 200060013]
        },
        "200060011": {
          "id": 200060011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "……",
          "next": [200060012]
        },
        "200060012": {
          "id": 200060012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "なるほど、今のカテゴリーのままでよさそうだ",
          "next": [200060015]
        },
        "200060013": {
          "id": 200060013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "ギャグじゃなくてホラーじゃん……",
          "next": [200060014]
        },
        "200060014": {
          "id": 200060014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "そうなのか？俺には理解できなかったが…別のカテゴリーに分けたほうがいいかもしれないな",
          "next": [200060015]
        },
        "200060015": {
          "id": 200060015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "ログはここで終わっている。姫子さんとパムにも確認してみたが、2人とも何も覚えていないと言っていた",
          "next": [200060016]
        },
        "200060016": {
          "id": 200060016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "「寝言を言っていたのは、いったい誰なのか」――アーカイブの他の項目の中から証拠が見つかることを祈るしかなさそうだ",
          "next": [200060018]
        },
        "200060017": {
          "id": 200060017,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "「いいね」",
          "next": []
        },
        "200060018": {
          "id": 200060018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "アーカイブは万能だと思いたい",
          "next": [200060017]
        }
      },
      "participatingContactsIds": [1002, 8001]
    }
  ]
}
