        "121230001": {
          "id": 121230001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "なんで{F#あんた}{M#お前}が二百年経っても一人前になれないのか、分かった{F#よ}{M#}",
          "next": [
            121230002
          ]
        },
        "121230002": {
          "id": 121230002,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "見損なった{F#よ}、{F#あんた}{M#お前}みたいなヤツのために時間を無駄にしたくない",
          "next": [
            121230003
          ]
        },
        "121230003": {
          "id": 121230003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 54,
          "text": "…すまん、{NICKNAME}、恥ずかしいところを見せたな",
