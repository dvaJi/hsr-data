        },
        "200670002": {
          "id": 200670002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "叶ったと思う。今を気に入ってる{F#よ}{M#}",
          "next": [200670005]
        },
        "200670003": {
          "id": 200670003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "叶ってない。でも、今のままでも悪くない{F#かな}{M#}",
          "next": [200670005]
        },
        "200670004": {
          "id": 200670004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "よく分からない{F#かも}{M#}。昔の夢なんてもう覚えていないから",
