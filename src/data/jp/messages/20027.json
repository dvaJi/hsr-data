        },
        "200270018": {
          "id": 200270018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "すまない、お嬢様に俺のスマホを貸していたんだ",
          "next": [200270019]
        },
        "200270019": {
          "id": 200270019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
