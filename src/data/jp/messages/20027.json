        },
        "200270011": {
          "id": 200270011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "新しいランニングシューズを買おうと思ってるんだ。だから貴様の意見を聞いた",
          "next": [200270012]
        },
        "200270012": {
          "id": 200270012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "届いたら見せよう",
