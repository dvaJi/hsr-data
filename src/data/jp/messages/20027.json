        },
        "200270007": {
          "id": 200270007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "貴様がいいと言うなら、今度買って来てみよう",
          "next": [200270011]
        },
        "200270008": {
          "id": 200270008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "自撮りを送って。様子が変{M#だ}{F#だよ}",
