      "messages": {
        "111170000": {
          "id": 111170000,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "ヨウおじちゃん、ヨウおじちゃん——",
          "next": [111170003]
        },
        "111170003": {
          "id": 111170003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "ここにいるぞ。どうしたんだ、{NICKNAME}？",
          "next": [111170004, 111170005]
        },
        "111170004": {
          "id": 111170004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "その、星穹列車は新しい乗員を募集してる{M#か}？",
          "next": [111170006]
        },
        "111170005": {
          "id": 111170005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "列車に乗りたがってる友達がいるんだけど、ヴェルトはどう思う？",
          "next": [111170007]
        },
        "111170006": {
          "id": 111170006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "なるほど。もちろん、俺は歓迎するぞ",
          "next": [111170008]
        },
        "111170007": {
          "id": 111170007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "なるほど、ここで友人ができたのはいいことだ。俺は歓迎する",
          "next": [111170008]
        },
        "111170008": {
          "id": 111170008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "だが、1つ質問させてくれ",
          "next": [111170009]
        },
        "111170009": {
          "id": 111170009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "{NICKNAME}の友人は、どのような人物なんだ？",
          "next": [111170010, 111170011, 111170012]
        },
        "111170010": {
          "id": 111170010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "彼女は緻密な思考を持つ機械工{M#だ}{F#だよ}",
          "next": [111170013]
        },
        "111170011": {
          "id": 111170011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "彼女はさっぱりした性格のロックスター{M#だ}{F#だよ}",
          "next": [111170014]
        },
        "111170012": {
          "id": 111170012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "彼女は…なんて言えばいいかわからない",
          "next": [111170015]
        },
        "111170013": {
          "id": 111170013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "それなら姫子と話が合いそうだな",
          "next": [111170016]
        },
        "111170014": {
          "id": 111170014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "ほう？それはいい趣味だ、俺も興味がある",
          "next": [111170016]
        },
        "111170015": {
          "id": 111170015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "気にすることはない。俺は君の人を見る目を信用している、きっと見誤ることもないだろう",
          "next": [111170016]
        },
        "111170016": {
          "id": 111170016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "とにかく、俺は君の友人を歓迎する",
          "next": [111170017]
        },
        "111170017": {
          "id": 111170017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "ただ、この件に関しては俺に決定権があるわけではないんだ",
          "next": [111170018]
        },
        "111170018": {
          "id": 111170018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "経験上、姫子が異論を唱えることはないと思うが…俺からも口添えしておこう",
          "next": [111170019, 111170020]
        },
        "111170019": {
          "id": 111170019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "ありがとう、ヴェルト",
          "next": [111170021]
        },
        "111170020": {
          "id": 111170020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "わかった。ヨウおじちゃん、大好き！",
          "next": [111170021]
        },
        "111170021": {
          "id": 111170021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "どういたしまして",
          "next": []
        }
      },
