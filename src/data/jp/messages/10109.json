      ],
      "messages": {
        "101090000": {
          "id": 101090000,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "「名乗るほどの者ではない」が「必殺技を顔面で受ける」をグループに招待し、最近10件のトーク履歴を送信しました",
          "next": [
            101090001
          ]
        },
        "101090001": {
          "id": 101090001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 16,
          "text": "前々から彼女には問題があると思っていた",
          "next": [
            101090002
          ]
        },
        "101090002": {
          "id": 101090002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "くわしく",
          "next": [
            101090003
          ]
        },
        "101090003": {
          "id": 101090003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "くわしく",
          "next": [
            101090004
          ]
        },
        "101090004": {
          "id": 101090004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "こんなに大きな宇宙ステーションだぞ。うまい汁がどれだけあると思う？考えてみろよ",
          "next": [
            101090005
          ]
        },
        "101090005": {
          "id": 101090005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "むやみに変なことを言ってはダメだよ。彼女はミス・ヘルタの教え子なんだから",
          "next": [
            101090006
          ]
        },
        "101090006": {
          "id": 101090006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "教え子だから何？教え子だったら何も言っちゃいけないの？",
          "next": [
            101090007
          ]
        },
        "101090007": {
          "id": 101090007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "彼女か……",
          "next": [
            101090008
          ]
        },
        "101090008": {
          "id": 101090008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "彼女には黒い情報が多いんだ",
          "next": [
            101090009
          ]
        },
        "101090009": {
          "id": 101090009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "くわしく",
          "next": [
            101090010
          ]
        },
        "101090010": {
          "id": 101090010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "それはできない",
          "next": [
            101090011
          ]
        },
        "101090011": {
          "id": 101090011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "だったら最初から言うなよ！！",
          "next": [
            101090012,
            101090014,
            101090018
          ]
        },
        "101090012": {
          "id": 101090012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "これ、何のグループ？",
          "next": [
            101090013
          ]
        },
        "101090013": {
          "id": 101090013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "@必殺技を顔面で受ける 自分でグループ名を見てみて",
          "next": [
            101090020
          ]
        },
        "101090014": {
          "id": 101090014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "誰のことを言ってる{M#んだ}{F#の}？",
          "next": [
            101090015
          ]
        },
        "101090015": {
          "id": 101090015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "新人はルールを分かってないから困る",
          "next": [
            101090016
          ]
        },
        "101090016": {
          "id": 101090016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "アスター",
          "next": [
            101090017
          ]
        },
        "101090017": {
          "id": 101090017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "アスターの小娘め",
          "next": [
            101090020
          ]
        },
        "101090018": {
          "id": 101090018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "だれかあらすじを教えて――",
          "next": [
            101090019
          ]
        },
        "101090019": {
          "id": 101090019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "自分で上の履歴を読んでくれればいい",
          "next": [
            101090020
          ]
        },
        "101090020": {
          "id": 101090020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "話の腰を折らないで。@お喋り茶 早く教えて",
          "next": [
            101090021
          ]
        },
        "101090021": {
          "id": 101090021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "分かった、分かった",
          "next": [
            101090022
          ]
        },
        "101090022": {
          "id": 101090022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 16,
          "text": "匿名にしてるのに途中までしか言わないなんて、何考えてんだ",
          "next": [
            101090023
          ]
        },
        "101090023": {
          "id": 101090023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "君たちだって知らないわけではないだろうに……",
          "next": [
            101090024
          ]
        },
        "101090024": {
          "id": 101090024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "余計な話はいいから早く！",
          "next": [
            101090025
          ]
        },
        "101090025": {
          "id": 101090025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "公金横領のことだ",
          "next": [
            101090026
          ]
        },
        "101090026": {
          "id": 101090026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "接待ホールに山のように積まれている荷物を見たか？どれも受取人は若き所長様だ",
          "next": [
            101090027
          ]
        },
        "101090027": {
          "id": 101090027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "やってくれるな。あんなケチな予算で金がどこに使われているのかと思っていたら",
          "next": [
            101090028
          ]
        },
        "101090028": {
          "id": 101090028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "そんなこと言うもんじゃないよ",
          "next": [
            101090029
          ]
        },
        "101090029": {
          "id": 101090029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "それだけじゃ証拠にならないじゃないか。確かめないと",
          "next": [
            101090030
          ]
        },
        "101090030": {
          "id": 101090030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "確かめる？接待ホールに山のように積まれている荷物を見たか？どれも受取人は若き所長様だ",
          "next": [
            101090031,
            101090033
          ]
        },
        "101090031": {
          "id": 101090031,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "荷物の中身は？",
          "next": [
            101090032
          ]
        },
        "101090032": {
          "id": 101090032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "チラッと見ただけだが、どれも高級品ばかりだった",
          "next": [
            101090035
          ]
        },
        "101090033": {
          "id": 101090033,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "個人の名義で買ったの？",
          "next": [
            101090034
          ]
        },
        "101090034": {
          "id": 101090034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "個人の名義に決まってる！宇宙ステーションにすると帳簿に書かれてしまう。所長がそんなマヌケなはずないだろ",
          "next": [
            101090035
          ]
        },
        "101090035": {
          "id": 101090035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "自分のお金で買ったのかもしれないよ",
          "next": [
            101090036
          ]
        },
        "101090036": {
          "id": 101090036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "あんなのヘルタ様以外に誰が買えるんだ",
          "next": [
            101090037
          ]
        },
        "101090037": {
          "id": 101090037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "防衛課のアーランが受付でコソコソしているのを見かけたんだ。あいつがビクビクしていなかったら気づかなかっただろう",
          "next": [
            101090038
          ]
        },
        "101090038": {
          "id": 101090038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "へー、そう言えば…アーランが緊張しながら何かの荷物を受け取っているところを見たことがあるような…",
          "next": [
            101090039
          ]
        },
        "101090039": {
          "id": 101090039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "ほら見ろ",
          "next": [
            101090040
          ]
        },
        "101090040": {
          "id": 101090040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "話が噛み合った",
          "next": [
            101090041
          ]
        },
        "101090041": {
          "id": 101090041,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "話が噛み合った",
          "next": [
            101090042,
            101090044,
            101090047
          ]
        },
        "101090042": {
          "id": 101090042,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "つまり、アスターとアーランがグルになって私腹を肥やしてるってこと？",
          "next": [
            101090043
          ]
        },
        "101090043": {
          "id": 101090043,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "知るかよ。@お喋り茶 が言ったんだ",
          "next": [
            101090051
          ]
        },
        "101090044": {
          "id": 101090044,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "あくまでも一方的な思い込みで、確証はないような気がするけど……",
          "next": [
            101090045
          ]
        },
        "101090045": {
          "id": 101090045,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "信じるか信じないかは勝手だ",
          "next": [
            101090046
          ]
        },
        "101090046": {
          "id": 101090046,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "バカ言え｡他にもあるぞ｡アーランがあの女の手先なのは明らかだろう？",
          "next": [
            101090051
          ]
        },
        "101090047": {
          "id": 101090047,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "そんな驚きの情報…タダで聞いていいの？",
          "next": [
            101090048
          ]
        },
        "101090048": {
          "id": 101090048,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "言っておくけど､ここに書かれている内容は僕とは関係ないからね！",
          "next": [
            101090049
          ]
        },
        "101090049": {
          "id": 101090049,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "言っておくけど､ここに書かれている内容は僕とは関係ないからな！",
          "next": [
            101090050
          ]
        },
        "101090050": {
          "id": 101090050,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "僕は何があろうとミス・ヘルタに言われた通りにするよ！",
          "next": [
            101090051
          ]
        },
        "101090051": {
          "id": 101090051,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 16,
          "text": "…ったく、大きな事件に繋がるのかと思っていたのに、これで終わりなの？",
          "next": [
            101090052
          ]
        },
        "101090052": {
          "id": 101090052,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "そうだよ。結局のところ確証はないでしょ",
          "next": [
            101090053
          ]
        },
        "101090053": {
          "id": 101090053,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "@マーシーロボット 自分だってアーランがコソコソしているって言ったじゃないか",
          "next": [
            101090054
          ]
        },
        "101090054": {
          "id": 101090054,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "ちょっと言っただけだよ",
          "next": [
            101090055
          ]
        },
        "101090055": {
          "id": 101090055,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "実は、彼女の莫大な資産の出所を密かに調べていたんだ。ただ、まだ公表する時期ではない",
          "next": [
            101090056
          ]
        },
        "101090056": {
          "id": 101090056,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "待っているがいい。数日中に宇宙ステーションに激震が走るだろう！",
          "next": [
            101090057
          ]
        },
        "101090057": {
          "id": 101090057,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 16,
          "text": "そうだといいけど",
          "next": []
        }
      },
