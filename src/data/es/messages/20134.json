        },
        "201340022": {
          "id": 201340022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Que me salga un número más alto!",
          "next": [
            201340023
          ]
        },
        "201340023": {
          "id": 201340023,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "{NICKNAME} lanza un dado y saca un 4.",
          "next": [
            201340024
          ]
        },
        "201340024": {
          "id": 201340024,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Saqué un número más alto. Habla.",
          "next": [
            201340025
          ]
        },
        "201340025": {
          "id": 201340025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Tras luchar contra la Emanadora, me adentré en un vasto río que nunca había visto en mi vida.",
          "next": [
            201340026
          ]
        },
        "201340026": {
          "id": 201340026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Creí que había llegado al final de mi vida.",
          "next": [
            201340027
          ]
        },
        "201340027": {
          "id": 201340027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Pero... como puedes ver, me alegro de seguir con vida.",
          "next": [
            201340028
          ]
        },
        "201340028": {
          "id": 201340028,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Aventurino lanza un dado y saca un 1.",
          "next": [
            201340029
          ]
        },
        "201340029": {
          "id": 201340029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Te toca.",
          "next": [
