      "messages": {
        "111330000": {
          "id": 111330000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "¿Cómo has estado, {NICKNAME}? ¿Sigues en Belobog?",
          "next": [111330001, 111330003]
        },
        "111330001": {
          "id": 111330001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Voy a quedarme un poco más.",
          "next": [111330002]
        },
        "111330002": {
          "id": 111330002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Oh, qué bien, hay algo con lo que me gustaría que me ayudaras.",
          "next": [111330008]
        },
        "111330003": {
          "id": 111330003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Me voy pronto.",
          "next": [111330004]
        },
        "111330004": {
          "id": 111330004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Ah, ¿sí? No te molestaré entonces.",
          "next": [111330005, 111330006]
        },
        "111330005": {
          "id": 111330005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "No pasa nada por preguntar.",
          "next": [111330007]
        },
        "111330006": {
          "id": 111330006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "No puedes dejarme en suspenso...",
          "next": [111330007]
        },
        "111330007": {
          "id": 111330007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Seré franco, hay algo con lo que me gustaría que me ayudaras.",
          "next": [111330008]
        },
        "111330008": {
          "id": 111330008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Llevo poco tiempo en el Supramundo y, antes de que pudiera acostumbrarme, la Guardia Crinargenta ya estaba pidiéndome que me convierta en su asesor...",
          "next": [111330009]
        },
        "111330009": {
          "id": 111330009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Acaban de terminar con eso del Estelaron, y esta gente que no puede estarse quieta ya está pensando en una contraofensiva en el Fragmentum.",
          "next": [111330010]
        },
        "111330010": {
          "id": 111330010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Pero si hablamos del Fragmentum, creo que ustedes son los únicos que pueden ser considerados expertos.",
          "next": [111330011, 111330013]
        },
        "111330011": {
          "id": 111330011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Otra pelea?",
          "next": [111330012]
        },
        "111330012": {
          "id": 111330012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Ja, ja, algo así.",
          "next": [111330015]
        },
        "111330013": {
          "id": 111330013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "El Fragmentum es mi hogar.",
          "next": [111330014]
        },
        "111330014": {
          "id": 111330014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Ja, ja, me gusta tu espíritu.",
          "next": [111330015]
        },
        "111330015": {
          "id": 111330015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "Si planeas unirte a la ofensiva, ve a buscar a un oficial de enlace llamado William en la zona restringida de la Guardia Crinargenta.",
          "next": [111330016]
        },
        "111330016": {
          "id": 111330016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 12,
          "text": "El Fragmentum que quieren despejar se llama Pasadizo de los ecos apagados. Un nombre bastante escalofriante, ¿no? Pero supongo que será pan comido para ti...",
          "next": []
        }
      },
      "participatingContactsIds": [12, 8001]
    }
  ]
}
