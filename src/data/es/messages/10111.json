  "relatedMessages": [10100, 10107, 10111, 10119, 20014, 20027, 20028],
  "sections": [
    {
      "id": 1011100,
      "startingMessageId": [101110000],
      "messages": {
        "101110000": {
          "id": 101110000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Agradezco su ayuda para descubrir al perpetrador. Permítanme darles las gracias en nombre de Asta. Déjenme el resto a mí.",
          "next": [101110001, 101110003]
        },
        "101110001": {
          "id": 101110001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Herta no castigó a Asta, ¿verdad?",
          "next": [101110002]
        },
        "101110002": {
          "id": 101110002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Realmente no conoces a la señora Herta, ¿verdad? Ella ni siquiera se preocupa por asuntos triviales como este.",
          "next": [101110005]
        },
        "101110003": {
          "id": 101110003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Es verdad que Asta compró todas esas cosas con su propio dinero?",
          "next": [101110004]
        },
        "101110004": {
          "id": 101110004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Realmente no lo sabes...",
          "next": [101110005]
        },
        "101110005": {
          "id": 101110005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Esta Estación puede ser propiedad de la señora Herta, pero ella no desembolsó ni un solo crédito para su construcción.",
          "next": [101110006]
        },
        "101110006": {
          "id": 101110006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Asta tiene conexiones con la Corporación, así que decir que es rica se quedaría corto. La mayoría de las instalaciones que hay en la Estación Espacial las pagó ella...",
          "next": [101110007]
        },
        "101110007": {
          "id": 101110007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Así que comprar todas esas cosas no fue prácticamente nada para ella.",
          "next": [101110008, 101110010, 101110013]
        },
        "101110008": {
          "id": 101110008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Qué es exactamente la Corporación para la Paz Interastral?",
          "next": [101110009]
        },
        "101110009": {
          "id": 101110009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Humm... Para decirlo de manera sencilla, es la organización más rica de la galaxia.",
          "next": [101110015]
        },
        "101110010": {
          "id": 101110010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Crees que aún estoy a tiempo de hacerme amig{F#a}{M#o} de la Srta. Asta?",
          "next": [101110011]
        },
        "101110011": {
          "id": 101110011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "... Por favor, no bromees con eso, me lo tomaré en serio.",
          "next": [101110012]
        },
        "101110012": {
          "id": 101110012,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "...",
          "next": [101110015]
        },
        "101110013": {
          "id": 101110013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Han decidido qué hacer con Richard?",
          "next": [101110014]
        },
        "101110014": {
          "id": 101110014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "... No creo que quieras saber los detalles.",
          "next": [101110015]
        },
        "101110015": {
          "id": 101110015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "De todas formas, tengo que agradecerte que hayas ayudado a Asta con este incidente.",
          "next": []
        }
      },
      "participatingContactsIds": [1008, 8001]
    }
  ]
}
