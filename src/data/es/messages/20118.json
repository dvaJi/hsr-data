        "201180000": {
          "id": 201180000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "<u>Toca para registrarte en la Copa Rosetta-Rosenthal. Esperamos verte pronto.</u>",
          "next": [
            201180001
          ]
        },
        "201180001": {
          "id": 201180001,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Se ha enviado el adjunto \"Anuncio de la primera ronda de debates\".",
          "next": [
            201180002
          ]
        },
        "201180002": {
          "id": 201180002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "{NICKNAME}, te envío información sobre la competencia de debate que se llevó a cabo hace poco en la Sociedad. El tema es \"¿La ciencia y la tecnología son neutrales?\".",
          "next": [
            201180003
          ]
        },
        "201180003": {
          "id": 201180003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "No me malinterpretes. Solo te lo estoy reenviando.",
          "next": [
            201180004
          ]
        },
        "201180004": {
          "id": 201180004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "El tema es para principiantes.",
          "next": [
            201180005,
            201180008,
            201180010
          ]
        },
        "201180005": {
          "id": 201180005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Soy lo suficientemente elocuente.",
          "next": [
            201180006
          ]
        },
        "201180006": {
          "id": 201180006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "No lo creo.",
          "next": [
            201180007
          ]
        },
        "201180007": {
          "id": 201180007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "No hay límites en el arte del lenguaje",
          "next": [
            201180012
          ]
        },
        "201180008": {
          "id": 201180008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Pero no sé nada sobre debatir.",
          "next": [
            201180009
          ]
        },
        "201180009": {
          "id": 201180009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Con mayor razón, deberías aprender.",
          "next": [
            201180012
          ]
        },
        "201180010": {
          "id": 201180010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Perdón, soy antisocial.",
          "next": [
            201180011
          ]
        },
        "201180011": {
          "id": 201180011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "No te conoces lo suficiente.",
          "next": [
            201180012
          ]
        },
        "201180012": {
          "id": 201180012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "No importa si te unes a la competencia o no.",
          "next": [
            201180013
          ]
        },
        "201180013": {
          "id": 201180013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Pero quisiera saber qué tipo de actividad es un debate para ti.",
          "next": [
            201180014,
            201180015,
            201180016
          ]
        },
        "201180014": {
          "id": 201180014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "El debate es argumentar con tu oponente.",
          "next": [
            201180017
          ]
        },
        "201180015": {
          "id": 201180015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "El debate es hacer enojar a tu oponente.",
          "next": [
            201180017
          ]
        },
        "201180016": {
          "id": 201180016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "El debate es usar sarcasmo con tu oponente.",
          "next": [
            201180017
          ]
        },
        "201180017": {
          "id": 201180017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Déjame explicártelo usando el tema de este debate como ejemplo.",
          "next": [
            201180018
          ]
        },
        "201180018": {
          "id": 201180018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "¿Qué lado eliges?",
          "next": [
            201180019,
            201180025
          ]
        },
        "201180019": {
          "id": 201180019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "La tecnología es neutral.",
          "next": [
            201180020
          ]
        },
        "201180020": {
          "id": 201180020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Entonces tomaré el lado opuesto. Tú hablas primero.",
          "next": [
            201180021
          ]
        },
        "201180021": {
          "id": 201180021,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "La tecnología es una espada de doble filo, ya que se puede usar para el bien y el mal.",
          "next": [
            201180022
          ]
        },
        "201180022": {
          "id": 201180022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "¿Has escuchado hablar del experimento del Dr. Primitivo?",
          "next": [
            201180023
          ]
        },
        "201180023": {
          "id": 201180023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Para inducir una respuesta de estrés en sus sujetos, desarrolló una serie de tecnologías para torturarlos y causarles dolor.",
          "next": [
            201180024
          ]
        },
        "201180024": {
          "id": 201180024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Esas tecnologías son abominables.",
          "next": [
            201180030
          ]
        },
        "201180025": {
          "id": 201180025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "La tecnología no es neutral.",
          "next": [
            201180026
          ]
        },
        "201180026": {
          "id": 201180026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Tomaré entonces el lado opuesto. Tú hablas primero.",
          "next": [
            201180027
          ]
        },
        "201180027": {
          "id": 201180027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Algunas tecnologías son inherentemente buenas o malas.",
          "next": [
            201180028
          ]
        },
        "201180028": {
          "id": 201180028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Las tecnologías médicas avanzadas han salvado muchas vidas, pero los tratamientos médicos no siempre extienden la vida de alguien.",
          "next": [
            201180029
          ]
        },
        "201180029": {
          "id": 201180029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "¿No han visitado la Alianza Xianzhou? Debes conocer el precio de la inmortalidad mejor que yo.",
          "next": [
            201180030
          ]
        },
        "201180030": {
          "id": 201180030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Ya terminé.",
          "next": [
            201180031
          ]
        },
        "201180031": {
          "id": 201180031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Mira, esto confirma mi punto.",
          "next": [
            201180032,
            201180035
          ]
        },
        "201180032": {
          "id": 201180032,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Aún tengo más qué decir!",
          "next": [
            201180033
          ]
        },
        "201180033": {
          "id": 201180033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Es muy común, la habilidad de hablar rápido es un signo de alguien que piensa rápido.",
          "next": [
            201180034
          ]
        },
        "201180034": {
          "id": 201180034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Pero también significa que las personas así nunca piensan antes de hablar.",
          "next": [
            201180038
          ]
        },
        "201180035": {
          "id": 201180035,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Dices tonterías.",
          "next": [
            201180036
          ]
        },
        "201180036": {
          "id": 201180036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Perderás si dices eso.",
          "next": [
            201180037
          ]
        },
        "201180037": {
          "id": 201180037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Pero tienes razón.",
          "next": [
            201180038
          ]
        },
        "201180038": {
          "id": 201180038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Así que la conclusión es evidente. Ninguno puede convencer al otro.",
          "next": [
            201180039
          ]
        },
        "201180039": {
          "id": 201180039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "¿Ya lo entiendes? La verdad a veces se vuelve más clara con el debate, y a veces no.",
          "next": [
            201180040
          ]
        },
        "201180040": {
          "id": 201180040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "La mayoría de debates solo son un juego de palabras.",
          "next": [
            201180041
          ]
        },
        "201180041": {
          "id": 201180041,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "En cuanto a los otros... Aunque no fue mi idea, me han invitado a participar en debates que van más allá del reino del lenguaje.",
          "next": [
            201180042
          ]
        },
        "201180042": {
          "id": 201180042,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Más allá del reino del lenguaje?",
          "next": [
            201180043
          ]
        },
        "201180043": {
          "id": 201180043,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Un cuerpo sano es vital en todo momento. Recuerda eso, {NICKNAME}.",
          "next": []
        }
      },
