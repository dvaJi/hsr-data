        "201490001": {
          "id": 201490001,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "<Grand Melodie, Momento Dorado>",
          "next": [
            201490002
          ]
        },
        "201490002": {
          "id": 201490002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Hace poco ayudé a una señora en Colonipenal. No sabía que era la chef de un restaurante de lujo. Este es su restaurante.",
          "next": [
            201490003
          ]
        },
        "201490003": {
          "id": 201490003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Pronto sacará un plato con temática de duraznos. Te lo recomiendo.",
          "next": [
            201490004,
            201490005,
            201490007
          ]
        },
        "201490004": {
          "id": 201490004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Iré de todas maneras!",
          "next": [
            201490009
          ]
        },
        "201490005": {
          "id": 201490005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Tú no vienes?",
          "next": [
            201490006
          ]
        },
        "201490006": {
          "id": 201490006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Yo ya lo he probado.",
          "next": [
            201490009
          ]
        },
        "201490007": {
          "id": 201490007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Eh... Parece un plato de \"cocina oscura\"...",
          "next": [
            201490008
          ]
        },
        "201490008": {
          "id": 201490008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "¿Oscura?... ¿Acaso también hay cocina brillante?",
          "next": [
            201490009
          ]
        },
        "201490009": {
          "id": 201490009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Cocinar... me trae recuerdos. Antes me encantaba cocinar.",
          "next": [
            201490010
          ]
        },
        "201490010": {
          "id": 201490010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Pero es difícil mantener una afición así cuando tus sentidos se van embotando.",
          "next": [
            201490011,
            201490012
          ]
        },
        "201490011": {
          "id": 201490011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Todavía cocinas?",
          "next": [
            201490014
          ]
        },
        "201490012": {
          "id": 201490012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿También se debe a la Nihilidad?",
          "next": [
            201490013
          ]
        },
        "201490013": {
          "id": 201490013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Quizás.",
          "next": [
            201490014
          ]
        },
        "201490014": {
          "id": 201490014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "A veces cocino mi propia comida. Es una pena que ya apenas pueda saborearla.",
          "next": [
            201490015
          ]
        },
        "201490015": {
          "id": 201490015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Pero también hay otras formas de compensar. ¿Sabes que puedes hacer helado con bolas de nieve?",
          "next": [
            201490016
          ]
        },
        "201490016": {
          "id": 201490016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Ese frescor, esa sensación de humedad y esa textura... Es un sabor que no olvidaré.",
          "next": [
            201490017
          ]
        },
        "201490017": {
          "id": 201490017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Te invitaré a probarla cuando haya ocasión.",
          "next": [
            201490018
          ]
        },
        "201490018": {
          "id": 201490018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "Y no puedes perderte el plato de durazno.",
          "next": [
            201490019
          ]
        },
