      "messages": {
        "121240000": {
          "id": 121240000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Disculpa, me dejaré de tonterías e iré directamente al grano.",
          "next": [121240001]
        },
        "121240001": {
          "id": 121240001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "¿El maestro Chengjie se ha puesto en contacto contigo recientemente?",
          "next": [121240002, 121240011, 121240012]
        },
        "121240002": {
          "id": 121240002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Quién es el maestro Chengjie? No lo recuerdo.",
          "next": [121240003]
        },
        "121240003": {
          "id": 121240003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Vaya, eres olvidadiz{F#a}{M#o}.",
          "next": [121240004]
        },
        "121240004": {
          "id": 121240004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Te daré unas cuantas pistas para que lo recuerdes.",
          "next": [121240005]
        },
        "121240005": {
          "id": 121240005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "El sueño del maestro.",
          "next": [121240006]
        },
        "121240006": {
          "id": 121240006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Es aprendiz en la Comisión de Artesanía.",
          "next": [121240007]
        },
        "121240007": {
          "id": 121240007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "El que abandonó después de tres días de lucha por su sueño.",
          "next": [121240008, 121240010]
        },
        "121240008": {
          "id": 121240008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Lo siento, no lo conozco.",
          "next": [121240009]
        },
        "121240009": {
          "id": 121240009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Parece que, o tu memoria dura solo siete segundos, o no han estado en contacto durante mucho tiempo, o la pereza de este tipo está perjudicando tu motivación...",
          "next": [121240013]
        },
        "121240010": {
          "id": 121240010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ahora lo recuerdo, pero hace tiempo que no hablamos.",
          "next": [121240013]
        },
        "121240011": {
          "id": 121240011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hace tiempo que no hablamos.",
          "next": [121240013]
        },
        "121240012": {
          "id": 121240012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Chengjie me dijo que esperara noticias suyas, y luego no volví a saber nada de él.",
          "next": [121240013]
        },
        "121240013": {
          "id": 121240013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "No nos veíamos mucho, pero como su compañera de juego, me preocupa su estado mental...",
          "next": [121240014]
        },
        "121240014": {
          "id": 121240014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "¿Podrá seguir jugando Jade celestial conmigo?",
          "next": [121240015]
        },
        "121240015": {
          "id": 121240015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Vamos a verlo. La Comisión de Artesanía te está esperando.",
          "next": []
        }
      },
      "participatingContactsIds": [1201, 8001]
    }
  ]
