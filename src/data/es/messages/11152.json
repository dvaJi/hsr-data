      "messages": {
        "111520000": {
          "id": 111520000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "He oído que regresaste a Belobog, {NICKNAME}.",
          "next": [111520001]
        },
        "111520001": {
          "id": 111520001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Escríbeme cuando tengas un minuto. Es urgente.",
          "next": [111520002]
        },
        "111520002": {
          "id": 111520002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Qué sucede?",
          "next": [111520003]
        },
        "111520003": {
          "id": 111520003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Un grupo de bribones llegó al Bajomundo y se apoderó a la fuerza de la mina principal.",
          "next": [111520004]
        },
        "111520004": {
          "id": 111520004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Le pregunté a Bronya qué hacer y me dijo que no me metiera con ellos por el momento.",
          "next": [111520005, 111520007]
        },
        "111520005": {
          "id": 111520005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bronya tiene razón. Probablemente sea mejor no meterse con ellos.",
          "next": [111520006]
        },
        "111520006": {
          "id": 111520006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Demasiado tarde. La Guardia Crinargenta y Llamarada ya se enfrentaron a ellos.",
          "next": [111520009]
        },
        "111520007": {
          "id": 111520007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡No podemos dejar que esos tipos intimiden a los mineros! ¡Debemos luchar!",
          "next": [111520008]
        },
        "111520008": {
          "id": 111520008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Sí, ya estamos en eso. La Guardia Crinargenta y Llamarada ya se enfrentaron a ellos.",
          "next": [111520009]
        },
        "111520009": {
          "id": 111520009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Pero esos idiotas tienen armas superavanzadas con gran potencial de daño. No podremos con ellos.",
          "next": [111520010]
        },
        "111520010": {
          "id": 111520010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1102,
          "text": "Te envío este SOS porque me estoy quedando sin ideas.",
          "next": [111520011]
        },
        "111520011": {
          "id": 111520011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aguanta. Estamos en camino.",
          "next": []
        }
      },
      "participatingContactsIds": [1102, 8001]
    }
  ]
