    {
      "id": 1302003,
      "startingMessageId": [130200301],
      "messages": {
        "130200301": {
          "id": 130200301,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Pequeña Gui, me he dado cuenta de que ya eres muy famosa.",
          "next": [130200302]
        },
        "130200302": {
          "id": 130200302,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "¿Cómo te diste cuenta?",
          "next": [130200303]
        },
        "130200303": {
          "id": 130200303,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿Recién lo notas?",
          "next": [130200304]
        },
        "130200304": {
          "id": 130200304,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "[Burla]",
          "next": [130200305]
        },
        "130200305": {
          "id": 130200305,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Hoy fui al Callejón Aurum y me encontré a todo el mundo hablando de ti.",
          "next": [130200306, 130200309, 130200311]
        },
        "130200306": {
          "id": 130200306,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Había oído que la Pequeña Gui era famosa, pero no lo había notado personalmente.",
          "next": [130200307]
        },
        "130200307": {
          "id": 130200307,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Parece que es famosa de verdad.",
          "next": [130200308]
        },
        "130200308": {
          "id": 130200308,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Te condeno a navegar por la red más a menudo.",
          "next": [130200314]
        },
        "130200309": {
          "id": 130200309,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sushang, ¿podría ser que oíste mal y en realidad estaban hablando de otra persona?",
          "next": [130200310]
        },
        "130200310": {
          "id": 130200310,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿Cómo puede ser? ¡Deben estar hablando de mí!",
          "next": [130200314]
        },
        "130200311": {
          "id": 130200311,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Habrán estado hablando de otra celebridad?",
          "next": [130200312]
        },
        "130200312": {
          "id": 130200312,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿Qué celebridad? Estaban diciendo mi nombre, Guinaifen.",
          "next": [130200313]
        },
        "130200313": {
          "id": 130200313,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Si seguimos hablando de esto, me lo voy a creer, ja, ja, ja.",
          "next": [130200314]
        },
        "130200314": {
          "id": 130200314,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "¿De qué están hablando?",
          "next": [130200315]
        },
        "130200315": {
          "id": 130200315,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Dicen que la Pequeña Gui anunció que va a dejar de hacer directos y están intentando adivinar por qué.",
          "next": [130200316]
        },
        "130200316": {
          "id": 130200316,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "¡¿Qué?!",
          "next": [130200317]
        },
        "130200317": {
          "id": 130200317,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¡¿Qué?!",
          "next": [130200318]
        },
        "130200318": {
          "id": 130200318,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "¡¿Qué?!",
          "next": [130200319, 130200321, 130200323]
        },
        "130200319": {
          "id": 130200319,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡¿Qué?!",
          "next": [130200320]
        },
        "130200320": {
          "id": 130200320,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Así que ustedes tampoco lo sabían.",
          "next": [130200325]
        },
        "130200321": {
          "id": 130200321,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sushang, ¿por qué dices \"¡¿Qué?!\"? ¡Si nos los estás contando tú!",
          "next": [130200322]
        },
        "130200322": {
          "id": 130200322,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Me sorprende que no lo supieran.",
          "next": [130200325]
        },
        "130200323": {
          "id": 130200323,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Me encantó el \"¡¿Qué?!\".",
          "next": [130200324]
        },
        "130200324": {
          "id": 130200324,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Así que ustedes tampoco lo sabían.",
          "next": [130200325]
        },
        "130200325": {
          "id": 130200325,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Casi haces que mi cerebro haga cortocircuito.",
          "next": [130200326]
        },
        "130200326": {
          "id": 130200326,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Dices que todo el mundo dice que voy a dejarlo, ¿verdad?",
          "next": [130200327]
        },
        "130200327": {
          "id": 130200327,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Sí, eso es.",
          "next": [130200328]
        },
        "130200328": {
          "id": 130200328,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "¿Entonces es verdad?",
          "next": [130200329]
        },
        "130200329": {
          "id": 130200329,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿Cómo va a ser verdad? Es una noticia falsa.",
          "next": [130200330]
        },
        "130200330": {
          "id": 130200330,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Cielos, ustedes dos ni siquiera vieron mi directo. ¿Tú lo viste, {NICKNAME}?",
          "next": [130200331, 130200333, 130200335]
        },
        "130200331": {
          "id": 130200331,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Lo siento, no suelo ver directos.",
          "next": [130200332]
        },
        "130200332": {
          "id": 130200332,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿Cómo puede ser posible?",
          "next": [130200337]
        },
        "130200333": {
          "id": 130200333,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Yo vi el directo, y eso fue lo que dijiste.",
          "next": [130200334]
        },
        "130200334": {
          "id": 130200334,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿Qué clase de directo falso viste?",
          "next": [130200337]
        },
        "130200335": {
          "id": 130200335,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Yo vi el directo, y no dijiste eso.",
          "next": [130200336]
        },
        "130200336": {
          "id": 130200336,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Tú sí eres {F#una buena}{M#un buen} amig{F#a}{M#o}.",
          "next": [130200337]
        },
        "130200337": {
          "id": 130200337,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ahora que lo pienso, esto sería un buen contenido...",
          "next": [130200338]
        },
        "130200338": {
          "id": 130200338,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Hum, voy a hacer un directo esta noche para desmentir los rumores.",
          "next": [130200339]
        },
        "130200339": {
          "id": 130200339,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "[En directo]",
          "next": []
        }
      },
      "participatingContactsIds": [1206, 1217, 1210, 8001]
    },
    {
      "id": 1302004,
      "startingMessageId": [130200401],
      "messages": {
        "130200401": {
          "id": 130200401,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Hace un rato estaba de compras con Huohuo y nos encontramos con unas conocidas suyas.",
          "next": [130200402]
        },
        "130200402": {
          "id": 130200402,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Huohuo estaba tartamudeando.",
          "next": [130200403]
        },
        "130200403": {
          "id": 130200403,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Las otras tampoco sabían qué decir.",
          "next": [130200404]
        },
        "130200404": {
          "id": 130200404,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Vi que la situación era un poco incómoda, así que intenté entablar una conversación...",
          "next": [130200405]
        },
        "130200405": {
          "id": 130200405,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Y resulta que eran muy simpáticas.",
