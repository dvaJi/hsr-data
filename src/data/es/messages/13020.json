  "relatedMessages": [12182, 12195, 12201, 12206, 12207, 12208, 13020],
  "sections": [
    {
      "id": 1302000,
      "startingMessageId": [130200000],
      "messages": {
        "130200000": {
          "id": 130200000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ejem, ejem, hoy no tengo información para compartir.",
          "next": [130200001]
        },
        "130200001": {
          "id": 130200001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¡Cuánta tranquilidad! Nadie dice nada en el grupo. Qué diferencia con otros grupos...",
          "next": [130200002]
        },
        "130200002": {
          "id": 130200002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Es posible que todo el mundo esté ocupado.",
          "next": [130200003]
        },
        "130200003": {
          "id": 130200003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Oh, ocupad{F#as}{M#os}, claro. Eso es bueno.",
          "next": [130200004]
        },
        "130200004": {
          "id": 130200004,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "[Llora]",
          "next": [130200005]
        },
        "130200005": {
          "id": 130200005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Pequeña Gui...",
          "next": [130200006]
        },
        "130200006": {
          "id": 130200006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "@{NICKNAME} @Huohuo Vengan a animar a Gui, se siente sola.",
          "next": [130200007]
        },
        "130200007": {
          "id": 130200007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "¿Q-qué pasa?",
          "next": [130200008]
        },
        "130200008": {
          "id": 130200008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Solo quieren buscar a gente para charlar.",
          "next": [130200009]
        },
        "130200009": {
          "id": 130200009,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "[De acuerdo]",
          "next": [130200010]
        },
        "130200010": {
          "id": 130200010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ya veo.",
          "next": [130200011, 130200016, 130200026]
        },
        "130200011": {
          "id": 130200011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Este es el grupo de la brigada cazafantasmas.",
          "next": [130200012]
        },
        "130200012": {
          "id": 130200012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Lo sé, somos nosotras quienes deben callarse.",
          "next": [130200013]
        },
        "130200013": {
          "id": 130200013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "No, no le hagas caso, Shangshang.",
          "next": [130200014]
        },
        "130200014": {
          "id": 130200014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Es de sentido común que los cazafantasmas no hablen de fantasmas en el grupo.",
          "next": [130200015]
        },
        "130200015": {
          "id": 130200015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "¿Ah, sí? ¿De sentido común?",
          "next": [130200030]
        },
        "130200016": {
          "id": 130200016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Veo que estas dos están aburridas y solo buscan a alguien para charlar. No tienes que hacerles caso, Huohuo.",
          "next": [130200017]
        },
        "130200017": {
          "id": 130200017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Está bien, no les haré caso.",
          "next": [130200018]
        },
        "130200018": {
          "id": 130200018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¡Noooo!",
          "next": [130200019]
        },
        "130200019": {
          "id": 130200019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Imposible, imposible, imposible.",
          "next": [130200020]
        },
        "130200020": {
          "id": 130200020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿Qué hacemos, Shangshang?",
          "next": [130200021]
        },
        "130200021": {
          "id": 130200021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Si lo hubiera sabido antes, no hubiera permitido que esto pasara (TдT)",
          "next": [130200022]
        },
        "130200022": {
          "id": 130200022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "La Pequeña Gui me obligó a hacer eso (TдT)",
          "next": [130200023]
        },
        "130200023": {
          "id": 130200023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Siento no haberla detenido (TдT)",
          "next": [130200024]
        },
        "130200024": {
          "id": 130200024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Huohuo, ¿te has enfadado? (TдT) Sería lo normal (TдT)",
          "next": [130200025]
        },
        "130200025": {
          "id": 130200025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "No-no me he enfadado, no te pongas tan seria.",
          "next": [130200030]
        },
        "130200026": {
          "id": 130200026,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Yo también quiero charlar, ¡hablemos de algo!",
          "next": [130200027]
        },
        "130200027": {
          "id": 130200027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "¡No esperaba menos de ti!",
          "next": [130200028]
        },
        "130200028": {
          "id": 130200028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿Y tú, Huohuo?",
          "next": [130200029]
        },
        "130200029": {
          "id": 130200029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "No esperaba que fueras a preguntarme específicamente, Guinaifen.",
          "next": [130200030]
        },
        "130200030": {
          "id": 130200030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "El grupo de la brigada cazafantasmas tampoco es tan serio.",
          "next": [130200031]
        },
        "130200031": {
          "id": 130200031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Aquí pueden hablar de todo lo que quieran.",
          "next": [130200032]
        },
        "130200032": {
          "id": 130200032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¡Genial!",
          "next": [130200033]
        },
        "130200033": {
          "id": 130200033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "¡Genial!",
          "next": [130200034]
        },
        "130200034": {
          "id": 130200034,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Genial!",
          "next": [130200035]
        },
        "130200035": {
          "id": 130200035,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Genial]",
          "next": []
        }
      },
      "participatingContactsIds": [1210, 1206, 1217, 8001]
    },
    {
      "id": 1302001,
      "startingMessageId": [130200101],
      "messages": {
        "130200101": {
          "id": 130200101,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Quiero recomendarles lo que hago para relajarme.",
          "next": [130200102]
        },
        "130200102": {
          "id": 130200102,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "¡Voy a la Dársena de astroesquifes, busco un lugar desde el que se vea volar a los astroesquifes y me quedo allí absorta!",
          "next": [130200103]
        },
        "130200103": {
          "id": 130200103,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "... También se puede hacer sentada en el Conde Insomne.",
          "next": [130200104]
        },
        "130200104": {
          "id": 130200104,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ver pasar a los astroesquifes...",
          "next": [130200105]
        },
        "130200105": {
          "id": 130200105,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Hace que el tiempo vuele sin darte cuenta.",
          "next": [130200106]
        },
        "130200106": {
          "id": 130200106,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Y te hace sentir muy calmada.",
          "next": [130200107]
        },
        "130200107": {
          "id": 130200107,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ni idea.",
          "next": [130200108]
        },
        "130200108": {
          "id": 130200108,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ni idea.",
          "next": [130200109, 130200112]
        },
        "130200109": {
          "id": 130200109,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ni idea.",
          "next": [130200110]
        },
        "130200110": {
          "id": 130200110,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ahora me preocupa tu salud mental, Huohuo.",
          "next": [130200111]
        },
        "130200111": {
          "id": 130200111,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Iremos a probarlo, pero cuando te preocupe algo nos lo tienes que contar, Huohuo.",
          "next": [130200117]
        },
        "130200112": {
          "id": 130200112,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Es una forma de relajarse, igual que escuchar el sonido de la lluvia.",
          "next": [130200113]
        },
        "130200113": {
          "id": 130200113,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Yo lo entiendo muy bien.",
          "next": [130200114]
        },
        "130200114": {
          "id": 130200114,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "¡Eso es!",
          "next": [130200115]
        },
        "130200115": {
          "id": 130200115,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Me preocupa la salud mental de ustedes dos.",
          "next": [130200116]
        },
        "130200116": {
          "id": 130200116,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Cuando les preocupe algo, nos lo tienen que contar.",
          "next": [130200117]
        },
        "130200117": {
          "id": 130200117,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ya llevo un buen rato aquí mirándolos, me siento en paz.",
          "next": [130200118]
        },
        "130200118": {
          "id": 130200118,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Oh, un momento.",
          "next": [130200119]
        },
        "130200119": {
          "id": 130200119,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿Eh?",
          "next": [130200120]
        },
        "130200120": {
          "id": 130200120,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "¿Qué pasa?",
          "next": [130200121]
        },
        "130200121": {
          "id": 130200121,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[Confusión]",
          "next": [130200122]
        },
        "130200122": {
          "id": 130200122,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ya estoy aquí.",
          "next": [130200123]
        },
        "130200123": {
          "id": 130200123,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Acaba de venir el Sr. Xiyan. Sin decir nada, me dio un puñado de golosinas y se marchó.",
          "next": [130200124]
        },
        "130200124": {
          "id": 130200124,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "¿Qué significa eso?",
          "next": [130200125, 130200126]
        },
        "130200125": {
          "id": 130200125,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Significa que eres adorable. Yo si estuviera allí, también te hubiera dado golosinas.",
          "next": [130200128]
        },
        "130200126": {
          "id": 130200126,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Le darías pena, siempre mirando los astroesquifes.",
          "next": [130200127]
        },
        "130200127": {
          "id": 130200127,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Todas las personas amables harían algo así.",
          "next": [130200128]
        },
        "130200128": {
          "id": 130200128,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿Es como cuando no puedes evitar acercarte al estanque y echar de comer a los peces?",
          "next": [130200129]
        },
        "130200129": {
          "id": 130200129,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "¿Eso no está prohibido?",
          "next": [130200130]
        },
        "130200130": {
          "id": 130200130,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Pero no está prohibido darle de comer a Huohuo.",
          "next": [130200131]
        },
        "130200131": {
          "id": 130200131,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ya veo.",
          "next": [130200132]
        },
        "130200132": {
          "id": 130200132,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "... Nos desviamos del tema, ¿no?",
          "next": [130200133]
        },
        "130200133": {
          "id": 130200133,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Yo también tengo golosinas, ¿dónde estás, Huohuo?",
          "next": [130200134]
        },
        "130200134": {
          "id": 130200134,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Yo también tengo golosinas, ¡vamos!",
          "next": [130200135]
        },
        "130200135": {
          "id": 130200135,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[Vamos]",
          "next": [130200136]
        },
        "130200136": {
          "id": 130200136,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "No hace falta que vengan, ya me fui.",
          "next": [130200137]
        },
        "130200137": {
          "id": 130200137,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ay...",
          "next": []
        }
      },
      "participatingContactsIds": [1217, 1210, 1206, 8001]
    },
    {
      "id": 1302002,
      "startingMessageId": [130200201],
      "messages": {
        "130200201": {
          "id": 130200201,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿Quién quiere té con leche? Acabo de pedir dos de más. ¡Para quien lo diga primero!",
          "next": [130200202]
        },
        "130200202": {
          "id": 130200202,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "¡Yooo!",
          "next": [130200203]
        },
        "130200203": {
          "id": 130200203,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "¡Yo!",
          "next": [130200204]
        },
        "130200204": {
          "id": 130200204,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Bueno, entonces ustedes dos.",
          "next": [130200205, 130200208, 130200212]
        },
        "130200205": {
          "id": 130200205,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Yooo!",
          "next": [130200206]
        },
        "130200206": {
          "id": 130200206,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "¿También hay para mí?",
          "next": [130200207]
        },
        "130200207": {
          "id": 130200207,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ya no.",
          "next": [130200216]
        },
        "130200208": {
          "id": 130200208,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Llego un poco tarde, ¿no?",
          "next": [130200209]
        },
        "130200209": {
          "id": 130200209,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ustedes dos siempre están pendientes del ábaco de jade, ¿no?",
          "next": [130200210]
        },
        "130200210": {
          "id": 130200210,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "No.",
          "next": [130200211]
        },
        "130200211": {
          "id": 130200211,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Yo tampoco.",
          "next": [130200216]
        },
        "130200212": {
          "id": 130200212,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Pequeña Gui...",
          "next": [130200213]
        },
        "130200213": {
          "id": 130200213,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Que tres personas se peleen por dos tés con leche ha puesto a prueba nuestra amistad.",
          "next": [130200214]
        },
        "130200214": {
          "id": 130200214,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "¿Cómo vas a compensármelo?",
          "next": [130200215]
        },
        "130200215": {
          "id": 130200215,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Te lo estás tomando demasiado en serio.",
          "next": [130200216]
        },
        "130200216": {
          "id": 130200216,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Lo siento, no tengo más para ti. Te compraré uno la próxima vez.",
          "next": [130200217, 130200219]
        },
        "130200217": {
          "id": 130200217,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Entonces estás perdida. Ya lo anoté en mi libreta.",
          "next": [130200218]
        },
        "130200218": {
          "id": 130200218,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Si no me invitas la próxima vez, es cosa tuya.",
          "next": [130200221]
        },
        "130200219": {
          "id": 130200219,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "No te preocupes. No me importa en absoluto.",
          "next": [130200220]
        },
        "130200220": {
          "id": 130200220,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "De verdad, está bien. No tienes que tomártelo a pecho.",
