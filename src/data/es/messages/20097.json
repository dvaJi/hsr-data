      "messages": {
        "200970000": {
          "id": 200970000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¡Amig{F#a}{M#o}! ¿Te acuerdas de mí?",
          "next": [
            200970001,
            200970003,
            200970006
          ]
        },
        "200970001": {
          "id": 200970001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Claro que sí, Pequeña Gui!",
          "next": [
            200970002
          ]
        },
        "200970002": {
          "id": 200970002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿Ahora también tú me llamas Pequeña Gui?",
          "next": [
            200970010
          ]
        },
        "200970003": {
          "id": 200970003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Lo siento, he conocido a demasiada gente últimamente, ¿quién eres?",
          "next": [
            200970004
          ]
        },
        "200970004": {
          "id": 200970004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Uf, pensé que sería inolvidable.",
          "next": [
            200970005
          ]
        },
        "200970005": {
          "id": 200970005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¡Guerras Etéreas! ¡Festival del Torneo Interastral! ¡Guinaifen! ¿Te acuerdas ahora?",
          "next": [
            200970010
          ]
        },
        "200970006": {
          "id": 200970006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Lo siento, no me interesan las hojas de té ni los seguros, no quiero nada.",
          "next": [
            200970007
          ]
        },
        "200970007": {
          "id": 200970007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¡Ay!",
          "next": [
            200970008
          ]
        },
        "200970008": {
          "id": 200970008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿Qué es todo eso?",
          "next": [
            200970009
          ]
        },
        "200970009": {
          "id": 200970009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¡Guerras Etéreas! ¡Festival del Torneo Interastral! ¡Guinaifen! ¿Te acuerdas ahora?",
          "next": [
            200970010
          ]
        },
        "200970010": {
          "id": 200970010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Un rival es un amigo competitivo, así que ahora somos amig{F#as}{M#os}.",
          "next": [
            200970011
          ]
        },
        "200970011": {
          "id": 200970011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "No llegué a preguntártelo, pero ¿cuál es tu planeta natal?",
          "next": [
            200970012,
            200970015,
            200970018
          ]
        },
        "200970012": {
          "id": 200970012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Soy {F#una}{M#uno} de los Anónimos a bordo del Expreso Astral.",
          "next": [
            200970013
          ]
        },
        "200970013": {
          "id": 200970013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Incluso los Anónimos vienen de alguna parte.",
          "next": [
            200970014
          ]
        },
        "200970014": {
          "id": 200970014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "No pasa nada si no quieres decírmelo, todo el mundo es un forastero en cualquier otro lugar.",
          "next": [
            200970021
          ]
        },
        "200970015": {
          "id": 200970015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mi hogar es el universo.",
          "next": [
            200970016
          ]
        },
        "200970016": {
          "id": 200970016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Incluso alguien que viene del otro lado de las estrellas tiene que ser de alguna parte.",
          "next": [
            200970017
          ]
        },
        "200970017": {
          "id": 200970017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "No pasa nada si no quieres decírmelo, todo el mundo es un forastero en cualquier otro lugar.",
          "next": [
            200970021
          ]
        },
        "200970018": {
          "id": 200970018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "En realidad, no recuerdo de dónde vengo.",
          "next": [
            200970019
          ]
        },
        "200970019": {
          "id": 200970019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Oh...",
          "next": [
            200970020
          ]
        },
        "200970020": {
          "id": 200970020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Parece que este asunto es algo delicado, lo siento.",
          "next": [
            200970021
          ]
        },
        "200970021": {
          "id": 200970021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "La Legión destruyó mi planeta natal, así que técnicamente ya no tengo uno.",
          "next": [
            200970022
          ]
        },
        "200970022": {
          "id": 200970022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Amb{F#as}{M#os} somos foraster{F#as}{M#os} que temporalmente llamamos a Xianzhou nuestro hogar, supongo.",
          "next": [
            200970023
          ]
        },
        "200970023": {
          "id": 200970023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Si no tienes claro cómo es la vida en Xianzhou, o si tienes algún problema, ven a buscarme cuando quieras.",
          "next": [
            200970024
          ]
        },
        "200970024": {
          "id": 200970024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Casi siempre estoy en el Callejón Aurum haciendo espectáculos callejeros, aunque a veces también hago transmisiones.",
          "next": [
            200970025
          ]
        },
        "200970025": {
          "id": 200970025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Si tienes algo que no puedas resolver o algún villano del que no puedas librarte, avísame.",
          "next": [
            200970026
          ]
        },
        "200970026": {
          "id": 200970026,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Soy muy fuerte.",
          "next": [
            200970027
          ]
        },
        "200970027": {
          "id": 200970027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¡Sí, sí, mi {F#nueva amiga}{M#nuevo amigo} de confianza!",
          "next": [
            200970028
          ]
        },
        "200970028": {
          "id": 200970028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¡Enfrentémonos otra vez en algún momento! ¡Adiós!",
          "next": []
        }
