        },
        "201240021": {
          "id": 201240021,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Me interesa, ¡cuéntame más!",
          "next": [
            201240022
          ]
        },
        "201240022": {
          "id": 201240022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 124,
          "text": "<u>¡No te lo pierdas! Regístrate rápidamente con tu número en este enlace y sube al escenario con Robin</u>",
