{
  "id": 11125,
  "contacts": {
    "id": 48,
    "name": "Emily",
    "icon": "UI_Message_Contacts_Emilie",
    "signature": "¡Hacer buenas obras, decir buenas palabras y tener un buen corazón!",
    "typeId": 2,
    "type": "Otros"
  },
  "relatedMessages": [
