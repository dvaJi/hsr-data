      "messages": {
        "200110000": {
          "id": 200110000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "¡Un nuevo día, {NICKNAME}!",
          "next": [200110001]
        },
        "200110001": {
          "id": 200110001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Cuando subimos al Expreso y seguimos las huellas de los Anónimos, tenemos que acostumbrarnos a cosas que se salen de lo ordinario.",
          "next": [200110002]
        },
        "200110002": {
          "id": 200110002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "A veces, incluso tenemos que sobrepasar nuestros límites.",
          "next": [200110003]
        },
        "200110003": {
          "id": 200110003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "{NICKNAME}, ¿a ti te ha pasado algo así?",
          "next": [200110004, 200110005]
        },
        "200110004": {
          "id": 200110004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sí, de repente recibo muchísimos mensajes.",
          "next": [200110006]
        },
        "200110005": {
          "id": 200110005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sí, de repente recibo muchísimos encargos.",
          "next": [200110009]
        },
        "200110006": {
          "id": 200110006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Los mensajes representan la conexión entre las personas.",
          "next": [200110007]
        },
        "200110007": {
          "id": 200110007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Si todos los días recibes mensajes, quiere decir que has profundizado en los lazos que tienes con las personas que conociste en tus viajes trazacaminos.",
          "next": [200110008]
        },
        "200110008": {
          "id": 200110008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Pero esos lazos tienen que mantenerse. No olvides planificar tu tiempo.",
          "next": [200110012, 200110013]
        },
        "200110009": {
          "id": 200110009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Si no aceptas varios encargos al mismo tiempo, no puedes decir que seas un auténtico Anónimo.",
          "next": [200110010]
        },
        "200110010": {
          "id": 200110010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Hay que planear la ruta más óptima para cumplir los encargos sin dar un paso más de lo necesario.",
          "next": [200110011]
        },
        "200110011": {
          "id": 200110011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Antes de que te des cuenta, los harás automáticamente.",
          "next": [200110012, 200110013]
        },
        "200110012": {
          "id": 200110012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Es la voz de la experiencia, Sr. Yang!",
          "next": [200110014]
        },
        "200110013": {
          "id": 200110013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Es el deber de {F#una}{M#un} Trazacaminos y estoy dispuest{F#a}{M#o} a cumplirlo!",
          "next": [200110016]
        },
        "200110014": {
          "id": 200110014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Es la experiencia de los Anónimos, una lección de sudor y sangre.",
          "next": [200110015]
        },
        "200110015": {
          "id": 200110015,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "[De acuerdo]",
          "next": []
        },
        "200110016": {
          "id": 200110016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Con lo joven que eres, y ya te das cuenta.",
          "next": [200110017]
        },
        "200110017": {
          "id": 200110017,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "[Me gusta]",
          "next": []
        }
      },
