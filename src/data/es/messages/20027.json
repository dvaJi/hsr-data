      "messages": {
        "200270000": {
          "id": 200270000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "{NICKNAME}, ¿qué color me queda mejor? ¿El azul o el gris?",
          "next": [
            200270001
          ]
        },
        "200270001": {
          "id": 200270001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Eres mi amig{F#a}{M#o}, con que me digas la primera impresión que tengas me vale.",
          "next": [
            200270002,
            200270005,
            200270008
          ]
        },
        "200270002": {
          "id": 200270002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "El azul. A juego con tu uniforme.",
          "next": [
            200270003
          ]
        },
        "200270003": {
          "id": 200270003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "¡Sí! El azul no está mal.",
          "next": [
            200270004
          ]
        },
        "200270004": {
          "id": 200270004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "¡Parece que tenemos gustos similares!",
          "next": [
            200270011
          ]
        },
        "200270005": {
          "id": 200270005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿El gris? A juego con tu cabello.",
          "next": [
            200270006
          ]
        },
        "200270006": {
          "id": 200270006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "El gris... ¡me vale!",
          "next": [
            200270007
          ]
        },
        "200270007": {
          "id": 200270007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Si dices que me queda bien, me lo compro, a ver qué tal.",
          "next": [
            200270011
          ]
        },
        "200270008": {
          "id": 200270008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Envíame un selfi, anda, hablas como si no fueses tú.",
          "next": [
            200270009
          ]
        },
        "200270009": {
          "id": 200270009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "¿Cómo que no soy yo? Lo único es que estoy emocionado.",
          "next": [
            200270010
          ]
        },
        "200270010": {
          "id": 200270010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Da igual, déjalo. Me compro los dos colores, y ya veré cuál me queda mejor.",
          "next": [
            200270011
          ]
        },
        "200270011": {
          "id": 200270011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Quería comprarme unos zapatos deportivos nuevos, por eso necesitaba tu opinión como amig{F#a}{M#o}.",
          "next": [
            200270012
          ]
        },
        "200270012": {
          "id": 200270012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Te enviaré una foto con ellos cuando lleguen.",
          "next": [
            200270013
          ]
        },
        "200270013": {
          "id": 200270013,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "[Me gusta]",
          "next": [
            200270014,
            200270015
          ]
        },
        "200270014": {
          "id": 200270014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Oye, devuélvele ya el teléfono.",
          "next": [
            200270016
          ]
        },
        "200270015": {
          "id": 200270015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Asta, deja de hacer el tonto.",
          "next": [
            200270016
          ]
        },
        "200270016": {
          "id": 200270016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "¿Eh? ¿Cómo lo has sabido? ¿Tanto se nota que no soy Arlan?",
          "next": [
            200270017
          ]
        },
        "200270017": {
          "id": 200270017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Vi que editó el mensaje para ti una y otra vez, para luego ni enviarlo.",
          "next": [
            200270018
          ]
        },
        "200270018": {
          "id": 200270018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Lo siento, Asta me quitó el teléfono hace un momento.",
          "next": [
            200270019
          ]
        },
        "200270019": {
          "id": 200270019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Perdona por las molestias. No volverá a pasar.",
          "next": [
            200270020
          ]
        },
        "200270020": {
          "id": 200270020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Luego hablamos. Asta ha empezado a buscar su llave de encriptación de pago. No puedo dejar que la encuentre.",
          "next": []
        }
      },
