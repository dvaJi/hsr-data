      "messages": {
        "201820000": {
          "id": 201820000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "<b>Hemos</b> desarrollado una prueba de personalidad combinando las artes adivinatorias de Janópolis.",
          "next": [
            201820001
          ]
        },
        "201820001": {
          "id": 201820001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "A cada uno se le asigna un carácter único e irrepetible.",
          "next": [
            201820002
          ]
        },
        "201820002": {
          "id": 201820002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Agli es como un rollo frío de huevo dorado; Cas es romero y ajenjo; Fai es un guiso caliente; Dei es un chuletón; Ciny es como una nube de bayas burbujeantes, y Dan es limonada helada.",
          "next": [
            201820003
          ]
        },
        "201820003": {
          "id": 201820003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "¡Ah! Y también está Naxi, que nunca se ha hecho la prueba... ¡Seguro que es un helado de menta loco!",
          "next": [
            201820004,
            201820006,
            201820008
          ]
        },
        "201820004": {
          "id": 201820004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Entonces yo soy soja gris.",
          "next": [
            201820005
          ]
        },
        "201820005": {
          "id": 201820005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "¿Eh? ¿Aún estamos a tiempo de añadir soja a los resultados de la prueba?",
          "next": [
            201820011
          ]
        },
        "201820006": {
          "id": 201820006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Los Herederos de Crisos se pueden comer?",
          "next": [
            201820007
          ]
        },
        "201820007": {
          "id": 201820007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "¡Por supuesto que no! ¡Es solo una metáfora!",
          "next": [
            201820011
          ]
        },
        "201820008": {
          "id": 201820008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ya entiendo, eres una tartaleta de manzana roja.",
          "next": [
            201820009
          ]
        },
        "201820009": {
          "id": 201820009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Je, je, te equivocas.",
          "next": [
            201820010
          ]
        },
        "201820010": {
          "id": 201820010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Soy una tartaleta de manzana roja recién sacada del horno.",
          "next": [
            201820011
          ]
        },
        "201820011": {
          "id": 201820011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Cómo funciona la prueba?",
          "next": [
            201820012
          ]
        },
        "201820012": {
          "id": 201820012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "La prueba completa es muy larga, pero <b>podemos</b> echar un vistazo a las preguntas más importantes.",
          "next": [
            201820013
          ]
        },
        "201820013": {
          "id": 201820013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Por ejemplo, esta:",
          "next": [
            201820014
          ]
        },
        "201820014": {
          "id": 201820014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Si tuvieras el poder de crear infinitamente una sola cosa, ¿qué crearías?",
          "next": [
            201820015,
            201820017,
            201820019
          ]
        },
        "201820015": {
          "id": 201820015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Tribbies!",
          "next": [
            201820016
          ]
        },
        "201820016": {
          "id": 201820016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Antes había muchas de <b>nosotras</b>.",
          "next": [
            201820022
          ]
        },
        "201820017": {
          "id": 201820017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Jades estelares!",
          "next": [
            201820018
          ]
        },
        "201820018": {
          "id": 201820018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "¡A <b>nosotras</b> también nos gustan las estrellas!",
          "next": [
            201820022
          ]
        },
        "201820019": {
          "id": 201820019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Créditos!",
          "next": [
            201820020
          ]
        },
        "201820020": {
          "id": 201820020,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Para que todo el mundo pueda ser millonario.",
          "next": [
            201820021
          ]
        },
        "201820021": {
          "id": 201820021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "¡Qué considerado de tu parte, pelo gris!",
          "next": [
            201820022
          ]
        },
        "201820022": {
          "id": 201820022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "¿Se te ocurre alguna pregunta que podamos incluir en la prueba?",
          "next": [
            201820023,
            201820025,
            201820027
          ]
        },
        "201820023": {
          "id": 201820023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Si pudieras, ¿te subirías al Expreso Astral?",
          "next": [
            201820024
          ]
        },
        "201820024": {
          "id": 201820024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "¡Es una buena pregunta para evaluar el espíritu aventurero de una persona!",
          "next": [
            201820029
          ]
        },
        "201820025": {
          "id": 201820025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Si pudieras, ¿te convertirías en enemigo del Expreso?",
          "next": [
            201820026
          ]
        },
        "201820026": {
          "id": 201820026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Esa pregunta da un poco de miedo...",
          "next": [
            201820029
          ]
        },
        "201820027": {
          "id": 201820027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Si pudieras, ¿tomarías café en el Expreso?",
          "next": [
            201820028
          ]
        },
        "201820028": {
          "id": 201820028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "¿Eh? ¿Esa pregunta es importante?",
          "next": [
            201820029
          ]
        },
        "201820029": {
          "id": 201820029,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Se ha enviado el adjunto: «Las 985 preguntas de la prueba del destino de Jano».",
          "next": [
            201820030
          ]
        },
        "201820030": {
          "id": 201820030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "<b>Déjanos</b> pensarlo...",
          "next": [
            201820031
          ]
        },
        "201820031": {
          "id": 201820031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1403,
          "text": "Te <b>enviamos</b> la versión actual, pero si tienes nuevas ideas, no dudes en <b>decírnoslas</b>.",
          "next": []
        }
      },
