  "relatedMessages": [20070, 20071, 20072],
  "sections": [
    {
      "id": 2007100,
      "startingMessageId": [200710000],
      "messages": {
        "200710000": {
          "id": 200710000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "{NICKNAME}, cuando viniste al Luofu, ¿dónde te alojaste? ¿Qué tal era?",
          "next": [200710001]
        },
        "200710001": {
          "id": 200710001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Espera, déjame adivinar...",
          "next": [200710002]
        },
        "200710002": {
          "id": 200710002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "¿La Posada Petricor?",
          "next": [200710003, 200710007]
        },
        "200710003": {
          "id": 200710003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Así es, me quedé en la Posada Petricor.",
          "next": [200710004]
        },
        "200710004": {
          "id": 200710004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Una habitación superior, no estaba nada mal.",
          "next": [200710005]
        },
        "200710005": {
          "id": 200710005,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Me gusta]",
          "next": [200710006]
        },
        "200710006": {
          "id": 200710006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "La suite premium no es fácil de reservar, la mayoría de la gente solo iría allí por las vistas nocturnas de la avenida Otearastros.",
          "next": [200710012]
        },
        "200710007": {
          "id": 200710007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Así es, me quedé en la Posada Petricor.",
          "next": [200710008]
        },
        "200710008": {
          "id": 200710008,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Sinceramente, no es nada del otro mundo.",
          "next": [200710009]
        },
        "200710009": {
          "id": 200710009,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Al fin y al cabo, cada vez que me alojo en un hotel, me preocupa que pasen cosas malas, así que no tengo tiempo para pensar en otras cosas.",
          "next": [200710010]
        },
        "200710010": {
          "id": 200710010,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Llora]",
          "next": [200710011]
        },
        "200710011": {
          "id": 200710011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Parece que has pasado por una mala racha.",
          "next": [200710012]
        },
        "200710012": {
          "id": 200710012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Yo también me alojo en la Posada Petricor a menudo. Después de todo, es fácil llegar a cualquier parte desde allí.",
          "next": [200710013]
        },
        "200710013": {
          "id": 200710013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Como comerciante itinerante, no me consiento mucho en mis viajes, pero me aburro después de un tiempo.",
          "next": [200710014]
        },
        "200710014": {
          "id": 200710014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Hay algunos alojamientos en el área residencial Pabellones Celestiales que permiten alquileres a corto plazo, así que me fui allí.",
          "next": [200710015]
        },
        "200710015": {
          "id": 200710015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Es donde suelen alojarse los residentes de Xianzhou.",
          "next": [200710016, 200710020]
        },
        "200710016": {
          "id": 200710016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "No parece muy conveniente, todos los ojos estarán puestos en ti.",
          "next": [200710017]
        },
        "200710017": {
          "id": 200710017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Eso depende de cómo se vea.",
          "next": [200710018]
        },
        "200710018": {
          "id": 200710018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "La vida en Xianzhou no es tan cerrada. La gente es amable, así que experimentar y observar su forma de vida es mi pasatiempo...",
          "next": [200710019]
        },
        "200710019": {
          "id": 200710019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Pero eso no es del todo correcto, la mayoría de las veces soy yo el observado.",
          "next": [200710023]
        },
        "200710020": {
          "id": 200710020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Ahora también eres un nativo de Xianzhou!",
          "next": [200710021]
        },
        "200710021": {
          "id": 200710021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Oh, me conoces.",
          "next": [200710022]
        },
        "200710022": {
          "id": 200710022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Sí, de esta manera, me integro en la vida del Luofu.",
          "next": [200710023]
        },
        "200710023": {
          "id": 200710023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "No tengo un lugar fijo donde vivir. No tengo un sitio al que llamar hogar, y si puedo quedarme en los Pabellones Celestiales durante un tiempo, puedo olvidar por un momento mi condición de comerciante itinerante.",
          "next": [200710024]
        },
        "200710024": {
          "id": 200710024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Así que lo disfruto bastante.",
          "next": [200710025, 200710027]
        },
        "200710025": {
          "id": 200710025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Me gustaría probar la próxima vez.",
          "next": [200710026]
        },
        "200710026": {
          "id": 200710026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Bueno, tal vez entonces tendremos la suerte de ser vecinos.",
          "next": []
        },
        "200710027": {
          "id": 200710027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Probablemente no puedo vivir allí.",
          "next": [200710028]
        },
        "200710028": {
          "id": 200710028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "¿Puedo preguntar por qué?",
          "next": [200710029, 200710030, 200710031]
        },
        "200710029": {
          "id": 200710029,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Me da miedo!",
          "next": [200710032]
        },
        "200710030": {
          "id": 200710030,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡No puedo dormir en otra cama!",
          "next": [200710032]
        },
        "200710031": {
          "id": 200710031,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Soy {F#una}{M#un} maniátic{F#a}{M#o} de la limpieza!",
          "next": [200710032]
        },
        "200710032": {
          "id": 200710032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Ya veo.",
          "next": [200710033]
        },
        "200710033": {
          "id": 200710033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Entiendo y respeto tu decisión, {NICKNAME}.",
          "next": [200710034]
        },
        "200710034": {
          "id": 200710034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Si un día cambias de opinión, volveremos a hablar.",
          "next": []
        }
      },
      "participatingContactsIds": [1203, 8001]
    }
  ]
}
