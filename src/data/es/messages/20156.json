        "201560001": {
          "id": 201560001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Alguien ha estado usando comida deliciosa para convencer al maestro de que le enseñe artes marciales.",
          "next": [
            201560002
          ]
        },
        "201560002": {
          "id": 201560002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Espera.",
          "next": [
