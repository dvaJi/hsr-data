        "201560005": {
          "id": 201560005,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "¿Cuáles son los chismes del momento?",
          "next": [
            201560006
          ]
        },
        "201560006": {
          "id": 201560006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "En este momento, estoy escuchando a dos espadas susurrar.",
          "next": [
            201560007
          ]
        },
        "201560007": {
          "id": 201560007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Son bastante ruidosas.",
          "next": [
            201560008
          ]
        },
        "201560008": {
          "id": 201560008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Así que no creo que esté espiando.",
          "next": [
            201560009
          ]
        },
        "201560009": {
          "id": 201560009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "La espada ancha dice que su maestro es un niño tonto que no puede hacer nada bien.",
