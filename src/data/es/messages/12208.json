      "messages": {
        "122080000": {
          "id": 122080000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "El caballero divino del general es impresionante.",
          "next": [122080001]
        },
        "122080001": {
          "id": 122080001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "¡Fantástico!",
          "next": [122080002]
        },
        "122080002": {
          "id": 122080002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Es genial. Esta es la primera vez que lo veo.",
          "next": [122080003, 122080007]
        },
        "122080003": {
          "id": 122080003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Cada vez que veo aparecer al caballero divino, pienso que es impresionante.",
          "next": [122080004]
        },
        "122080004": {
          "id": 122080004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿A que sí?",
          "next": [122080011]
        },
        "122080007": {
          "id": 122080007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Lo he visto tantas veces que ya no me produce ninguna emoción.",
          "next": [122080008]
        },
        "122080008": {
          "id": 122080008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿¿De verdad??",
          "next": [122080011]
        },
        "122080011": {
          "id": 122080011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Volviendo al tema, ¡felicidades a Huohuo por encontrar a Cola!",
          "next": [122080012]
        },
        "122080012": {
          "id": 122080012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Sí, ¿quién hubiera pensado que Cola estaba con Hanya?",
          "next": [122080013]
        },
        "122080013": {
          "id": 122080013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Oí que Cola tardó mucho en recuperarse. Debe haber sido difícil.",
          "next": [122080014]
        },
        "122080014": {
          "id": 122080014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Espera, se me acaba de ocurrir algo.",
          "next": [122080015]
        },
        "122080015": {
          "id": 122080015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "¿Quieres tomarle una foto a Cola?",
          "next": [122080016]
        },
        "122080016": {
          "id": 122080016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿Cómo lo supo?",
          "next": [122080017]
        },
        "122080017": {
          "id": 122080017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Bueno...",
          "next": [122080018, 122080020]
        },
        "122080018": {
          "id": 122080018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "No te presiones, Huohuo. No tienes que hablar si no quieres.",
          "next": [122080019]
        },
        "122080019": {
          "id": 122080019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Gracias, {NICKNAME}.",
          "next": [122080022]
        },
        "122080020": {
          "id": 122080020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Por qué no le preguntan a Cola primero? ¿A ver qué dice?",
          "next": [122080021]
        },
        "122080021": {
          "id": 122080021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Oh, el Sr. Cola dice que no está interesado.",
          "next": [122080022]
        },
        "122080022": {
          "id": 122080022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Bueno, si el Sr. Cola no está interesado, está bien. Tenemos muchos otros temas que tratar.",
          "next": [122080023]
        },
        "122080023": {
          "id": 122080023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Bien, echemos un vistazo a nuestra cuenta.",
          "next": [122080024]
        },
