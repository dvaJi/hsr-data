        },
        "111440004": {
          "id": 111440004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "He estado muy ocupad{F#a}{M#o}... pero bastante satisfech{F#a}{M#o}.",
