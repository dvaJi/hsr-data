  "relatedMessages": [13041, 13052, 20124, 20125, 20126],
  "sections": [
    {
      "id": 2012500,
      "startingMessageId": [201250000],
      "messages": {
        "201250000": {
          "id": 201250000,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Pom-Pom te ha enviado una solicitud de mensaje.",
          "next": [201250001]
        },
        "201250001": {
          "id": 201250001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¡{NICKNAME}! ¡¡Ayuda!!",
          "next": [201250002]
        },
        "201250002": {
          "id": 201250002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¡¡¡Parece que se ha colado una cosa rara en el Expreso!!!",
          "next": [201250003]
        },
        "201250003": {
          "id": 201250003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¡¡¡Lo perseguí hasta la sala de máquinas!!!",
          "next": [201250004]
        },
        "201250004": {
          "id": 201250004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¡¡¡Ven y sálvame!!!",
          "next": [201250005, 201250006]
        },
        "201250005": {
          "id": 201250005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Quién eres?",
          "next": [201250007]
        },
        "201250006": {
          "id": 201250006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Sparkle? ¿Otra vez tú?",
          "next": [201250007]
        },
        "201250007": {
          "id": 201250007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿De qué estás hablando? Soy Pom-Pom, ¡el revisor del Expreso!",
          "next": [201250008]
        },
        "201250008": {
          "id": 201250008,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1000,
          "next": [201250009]
        },
        "201250009": {
          "id": 201250009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "El dispositivo de comunicaciones se ha estropeado durante una inspección rutinaria, así que he tenido que usar una cuenta temporal",
          "next": [201250010, 201250022]
        },
        "201250010": {
          "id": 201250010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sigo sospechando que eres Sparkle.",
          "next": [201250011]
        },
        "201250011": {
          "id": 201250011,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Porque...",
          "next": [201250012, 201250015, 201250018]
        },
        "201250012": {
          "id": 201250012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Pom-Pom nunca hablaría así!",
          "next": [201250013]
        },
        "201250013": {
          "id": 201250013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Ooh... Ya veo...",
          "next": [201250014]
        },
        "201250014": {
          "id": 201250014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Mi plan era ser fiel al personaje, pero me temo que he fracasado estrepitosamente...",
          "next": [201250045]
        },
        "201250015": {
          "id": 201250015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Pom-Pom castigará a cualquiera que entre a la sala de máquinas!",
          "next": [201250016]
        },
        "201250016": {
          "id": 201250016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¡Vaya!",
          "next": [201250017]
        },
        "201250017": {
          "id": 201250017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "No imaginé que ese peluchito sería un peligro. Es genial...",
          "next": [201250045]
        },
        "201250018": {
          "id": 201250018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Nadie puede infiltrarse en el Expreso a excepción de Anónimos e invitados con invitación explícita!",
          "next": [201250019]
        },
        "201250019": {
          "id": 201250019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿Eh?",
          "next": [201250020]
        },
        "201250020": {
          "id": 201250020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "No lo creo. Hay un gran número de polizones que lograron subir al tren",
          "next": [201250021]
        },
        "201250021": {
          "id": 201250021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿Crees que entre ellos no hay una Bufona Enmascarada?",
          "next": [201250045]
        },
        "201250022": {
          "id": 201250022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Eres tú, el revisor del Expreso!",
          "next": [201250023]
        },
        "201250023": {
          "id": 201250023,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ahora usas una cuenta temporal sospechosa, ¡eso significa que aquí hay gato encerrado!",
          "next": [201250024]
        },
        "201250024": {
          "id": 201250024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿?",
          "next": [201250025]
        },
        "201250025": {
          "id": 201250025,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Lo siento, pero... yo siempre he conocido tu secreto... el secreto del Expreso...",
          "next": [201250026]
        },
        "201250026": {
          "id": 201250026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿?",
          "next": [201250027, 201250032, 201250037]
        },
        "201250027": {
          "id": 201250027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "En realidad, lo que se oculta en la sala de máquinas es...",
          "next": [201250028]
        },
        "201250028": {
          "id": 201250028,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "¡El revisor anterior del Expreso!",
          "next": [201250029]
        },
        "201250029": {
          "id": 201250029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿?",
          "next": [201250030]
        },
        "201250030": {
          "id": 201250030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿Eh? ¿Es eso cierto?",
          "next": [201250031]
        },
        "201250031": {
          "id": 201250031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿Los del Expreso Astral se atreverían a llegar tan lejos?",
          "next": [201250042]
        },
        "201250032": {
          "id": 201250032,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Eres un Bufón Enmascarado...",
          "next": [201250033]
        },
        "201250033": {
          "id": 201250033,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "¡Pom-Pom y Aha son familia!",
          "next": [201250034]
        },
        "201250034": {
          "id": 201250034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿?",
          "next": [201250035]
        },
        "201250035": {
          "id": 201250035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿Eh? ¿Cómo no lo he sabido hasta ahora?",
          "next": [201250036]
        },
        "201250036": {
          "id": 201250036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿Los Anónimos llegarían a tanto?",
          "next": [201250042]
        },
        "201250037": {
          "id": 201250037,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡El revisor fue quien creó a todos los del Expreso!",
          "next": [201250038]
        },
        "201250038": {
          "id": 201250038,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "¡Todos son Pom-Pom!",
          "next": [201250039]
        },
        "201250039": {
          "id": 201250039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿?",
          "next": [201250040]
        },
        "201250040": {
          "id": 201250040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿Eh? Ni yo podría inventarme algo así",
          "next": [201250041]
        },
        "201250041": {
          "id": 201250041,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¡Te has pasado, amig{F#a}{M#o}!",
          "next": [201250042]
        },
        "201250042": {
          "id": 201250042,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sparkle, ¡debería darte vergüenza!",
          "next": [201250043]
        },
        "201250043": {
          "id": 201250043,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Ah, lo cierto es que pensé que era verdad... Me emocioné tanto que lo olvidé por completo",
          "next": [201250044]
        },
        "201250044": {
          "id": 201250044,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿Cómo te atreves a mentir así?",
          "next": [201250045]
        },
        "201250045": {
          "id": 201250045,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Es muy vergonzoso que me reconozcan tan rápido...",
          "next": [201250046]
        },
        "201250046": {
          "id": 201250046,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Necesito prepararme mejor para la próxima vez que hablemos. Regresaré cuando se me ocurra algo bueno",
          "next": [201250047, 201250048]
        },
        "201250047": {
          "id": 201250047,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Vamos, ¡veamos quién engaña a quién la próxima vez!",
          "next": [201250049]
        },
        "201250048": {
          "id": 201250048,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Por favor, no regreses! Nada sale bien cuando andas con una bufona...",
          "next": [201250049]
        },
        "201250049": {
          "id": 201250049,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Oooh...",
          "next": [201250050]
        },
        "201250050": {
          "id": 201250050,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Empiezas a parecer un loro de tanto repetir lo mismo...",
          "next": [201250051]
        },
        "201250051": {
          "id": 201250051,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿Así es como tú llamas a la diversión?",
          "next": [201250052]
        },
        "201250052": {
          "id": 201250052,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿Has escuchado la frase: \"Un poco de diversión al día mantiene alejado al Bufón Enmascarado\"?",
          "next": [201250053]
        },
        "201250053": {
          "id": 201250053,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¡A los Bufones Enmascarados les gusta alterar las cosas en la vida que son inherentemente aburridas para hacerlas más maravillosas!",
          "next": [201250054]
        },
        "201250054": {
          "id": 201250054,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Bien, tengo cosas que hacer. Si no quieres que regrese, no lo haré",
          "next": [201250055]
        },
        "201250055": {
          "id": 201250055,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Avísame la próxima vez que tengas algo divertido entre manos",
          "next": [201250056]
        },
