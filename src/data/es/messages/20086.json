        },
        "200860007": {
          "id": 200860007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1110,
          "text": "Es un tipo de raíz de lirio que solo crece en las llanuras nevadas.",
          "next": [200860008]
        },
        "200860008": {
          "id": 200860008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1110,
          "text": "Es blanca y blandita. ¡Está buenísima!",
          "next": [200860013, 200860033]
        },
        "200860009": {
          "id": 200860009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Quieres cocinar una sopa solo con eso?",
