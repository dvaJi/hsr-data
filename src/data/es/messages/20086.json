          "id": 200860000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1110,
          "text": "Hoy he desenterrado un montón de rábanos nevados y raíces de lirio congeladas.",
          "next": [200860001]
        },
        "200860001": {
          "id": 200860001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1110,
          "text": "¡Puedo hacerme una buena sopa!",
          "next": [200860002]
        },
        "200860002": {
          "id": 200860002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1110,
          "text": "¡Sí!",
          "next": [200860003, 200860006, 200860009]
        },
        "200860003": {
          "id": 200860003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Rábanos nevados?",
          "next": [200860004]
        },
        "200860004": {
          "id": 200860004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1110,
          "text": "Es un tipo de rábano que solo crece en las llanuras nevadas.",
          "next": [200860005]
        },
        "200860005": {
          "id": 200860005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1110,
          "text": "Es rojo y dulzón. ¡Está delicioso!",
          "next": [200860013, 200860033]
        },
        "200860006": {
          "id": 200860006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Raíces de lirio congeladas?",
