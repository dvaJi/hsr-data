        "200960001": {
          "id": 200960001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1112,
          "text": "Estoy reflexionando sobre algo y necesito tu opinión",
          "next": [
            200960002
          ]
        },
        "200960002": {
