          ]
        },
        "122380009": {
          "id": 122380009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Es un asunto muy urgente.",
          "next": [
            122380010
          ]
        },
        "122380010": {
          "id": 122380010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Tranquilizaos, amig{F#a}{M#o}.",
