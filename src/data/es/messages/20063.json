        "200630000": {
          "id": 200630000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Qué día tan incómodo...",
          "next": [200630001]
        },
        "200630001": {
          "id": 200630001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Quería ir al Conde Insomne a escuchar historias, y adivina qué...",
          "next": [200630003]
        },
        "200630002": {
          "id": 200630002,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Confusión]",
          "next": [200630004]
        },
        "200630003": {
          "id": 200630003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Qué pasó?",
          "next": [200630002]
        },
        "200630004": {
          "id": 200630004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Me encontré con un antiguo catedrático del liceo.",
          "next": [200630005]
        },
        "200630005": {
          "id": 200630005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "No sabía que ahora mismo estaba en el Luofu, qué mala suerte...",
          "next": [200630006]
        },
        "200630006": {
          "id": 200630006,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[Llora]",
          "next": [200630007, 200630012]
        },
        "200630007": {
          "id": 200630007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Pensaba que encontrarse con profesores antiguos era algo bueno.",
          "next": [200630008]
        },
        "200630008": {
          "id": 200630008,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "¿Por qué es mala suerte?",
          "next": [200630009]
        },
        "200630009": {
          "id": 200630009,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Confusión]",
          "next": [200630010]
        },
        "200630010": {
          "id": 200630010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ah, es que no lo sabes.",
          "next": [200630011]
        },
        "200630011": {
          "id": 200630011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Cuando era estudiante, apenas se fijaba en mí.",
          "next": [200630019]
        },
        "200630012": {
          "id": 200630012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Qué es un liceo? ¿Y qué es un catedrático?",
          "next": [200630013]
        },
        "200630013": {
          "id": 200630013,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Soy de otro planeta, no entiendo.",
          "next": [200630014]
        },
        "200630014": {
          "id": 200630014,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Confusión]",
          "next": [200630015]
        },
        "200630015": {
          "id": 200630015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Oh, no pasa nada. Probablemente no sé mucho más que tú de todos modos.",
          "next": [200630016]
        },
        "200630016": {
          "id": 200630016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Déjame pensar en cómo explicártelo, espera.",
          "next": [200630017]
        },
        "200630017": {
          "id": 200630017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "¡Ah, sí!",
          "next": [200630018]
        },
        "200630018": {
          "id": 200630018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Un liceo es una escuela y un catedrático es un profesor.",
          "next": [200630019]
        },
        "200630019": {
          "id": 200630019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ay, realmente no quería recordar eso. Es tan incómodo...",
          "next": [200630020]
        },
        "200630020": {
          "id": 200630020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Cuéntame, comparte la incomodidad.",
          "next": [200630021]
        },
        "200630021": {
          "id": 200630021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Como vengo de una familia militar, mi madre ha sido muy estricta con mi entrenamiento de artes marciales desde pequeña.",
          "next": [200630022]
        },
        "200630022": {
          "id": 200630022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Así que tenía que levantarme para entrenar con ella antes del amanecer, y luego ir al liceo.",
          "next": [200630023]
        },
        "200630023": {
          "id": 200630023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Pero para ese momento, ya estaba agotada. Ni siquiera podía escuchar nada de lo que decía el catedrático, solo me quedaba dormida.",
          "next": [200630024]
        },
        "200630024": {
          "id": 200630024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Justo ahora, cuando me vio, gritó: \"¡Oh, pero si es la dormilona!\".",
          "next": [200630025]
        },
        "200630025": {
          "id": 200630025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Todo el mundo en la calle volteó a mirarme, fue tan vergonzoso.",
          "next": [200630026]
        },
        "200630026": {
          "id": 200630026,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[Llora]",
          "next": [200630027]
        },
        "200630027": {
          "id": 200630027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Debe haber sido realmente vergonzoso.",
          "next": [200630028]
        },
        "200630028": {
          "id": 200630028,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Sorpresa]",
          "next": [200630029]
        },
        "200630029": {
          "id": 200630029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ay, espero no encontrarme con él nunca más...",
          "next": []
        }
      },
