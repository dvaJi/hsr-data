      "messages": {
        "111170000": {
          "id": 111170000,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Sr. Yang, ¿está ahí? ¡Sr. Yaaang!",
          "next": [
            111170003
          ]
        },
        "111170003": {
          "id": 111170003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "He estado aquí todo el tiempo. ¿Qué pasa, {NICKNAME}?",
          "next": [
            111170004,
            111170005
          ]
        },
        "111170004": {
          "id": 111170004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Nuestro Expreso acepta nuevos miembros?",
          "next": [
            111170006
          ]
        },
        "111170005": {
          "id": 111170005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tengo una amiga que quisiera unirse al Expreso, ¿qué opina, Sr. Yang?",
          "next": [
            111170007
          ]
        },
        "111170006": {
          "id": 111170006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "¿Oh? Desde luego que sí.",
          "next": [
            111170008
          ]
        },
        "111170007": {
          "id": 111170007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "¿Oh? Es bueno hacer amigos aquí, los nuevos miembros son bienvenidos.",
          "next": [
            111170008
          ]
        },
        "111170008": {
          "id": 111170008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Sin embargo, ¿te importa que pregunte?",
          "next": [
            111170009
          ]
        },
        "111170009": {
          "id": 111170009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Tengo curiosidad, ¿qué clase de persona es, {NICKNAME}?",
          "next": [
            111170010,
            111170011,
            111170012
          ]
        },
        "111170010": {
          "id": 111170010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Es una mecánica talentosa con una mente meticulosa.",
          "next": [
            111170013
          ]
        },
        "111170011": {
          "id": 111170011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Es una estrella del rock con una personalidad desinhibida.",
          "next": [
            111170014
          ]
        },
        "111170012": {
          "id": 111170012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ella es... Humm, no puedo describirla exactamente.",
          "next": [
            111170015
          ]
        },
        "111170013": {
          "id": 111170013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Supongo que eso significa que tendrá mucho en común con Himeko, no está mal.",
          "next": [
            111170016
          ]
        },
        "111170014": {
          "id": 111170014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "¿Oh? Es un buen pasatiempo, a mí también me interesa.",
          "next": [
            111170016
          ]
        },
        "111170015": {
          "id": 111170015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "No hay problema. Confío en tu juicio, no debería fallar mucho.",
          "next": [
            111170016
          ]
        },
        "111170016": {
          "id": 111170016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "En cualquier caso, me gustaría darle la bienvenida.",
          "next": [
            111170017
          ]
        },
        "111170017": {
          "id": 111170017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Sin embargo, a mí no me corresponde tomar las decisiones aquí. Pero trataré de convencer a Himeko por ti.",
          "next": [
            111170018
          ]
        },
        "111170018": {
          "id": 111170018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Según mi experiencia, no debería tener muchas objeciones. Así que no te preocupes.",
          "next": [
            111170019,
            111170020
          ]
        },
        "111170019": {
          "id": 111170019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Gracias, Sr. Yang.",
          "next": [
            111170021
          ]
        },
        "111170020": {
          "id": 111170020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Qué bien! ¡De verdad lo aprecio, Sr. Yang!",
          "next": [
            111170021
          ]
        },
        "111170021": {
          "id": 111170021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "No hay de qué, es mi deber.",
          "next": []
        }
