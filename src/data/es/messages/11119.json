        },
        "111190018": {
          "id": 111190018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "¿Pero no parece un pequeño robot normal? Además...",
          "next": [
            111190021
          ]
        },
        "111190019": {
          "id": 111190019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "¿Eso crees?",
          "next": [
