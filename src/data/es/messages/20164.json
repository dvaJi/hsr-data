          ]
        },
        "201640003": {
          "id": 201640003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Renunciaste a tu trabajo?",
          "next": [
            201640004
          ]
        },
        "201640004": {
          "id": 201640004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "No. Estoy de vacaciones.",
