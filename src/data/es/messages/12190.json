        "121900008": {
          "id": 121900008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "A propósito... tu ID, Rich Sabelotodo, me resulta familiar. ¿Eres pariente de Richard Sabelotodo?",
