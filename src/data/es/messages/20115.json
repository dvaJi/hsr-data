      "messages": {
        "201150000": {
          "id": 201150000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "¿Tienes tiempo para hablar?",
          "next": [
            201150001,
            201150003
          ]
        },
        "201150001": {
          "id": 201150001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Claro. ¿De qué quieres hablar, Xueyi?",
          "next": [
            201150002
          ]
        },
        "201150002": {
          "id": 201150002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Parece que tienes tiempo. Genial.",
          "next": [
            201150006
          ]
        },
        "201150003": {
          "id": 201150003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Pensaba que eras una persona de pocas palabras porque siempre pareces distante.",
          "next": [
            201150004
          ]
        },
        "201150004": {
          "id": 201150004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Sí parezco distante.",
          "next": [
            201150005
          ]
        },
        "201150005": {
          "id": 201150005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Pero hablo.",
          "next": [
            201150006
          ]
        },
        "201150006": {
          "id": 201150006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Tengo algunas preguntas sobre la misión que acaba de terminar. Espero que puedas responderme.",
          "next": [
            201150007
          ]
        },
        "201150007": {
          "id": 201150007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "La Comisión de los Diez Líderes ha recibido noticias de que alguien publicó repetidamente en línea \"estoy poseído por Mara\".",
          "next": [
            201150008
          ]
        },
        "201150008": {
          "id": 201150008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Hay muchas publicaciones así.",
          "next": [
            201150009
          ]
        },
        "201150009": {
          "id": 201150009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Tal vez es un mensaje pidiendo ayuda.",
          "next": [
            201150010
          ]
        },
        "201150010": {
          "id": 201150010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Así que hice una investigación.",
          "next": [
            201150011,
            201150014
          ]
        },
        "201150011": {
          "id": 201150011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "En realidad no está poseído por Mara, ¿verdad?",
          "next": [
            201150012
          ]
        },
        "201150012": {
          "id": 201150012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "No.",
          "next": [
            201150013
          ]
        },
        "201150013": {
          "id": 201150013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Ni siquiera tiene los síntomas.",
          "next": [
            201150017
          ]
        },
        "201150014": {
          "id": 201150014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Creo que solo lo hizo de broma.",
          "next": [
            201150015
          ]
        },
        "201150015": {
          "id": 201150015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Parece que me encontré con muchas cabezas huecas.",
          "next": [
            201150016
          ]
        },
        "201150016": {
          "id": 201150016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "No está poseído por Mara. Ni siquiera tiene los síntomas.",
          "next": [
            201150017
          ]
        },
        "201150017": {
          "id": 201150017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Tengo tiempo libre ahora que ya me aseguré de que no está poseído por Mara.",
          "next": [
            201150018
          ]
        },
        "201150018": {
          "id": 201150018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Entonces, ¿por qué les dice a todos que sí? Me impresiona.",
          "next": [
            201150019,
            201150023
          ]
        },
        "201150019": {
          "id": 201150019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tal vez así puede hacer lo que quiera.",
          "next": [
            201150020
          ]
        },
        "201150020": {
          "id": 201150020,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "\"No te metas conmigo, porque estoy poseído por Mara\". Algo así.",
          "next": [
            201150021
          ]
        },
        "201150021": {
          "id": 201150021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Ya veo. Entiendo.",
          "next": [
            201150022
          ]
        },
        "201150022": {
          "id": 201150022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "En otras palabras, no quiere ser responsable de sus palabras y acciones.",
          "next": [
            201150027
          ]
        },
        "201150023": {
          "id": 201150023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Podría ser solo se está burlando de sí mismo, como una broma. He visto algo parecido antes.",
          "next": [
            201150024
          ]
        },
        "201150024": {
          "id": 201150024,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "\"¡Gracias, estuve a punto de ser poseído por Mara, pero no fue así!\". Algo así.",
          "next": [
            201150025
          ]
        },
        "201150025": {
          "id": 201150025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "¿Acaso es gracioso?",
          "next": [
            201150026
          ]
        },
        "201150026": {
          "id": 201150026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Parece que no tengo un buen sentido del humor.",
          "next": [
            201150027
          ]
        },
        "201150027": {
          "id": 201150027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Para mí, eso no es algo con lo que bromear.",
          "next": [
            201150028
          ]
        },
        "201150028": {
          "id": 201150028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Gracias. Aprendí mucho de esta conversación.",
          "next": []
        }
      },
