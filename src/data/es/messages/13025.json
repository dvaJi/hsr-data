  "sections": [
    {
      "id": 1302001,
      "startingMessageId": [130200101],
      "messages": {
        "130200101": {
          "id": 130200101,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Quiero recomendarles lo que hago para relajarme.",
          "next": [130200102]
        },
        "130200102": {
          "id": 130200102,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "¡Voy a la Dársena de astroesquifes, busco un lugar desde el que se vea volar a los astroesquifes y me quedo allí absorta!",
          "next": [130200103]
        },
        "130200103": {
          "id": 130200103,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "... También se puede hacer sentada en el Conde Insomne.",
          "next": [130200104]
        },
        "130200104": {
          "id": 130200104,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ver pasar a los astroesquifes...",
          "next": [130200105]
        },
        "130200105": {
          "id": 130200105,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Hace que el tiempo vuele sin darte cuenta.",
          "next": [130200106]
        },
        "130200106": {
          "id": 130200106,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Y te hace sentir muy calmada.",
          "next": [130200107]
        },
        "130200107": {
          "id": 130200107,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ni idea.",
          "next": [130200108]
        },
        "130200108": {
          "id": 130200108,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ni idea.",
          "next": [130200109, 130200112]
        },
        "130200109": {
          "id": 130200109,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ni idea.",
          "next": [130200110]
        },
        "130200110": {
          "id": 130200110,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ahora me preocupa tu salud mental, Huohuo.",
          "next": [130200111]
        },
        "130200111": {
          "id": 130200111,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Iremos a probarlo, pero cuando te preocupe algo nos lo tienes que contar, Huohuo.",
          "next": [130200117]
        },
        "130200112": {
          "id": 130200112,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Es una forma de relajarse, igual que escuchar el sonido de la lluvia.",
          "next": [130200113]
        },
        "130200113": {
          "id": 130200113,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Yo lo entiendo muy bien.",
          "next": [130200114]
        },
        "130200114": {
          "id": 130200114,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "¡Eso es!",
          "next": [130200115]
        },
        "130200115": {
          "id": 130200115,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Me preocupa la salud mental de ustedes dos.",
          "next": [130200116]
        },
        "130200116": {
          "id": 130200116,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Cuando les preocupe algo, nos lo tienen que contar.",
          "next": [130200117]
        },
        "130200117": {
          "id": 130200117,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ya llevo un buen rato aquí mirándolos, me siento en paz.",
          "next": [130200118]
        },
        "130200118": {
          "id": 130200118,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Oh, un momento.",
          "next": [130200119]
        },
        "130200119": {
          "id": 130200119,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿Eh?",
          "next": [130200120]
        },
        "130200120": {
          "id": 130200120,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "¿Qué pasa?",
          "next": [130200121]
        },
        "130200121": {
          "id": 130200121,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[Confusión]",
          "next": [130200122]
        },
        "130200122": {
          "id": 130200122,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ya estoy aquí.",
          "next": [130200123]
        },
        "130200123": {
          "id": 130200123,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Acaba de venir el Sr. Xiyan. Sin decir nada, me dio un puñado de golosinas y se marchó.",
          "next": [130200124]
        },
        "130200124": {
          "id": 130200124,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "¿Qué significa eso?",
          "next": [130200125, 130200126]
        },
        "130200125": {
          "id": 130200125,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Significa que eres adorable. Yo si estuviera allí, también te hubiera dado golosinas.",
          "next": [130200128]
        },
        "130200126": {
          "id": 130200126,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Le darías pena, siempre mirando los astroesquifes.",
          "next": [130200127]
        },
        "130200127": {
          "id": 130200127,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Todas las personas amables harían algo así.",
          "next": [130200128]
        },
        "130200128": {
          "id": 130200128,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "¿Es como cuando no puedes evitar acercarte al estanque y echar de comer a los peces?",
          "next": [130200129]
        },
        "130200129": {
          "id": 130200129,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "¿Eso no está prohibido?",
          "next": [130200130]
        },
        "130200130": {
          "id": 130200130,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Pero no está prohibido darle de comer a Huohuo.",
          "next": [130200131]
        },
        "130200131": {
          "id": 130200131,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ya veo.",
          "next": [130200132]
        },
        "130200132": {
          "id": 130200132,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "... Nos desviamos del tema, ¿no?",
          "next": [130200133]
        },
        "130200133": {
          "id": 130200133,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Yo también tengo golosinas, ¿dónde estás, Huohuo?",
          "next": [130200134]
        },
        "130200134": {
          "id": 130200134,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Yo también tengo golosinas, ¡vamos!",
          "next": [130200135]
        },
        "130200135": {
          "id": 130200135,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[Vamos]",
          "next": [130200136]
        },
        "130200136": {
          "id": 130200136,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "No hace falta que vengan, ya me fui.",
          "next": [130200137]
        },
        "130200137": {
          "id": 130200137,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ay...",
          "next": []
        }
      },
      "participatingContactsIds": [1217, 1210, 1206, 8001]
    }
  ]
}
