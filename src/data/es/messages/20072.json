        },
        "200720009": {
          "id": 200720009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Sin embargo, los habitantes de Xianzhou son tan hospitalarios que, aunque no compren, charlan un rato conmigo.",
          "next": [
            200720014
          ]
        },
        "200720010": {
          "id": 200720010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Has ido de compras?",
