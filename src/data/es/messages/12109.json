        "121090002": {
          "id": 121090002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Quién eres? Tu nombre no me suena.",
          "next": [
            121090003
          ]
        },
        "121090003": {
          "id": 121090003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 44,
          "text": "Qué mala memoria tienes. Bailu y tú me rescataron en el Vado de las Nubes.",
