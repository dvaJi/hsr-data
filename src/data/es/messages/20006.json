      "messages": {
        "200060000": {
          "id": 200060000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Encontré un antiguo registro de comunicaciones del Expreso, y una de las categorías se llama «Chistes malos».",
          "next": [200060001]
        },
        "200060001": {
          "id": 200060001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "No la entiendo muy bien, pero mi instinto me dice que tú deberías poder entenderla.",
          "next": [200060002, 200060004]
        },
        "200060002": {
          "id": 200060002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Te parezco ese tipo de persona?",
          "next": [200060003]
        },
        "200060003": {
          "id": 200060003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Sí.",
          "next": [200060006]
        },
        "200060004": {
          "id": 200060004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "No lo entiendo.",
          "next": [200060005]
        },
        "200060005": {
          "id": 200060005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Creo que aún no has visto este registro...",
          "next": [200060006]
        },
        "200060006": {
          "id": 200060006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Esto es lo que dice.",
          "next": [200060007]
        },
        "200060007": {
          "id": 200060007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "\"Haciendo guardia con Himeko. Está hablando en sueños\". Pom-Pom",
          "next": [200060008]
        },
        "200060008": {
          "id": 200060008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Hay otra entrada una hora después.",
          "next": [200060009]
        },
        "200060009": {
          "id": 200060009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "\"Haciendo guardia con Pom-Pom. Lleva casi una hora hablando en sueños\". Himeko",
          "next": [200060010]
        },
        "200060010": {
          "id": 200060010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "¿Qué opinas, {NICKNAME}?",
          "next": [200060011, 200060013]
        },
        "200060011": {
          "id": 200060011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "...",
          "next": [200060012]
        },
        "200060012": {
          "id": 200060012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Bien, parece que las categorías de las etiquetas se pueden mantener por ahora.",
          "next": [200060015]
        },
        "200060013": {
          "id": 200060013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Esto es claramente una historia de terror, ¿no?",
          "next": [200060014]
        },
        "200060014": {
          "id": 200060014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "¿Lo es? No la he entendido, pero parece que necesitamos una categoría adicional para ella.",
          "next": [200060015]
        },
        "200060015": {
          "id": 200060015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "El registro se detiene allí. Le pregunté a Himeko y a Pom-Pom. Ninguno de ellos recordaba nada.",
          "next": [200060016]
        },
        "200060016": {
          "id": 200060016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "\"¿Quién es el que habla en sueños?\". Parece que solo podemos esperar encontrar otras entradas de los archivos para corroborar esta información.",
          "next": [200060018]
        },
        "200060017": {
          "id": 200060017,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Me gusta]",
          "next": []
        },
        "200060018": {
          "id": 200060018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tiendo a creer que los archivos son omnipotentes.",
          "next": [200060017]
        }
      },
      "participatingContactsIds": [1002, 8001]
    }
  ]
}
