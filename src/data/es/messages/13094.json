      "messages": {
        "130940000": {
          "id": 130940000,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "La muñeca de mensajes te ha enviado una solicitud de mensaje.",
          "next": [
            130940001
          ]
        },
        "130940001": {
          "id": 130940001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "¡Estupendo!",
          "next": [
            130940002
          ]
        },
        "130940002": {
          "id": 130940002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Pienso que es estupendo, ¿y tú?",
          "next": [
            130940003,
            130940008,
            130940013
          ]
        },
        "130940003": {
          "id": 130940003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿? ¿Más bombas para desactivar?",
          "next": [
            130940004
          ]
        },
        "130940004": {
          "id": 130940004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Dame las coordenadas y desactivaré la bomba de inmediato.",
          "next": [
            130940005
          ]
        },
        "130940005": {
          "id": 130940005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Bueno, solo había 1000 bombas. La Gran Sparkle siempre cumple su palabra.",
          "next": [
            130940006
          ]
        },
        "130940006": {
          "id": 130940006,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "[Imagen] Emoji de máscara llorando",
          "next": [
            130940007
          ]
        },
        "130940007": {
          "id": 130940007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Solo estoy aquí para confesar sus verdaderos motivos, ¿entendido?",
          "next": [
            130940017,
            130940018
          ]
        },
        "130940008": {
          "id": 130940008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Claro...",
          "next": [
            130940009
          ]
        },
        "130940009": {
          "id": 130940009,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "No me envíes más mensajes. No quiero que Pom-Pom se lleve una idea equivocada.",
          "next": [
            130940010
          ]
        },
        "130940010": {
          "id": 130940010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "¿Quieres provocarme? ¡Pues mala suerte! ¡Estaré siempre pegada a ti como una goma de mascar que has pisado!",
          "next": [
            130940011
          ]
        },
        "130940011": {
          "id": 130940011,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "[Imagen] Emoji de sonrisa malévola",
          "next": [
            130940012
          ]
        },
        "130940012": {
          "id": 130940012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Además, ¡la Gran Sparkle aún no ha revelado el motivo de su fechoría! ¿No tienes curiosidad?",
          "next": [
            130940017,
            130940018
          ]
        },
        "130940013": {
          "id": 130940013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Fuegos artificiales! ¡Estupendo!",
          "next": [
            130940014
          ]
        },
        "130940014": {
          "id": 130940014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "¡Bien! Veo que vas aprendiendo.",
          "next": [
            130940015
          ]
        },
        "130940015": {
          "id": 130940015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "¿Quieres saber por qué la Gran Sparkle preparó todo esto?",
          "next": [
            130940016
          ]
        },
        "130940016": {
          "id": 130940016,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "[Imagen] Emoji de prender encendedor",
          "next": [
            130940017,
            130940018
          ]
        },
        "130940017": {
          "id": 130940017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "A ver, cuenta.",
          "next": [
            130940021
          ]
        },
        "130940018": {
          "id": 130940018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Me tengo que desconectar. Hasta la próxima.",
          "next": [
            130940019
          ]
        },
        "130940019": {
          "id": 130940019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "¡No! ¡No te desconectes! ¡Buaa!",
          "next": [
            130940020
          ]
        },
        "130940020": {
          "id": 130940020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "¡Es broma! De vuelta al tema...",
          "next": [
            130940021
          ]
        },
        "130940021": {
          "id": 130940021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "¿Recuerdas las \"tres muertes\" profetizadas?",
          "next": [
            130940022
          ]
        },
        "130940022": {
          "id": 130940022,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "¿Qué tiene que ver eso contigo?",
          "next": [
            130940023
          ]
        },
        "130940023": {
          "id": 130940023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "¿Quién sabe? Lo único que puedo decir es que alguien pagó una buena suma para que la Gran Sparkle viniera a Colonipenal a ser directora.",
          "next": [
            130940024
          ]
        },
        "130940024": {
          "id": 130940024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Parece que era una amistad que conoció en algún juego de red. El cliente le pidió a la Gran Sparkle que se asegurase de que esas \"tres muertes\" sucedieran de la forma más segura posible.",
          "next": [
            130940025
          ]
        },
        "130940025": {
          "id": 130940025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Como parte del trato, esa persona compartió su propio guion con la Gran Sparkle.",
          "next": [
            130940026
          ]
        },
        "130940026": {
          "id": 130940026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "En cuanto a lo que pasó después... con lo inteligente que eres, pelo gris, seguro que puedes adivinarlo.",
          "next": [
            130940027
          ]
        },
        "130940027": {
          "id": 130940027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Pero la Gran Sparkle no es una directora de esas obsesionadas con el control. Dejó que los actores decidieran ellos mismos cómo desarrollar la trama.",
          "next": [
            130940028
          ]
        },
        "130940028": {
          "id": 130940028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "¡Pregunta sorpresa! ¿Pulsaste el botón de destrucción garantizada de la Gran Sparkle?",
          "next": [
            130940029,
            130940031,
            130940034
          ]
        },
        "130940029": {
          "id": 130940029,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sí, lo pulsé como loc{F#a}{M#o}.",
          "next": [
            130940030
          ]
        },
        "130940030": {
          "id": 130940030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "¡Felicidades! ¡Tú mism{F#a}{M#o} reeditaste esa \"tercera muerte\"!",
          "next": [
            130940037
          ]
        },
        "130940031": {
          "id": 130940031,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "No, ese botón era demasiado sospechoso.",
          "next": [
            130940032
          ]
        },
        "130940032": {
          "id": 130940032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "¡Hum! Qué precavid{F#a}{M#o}.",
          "next": [
            130940033
          ]
        },
        "130940033": {
          "id": 130940033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Pero no pasa nada. Como más de la mitad de los actores presionaron el botón, ¡la tercera muerte se desarrolló como estaba planeado!",
          "next": [
            130940037
          ]
        },
        "130940034": {
          "id": 130940034,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Ni siquiera recibí el botón!",
          "next": [
            130940035
          ]
        },
        "130940035": {
          "id": 130940035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "¿En serio? Espero que puedas conseguir entradas la próxima vez.",
          "next": [
            130940036
          ]
        },
        "130940036": {
          "id": 130940036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Pero no pasa nada. Como más de la mitad de los actores presionaron el botón, ¡la tercera muerte se desarrolló como estaba planeado!",
          "next": [
            130940037
          ]
        },
        "130940037": {
          "id": 130940037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Ella ascendió a los cielos y prendió los fuegos artificiales más espectaculares en la historia de Colonipenal, un colofón digno del dulce sueño.",
          "next": [
            130940038
          ]
        },
        "130940038": {
          "id": 130940038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "¡Y ella está sana y salva! ¡Es un final perfecto!",
          "next": [
            130940039
          ]
        },
        "130940039": {
          "id": 130940039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Así que, felicidades, pelo gris. Después de todo, tú tomaste la decisión, y tú salvaste a esa chica luciérnaga.",
          "next": [
            130940040
          ]
        },
        "130940040": {
          "id": 130940040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "¡Y así la Gran Sparkle recibirá su paga según lo acordado!",
          "next": [
            130940041
          ]
        },
        "130940041": {
          "id": 130940041,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Oh.",
          "next": [
            130940042
          ]
        },
        "130940042": {
          "id": 130940042,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Por último, para celebrar que completaste tu tarea, la Gran Sparkle ha decidido contarte otro secreto, pelo gris.",
          "next": [
            130940043,
            130940044
          ]
        },
        "130940043": {
          "id": 130940043,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿De qué se trata?",
          "next": [
            130940045
          ]
        },
        "130940044": {
          "id": 130940044,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "De acuerdo, soy tod{F#a}{M#o} oídos.",
          "next": [
            130940045
          ]
        },
        "130940045": {
          "id": 130940045,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "¿Sabes?",
          "next": [
            130940046
          ]
        },
        "130940046": {
          "id": 130940046,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "En realidad...",
          "next": [
            130940047
          ]
        },
        "130940047": {
          "id": 130940047,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Al Dr. Primitivo le encantan los plátanos.",
          "next": [
            130940048,
            130940049,
            130940050
          ]
        },
        "130940048": {
          "id": 130940048,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Eh?",
          "next": [
            130940051
          ]
        },
        "130940049": {
          "id": 130940049,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Qué quieres decir?",
          "next": [
            130940051
          ]
        },
        "130940050": {
          "id": 130940050,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Y qué más?",
          "next": [
            130940051
          ]
        },
