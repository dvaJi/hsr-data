  "sections": [
    {
      "id": 2016500,
      "startingMessageId": [
        201650000
      ],
      "messages": {
        "201650000": {
          "id": 201650000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "¿Alguna vez has intentado asesinar a uno de tus superiores en el Expreso Astral?",
          "next": [
            201650001,
            201650006
          ]
        },
        "201650001": {
          "id": 201650001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Quiénes son mis superiores?",
          "next": [
            201650002
          ]
        },
        "201650002": {
          "id": 201650002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "No lo sé.",
          "next": [
            201650003
          ]
        },
        "201650003": {
          "id": 201650003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Somos una organización sin jerarquías.",
          "next": [
            201650004
          ]
        },
        "201650004": {
          "id": 201650004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Tenemos un revisor, pero no un líder.",
          "next": [
            201650005
          ]
        },
        "201650005": {
          "id": 201650005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Oh.",
          "next": [
            201650009
          ]
        },
        "201650006": {
          "id": 201650006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡¿Asesinar?! ¿Por qué asesinaría al revisor?",
          "next": [
            201650007
          ]
        },
        "201650007": {
          "id": 201650007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Como desafío.",
          "next": [
            201650008
          ]
        },
        "201650008": {
          "id": 201650008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Está bien.",
          "next": [
            201650009
          ]
        },
        "201650009": {
          "id": 201650009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Parece que tienen una relación muy simple entre ustedes.",
          "next": [
            201650010
          ]
        },
        "201650010": {
          "id": 201650010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "No se parece en nada a mi trabajo. Tenemos rencores, deudas de gratitud, superiores y subalternos, y amigos para almorzar.",
          "next": [
            201650011
          ]
        },
        "201650011": {
          "id": 201650011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Eso es demasiado complicado.",
          "next": [
            201650012
          ]
        },
        "201650012": {
          "id": 201650012,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Cuanto menos sepa, mejor.",
          "next": [
            201650013
          ]
        },
        "201650013": {
          "id": 201650013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "En cuanto a la venganza, la general Feixiao me permite desafiarla en cualquier momento que quiera.",
          "next": [
            201650014
          ]
        },
        "201650014": {
          "id": 201650014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Intenté ataques furtivos, trampas y duelos públicos, pero todos fracasaron.",
          "next": [
            201650015
          ]
        },
        "201650015": {
          "id": 201650015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Si no tienes experiencia, puedo informarle.",
          "next": [
            201650016
          ]
        },
        "201650016": {
          "id": 201650016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Informarle?",
          "next": [
            201650017
          ]
        },
        "201650017": {
          "id": 201650017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Informar a la general Feixiao de que no tienes experiencia asesinando a tus superiores.",
          "next": [
            201650018
          ]
        },
        "201650018": {
          "id": 201650018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Hoy fallé otra vez al intentar asesinar a la general. Ella me sugirió que hablara con otros para pensar en nuevas ideas.",
          "next": [
            201650019,
            201650020
          ]
        },
        "201650019": {
          "id": 201650019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Qué sugerencia tan audaz! Se merece mi respeto.",
          "next": [
            201650021
          ]
        },
        "201650020": {
          "id": 201650020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Es una locura! ¿Por qué aceptarías esa sugerencia?",
          "next": [
            201650021
          ]
        },
        "201650021": {
          "id": 201650021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Creo que aprender más es una buena idea.",
          "next": [
            201650022
          ]
        },
        "201650022": {
          "id": 201650022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Sin embargo, las técnicas de asesinato tienen que ver con la experiencia, no con la cantidad.",
          "next": [
            201650023
          ]
        },
        "201650023": {
          "id": 201650023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Aunque me dieras tu consejo, necesitaría perfeccionarlo yo mismo.",
          "next": [
            201650024
          ]
        },
        "201650024": {
          "id": 201650024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Gracias por tu respuesta. Le preguntaré a alguien más.",
          "next": []
        }
      },
      "participatingContactsIds": [
        1223,
        8001
      ]
    }
  ]
}