        "127040009": {
          "id": 127040009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Soy {F#una gran verificadora}{M#un gran verificador} e incluso soy {F#una miembro vitalicia}{M#un miembro vitalicio} de la Sociedad Secreta Hexanexus.",
          "next": [
            127040010
