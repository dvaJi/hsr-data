        "111180002": {
          "id": 111180002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "[Respuesta automática] Me encuentro conduciendo el Expreso. Por favor, deje un mensaje si es urgente.",
          "next": [111180003, 111180004]
        },
        "111180003": {
          "id": 111180003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Puedo ver que estás escribiendo...",
          "next": [111180005]
        },
        "111180004": {
          "id": 111180004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Por qué se van? ¡Aún no estoy en el Expreso!",
          "next": [111180006]
        },
        "111180005": {
          "id": 111180005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Rayos, ¡me han descubierto!",
          "next": [111180007]
        },
        "111180006": {
          "id": 111180006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¡Te tengo, pasajer{F#a}{M#o} {NICKNAME}!",
          "next": [111180007]
        },
        "111180007": {
          "id": 111180007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¡Es broma! Bueno, me dejaré de tonterías. ¿En qué te puedo ayudar, {NICKNAME}?",
          "next": [111180008, 111180009]
        },
        "111180008": {
          "id": 111180008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Nuestro Expreso acepta nuevos miembros?",
          "next": [111180010]
        },
        "111180009": {
          "id": 111180009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "En realidad, tengo una amiga...",
          "next": [111180010]
        },
        "111180010": {
          "id": 111180010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Oh, ¿quieres traer a tu amiga a bordo?",
          "next": [111180011]
        },
        "111180011": {
          "id": 111180011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Pero, cuéntame, ¿qué tipo de amiga es?",
          "next": [111180012]
        },
        "111180012": {
          "id": 111180012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Por favor, organiza tus pensamientos antes de responder, ¡quiero considerarlo adecuadamente!",
          "next": [111180013, 111180014, 111180015]
        },
        "111180013": {
          "id": 111180013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Es una mecánica talentosa con una mente meticulosa.",
          "next": [111180016]
        },
        "111180014": {
          "id": 111180014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Es una estrella del rock con una personalidad desinhibida.",
          "next": [111180017]
        },
        "111180015": {
          "id": 111180015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ella es... Humm, no puedo describirla exactamente.",
          "next": [111180018]
        },
        "111180016": {
          "id": 111180016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¡Oh, la señorita Himeko seguramente estará encantada!",
          "next": [111180019]
        },
        "111180017": {
          "id": 111180017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿Estrella de rock? ¿Puedes conseguirme su música?",
          "next": [111180019]
        },
        "111180018": {
          "id": 111180018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¡Olvídalo entonces! ¡Solo preguntaba de todos modos!",
          "next": [111180019]
        },
        "111180019": {
          "id": 111180019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "De todos modos, creo que no hay problema. Pero...",
          "next": [111180020]
        },
        "111180020": {
          "id": 111180020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¡La sala de máquinas está prohibida!",
          "next": [111180021]
        },
        "111180021": {
          "id": 111180021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¡No destrocen nada!",
          "next": [111180022]
        },
        "111180022": {
          "id": 111180022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "¿Crees que pueda hacerlo?",
          "next": [111180023, 111180024, 111180025]
        },
        "111180023": {
          "id": 111180023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Sin problemas!",
          "next": [111180026]
        },
        "111180024": {
          "id": 111180024,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Me encargaré de ello...",
          "next": [111180026]
        },
        "111180025": {
          "id": 111180025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "No creo que pueda controlarla.",
          "next": [111180026]
        },
        "111180026": {
          "id": 111180026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "En cualquier caso, ¡esas cosas están prohibidas! ¡Mientras eso se cumpla, no tengo ninguna objeción!",
          "next": [111180027, 111180028]
        },
        "111180027": {
          "id": 111180027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Entendido. Gracias, revisor.",
          "next": [111180029]
        },
        "111180028": {
          "id": 111180028,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¡Grandioso! ¡Eres el mejor!",
