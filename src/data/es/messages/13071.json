        "130710023": {
          "id": 130710023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "¡No seáis grosero, vaquero plateado!",
          "next": [
            130710024,
            130710025
          ]
        },
        "130710024": {
          "id": 130710024,
