        "200920002": {
          "id": 200920002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "¿Qué sueñas?",
          "next": [200920003, 200920004, 200920005]
        },
        "200920003": {
          "id": 200920003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sueño que destruyo el mundo.",
