        "201260000": {
          "id": 201260000,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Cubo de basura te ha enviado una solicitud de mensaje.",
          "next": [201260001]
        },
        "201260001": {
          "id": 201260001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 125,
          "text": "Hola.",
          "next": [201260002, 201260003, 201260004]
        },
        "201260002": {
          "id": 201260002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hola.",
          "next": [201260005]
        },
        "201260003": {
          "id": 201260003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Quién eres?",
          "next": [201260005]
        },
        "201260004": {
          "id": 201260004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿?",
          "next": [201260005]
        },
        "201260005": {
          "id": 201260005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 125,
          "text": "Así es, soy tu cubo de basura favorito.",
          "next": [201260006]
        },
        "201260006": {
          "id": 201260006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 125,
          "text": "Tu pasión ardiente prendió mi corazón de basura que había permanecido apagado durante tanto tiempo.",
          "next": [201260007, 201260009]
        },
        "201260007": {
          "id": 201260007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "¿Prender qué? ¿En serio? Ni que fuera un lanzallamas.",
          "next": [201260008]
        },
        "201260008": {
          "id": 201260008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 125,
          "text": "¡Cierto! ¡Hiciste que mi corazón de hielo se derritiera y que recuperara la energía que me impulsa a seguir viviendo!",
          "next": [201260011]
        },
        "201260009": {
          "id": 201260009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Un cubo de basura me manda mensajes para confesarme sus sentimientos... ¿Qué clase de artimaña es esta?",
          "next": [201260010]
        },
        "201260010": {
          "id": 201260010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 125,
          "text": "No deberías sorprenderte. El amor puede mover montañas y llenar océanos. El amor va más allá de las fronteras entre humanos y cubos de basura.",
          "next": [201260011]
        },
        "201260011": {
          "id": 201260011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 125,
          "text": "Ya no soy un cubo de basura como los demás.",
          "next": [201260012]
        },
        "201260012": {
          "id": 201260012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 126,
          "text": "Ahora soy un cubo de basura ardiente.",
          "next": [201260013]
        },
        "201260013": {
          "id": 201260013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 126,
          "text": "Bajo mi exterior frío como el acero... arde un corazón brillante y resplandeciente como fuegos artificiales.",
          "next": [201260014]
        },
        "201260014": {
          "id": 201260014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sparkle, ¿a qué juegas ahora?",
          "next": [201260015]
        },
        "201260015": {
          "id": 201260015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 126,
          "text": "¡No soy Sparkle! Solo soy...",
          "next": [201260016]
        },
        "201260016": {
          "id": 201260016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 127,
          "text": "... ¡Un cubo de basura con una pasión tan ardiente como el fuego!",
          "next": [201260017]
        },
        "201260017": {
          "id": 201260017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 127,
          "text": "No puedo creer que me confundieras con otra persona. Esto es decepcionante... ¡Eres una persona horrible!",
          "next": [201260018]
        },
        "201260018": {
          "id": 201260018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 128,
          "text": "Ya no creo en el amor... Adiós.",
          "next": [201260019, 201260020]
        },
        "201260019": {
          "id": 201260019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ensaya un poco más tu actuación a la próxima, a ver si cuela...",
          "next": [201260021]
        },
        "201260020": {
          "id": 201260020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Adiós...",
          "next": [201260021]
        },
