        },
        "200620009": {
          "id": 200620009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "就景元將軍身邊跟著的那個小孩子",
          "next": [200620010]
        },
        "200620010": {
          "id": 200620010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "將軍的護衛？",
