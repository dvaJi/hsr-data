        "200630000": {
          "id": 200630000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "今天真是尷尬死我了",
          "next": [200630001]
        },
        "200630001": {
          "id": 200630001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "本姑娘今天正準備去不夜侯聽書呢，結果你猜怎麼著",
          "next": [200630003]
        },
        "200630002": {
          "id": 200630002,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[疑惑]",
          "next": [200630004]
        },
        "200630003": {
          "id": 200630003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "怎麼了？",
          "next": [200630002]
        },
        "200630004": {
          "id": 200630004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "本姑娘碰見以前黌學的先生了！",
          "next": [200630005]
        },
        "200630005": {
          "id": 200630005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "沒想到他現在也在羅浮，真是孽緣一場…",
          "next": [200630006]
        },
        "200630006": {
          "id": 200630006,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[哭]",
          "next": [200630007, 200630012]
        },
        "200630007": {
          "id": 200630007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "碰見老師感覺是件好事？",
          "next": [200630008]
        },
        "200630008": {
          "id": 200630008,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "為什麼是孽緣？",
          "next": [200630009]
        },
        "200630009": {
          "id": 200630009,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[疑惑]",
          "next": [200630010]
        },
        "200630010": {
          "id": 200630010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "哎呀你不知道！",
          "next": [200630011]
        },
        "200630011": {
          "id": 200630011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "本姑娘讀書的時候，先生可不待見我了",
          "next": [200630019]
        },
        "200630012": {
          "id": 200630012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "黌學是什麼？先生又是什麼？",
          "next": [200630013]
        },
        "200630013": {
          "id": 200630013,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "我是外星人，不太懂",
          "next": [200630014]
        },
        "200630014": {
          "id": 200630014,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[疑惑]",
          "next": [200630015]
        },
        "200630015": {
          "id": 200630015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "啊，沒關係！本姑娘懂得也未必比你多",
          "next": [200630016]
        },
        "200630016": {
          "id": 200630016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "我查查用你們的話怎麼說啊，稍等",
          "next": [200630017]
        },
        "200630017": {
          "id": 200630017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "喔，有了",
          "next": [200630018]
        },
        "200630018": {
          "id": 200630018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "黌學就是學校，先生就是老師",
          "next": [200630019]
        },
        "200630019": {
          "id": 200630019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "哎，真不想回憶起來，太尷尬了",
          "next": [200630020]
        },
        "200630020": {
          "id": 200630020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "說來聽聽，讓我也尷尬一下",
          "next": [200630021]
        },
        "200630021": {
          "id": 200630021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "就我身在軍人世家嘛，我娘從小就狠抓我習武",
          "next": [200630022]
        },
        "200630022": {
          "id": 200630022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "每天大清早練武，練完了還得去黌學上課",
          "next": [200630023]
        },
        "200630023": {
          "id": 200630023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "但我早沒精力了，聽課根本聽不進去，就只好睡大覺了",
          "next": [200630024]
        },
        "200630024": {
          "id": 200630024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "今天他見著我就喊：「喲！這不是李大枕頭嗎！」",
          "next": [200630025]
        },
        "200630025": {
          "id": 200630025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "街上的人立馬全看向我，太尷尬了真是",
          "next": [200630026]
        },
        "200630026": {
          "id": 200630026,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[哭]",
          "next": [200630027]
        },
        "200630027": {
          "id": 200630027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "確實尷尬",
          "next": [200630028]
        },
        "200630028": {
          "id": 200630028,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[震驚]",
          "next": [200630029]
        },
        "200630029": {
          "id": 200630029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "唉，希望以後別再撞見他了…",
          "next": []
        }
