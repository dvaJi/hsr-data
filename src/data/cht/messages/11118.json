      "messages": {
        "111180000": {
          "id": 111180000,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "列車長快救救我呀——",
          "next": [111180002]
        },
        "111180002": {
          "id": 111180002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "[自動回覆]駕駛列車中，有事請留言！",
          "next": [111180003, 111180004]
        },
        "111180003": {
          "id": 111180003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "我都看到你正在輸入了…",
          "next": [111180005]
        },
        "111180004": {
          "id": 111180004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "怎麼要開走了？我還沒上車啊！",
          "next": [111180006]
        },
        "111180005": {
          "id": 111180005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "可惡，露餡了！",
          "next": [111180007]
        },
        "111180006": {
          "id": 111180006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "耶，騙到你了，{NICKNAME}乘客！",
          "next": [111180007]
        },
        "111180007": {
          "id": 111180007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "不開玩笑了，找我有什麼事嗎，{NICKNAME}乘客？",
          "next": [111180008, 111180009]
        },
        "111180008": {
          "id": 111180008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "請問我們的列車歡迎新成員嗎？",
          "next": [111180010]
        },
        "111180009": {
          "id": 111180009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "其實，我有個朋友…",
          "next": [111180010]
        },
        "111180010": {
          "id": 111180010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "喔，你想要帶朋友上車嗎？",
          "next": [111180011]
        },
        "111180011": {
          "id": 111180011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "那你說說看，這是個怎麼樣的朋友？",
          "next": [111180012]
        },
        "111180012": {
          "id": 111180012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "你先仔細想想再說，帕姆需要慎重考慮！",
          "next": [111180013, 111180014, 111180015]
        },
        "111180013": {
          "id": 111180013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "她是一位心思縝密的天才機械師",
          "next": [111180016]
        },
        "111180014": {
          "id": 111180014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "她是個性格灑脫的搖滾明星",
          "next": [111180017]
        },
        "111180015": {
          "id": 111180015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "她…我好像也不知道怎麼形容",
          "next": [111180018]
        },
        "111180016": {
          "id": 111180016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "喔！那麼姬子小姐一定會表示歡迎！",
          "next": [111180019]
        },
        "111180017": {
          "id": 111180017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "搖滾明星？可以給帕姆一張唱片嗎？",
          "next": [111180019]
        },
        "111180018": {
          "id": 111180018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "那就算了！其實帕姆也只是隨便問問！",
          "next": [111180019]
        },
        "111180019": {
          "id": 111180019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "總之，帕姆覺得沒問題！但是——",
          "next": [111180020]
        },
        "111180020": {
          "id": 111180020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "不准進引擎室！",
          "next": [111180021]
        },
        "111180021": {
          "id": 111180021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "不准搞破壞！",
          "next": [111180022]
        },
        "111180022": {
          "id": 111180022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "能做到嗎？",
          "next": [111180023, 111180024, 111180025]
        },
        "111180023": {
          "id": 111180023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "沒問題！",
          "next": [111180026]
        },
        "111180024": {
          "id": 111180024,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "我盡量說服她…",
          "next": [111180026]
        },
        "111180025": {
          "id": 111180025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "她應該很難控制",
          "next": [111180026]
        },
        "111180026": {
          "id": 111180026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "反正，這些事情不准！能做到，帕姆就沒意見！",
          "next": [111180027, 111180028]
        },
        "111180027": {
          "id": 111180027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "知道了，謝謝列車長",
          "next": [111180029]
        },
        "111180028": {
          "id": 111180028,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "好！列車長最棒啦——",
          "next": [111180029]
        },
        "111180029": {
          "id": 111180029,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "（發了一個萬能貼圖）",
          "next": []
        }
      },
