        "201440008": {
          "id": 201440008,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "不過，你也起床這麼早？",
          "next": [
            201440012
          ]
        },
        "201440009": {
          "id": 201440009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "我不用睡覺，沒想到吧？",
