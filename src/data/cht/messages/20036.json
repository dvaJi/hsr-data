        },
        "200360017": {
          "id": 200360017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "希望吧",
          "next": [200360020]
        },
        "200360018": {
          "id": 200360018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "很多人一輩子都沒看過雪",
