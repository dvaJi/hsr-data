      "id": 2004100,
      "startingMessageId": [200410000],
      "messages": {
        "200410000": {
          "id": 200410000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "{NICKNAME}，有一件事想與你討論",
          "next": [200410001]
        },
        "200410001": {
          "id": 200410001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "你有空嗎？",
          "next": [200410002]
        },
        "200410002": {
          "id": 200410002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "有空，你說",
          "next": [200410003]
        },
        "200410003": {
          "id": 200410003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "其實是我們朗道家的妹妹參加科學調查就要回來了",
          "next": [200410004]
        },
        "200410004": {
          "id": 200410004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "我想送她一件禮物，你覺得該送什麼好？",
          "next": [200410005, 200410014, 200410023]
        },
        "200410005": {
          "id": 200410005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "她喜歡什麼？",
          "next": [200410006]
        },
        "200410006": {
          "id": 200410006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "說來慚愧，我也不太清楚她的喜好",
          "next": [200410007]
        },
        "200410007": {
          "id": 200410007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "我只知道她非常熱愛實地考察的事業",
          "next": [200410008]
        },
        "200410008": {
          "id": 200410008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "啊，我有想法了！不如就送她新款的調查裝備吧",
          "next": [200410009]
        },
        "200410009": {
          "id": 200410009,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[讚]",
          "next": [200410010]
        },
        "200410010": {
          "id": 200410010,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "聽起來不錯啊",
          "next": [200410011]
        },
        "200410011": {
          "id": 200410011,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "只要她喜歡就好",
          "next": [200410012]
        },
        "200410012": {
          "id": 200410012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "你說得對！謝謝你，幫我釐清了想法",
          "next": [200410013]
        },
        "200410013": {
          "id": 200410013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "我這就去挑選適合的禮物，回頭見",
          "next": []
        },
        "200410014": {
          "id": 200410014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "可以試著買一束花給她",
          "next": [200410015]
        },
        "200410015": {
          "id": 200410015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "我也考慮過送花，但我總記得她對普通植物不感興趣",
          "next": [200410016]
        },
        "200410016": {
          "id": 200410016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "她喜歡雪原裡那種不常在城裡看到的植物",
          "next": [200410017]
        },
        "200410017": {
          "id": 200410017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "有了，我下次外出巡邏時，仔細留意一下吧",
          "next": [200410018]
        },
        "200410018": {
          "id": 200410018,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[讚]",
          "next": [200410019]
        },
        "200410019": {
          "id": 200410019,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "聽起來不錯啊",
          "next": [200410020]
        },
        "200410020": {
          "id": 200410020,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "但是注意安全",
          "next": [200410021]
        },
        "200410021": {
          "id": 200410021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "哈哈，謝謝你，我會的",
          "next": [200410022]
        },
        "200410022": {
          "id": 200410022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "那我先去忙其他事情了，回頭見",
          "next": []
        },
        "200410023": {
          "id": 200410023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "可以買件新衣服給她",
          "next": [200410024]
        },
        "200410024": {
          "id": 200410024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "我也考慮過買新衣服給她，可惜我不太懂穿搭，不敢亂買",
          "next": [200410025]
        },
        "200410025": {
          "id": 200410025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "姐姐倒是常幫她買些好看的衣服",
          "next": [200410026]
        },
        "200410026": {
          "id": 200410026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "但我不想讓希露瓦知道這件事",
          "next": [200410027]
        },
        "200410027": {
          "id": 200410027,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[拒絕]",
          "next": [200410028]
        },
        "200410028": {
          "id": 200410028,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "幹嘛這麼彆扭",
          "next": [200410029]
        },
        "200410029": {
          "id": 200410029,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "都是自己人，去問問就好了",
          "next": [200410030]
        },
        "200410030": {
          "id": 200410030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "……",
          "next": [200410031]
        },
        "200410031": {
          "id": 200410031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "你說得對，還是問問看吧，免得買錯了",
          "next": [200410032]
        },
        "200410032": {
          "id": 200410032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "謝謝你為我提供建議，我這就去找希露瓦，回頭見",
          "next": []
        }
      },
      "participatingContactsIds": [1104, 8001]
    }
