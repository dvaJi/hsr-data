  "sections": [
    {
      "id": 2000600,
      "startingMessageId": [200060000],
      "messages": {
        "200060000": {
          "id": 200060000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "在智庫中找到了一則很久之前的列車通訊日誌，其中有一個標籤分類是「冷笑話」。",
          "next": [200060001]
        },
        "200060001": {
          "id": 200060001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "雖然我不理解，不過直覺告訴我{NICKNAME}你應該能理解。",
          "next": [200060002, 200060004]
        },
        "200060002": {
          "id": 200060002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "在你眼裡，我就是這樣的人嗎？",
          "next": [200060003]
        },
        "200060003": {
          "id": 200060003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "是的。",
          "next": [200060006]
        },
        "200060004": {
          "id": 200060004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "我不能理解",
          "next": [200060005]
        },
        "200060005": {
          "id": 200060005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "我想你應該還沒看過這篇日誌…",
          "next": [200060006]
        },
        "200060006": {
          "id": 200060006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "日誌是這樣的。",
          "next": [200060007]
        },
        "200060007": {
          "id": 200060007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "「和姬子輪流值崗，她說夢話了。」——帕姆",
          "next": [200060008]
        },
        "200060008": {
          "id": 200060008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "同時還有另一筆紀錄，記錄於一小時後。",
          "next": [200060009]
        },
        "200060009": {
          "id": 200060009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "「和帕姆輪流值崗，它說了快一個小時的夢話了。」——姬子",
          "next": [200060010]
        },
        "200060010": {
          "id": 200060010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "{NICKNAME}，你怎麼看？",
          "next": [200060011, 200060013]
        },
        "200060011": {
          "id": 200060011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "……",
          "next": [200060012]
        },
        "200060012": {
          "id": 200060012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "好，看來目前的標籤分類可以保留。",
          "next": [200060015]
        },
        "200060013": {
          "id": 200060013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "明明是恐怖故事吧",
          "next": [200060014]
        },
        "200060014": {
          "id": 200060014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "這樣嗎？雖然我也沒有理解，但看來需要加一個標籤分類。",
          "next": [200060015]
        },
        "200060015": {
          "id": 200060015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "日誌就此戛然而止。我詢問了姬子和帕姆，他們都不記得後來的事了。",
          "next": [200060016]
        },
        "200060016": {
          "id": 200060016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "「說夢話的人到底是誰」——看來只能寄希望於在智庫的其他條目中找到佐證。",
          "next": [200060018]
        },
        "200060017": {
          "id": 200060017,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[讚]",
          "next": []
        },
        "200060018": {
          "id": 200060018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "我願稱智庫是萬能的",
          "next": [200060017]
        }
      },
      "participatingContactsIds": [1002, 8001]
    }
  ]
}
