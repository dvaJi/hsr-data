        },
        "201380014": {
          "id": 201380014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "廚師小帽是…",
          "next": [
            201380015
          ]
        },
        "201380015": {
          "id": 201380015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1309,
          "text": "是送給列車長的",
