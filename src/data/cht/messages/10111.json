      "messages": {
        "101110000": {
          "id": 101110000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "多虧你幫忙揪出了造謠者，我代我家小姐謝謝你，後面的事就交給我吧",
          "next": [101110001, 101110003]
        },
        "101110001": {
          "id": 101110001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "黑塔沒責罰艾絲妲吧？",
          "next": [101110002]
        },
        "101110002": {
          "id": 101110002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "你是真不瞭解黑塔女士啊——這件事她根本懶得管",
          "next": [101110005]
        },
        "101110003": {
          "id": 101110003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "真是艾絲妲自掏腰包的嗎？",
          "next": [101110004]
        },
        "101110004": {
          "id": 101110004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "你是真不知道啊…",
          "next": [101110005]
        },
        "101110005": {
          "id": 101110005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "這座太空站雖然是黑塔女士的私產，但整個建造過程，黑塔女士一毛錢也沒出",
          "next": [101110006]
        },
        "101110006": {
          "id": 101110006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "小姐家跟「公司」有點關係，所以她…很有錢，現在太空站裡的大部分設施，基本上都是小姐出資的…",
          "next": [101110007]
        },
        "101110007": {
          "id": 101110007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "所以買那些東西的費用…對小姐來說根本不算什麼",
          "next": [101110008, 101110010, 101110013]
        },
        "101110008": {
          "id": 101110008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "「星際和平公司」到底是什麼？",
          "next": [101110009]
        },
        "101110009": {
          "id": 101110009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "這個…你就當作是這宇宙間最…富有的組織",
          "next": [101110015]
        },
        "101110010": {
          "id": 101110010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "現在抱小姐大腿還來得及嗎？",
          "next": [101110011]
        },
        "101110011": {
          "id": 101110011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "…請不要開這種玩笑，我會當真的",
          "next": [101110012]
        },
        "101110012": {
          "id": 101110012,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "…",
          "next": [101110015]
        },
        "101110013": {
          "id": 101110013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "理查你們打算怎麼處理？",
          "next": [101110014]
        },
        "101110014": {
          "id": 101110014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "…我覺得，或許你不會想知道",
          "next": [101110015]
        },
        "101110015": {
          "id": 101110015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "總之，小姐的事，謝謝你費心了",
          "next": []
        }
      },
      "participatingContactsIds": [1008, 8001]
    }
  ]
}
