        },
        "200970006": {
          "id": 200970006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "不好意思，不買茶葉、不買保險，什麼都不買",
          "next": [
            200970007
          ]
        },
        "200970007": {
          "id": 200970007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
