      "messages": {
        "200590000": {
          "id": 200590000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Oh ya, ada yang ingin kumintai tolong.",
          "next": [200590001]
        },
        "200590001": {
          "id": 200590001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Aku pergi mendengar cerita Tuan Xiyan waktu pulang kerja, tapi dia berhenti di puncak cerita. Aku tidak bisa menahannya.",
          "next": [200590002]
        },
        "200590002": {
          "id": 200590002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Dia akan melanjutkannya besok di pagi hari.",
          "next": [200590003]
        },
        "200590003": {
          "id": 200590003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Aku tidak tahu apa aku masih sempat mengejarnya setelah pulang kerja.",
          "next": [200590004]
        },
        "200590004": {
          "id": 200590004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Kalau tidak sempat, bisakah kamu membantuku mendengarnya dulu?",
          "next": [200590006, 200590009]
        },
        "200590005": {
          "id": 200590005,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Suka]",
          "next": [200590007]
        },
        "200590006": {
          "id": 200590006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Boleh.",
          "next": [200590005]
        },
        "200590007": {
          "id": 200590007,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Tapi aku belum mendengar yang hari ini, memangnya aku bisa paham cerita besok?",
          "next": [200590011]
        },
        "200590008": {
          "id": 200590008,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Menangis]",
          "next": [200590010]
        },
        "200590009": {
          "id": 200590009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku besok sedikit sibuk.",
          "next": [200590008]
        },
        "200590010": {
          "id": 200590010,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Aku belum mendengar cerita yang hari ini. Aku tidak akan bisa memahami cerita besok.",
          "next": [200590011]
        },
        "200590011": {
          "id": 200590011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Oh, hari ini adalah cerita tentang Cloudstrider yang mempunyai sebuah kereta.",
          "next": [200590012]
        },
        "200590012": {
          "id": 200590012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Keretanya mirip dengan kapal Xianzhou yang bisa berkelana antarbintang.",
          "next": [200590013]
        },
        "200590013": {
          "id": 200590013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kok terdengar familier ya ....",
          "next": [200590014]
        },
        "200590014": {
          "id": 200590014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Iya kan?",
          "next": [200590015]
        },
        "200590015": {
          "id": 200590015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Karena itu aku meminta bantuanmu.",
          "next": [200590016]
        },
        "200590016": {
          "id": 200590016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Tuan Xiyan bilang, Cloudstrider pernah singgah di planet bernama Harilo.",
          "next": [200590017]
        },
        "200590017": {
          "id": 200590017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Tapi dia terjerat dalam konspirasi besar dan bertarung dengan robot emas raksasa.",
          "next": [200590018]
        },
        "200590018": {
          "id": 200590018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Jangan beri tahu aku kelanjutan ceritanya. Beri tahu aku versi Tuan Xiyan saja besok.",
          "next": [200590019, 200590020]
        },
        "200590019": {
          "id": 200590019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Di mana itu Harilo?",
          "next": [200590021]
        },
        "200590020": {
          "id": 200590020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Apa itu robot emas raksasa?",
          "next": [200590021]
        },
        "200590021": {
          "id": 200590021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "....",
          "next": [200590022]
        },
        "200590022": {
          "id": 200590022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Kamu tidak tahu?",
          "next": [200590023]
        },
        "200590023": {
          "id": 200590023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Baiklah, sepertinya aku harus pergi sendiri besok agar bisa tahu kelanjutannya.",
          "next": [200590024]
        },
        "200590024": {
          "id": 200590024,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku akan menceritakan cerita versiku lain kali.",
          "next": [200590025]
        },
        "200590025": {
          "id": 200590025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Baik!",
