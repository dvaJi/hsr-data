      "messages": {
        "130940000": {
          "id": 130940000,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Boneka \"Pesan\" memulai percakapan denganmu.",
          "next": [
            130940001
          ]
        },
        "130940001": {
          "id": 130940001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Asyik~",
          "next": [
            130940002
          ]
        },
        "130940002": {
          "id": 130940002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Seru banget ya tadi, gimana perasaan kalian?",
          "next": [
            130940003,
            130940008,
            130940013
          ]
        },
        "130940003": {
          "id": 130940003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Masih ada yang lebih hebat lagi?",
          "next": [
            130940004
          ]
        },
        "130940004": {
          "id": 130940004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Kirim koordinatnya. Aku langsung datang buat jinakkan bom.",
          "next": [
            130940005
          ]
        },
        "130940005": {
          "id": 130940005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Bomnya memang cuma ada 1.000, Nona Sparkle tidak pernah bohong~",
          "next": [
            130940006
          ]
        },
        "130940006": {
          "id": 130940006,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "[Gambar] Stiker topeng lagi menangis.",
          "next": [
            130940007
          ]
        },
        "130940007": {
          "id": 130940007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Aku cuma mewakili Nona Sparkle untuk mengakui motif kejahatannya. Kamu paham tidak?",
          "next": [
            130940017,
            130940018
          ]
        },
        "130940008": {
          "id": 130940008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ayolah!",
          "next": [
            130940009
          ]
        },
        "130940009": {
          "id": 130940009,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Jangan kirim pesan lagi. Aku tidak mau Pom-Pom salah paham.",
          "next": [
            130940010
          ]
        },
        "130940010": {
          "id": 130940010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Kamu berani macam-macam sama aku? Yah, anggap saja kamu injak permen karet di jalanan, sekarang aku bakal tempeli kamu selamanya!",
          "next": [
            130940011
          ]
        },
        "130940011": {
          "id": 130940011,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "[Gambar] Ekspresi senyum licik.",
          "next": [
            130940012
          ]
        },
        "130940012": {
          "id": 130940012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Lagi pula, Nona Sparkle belum akui motif kejahatannya! Memangnya kamu tidak penasaran ya?",
          "next": [
            130940017,
            130940018
          ]
        },
        "130940013": {
          "id": 130940013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kembang api seru banget!",
          "next": [
            130940014
          ]
        },
        "130940014": {
          "id": 130940014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Bagus! Sepertinya kita sudah sepaham nih~",
          "next": [
            130940015
          ]
        },
        "130940015": {
          "id": 130940015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Nah, apa kamu mau tahu kenapa Nona Sparkle sengaja siapkan pertunjukan itu?",
          "next": [
            130940016
          ]
        },
        "130940016": {
          "id": 130940016,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "[Gambar] Stiker korek api.",
          "next": [
            130940017,
            130940018
          ]
        },
        "130940017": {
          "id": 130940017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Coba jelaskan.",
          "next": [
            130940021
          ]
        },
        "130940018": {
          "id": 130940018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku sudah mau pergi. Lain kali saja ya.",
          "next": [
            130940019
          ]
        },
        "130940019": {
          "id": 130940019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Jangan pergi dong! Aku mohon huhuhu ....",
          "next": [
            130940020
          ]
        },
        "130940020": {
          "id": 130940020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Hehe, bercanda~ Sekarang serius ya.",
          "next": [
            130940021
          ]
        },
        "130940021": {
          "id": 130940021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Kamu masih ingat \"tiga kali kematian\" tidak?",
          "next": [
            130940022
          ]
        },
        "130940022": {
          "id": 130940022,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Apa hubungannya sama kamu?",
          "next": [
            130940023
          ]
        },
        "130940023": {
          "id": 130940023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Siapa yang tahu? Aku cuma bisa kasih tahu kalau ada orang yang bayar harga tinggi untuk undang Nona Sparkle ke Penacony untuk jadi \"sutradara\".",
          "next": [
            130940024
          ]
        },
        "130940024": {
          "id": 130940024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Sepertinya sih seorang kenalan waktu main game. Orang itu terus ingatkan Nona Sparkle supaya \"tiga kali kematian\" ini terjadi dengan cara yang paling aman.",
          "next": [
            130940025
          ]
        },
        "130940025": {
          "id": 130940025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Sebagai gantinya, orang itu juga bagikan \"naskah\" miliknya dengan Nona Sparkle.",
          "next": [
            130940026
          ]
        },
        "130940026": {
          "id": 130940026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Dan yang terjadi setelahnya ... Rambut-Abu-Abu kan pintar banget nih, kamu pasti sudah bisa tebak kelanjutannya~",
          "next": [
            130940027
          ]
        },
        "130940027": {
          "id": 130940027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Tapi karena Nona Sparkle bukan tipe sutradara yang suka atur-atur orang lain, dia biarkan aktornya sendiri yang tentukan alur ceritanya.",
          "next": [
            130940028
          ]
        },
        "130940028": {
          "id": 130940028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Inspeksi dadakan! Kamu tekan Tombol \"Penghancur Dua Arah\" dari Nona Sparkle tidak?",
          "next": [
            130940029,
            130940031,
            130940034
          ]
        },
        "130940029": {
          "id": 130940029,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Yoi, sudah kutekan berkali-kali.",
          "next": [
            130940030
          ]
        },
        "130940030": {
          "id": 130940030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Kalau begitu, selamat ya! Kamu berhasil menulis ulang \"kematian yang ketiga kali\" ini!",
          "next": [
            130940037
          ]
        },
        "130940031": {
          "id": 130940031,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Terlalu mencurigakan ah! Mana mungkin aku tekan!",
          "next": [
            130940032
          ]
        },
        "130940032": {
          "id": 130940032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Hmm. Ternyata kamu hati-hati juga.",
          "next": [
            130940033
          ]
        },
        "130940033": {
          "id": 130940033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Tapi, tidak apa-apa~ Karena jumlah aktor yang menekan tombol sudah lebih dari setengahnya, \"kematian yang ketiga kali\" masih dipentaskan sesuai jadwal!",
          "next": [
            130940037
          ]
        },
        "130940034": {
          "id": 130940034,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku sama sekali tidak terima tombol apa-apa!",
          "next": [
            130940035
          ]
        },
        "130940035": {
          "id": 130940035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Oh ya? Semoga lain kali kamu berhasil dapat tiketnya ya!",
          "next": [
            130940036
          ]
        },
        "130940036": {
          "id": 130940036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Tapi, tidak apa-apa~ Karena jumlah aktor yang menekan tombol sudah lebih dari setengahnya, \"kematian yang ketiga kali\" masih dipentaskan sesuai jadwal!",
          "next": [
            130940037
          ]
        },
        "130940037": {
          "id": 130940037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Dia terbang tinggi dan menyalakan kembang api yang paling terang di sepanjang sejarah Penacony, membawa penutup untuk mimpi indah ini.",
          "next": [
            130940038
          ]
        },
        "130940038": {
          "id": 130940038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Dan dia sendiri pun dalam keadaan sehat walafiat. Benar-benar akhir yang paling sempurna!",
          "next": [
            130940039
          ]
        },
        "130940039": {
          "id": 130940039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Jadi, selamat ya, Rambut Abu-Abu. Karena kamu yang ambil keputusan, orang yang selamatkan gadis kunang-kunang juga kamu.",
          "next": [
            130940040
          ]
        },
        "130940040": {
          "id": 130940040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Dengan begini, Nona Sparkle juga bisa dapat bayaran sesuai yang sudah dijanjikan~",
          "next": [
            130940041
          ]
        },
        "130940041": {
          "id": 130940041,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Oh.",
          "next": [
            130940042
          ]
        },
        "130940042": {
          "id": 130940042,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Yang terakhir, Nona Sparkle mau kasih tahu kamu satu rahasia lagi untuk rayakan keberhasilan Rambut-Abu-Abu dalam menyelesaikan pertunjukan ini dengan sempurna.",
          "next": [
            130940043,
            130940044
          ]
        },
        "130940043": {
          "id": 130940043,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Rahasia apa tuh?",
          "next": [
            130940045
          ]
        },
        "130940044": {
          "id": 130940044,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Oke, telingaku sudah siap nih.",
          "next": [
            130940045
          ]
        },
        "130940045": {
          "id": 130940045,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Kamu tahu tidak?",
          "next": [
            130940046
          ]
        },
        "130940046": {
          "id": 130940046,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Sebenarnya ...",
          "next": [
            130940047
          ]
        },
        "130940047": {
          "id": 130940047,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Dr. Primitive suka makan pisang.",
          "next": [
            130940048,
            130940049,
            130940050
          ]
        },
        "130940048": {
          "id": 130940048,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hah?",
          "next": [
            130940051
          ]
        },
        "130940049": {
          "id": 130940049,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Apa maksudnya?",
          "next": [
            130940051
          ]
        },
        "130940050": {
          "id": 130940050,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Terus?",
          "next": [
