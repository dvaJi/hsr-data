        },
        "122330011": {
          "id": 122330011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Kalau begitu, biar kuberikan satu saran pribadi: Kurangi menonton video-video blogger makanan.",
