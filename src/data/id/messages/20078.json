      "messages": {
        "200780000": {
          "id": 200780000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Hai, {NICKNAME}.",
          "next": [200780001, 200780003]
        },
        "200780001": {
          "id": 200780001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kenapa? Apa yang terjadi?",
          "next": [200780002]
        },
        "200780002": {
          "id": 200780002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Tidak kenapa-kenapa, cuma tiba-tiba teringat kamu saja.",
          "next": [200780005]
        },
        "200780003": {
          "id": 200780003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Lagi-lagi kamu mengirim pesan padaku.",
          "next": [200780004]
        },
        "200780004": {
          "id": 200780004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Iya, kenapa? Kamu tidak suka ya?",
          "next": [200780005]
        },
        "200780005": {
          "id": 200780005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Aku tidak menyangka, di dalam bioskop cuma ada aku seorang.",
          "next": [200780006]
        },
        "200780006": {
          "id": 200780006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Aku kira akan ada banyak orang yang suka film ini.",
          "next": [200780007]
        },
        "200780007": {
          "id": 200780007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Judulnya \"Out of the Past\"",
          "next": [200780008, 200780011]
        },
        "200780008": {
          "id": 200780008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kamu tidak pergi menonton bersama dengan yang lainnya?",
          "next": [200780009]
        },
        "200780009": {
          "id": 200780009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Kalau yang kamu maksud itu adalah anggota Stellaron Hunter yang lainnya ....",
          "next": [200780010]
        },
        "200780010": {
          "id": 200780010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Kami memliki hobi dan kesukaan yang berbeda-beda, aku tidak bisa memaksa mereka.",
          "next": [200780013]
        },
        "200780011": {
          "id": 200780011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Lain kali ayo kita nonton bareng film itu.",
          "next": [200780012]
        },
        "200780012": {
          "id": 200780012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Oke, kali ini kamu jangan sampai lupa ya.",
          "next": [200780013]
        },
        "200780013": {
          "id": 200780013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "{NICKNAME}, kamu suka film apa?",
          "next": [200780014]
        },
        "200780014": {
          "id": 200780014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Yang sekarang kamu suka.",
          "next": [200780015, 200780019, 200780022]
        },
        "200780015": {
          "id": 200780015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Film biografi.",
          "next": [200780016]
        },
        "200780016": {
          "id": 200780016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Aku juga suka film seperti itu, rasanya seperti sedang membaca dan mengamati orang lain.",
          "next": [200780017]
        },
        "200780017": {
          "id": 200780017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Tapi, semoga saja tidak ada orang yang membuat film tentang diriku ini.",
          "next": [200780018]
        },
        "200780018": {
          "id": 200780018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Aku tidak ingin ditampilkan di hadapan orang-orang hanya dengan modal satu perspektif dari seorang sutradara.",
          "next": [200780025]
        },
        "200780019": {
          "id": 200780019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Film aksi.",
          "next": [200780020]
        },
        "200780020": {
          "id": 200780020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Adegan di film-film seperti itu, aku biasanya mengalaminya langsung.",
          "next": [200780021]
        },
        "200780021": {
          "id": 200780021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Kalau untuk orang lain, film seperti itu mungkin seru. Tapi kalau untukku ... agak kurang.",
          "next": [200780025]
        },
        "200780022": {
          "id": 200780022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Film sci-fi.",
          "next": [200780023]
        },
        "200780023": {
          "id": 200780023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Meskipun film itu cerita dengan latar masa depan.",
          "next": [200780024]
        },
        "200780024": {
          "id": 200780024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Tapi apa yang terjadi di masa lalu biasanya akan selalu terulang di masa depan.",
          "next": [200780025]
        },
        "200780025": {
          "id": 200780025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Oke deh, filmnya sudah mau dimulai.",
          "next": [200780026]
        },
        "200780026": {
          "id": 200780026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Sudahan dulu ya kalau begitu, dadah~",
          "next": [200780027, 200780028]
        },
        "200780027": {
          "id": 200780027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ya sudah sana.",
          "next": [200780029]
        },
        "200780028": {
          "id": 200780028,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Dadah.",
          "next": [200780029]
        },
        "200780029": {
