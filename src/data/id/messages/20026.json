  "sections": [
    {
      "id": 2002600,
      "startingMessageId": [200260000],
      "messages": {
        "200260000": {
          "id": 200260000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "{NICKNAME}, kamu tahu Peppy belakangan main ke mana tidak?",
          "next": [200260001, 200260002]
        },
        "200260001": {
          "id": 200260001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Maaf, aku belakangan sibuk dan tidak memerhatikan Peppy.",
          "next": [200260003]
        },
        "200260002": {
          "id": 200260002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Peppy? Kurasa dia cuma berkeliaran di sekitar Stasiun Luar Angkasa. Bisa ke mana lagi dia?",
          "next": [200260003]
        },
        "200260003": {
          "id": 200260003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ada apa?",
          "next": [200260004]
        },
        "200260004": {
          "id": 200260004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Aku sedang menunggu hasil pemeriksaan fisik Peppy dari Departmen Medis.",
          "next": [200260005]
        },
        "200260005": {
          "id": 200260005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Kaki depannya sedikit cacat, tapi tidak ada luka yang terlihat jelas.",
          "next": [200260006]
        },
        "200260006": {
          "id": 200260006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Aku sudah memeriksa kamera pengawas Stasiun Luar Angkasa dan tidak melihat bagaimana kakinya terluka.",
          "next": [200260007, 200260009]
        },
        "200260007": {
          "id": 200260007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Binatang peliharaan memang nakal seperti anak-anak. Apa dia berpura-pura cacat?",
          "next": [200260008]
        },
        "200260008": {
          "id": 200260008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Tidak tahu ... Peppy tidak nakal kok.",
          "next": [200260011]
        },
        "200260009": {
          "id": 200260009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Dari deskripsimu, mungkin Peppy juga sama seperti Arlan yang tangannya terluka?",
          "next": [200260010]
        },
        "200260010": {
          "id": 200260010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Itu tidak masuk akal. Peppy tidak ada luka-luka kok.",
          "next": [200260011]
        },
        "200260011": {
          "id": 200260011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "....",
          "next": [200260012]
        },
        "200260012": {
          "id": 200260012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Aku sudah mendapatkan hasil tesnya.",
          "next": [200260013]
        },
        "200260013": {
          "id": 200260013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Dia sehat.",
          "next": [200260014]
        },
        "200260014": {
          "id": 200260014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Dia juga tidak terluka.",
          "next": [200260015]
        },
        "200260015": {
          "id": 200260015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Dan tidak ada tanda-tanda patah tulang.",
          "next": [200260016]
        },
        "200260016": {
          "id": 200260016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Jadi kenapa Peppy berpura-pura cacat? Kenapa dia seperti itu?",
          "next": [200260017]
        },
        "200260017": {
          "id": 200260017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Aku tidak paham sama sekali.",
          "next": [200260018, 200260022]
        },
        "200260018": {
          "id": 200260018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kamu belakangan sibuk. Kamu tidak banyak main dong sama dia?",
          "next": [200260019]
        },
        "200260019": {
          "id": 200260019,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Jadi dia berpura-pura cacat dan berharap kamu menghabiskan banyak waktu dengannya.",
          "next": [200260020]
        },
        "200260020": {
          "id": 200260020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Mungkin saja. Aku sudah lama tidak membawa Peppy jalan-jalan.",
          "next": [200260021]
        },
        "200260021": {
          "id": 200260021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Aku akan membeli beberapa stik gigi favorit dia.",
          "next": [200260026]
        },
        "200260022": {
          "id": 200260022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mungkin dia tanpa sengaja meniru Arlan.",
          "next": [200260023]
        },
        "200260023": {
          "id": 200260023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Tangan Arlan terluka saat Legion menyusup ke Stasiun Luar Angkasa.",
          "next": [200260024]
        },
        "200260024": {
          "id": 200260024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Peppy selalu bersamanya waktu itu.",
          "next": [200260025]
        },
        "200260025": {
          "id": 200260025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Kamu benar. Dia mungkin meniru kebiasaan orang-orang di sekitar dia.",
          "next": [200260026]
        },
        "200260026": {
          "id": 200260026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Yang pasti Peppy tidak terluka.",
          "next": [200260027]
        },
        "200260027": {
          "id": 200260027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Si kecil itu cukup pintar.",
          "next": [200260028]
        },
        "200260028": {
          "id": 200260028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Bagaimana ya bilangnya ....",
          "next": [200260029]
        },
        "200260029": {
          "id": 200260029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Peliharaan itu sama seperti tuannya.",
          "next": []
        }
      },
