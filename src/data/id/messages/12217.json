        },
        "122170004": {
          "id": 122170004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Maksudmu kondisiku? Aku baik-baik saja.",
          "next": [122170005]
        },
        "122170005": {
          "id": 122170005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Hm ....",
          "next": [122170009]
        },
        "122170006": {
          "id": 122170006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Maksudmu kondisi mereka? Mereka baik-baik saja.",
          "next": [122170007]
        },
        "122170007": {
          "id": 122170007,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Mereka hidup dengan baik di Stasiun Luar Angkasa. Selain merindukanmu, mereka tidak ada masalah lain lagi.",
          "next": [122170008]
        },
        "122170008": {
          "id": 122170008,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "[Menghela Napas]",
