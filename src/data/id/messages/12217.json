        "122170002": {
          "id": 122170002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Tak disangka mereka akan membuatku terkejut seperti ini. Waktu aku membudidayakan mereka, aku tidak menyangka hal seperti ini akan terjadi.",
          "next": [122170003]
        },
        "122170003": {
          "id": 122170003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Kamu yang merawat mereka sekarang? Bagaimana kondisi akhir-akhir ini?",
