      "messages": {
        "201340000": {
          "id": 201340000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Maaf, aku baru lihat ada pesan masuk.",
          "next": [
            201340001
          ]
        },
        "201340001": {
          "id": 201340001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Karena misiku di Penacony sudah selesai, aku akhirnya bisa pakai ponsel terang-terangan.",
          "next": [
            201340002
          ]
        },
        "201340002": {
          "id": 201340002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Kemarin rasanya seperti dikurung di penjara.",
          "next": [
            201340003,
            201340005,
            201340007
          ]
        },
        "201340003": {
          "id": 201340003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku lega lihat kamu baik-baik saja.",
          "next": [
            201340004
          ]
        },
        "201340004": {
          "id": 201340004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Haha. Kamu perhatian sekali deh.",
          "next": [
            201340009
          ]
        },
        "201340005": {
          "id": 201340005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Memangnya kamu ke mana saja?",
          "next": [
            201340006
          ]
        },
        "201340006": {
          "id": 201340006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Ceritanya panjang, kita lanjut lain hari saja ya.",
          "next": [
            201340009
          ]
        },
        "201340007": {
          "id": 201340007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Semoga kamu tenang di alam baka. Akhirnya selesai juga ya.",
          "next": [
            201340008
          ]
        },
        "201340008": {
          "id": 201340008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Haha. Ini baru mirip kamu yang biasanya.",
          "next": [
            201340009
          ]
        },
        "201340009": {
          "id": 201340009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Mumpung lagi santai, bagaimana kalau kita main dadu supaya indra kita tidak tumpul?",
          "next": [
            201340010
          ]
        },
        "201340010": {
          "id": 201340010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Kalau kamu melempar angka yang sama atau lebih besar dari angkaku, aku akan kasih tahu apa yang terjadi setelah insiden di taman hiburan.",
          "next": [
            201340011,
            201340054
          ]
        },
        "201340011": {
          "id": 201340011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Siapa takut? Ayo mulai!",
          "next": [
            201340012
          ]
        },
        "201340012": {
          "id": 201340012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Baiklah, mari kita mulai.",
          "next": [
            201340013
          ]
        },
        "201340013": {
          "id": 201340013,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "[Aventurine] melempar angka 3.",
          "next": [
            201340014
          ]
        },
        "201340014": {
          "id": 201340014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Giliran kamu. Sepertinya kamu punya peluang menang yang tinggi.",
          "next": [
            201340015,
            201340022
          ]
        },
        "201340015": {
          "id": 201340015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ah, aku mah santai.",
          "next": [
            201340016
          ]
        },
        "201340016": {
          "id": 201340016,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "[{NICKNAME}] melempar angka 2.",
          "next": [
            201340017
          ]
        },
        "201340017": {
          "id": 201340017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Mau lempar ulang? Tidak ada peraturan tidak boleh mengulang kok.",
          "next": [
            201340018
          ]
        },
        "201340018": {
          "id": 201340018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Oke, lempar ulang deh.",
          "next": [
            201340019
          ]
        },
        "201340019": {
          "id": 201340019,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "[{NICKNAME}] melempar angka 6.",
