  "relatedMessages": [20070, 20071, 20072],
  "sections": [
    {
      "id": 2007100,
      "startingMessageId": [200710000],
      "messages": {
        "200710000": {
          "id": 200710000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "{NICKNAME}, waktu kamu ke Luofu, kamu menginap di mana? Bagaimana perasaanmu?",
          "next": [200710001]
        },
        "200710001": {
          "id": 200710001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Tunggu, biar kutebak.",
          "next": [200710002]
        },
        "200710002": {
          "id": 200710002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Petrichor Inn?",
          "next": [200710003, 200710007]
        },
        "200710003": {
          "id": 200710003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Iya, aku menginap di Petrichor Inn.",
          "next": [200710004]
        },
        "200710004": {
          "id": 200710004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Kamar premium suite, bagus banget.",
          "next": [200710005]
        },
        "200710005": {
          "id": 200710005,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Suka]",
          "next": [200710006]
        },
        "200710006": {
          "id": 200710006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Sangat sulit untuk memesan kamar premium suite, kebanyakan orang ke sana cuma buat lihat pemandangan malam Starwatcher Avenue.",
          "next": [200710012]
        },
        "200710007": {
          "id": 200710007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Iya, aku menginap di Petrichor Inn.",
          "next": [200710008]
        },
        "200710008": {
          "id": 200710008,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Tapi jujur, biasa saja tempatnya.",
          "next": [200710009]
        },
        "200710009": {
          "id": 200710009,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Lagi pula, setiap kali aku menginap di hotel, aku khawatir tentang hal-hal buruk yang terjadi, jadi aku tidak punya waktu untuk memikirkan hal-hal lain.",
          "next": [200710010]
        },
        "200710010": {
          "id": 200710010,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Menangis]",
          "next": [200710011]
        },
        "200710011": {
          "id": 200710011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Sepertinya kamu sudah menjalani masa-masa sulit.",
          "next": [200710012]
        },
        "200710012": {
          "id": 200710012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Aku sering menginap di Petrichor Inn. Lagi pula, mudah untuk pergi ke mana pun dari sana.",
          "next": [200710013]
        },
        "200710013": {
          "id": 200710013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Sebagai pedagang keliling, aku tidak terlalu menikmati perjalananku, pada akhirnya aku bosan setelah beberapa saat.",
          "next": [200710014]
        },
        "200710014": {
          "id": 200710014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Ada beberapa unit di lingkungan Skyward Pavilion yang disewakan jangka pendek, jadi aku ke sana.",
          "next": [200710015]
        },
        "200710015": {
          "id": 200710015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Di situ adalah tempat di mana orang-orang Xianzhou biasanya tinggal.",
          "next": [200710016, 200710020]
        },
        "200710016": {
          "id": 200710016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Rasanya tidak terlalu nyaman, semua mata akan tertuju padamu.",
          "next": [200710017]
        },
        "200710017": {
          "id": 200710017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Tergantung bagaimana melihatnya.",
          "next": [200710018]
        },
        "200710018": {
          "id": 200710018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Kehidupan di Xianzhou tidak terlalu tertutup. Orang-orangnya ramah, jadi merasakan dan mengamati cara hidup mereka adalah hobiku ....",
          "next": [200710019]
        },
        "200710019": {
          "id": 200710019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Tapi pernyataan itu kurang tepat. Sering kali akulah yang diamati.",
          "next": [200710023]
        },
        "200710020": {
          "id": 200710020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kamu sudah jadi penduduk Xianzhou sendiri sekarang.",
          "next": [200710021]
        },
        "200710021": {
          "id": 200710021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Hehe, kamu mengenalku.",
          "next": [200710022]
        },
        "200710022": {
          "id": 200710022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Ya, dengan cara ini, aku sudah berbaur dengan penduduk di Luofu.",
          "next": [200710023]
        },
        "200710023": {
          "id": 200710023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Aku ini nomaden dan tidak punya tempat tetap. Tinggal di lingkungan Skyward Pavilion untuk sementara waktu bahkan membantuku melupakan identitasku sebagai pedagang keliling.",
          "next": [200710024]
        },
        "200710024": {
          "id": 200710024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Jadi aku cukup menyukainya.",
          "next": [200710025, 200710027]
        },
        "200710025": {
          "id": 200710025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Lain kali aku akan mencobanya juga.",
          "next": [200710026]
        },
        "200710026": {
          "id": 200710026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Baiklah, mungkin kita bisa jadi tetangga kalau kita beruntung.",
          "next": []
        },
        "200710027": {
          "id": 200710027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kemungkinan aku tidak bisa tinggal di sana.",
          "next": [200710028]
        },
        "200710028": {
          "id": 200710028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Bisakah aku menanyakan alasannya?",
          "next": [200710029, 200710030, 200710031]
        },
        "200710029": {
          "id": 200710029,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku takut!",
          "next": [200710032]
        },
        "200710030": {
          "id": 200710030,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku tidak terbiasa dengan tempat tidur lain!",
          "next": [200710032]
        },
        "200710031": {
          "id": 200710031,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku terobsesi pada kebersihan!",
          "next": [200710032]
        },
        "200710032": {
          "id": 200710032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Oh begitu.",
          "next": [200710033]
        },
        "200710033": {
          "id": 200710033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Aku mengerti, dan aku menghormati keputusanmu.",
          "next": [200710034]
        },
        "200710034": {
          "id": 200710034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Kalau suatu hari nanti kamu berubah pikiran, kita bicarakan lagi.",
          "next": []
        }
      },
      "participatingContactsIds": [1203, 8001]
    }
  ]
}
