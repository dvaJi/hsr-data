        "111440001": {
          "id": 111440001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "{NICKNAME}, aku dengar kamu sudah kembali ke Belobog lagi. Bagaimana kabarmu belakangan ini?",
          "next": [
            111440002,
            111440004
          ]
        },
        "111440002": {
          "id": 111440002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Orang-orang Belobog menyambutku dengan tangan terbuka, Nyonya Supreme Guardian.",
          "next": [
            111440003
          ]
        },
        "111440003": {
          "id": 111440003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Tidak perlu terlalu formal denganku, {NICKNAME}.",
          "next": [
            111440006
          ]
        },
        "111440004": {
          "id": 111440004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Belakangan ini aku sangat sibuk ... tapi hidupku sangat memuaskan.",
          "next": [
            111440005
          ]
        },
        "111440005": {
          "id": 111440005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Aku sudah mendengar kabarnya, terima kasih sudah meluangkan waktu untuk membantu kami.",
          "next": [
            111440006
          ]
        },
        "111440006": {
          "id": 111440006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Pela sudah melaporkan padaku tentang pengelolaan museum dan pemulihan barang pameran yang mengalami kemajuan karena bantuanmu.",
          "next": [
            111440007
          ]
        },
        "111440007": {
          "id": 111440007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Akan tetapi, pencurinya sampai sekarang masih belum tertangkap, dan mereka tidak boleh terus dibiarkan berkeliaran.",
          "next": [
            111440008
          ]
        },
        "111440008": {
          "id": 111440008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Kalau kamu punya waktu, bisakah kamu berkunjung ke Qlipoth Fort? Aku mau diskusi soal rencana penyelidikan lanjutan.",
          "next": [
            111440009
          ]
        },
        "111440009": {
          "id": 111440009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku lagi sama Pela sekarang, kami akan ke sana bersama.",
          "next": [
            111440010
          ]
        },
        "111440010": {
          "id": 111440010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Kamu lagi sama Pela? Baguslah.",
          "next": [
            111440011
          ]
        },
        "111440011": {
          "id": 111440011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Aku akan menunggu di kantorku.",
