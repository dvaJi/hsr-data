  "sections": [
    {
      "id": 2011500,
      "startingMessageId": [201150000],
      "messages": {
        "201150000": {
          "id": 201150000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Apa kamu punya waktu buat mengobrol?",
          "next": [201150001, 201150003]
        },
        "201150001": {
          "id": 201150001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Punya dong, Nyonya Xueyi mau ngobrol tentang apa?",
          "next": [201150002]
        },
        "201150002": {
          "id": 201150002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Baguslah, sepertinya kamu lagi senggang.",
          "next": [201150006]
        },
        "201150003": {
          "id": 201150003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku kira kamu tidak suka bicara, biasanya juga terlihat cuek.",
          "next": [201150004]
        },
        "201150004": {
          "id": 201150004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Biasanya aku memang kelihatan cuek",
          "next": [201150005]
        },
        "201150005": {
          "id": 201150005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "tapi aku bisa bicara kok.",
          "next": [201150006]
        },
        "201150006": {
          "id": 201150006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Misiku baru selesai, dan ada beberapa pertanyaan yang mau aku tanyakan.",
          "next": [201150007]
        },
        "201150007": {
          "id": 201150007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Ten-Lords Commission mendapat kabar kalau ada orang yang terus-terusan mengirim postingan \"Aku terserang Mara\" di internet.",
          "next": [201150008]
        },
        "201150008": {
          "id": 201150008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Jumlah postingannya banyak sekali.",
          "next": [201150009]
        },
        "201150009": {
          "id": 201150009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Jadi aku kira kemungkinan besar itu sinyal SOS.",
          "next": [201150010]
        },
        "201150010": {
          "id": 201150010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Lalu aku pun pergi menyelidikinya.",
          "next": [201150011, 201150014]
        },
        "201150011": {
          "id": 201150011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Dia tidak terserang Mara benaran, kan?",
          "next": [201150012]
        },
        "201150012": {
          "id": 201150012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Tidak.",
          "next": [201150013]
        },
        "201150013": {
          "id": 201150013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Bahkan tidak ada gejala terserang Mara sama sekali.",
          "next": [201150017]
        },
        "201150014": {
          "id": 201150014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku rasa dia cuma iseng, sembarang kirim postingan.",
          "next": [201150015]
        },
        "201150015": {
          "id": 201150015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Sepertinya kamu sudah sering bertemu orang yang suka bicara sembarangan ya.",
          "next": [201150016]
        },
        "201150016": {
          "id": 201150016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Dia tidak terserang Mara, bahkan sama sekali tidak ada gejala terserang Mara.",
          "next": [201150017]
        },
        "201150017": {
          "id": 201150017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Setelah memastikan dia tidak terserang Mara, aku punya waktu santai untuk sekarang.",
          "next": [201150018]
        },
        "201150018": {
          "id": 201150018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Jadi, kenapa dia mau mengumumkan kalau dirinya terserang Mara? Aku tidak mengerti.",
          "next": [201150019, 201150023]
        },
        "201150019": {
          "id": 201150019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mungkin dia merasa kalau dia bilang dirinya terserang Mara, dia bisa seenaknya ke orang lain.",
          "next": [201150020]
        },
        "201150020": {
          "id": 201150020,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Misalnya: \"Jangan macam-macam kamu! Kamu tidak tahu siapa aku? Aku ini orang yang terserang Mara!\" kurang lebih mungkin seperti itu.",
          "next": [201150021]
        },
        "201150021": {
          "id": 201150021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Ah, ternyata begitu ... sekarang aku paham.",
          "next": [201150022]
        },
        "201150022": {
          "id": 201150022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Dengan kata lain, dia tipe orang yang tidak bertanggung jawab.",
          "next": [201150027]
        },
        "201150023": {
          "id": 201150023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mungkin cuma prank, terkadang ada prank yang seperti ini, aku pernah lihat yang sejenis.",
          "next": [201150024]
        },
        "201150024": {
          "id": 201150024,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "\"Terima kasih, tadinya aku nyaris saja terserang Mara dan berubah menjadi Mara-Struck, tapi setelah melihat respon kalian, aku tiba-tiba sembuh!\" kurang lebih mungkin seperti itu.",
          "next": [201150025]
        },
        "201150025": {
          "id": 201150025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Ternyata ada orang yang menganggap hal seperti itu lucu ya?",
          "next": [201150026]
        },
        "201150026": {
          "id": 201150026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Atau mungkin aku yang selera humornya payah?",
          "next": [201150027]
        },
        "201150027": {
          "id": 201150027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Tapi menurutku, \"terserang Mara\" bukanlah sesuatu yang boleh sembarang diucapkan.",
          "next": [201150028]
        },
        "201150028": {
          "id": 201150028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Terima kasih, aku belajar banyak setelah mengobrol denganmu.",
          "next": []
        }
      },
      "participatingContactsIds": [1214, 8001]
    }
  ]
