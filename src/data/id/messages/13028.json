      "messages": {
        "130200401": {
          "id": 130200401,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Hari ini waktu jalan-jalan sama Huohuo, kami ketemu sama orang-orang yang kenal dekat dengan Huohuo.",
          "next": [130200402]
        },
        "130200402": {
          "id": 130200402,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Huohuo jadi terlihat bingung begitu.",
          "next": [130200403]
        },
        "130200403": {
          "id": 130200403,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Dia tidak tahu harus ngomong apa.",
          "next": [130200404]
        },
        "130200404": {
          "id": 130200404,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Karena aku melihat situasinya canggung, jadi aku coba mulai pembicaraan dulu deh.",
          "next": [130200405]
        },
        "130200405": {
          "id": 130200405,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Dan ternyata, mereka semua itu enak diajak bicara.",
          "next": [130200406]
        },
        "130200406": {
          "id": 130200406,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Sushang bahkan pada akhirnya mengajak kami semua makan bersama.",
          "next": [130200407]
        },
        "130200407": {
          "id": 130200407,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "[Terkejut]",
          "next": [130200408]
        },
        "130200408": {
          "id": 130200408,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Sudah kuduga kemampuan Shangshang berkomunikasi itu cuma satu tingkat di bawah aku!",
          "next": [130200409, 130200410]
        },
        "130200409": {
          "id": 130200409,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku bisa membayangkan kejadiannya di pikiranku.",
          "next": [130200411]
        },
        "130200410": {
          "id": 130200410,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sushang memang hebat, bisa dengan mudah melakukan hal yang tidak bisa Huohuo lakukan.",
          "next": [130200411]
        },
        "130200411": {
          "id": 130200411,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Kak Sushang memang hebat! Tidak seperti aku ....",
          "next": [130200412]
        },
        "130200412": {
          "id": 130200412,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Huohuo, jangan terlalu dipikirkan.",
          "next": [130200413]
        },
        "130200413": {
          "id": 130200413,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Lain kali sering-sering main sama kami deh, nanti kami bakal ajari kamu banyak hal. Kamu pasti bakal bisa melawan semua ketakutanmu, terus kamu bakal jadi jagoan dari Ten-Lords Commission!",
          "next": [130200414]
        },
        "130200414": {
          "id": 130200414,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Eh ....",
          "next": [130200415, 130200417, 130200419]
        },
        "130200415": {
          "id": 130200415,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Huohuo jadi jagoan ...? Kok aku sulit membayangkannya ya.",
          "next": [130200416]
        },
        "130200416": {
          "id": 130200416,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Kedengarannya ini bukan hal baik.",
          "next": [130200422]
        },
        "130200417": {
          "id": 130200417,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kalian tahu kan, memaksa orang introvert menjadi ekstrovert itu, sudah kayak menyuruh ikan manjat pohon.",
          "next": [130200418]
        },
        "130200418": {
          "id": 130200418,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Intinya, kalian ini sebenarnya malah mempersulit Huohuo.",
          "next": [130200422]
        },
        "130200419": {
          "id": 130200419,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Huohuo, setiap orang pasti punya momen untuk berubah, mungkin momen kamu itu sekarang!",
          "next": [130200420]
        },
        "130200420": {
          "id": 130200420,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Se-sekarang?!",
          "next": [130200421]
        },
        "130200421": {
          "id": 130200421,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Tidak, tidak.",
          "next": [130200422]
        },
        "130200422": {
          "id": 130200422,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Terima kasih kalian sudah perhatian, tapi aku tidak mau jadi jagoan ....",
          "next": [130200423]
        },
        "130200423": {
          "id": 130200423,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Tapi aku mau belajar supaya bisa berkomunikasi dengan lancar sama orang yang aku kenal.",
          "next": [130200424]
        },
        "130200424": {
          "id": 130200424,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Huohuo pasti bisa!",
          "next": [130200425]
        },
        "130200425": {
          "id": 130200425,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Oh, kalau begitu makan bareng saja! Sama orang-orang yang waktu itu ketemu kita, nanti aku bantu ajari kamu.",
          "next": [130200426]
        },
        "130200426": {
          "id": 130200426,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "?!",
          "next": [130200427]
        },
        "130200427": {
          "id": 130200427,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Maaf, pengguna yang Anda tuju sedang berada di luar jangkauan, silakan hubungi lagi nanti.",
          "next": [130200428]
        },
        "130200428": {
          "id": 130200428,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Eh ....",
          "next": [130200429]
        },
        "130200429": {
          "id": 130200429,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Eh ....",
          "next": [130200430, 130200432]
        },
        "130200430": {
          "id": 130200430,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Eh ....",
          "next": [130200431]
        },
        "130200431": {
          "id": 130200431,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Baru juga awal, sudah langsung kalian kasih yang susah.",
          "next": [130200434]
        },
        "130200432": {
          "id": 130200432,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sepertinya Huohuo tidak ikut, kalau begitu soal makan bareng, aku saja yang ikut.",
          "next": [130200433]
        },
        "130200433": {
          "id": 130200433,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Di grup ini ada 3 jagoan, tapi aku tidak mau sebut nama.",
          "next": [130200434]
        },
        "130200434": {
          "id": 130200434,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Sepertinya mengajari Huohuo itu harus pelan-pelan ya, selangkah demi selangkah.",
          "next": []
        }
