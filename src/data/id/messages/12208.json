        "122080001": {
          "id": 122080001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Iya, keren banget.",
          "next": [122080002]
        },
        "122080002": {
          "id": 122080002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Asli keren banget, ini pertama kalinya aku melihatnya.",
          "next": [122080003, 122080007]
        },
        "122080003": {
          "id": 122080003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Setiap kali melihat Lightning-Lord muncul, aku merasa dia keren banget.",
          "next": [122080004]
        },
        "122080004": {
          "id": 122080004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "IYA KAN??!",
          "next": [122080011]
        },
        "122080007": {
          "id": 122080007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku sudah sering melihatnya, sudah enek lihat muka dia.",
          "next": [122080008]
        },
        "122080008": {
          "id": 122080008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "HAH?",
          "next": [122080011]
        },
        "122080011": {
          "id": 122080011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ayo kita kembali ke topik semula. Selamat buat Huohuo yang sudah berhasil menemukan Tail!",
          "next": [122080012]
        },
        "122080012": {
          "id": 122080012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ya! Siapa sangka coba, ternyata Tail ada bersama Hanya!?",
          "next": [122080013]
        },
        "122080013": {
          "id": 122080013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ya, aku dengar Tail butuh waktu lama untuk pulih.",
          "next": [122080014]
        },
        "122080014": {
          "id": 122080014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Tunggu, aku jadi ingat sesuatu.",
          "next": [122080015]
        },
        "122080015": {
          "id": 122080015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Kamu mau foto si Tail ya?",
          "next": [122080016]
        },
        "122080016": {
          "id": 122080016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Kok kamu bisa tahu?",
          "next": [122080017]
        },
        "122080017": {
          "id": 122080017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ah, soal ini ....",
          "next": [122080018, 122080020]
        },
        "122080018": {
          "id": 122080018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Jangan sungkan, Huohuo. Bilang saja kalau tidak setuju.",
          "next": [122080019]
        },
        "122080019": {
          "id": 122080019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Terima kasih, {NICKNAME}.",
          "next": [122080022]
        },
        "122080020": {
          "id": 122080020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sebaiknya kalian tanya sama Tail dulu deh, dia mau atau tidak ....",
          "next": [122080021]
        },
        "122080021": {
          "id": 122080021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Oh, Tuan Tail bilang dia tidak mau.",
          "next": [122080022]
        },
        "122080022": {
          "id": 122080022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ya sudah, kalau Tuan Tail tidak mau, kita bisa mendiskusikan topik lain.",
