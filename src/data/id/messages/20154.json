        "201540022": {
          "id": 201540022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Pedang terkutuk haus darah dan suka membunuh. Orang yang memegangnya akan jadi boneka yang cuma tahu membunuh.",
