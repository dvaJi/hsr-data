  "relatedMessages": [20118, 20119, 20120],
  "sections": [
    {
      "id": 2011900,
      "startingMessageId": [201190000],
      "messages": {
        "201190000": {
          "id": 201190000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Setelah mengerjakan desain teknik selama tiga bulan berturut-turut, aku jadi agak lelah.",
          "next": [201190001]
        },
        "201190001": {
          "id": 201190001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "{NICKNAME}, coba beri aku pertanyaan untuk merangsang otakku.",
          "next": [201190002]
        },
        "201190002": {
          "id": 201190002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku baru pertama kali mendengar permintaan yang begitu.",
          "next": [201190003]
        },
        "201190003": {
          "id": 201190003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Memangnya aneh ya?",
          "next": [201190004]
        },
        "201190004": {
          "id": 201190004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Memang biasanya kalau kamu lagi istirahat, tidak begitu ya?",
          "next": [201190005, 201190016]
        },
        "201190005": {
          "id": 201190005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tolong rancang senjata buat Astral Express dong!",
          "next": [201190006]
        },
        "201190006": {
          "id": 201190006,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Lampiran \"[04-59] Proyek Kontemplator\" sudah terkirim.",
          "next": [201190007]
        },
        "201190007": {
          "id": 201190007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Ini, rancangan senjata anti-bintang yang dulu pernah aku desain, kebetulan cocok untuk Astral Express.",
          "next": [201190008]
        },
        "201190008": {
          "id": 201190008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Kalian kan menjelajahi alam semesta, jadi harus selalu sedia senjata berdaya besar.",
          "next": [201190009]
        },
        "201190009": {
          "id": 201190009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mana senjatanya?",
          "next": [201190010]
        },
        "201190010": {
          "id": 201190010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Senjatanya ya Astral Express itu sendiri.",
          "next": [201190011]
        },
        "201190011": {
          "id": 201190011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Astral Express kan kumpulan gerbong-gerbong, asalkan sebagian materialnya diganti dengan material yang ringan dan kokoh, maka Astral Express sendiri bisa menjadi senjata raksasa, tanpa merusak struktur internal.",
          "next": [201190012]
        },
        "201190012": {
          "id": 201190012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Di gambar itu ada rancangan Astral Express yang bisa berubah jadi senjata berbentuk manusia. Kalian bisa mengendalikannya untuk bertempur tanpa perlu pindah posisi.",
          "next": [201190013]
        },
        "201190013": {
          "id": 201190013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Material yang kamu bilang itu bisa aku dapatkan di mana?",
          "next": [201190014]
        },
        "201190014": {
          "id": 201190014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Di Intelligentsia Guild ada yang sedang meneliti material itu. Mungkin lima Era Amber lagi bisa ada hasilnya.",
          "next": [201190015]
        },
        "201190015": {
          "id": 201190015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Bagaimana? Mau kerja sama tidak?",
          "next": [201190032]
        },
        "201190016": {
          "id": 201190016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bagaimana cara membuat Astral Express agar menjadi mesin penggerak yang abadi, yang tidak bergantung pada sumber energi eksternal?",
          "next": [201190017]
        },
        "201190017": {
          "id": 201190017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Pertanyaan bagus, coba kupikirkan dulu.",
          "next": [201190018]
        },
        "201190018": {
          "id": 201190018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku cuma asal tanya loh ....",
          "next": [201190019]
        },
        "201190019": {
          "id": 201190019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Kebetulan, aku sudah lama mempelajari masalah soal energi.",
          "next": [201190020]
        },
        "201190020": {
          "id": 201190020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Aku yakin kalau aku bisa bantu Astral Express menjadi mesin penggerak yang abadi.",
          "next": [201190021]
        },
        "201190021": {
          "id": 201190021,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Lampiran \"[02-17] Proyek Penggerak Ganda\" sudah terkirim.",
          "next": [201190022]
        },
        "201190022": {
          "id": 201190022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Tunggu dulu.",
          "next": [201190023]
        },
        "201190023": {
          "id": 201190023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ada apa?",
          "next": [201190024]
        },
        "201190024": {
          "id": 201190024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Aku sudah mencari semua data tentang Astral Express di Intelligentsia Guild untuk memeriksa struktur dan konsumsi energinya.",
          "next": [201190025]
        },
        "201190025": {
          "id": 201190025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Aku lihat sejak Astral Express diberangkatkan kembali, penambahan bahan bakarnya benar-benar tidak beraturan, tapi bisa terus melakukan Jump.",
          "next": [201190026]
        },
        "201190026": {
          "id": 201190026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Dalam sudut pandang tertentu, dia sudah termasuk mesin penggerak yang abadi.",
          "next": [201190027]
        },
        "201190027": {
          "id": 201190027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Kekuatan apa yang membuat Astral Express bisa bertahan begitu lama?",
          "next": [201190028]
        },
        "201190028": {
          "id": 201190028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Suatu hari aku harus membongkar Astral Express untuk menemukan jawabannya.",
          "next": [201190029]
        },
        "201190029": {
          "id": 201190029,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Jangan sampai kedengaran kondektur ya, kondektur kalau marah seram sekali!",
          "next": [201190030]
        },
        "201190030": {
          "id": 201190030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Rahasianya mungkin ada di tubuh kondektur kalian.",
          "next": [201190031]
        },
        "201190031": {
          "id": 201190031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Suatu hari nanti, aku pasti akan membongkar rahasianya.",
          "next": [201190032]
        },
        "201190032": {
          "id": 201190032,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kalau yang ini bakal aku bilangin ke kondektur.",
          "next": [201190033]
        },
        "201190033": {
          "id": 201190033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Aku tunggu kabar darimu.",
          "next": []
        }
      },
      "participatingContactsIds": [1305, 8001]
    }
  ]
}
