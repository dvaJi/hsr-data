        "111490001": {
          "id": 111490001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Apa benar ini {NICKNAME}?",
          "next": [111490002]
        },
        "111490002": {
          "id": 111490002,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "[Menyapa]",
          "next": [111490003, 111490005]
        },
        "111490003": {
          "id": 111490003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ya. Kamu siapa?",
          "next": [111490004]
        },
        "111490004": {
          "id": 111490004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Oke, baguslah kalau begitu.",
          "next": [111490007]
        },
        "111490005": {
          "id": 111490005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bukan, kamu salah orang.",
          "next": [111490006]
        },
        "111490006": {
          "id": 111490006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Masa sih? Tapi nomornya di sini benar kok.",
          "next": [111490007]
        },
        "111490007": {
          "id": 111490007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Intinya, aku perkenalkan diriku dulu saja.",
          "next": [111490008]
        },
        "111490008": {
          "id": 111490008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Aku Wilder, staf \"Departemen Investasi Strategis\" Interastral Peace Corporation.",
          "next": [111490009]
        },
        "111490009": {
          "id": 111490009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Aku menghubungimu untuk merekomendasikan sebuah video game.",
          "next": [111490010, 111490012, 111490015]
        },
        "111490010": {
          "id": 111490010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Asyik! Game apa?",
          "next": [111490011]
        },
        "111490011": {
          "id": 111490011,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Suka]",
          "next": [111490018]
        },
        "111490012": {
          "id": 111490012,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Curiga]",
          "next": [111490013]
        },
        "111490013": {
          "id": 111490013,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Maaf, bisa bicara lain kali saja? Aku lagi sibuk sekarang.",
          "next": [111490014]
        },
        "111490014": {
          "id": 111490014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Hei, jangan langsung diblokir ya. Siapa tahu kamu jadi tertarik setelah mendengar pengenalannya.",
          "next": [111490018]
        },
        "111490015": {
          "id": 111490015,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Curiga]",
          "next": [111490016]
        },
        "111490016": {
          "id": 111490016,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Perusahaan kalian tidak punya metode promosi lainnya ya ....",
          "next": [111490017]
        },
        "111490017": {
          "id": 111490017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Haha. Meskipun sekarang teknologi sudah maju, tapi mengirim pesan singkat adalah cara termudah untuk mendekatkan hubungan.",
          "next": [111490018]
        },
        "111490018": {
          "id": 111490018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Aku lanjutkan pengenalannya ya.",
          "next": [111490019]
        },
        "111490019": {
          "id": 111490019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Game ini bernama \"Perang Aetherium\", sebuah merek budaya kelas berat taraf alam semesta yang pembuatannya dipimpin oleh Departemen Pengembangan Pemasaran, serta berkolaborasi dengan Jaringan Interastral dan Interastral Peace Film.",
          "next": [111490020, 111490022]
        },
        "111490020": {
          "id": 111490020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wah, hebat sekali.",
          "next": [111490021]
        },
        "111490021": {
          "id": 111490021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Haha. Ini kan salah satu proyek utama perusahaan kami.",
          "next": [111490024]
        },
        "111490022": {
          "id": 111490022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Langsung ke intinya saja ....",
          "next": [111490023]
        },
        "111490023": {
          "id": 111490023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Jangan buru-buru, sebentar lagi kok.",
          "next": [111490024]
        },
        "111490024": {
          "id": 111490024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Perang Aetherium adalah game pertarungan dengan mekanisme koleksi dan pertukaran kartu, tapi pasti berbeda jauh dari game kartu yang kamu bayangkan!",
          "next": [111490025]
        },
        "111490025": {
          "id": 111490025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Dalam game ini, kamu bisa menggunakan teknologi penyalinan Aether tercanggih milik perusahaan kami untuk merekam makhluk apa pun yang kamu temui ke dalam Aether Coin.",
          "next": [111490026]
        },
        "111490026": {
          "id": 111490026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Lalu, kamu bisa mencetaknya untuk ditandingkan dengan pemain lain, dan Aether Spirit milikmu akan bertarung untuk kehormatanmu!",
          "next": [111490027]
        },
        "111490027": {
          "id": 111490027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Bagaimana? Keren kan!",
          "next": [111490028, 111490031, 111490034]
        },
        "111490028": {
          "id": 111490028,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Suka]",
          "next": [111490029]
        },
        "111490029": {
          "id": 111490029,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Menarik juga. Game ini bisa dimainkan di mana?",
          "next": [111490030]
        },
        "111490030": {
          "id": 111490030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Haha, mantap! Akhirnya aku dapat pemain baru untuk game kesukaanku.",
          "next": [111490037]
        },
        "111490031": {
          "id": 111490031,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Curiga]",
          "next": [111490032]
        },
        "111490032": {
          "id": 111490032,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Rasanya masih kalah seru dari ... Perang Besar Alam Semesta.",
          "next": [111490033]
        },
        "111490033": {
          "id": 111490033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Apa itu Perang Besar Alam Semesta? Game cupu begitu pasti tidak sebanding dengan Perang Aetherium!",
          "next": [111490037]
        },
        "111490034": {
          "id": 111490034,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Terkejut]",
          "next": [111490035]
        },
        "111490035": {
          "id": 111490035,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Kenapa kamu tiba-tiba jadi semangat begitu?",
          "next": [111490036]
        },
        "111490036": {
          "id": 111490036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Jujur saja, aku ini penggemar berat Perang Aetherium.",
          "next": [111490037]
        },
        "111490037": {
          "id": 111490037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Bagaimanapun juga, kamu harus datang ke Plaza Rivet Town di Jarilo-Ⅵ supaya bisa mendapatkan info lebih lanjut secara langsung!",
          "next": [111490038]
        },
        "111490038": {
          "id": 111490038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Di sana, kami akan mengadakan festival offline penggemar Perang Aetherium — \"Festival Turnamen Interastral\"! Kamu harus datang ya!",
          "next": [111490039, 111490041]
        },
        "111490039": {
          "id": 111490039,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wah, kebetulan sekali! Sekarang aku ada di Jarilo-Ⅵ.",
          "next": [111490040]
        },
        "111490040": {
          "id": 111490040,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Oke, jangan ke mana-mana, aku akan segera otewe ke sana.",
          "next": [111490043]
        },
        "111490041": {
          "id": 111490041,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mm-hmm.",
          "next": [111490042]
        },
        "111490042": {
          "id": 111490042,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Aku akan ke sana kalau ada waktu.",
          "next": [111490043]
        },
        "111490043": {
          "id": 111490043,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Tidak masalah! Aku tunggu kamu di sini ya!",
          "next": []
        }
      },
