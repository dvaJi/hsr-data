  "sections": [
    {
      "id": 1212801,
      "startingMessageId": [
        121280101
      ],
      "messages": {
        "121280101": {
          "id": 121280101,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "{TEXTJOIN#24}, kamu yang menemukan semua Burung Pengintai?",
          "next": [
            121280102,
            121280103
          ]
        },
        "121280102": {
          "id": 121280102,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Benar, aku yang menemukan semuanya.",
          "next": [
            121280111
          ]
        },
        "121280103": {
          "id": 121280103,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kamu salah orang. Aku tidak tahu apa pun tentang {TEXTJOIN#24}.",
          "next": [
            121280104
          ]
        },
        "121280104": {
          "id": 121280104,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Kamu tenang saja. Aku tidak salah orang. Aku sudah mengecek data Heron Express.",
          "next": [
            121280105
          ]
        },
        "121280105": {
          "id": 121280105,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Tapi kamu bisa terus berpura-pura bukan {TEXTJOIN#24}.",
          "next": [
            121280106
          ]
        },
        "121280106": {
          "id": 121280106,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Anggap saja aku sedang berbicara sendiri.",
          "next": [
            121280111
          ]
        },
        "121280111": {
          "id": 121280111,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Aku adalah orang yang menambahkan kecerdasan pada Burung Pengintai.",
          "next": [
            121280112
          ]
        },
        "121280112": {
          "id": 121280112,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Aku sudah menerima paket. Pengirimnya ... mungkin kamu. Tapi dilihat dari isi suratnya, harusnya itu Heron Express.",
          "next": [
            121280113
          ]
        },
        "121280113": {
          "id": 121280113,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Maaf sudah merepotkan kalian.",
          "next": [
            121280114,
            121280115
          ]
        },
        "121280114": {
          "id": 121280114,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kenapa kamu menambahkan kecerdasan pada Burung Pengintai?",
          "next": [
            121280118
          ]
        },
        "121280115": {
          "id": 121280115,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Karena kamu menolak untuk berhubungan dengan mereka. Jadi mereka memintaku menghubungimu.",
          "next": [
            121280116
          ]
        },
        "121280116": {
          "id": 121280116,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Itu karena mereka ingin datang dan bertemu tatap muka.",
          "next": [
            121280117
          ]
        },
        "121280117": {
          "id": 121280117,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Tapi aku sudah lama tidak keluar dari rumah.",
          "next": [
            121280118
          ]
        },
        "121280118": {
          "id": 121280118,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Kalian bertanya kenapa aku menambah kecerdasan pada Burung Pengintai ... Mungkin kalian salah meletakkan fokus kalian.",
          "next": [
            121280119
          ]
        },
        "121280119": {
          "id": 121280119,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Aku terus berada di rumah, dan sudah ratusan tahun tidak berbicara dengan orang secara langsung.",
          "next": [
            121280120
          ]
        },
        "121280120": {
          "id": 121280120,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Kurasa berbicara dengan orang terlalu sulit, tapi aku bertemu dengan Burung Pengintai setiap hari.",
          "next": [
            121280121
          ]
        },
        "121280121": {
          "id": 121280121,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Jadi aku berpikir apa aku bisa mencoba berbicara dengan para Burung Pengintai. Mungkin dengan begitu aku bisa berbicara dengan orang-orang di masa mendatang.",
          "next": [
            121280122,
            121280123
          ]
        },
        "121280122": {
          "id": 121280122,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Jadi kecerdasan bukanlah fokus utamanya. Bisa berbicara adalah fokusnya.",
          "next": [
            121280124
          ]
        },
        "121280123": {
          "id": 121280123,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Jadi kamu cuma tinggal di rumah dan tiba-tiba merasa ingin bisa berbicara dengan orang, itulah alasannya.",
          "next": [
            121280124
          ]
        },
        "121280124": {
          "id": 121280124,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Ya, kecerdasan dengan karakter yang berbeda ini dibuat agar bisa meniru percakapan yang sesungguhnya.",
          "next": [
            121280125
          ]
        },
        "121280125": {
          "id": 121280125,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Pekerjaanku dapat diselesaikan di internet. Dan pendapatanku juga bisa didapatkan dari sana. Ada layanan pengiriman barang dan makanan. Aku hanya perlu membersihkan sampah secara rutin dan tidak perlu bertemu dengan siapa pun.",
          "next": [
            121280126
          ]
        },
        "121280126": {
          "id": 121280126,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Jujur, aku tidak perlu bertemu dengan siapa pun, dan aku merasa sangat aman.",
          "next": [
            121280127,
            121280130
          ]
        },
        "121280127": {
          "id": 121280127,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Jadi kamu masih tidak bisa bertatap muka dengan orang lain dan tidak bisa keluar rumah?",
          "next": [
            121280128
          ]
        },
        "121280128": {
          "id": 121280128,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Iya.",
          "next": [
            121280129
          ]
        },
        "121280129": {
          "id": 121280129,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Awalnya aku kira aku bisa berbicara dengan Burung Pengintai, tapi aku terlalu optimis dengan diriku. Aku ternyata sangat gugup.",
          "next": [
            121280133
          ]
        },
        "121280130": {
          "id": 121280130,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sudah tidak bertemu dengan orang lain selama ratusan tahun. Sungguh sulit dipercaya.",
          "next": [
            121280131
          ]
        },
        "121280131": {
          "id": 121280131,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Oh ya? Aku juga tidak menyangka bisa selama itu.",
          "next": [
            121280132
          ]
        },
        "121280132": {
          "id": 121280132,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Ratusan tahun itu tidak kulewati dengan sia-sia. Aku juga sudah mencoba berbicara dengan Burung Pengintai.",
          "next": [
            121280133
          ]
        },
        "121280133": {
          "id": 121280133,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Aku sudah mengalami pengalaman yang membuatku trauma. Aku terpukul berat.",
          "next": [
            121280134
          ]
        },
        "121280134": {
          "id": 121280134,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Tapi bukannya aku menolak untuk berkomunikasi. Lihatlah, aku masih bisa mengetik pesan seperti manusia normal.",
          "next": [
            121280135
          ]
        },
        "121280135": {
          "id": 121280135,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Hahahaha.",
          "next": [
            121280136
          ]
        },
        "121280136": {
          "id": 121280136,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Maaf, mungkin tidak lucu.",
          "next": [
            121280137,
            121280141
          ]
        },
        "121280137": {
          "id": 121280137,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Setiap orang punya kesulitannya tersendiri. Meski sekarang tidak bisa keluar rumah, kamu masih bisa hidup dengan baik.",
          "next": [
            121280138
          ]
        },
        "121280138": {
          "id": 121280138,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Terima kasih, aku akan berusaha.",
          "next": [
            121280139
          ]
        },
        "121280139": {
          "id": 121280139,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Aku akan membuat target untuk \"bisa berbicara dengan Burung Pengintai\".",
          "next": [
            121280140
          ]
        },
        "121280140": {
          "id": 121280140,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Mungkin aku perlu sedikit waktu lagi. Mungkin aku bisa keluar ratusan tahun lagi.",
          "next": [
            121280145
          ]
        },
        "121280141": {
          "id": 121280141,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Jangan memaksa diri. Bisa berkomunikasi dengan pesan juga sudah bagus.",
          "next": [
            121280142
          ]
        },
        "121280142": {
          "id": 121280142,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Terima kasih, kamu pandai menghibur.",
          "next": [
            121280143
          ]
        },
        "121280143": {
          "id": 121280143,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Tapi aku tidak pernah memaksa diriku. Mau berlatih berbicara adalah hasil dari usahaku.",
          "next": [
            121280144
          ]
        },
        "121280144": {
          "id": 121280144,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "\"Bisa berbicara dengan Burung Pengintai\" adalah targetku saat ini.",
          "next": [
            121280145
          ]
        },
        "121280145": {
          "id": 121280145,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Aku akan minta maaf dengan Heron Express.",
          "next": [
            121280146
          ]
        },
        "121280146": {
          "id": 121280146,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Aku sudah mengirimkan hadiah untukmu. Tapi aku tidak tahu alamatmu, jadi aku mengirimnya ke Ziqiao.",
          "next": [
            121280147
          ]
        },
        "121280147": {
          "id": 121280147,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Terima kasih.",
          "next": []
        }
      },
