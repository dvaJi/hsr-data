        "201790025": {
          "id": 201790025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1402,
          "text": "Pakaian itu harus disesuaikan dengan ukuran tubuh, dan tiap orang punya gayanya masing-masing.",
