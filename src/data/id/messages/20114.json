  "relatedMessages": [12188, 12205, 12217, 20112, 20113, 20114],
  "sections": [
    {
      "id": 2011400,
      "startingMessageId": [201140000],
      "messages": {
        "201140000": {
          "id": 201140000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "{NICKNAME}, kamu sering membantu Herta menguji Simulated Universe ya?",
          "next": [201140001, 201140003]
        },
        "201140001": {
          "id": 201140001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ya, tapi sayangnya sekarang aku lagi tidak di tempat.",
          "next": [201140002]
        },
        "201140002": {
          "id": 201140002,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Kalau lagi ada waktu luang aku bisa ke sana, memangnya ada apa?",
          "next": [201140005]
        },
        "201140003": {
          "id": 201140003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ya, dan kebetulan sekali sekarang aku lagi mengujinya!",
          "next": [201140004]
        },
        "201140004": {
          "id": 201140004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Apa ada yang bisa dibantu oleh \"asistenmu\" ini?",
          "next": [201140005]
        },
        "201140005": {
          "id": 201140005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Senar Ruan milikku ketinggalan di sana, seharusnya ada di kantor Herta.",
          "next": [201140006]
        },
        "201140006": {
          "id": 201140006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Aku punya cadangannya sih, jadi tidak buru-buru juga.",
          "next": [201140007]
        },
        "201140007": {
          "id": 201140007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Kalau kamu melihatnya, tolong bawakan ke aku ya, terima kasih.",
          "next": [201140008]
        },
        "201140008": {
          "id": 201140008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Kalau Herta ada tanya sesuatu, bilang saja aku kelupaan gara-gara terlalu fokus melihat potretnya yang indah, jadi dia tidak bakal tanya macam-macam.",
          "next": [201140009, 201140010]
        },
        "201140009": {
          "id": 201140009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Perkataanmu ini ketahuan banget bohongnya.",
          "next": [201140011]
        },
        "201140010": {
          "id": 201140010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kalimatmu ini kedengarannya manis sekali.",
          "next": [201140011]
        },
        "201140011": {
          "id": 201140011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Tapi yang aku bilang itu kan memang fakta.",
          "next": [201140012]
        },
        "201140012": {
          "id": 201140012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Sama seperti sifat buruknya, kecantikan Herta juga tidak perlu diragukan lagi.",
          "next": [201140013]
        },
        "201140013": {
          "id": 201140013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Setiap kali pergi ke Stasiun Luar Angkasa, aku sering terlalu fokus dengan hasil penelitian.Jadi kalau ada barang yang ketinggalan, aku selalu memakai alasan itu, dan selalu berhasil.",
          "next": [201140014]
        },
        "201140014": {
          "id": 201140014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kamu pernah ketinggalan barang yang lain tidak?",
          "next": [201140015]
        },
        "201140015": {
          "id": 201140015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Pernah, ada beberapa.",
          "next": [201140016]
        },
        "201140016": {
          "id": 201140016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Satu sulaman, satu catatan budi daya kehidupan, dan Phase Flame milik anggota #29 Sserkal ....",
          "next": [201140017, 201140021, 201140025]
        },
        "201140017": {
          "id": 201140017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sulaman ... itu hobimu kan?",
          "next": [201140018]
        },
        "201140018": {
          "id": 201140018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Ya. Waktu itu aku ada di dekat peron, jadi aku membuat sulaman The Blue.",
          "next": [201140019]
        },
        "201140019": {
          "id": 201140019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Tapi sampai sekarang sulamannya tidak ketemu, mungkin sudah dipungut peneliti lain.",
          "next": [201140020]
        },
        "201140020": {
          "id": 201140020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Semoga saja sulaman itu jatuh ke tangan orang yang menyukainya.",
          "next": []
        },
        "201140021": {
          "id": 201140021,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Catatan budi daya kehidupan? Sepertinya aku pernah lihat ....",
          "next": [201140022]
        },
        "201140022": {
          "id": 201140022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Ada peneliti yang mau menyalin hasil penelitianku, lalu mereka mengedarkan formula budi daya dengan spesifikasi yang lebih rendah secara diam-diam.",
          "next": [201140023]
        },
        "201140023": {
          "id": 201140023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Akan tetapi, yang paling penting dalam budi daya kehidupan itu bukan material,",
          "next": [201140024]
        },
        "201140024": {
          "id": 201140024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "tapi inspirasi yang menghasilkan formula itu.",
          "next": []
        },
        "201140025": {
          "id": 201140025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Phase Flame? Apa Herta tahu?",
          "next": [201140026]
        },
        "201140026": {
          "id": 201140026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Tahu. Yang itu cuma salah satu dari koleksi dia yang sangat banyak.",
          "next": [201140027]
        },
        "201140027": {
          "id": 201140027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Dia tidak akan peduli.",
          "next": []
        }
      },
      "participatingContactsIds": [1303, 8001]
    }
  ]
}
