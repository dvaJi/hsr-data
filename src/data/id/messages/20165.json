      "messages": {
        "201650000": {
          "id": 201650000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Apa kamu pernah mencoba membunuh atasanmu di Astral Express?",
          "next": [
            201650001,
            201650006
          ]
        },
        "201650001": {
          "id": 201650001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Siapa atasanku?",
          "next": [
            201650002
          ]
        },
        "201650002": {
          "id": 201650002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Aku tidak tahu.",
          "next": [
            201650003
          ]
        },
        "201650003": {
          "id": 201650003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kami menggunakan sistem manajemen datar.",
          "next": [
            201650004
          ]
        },
        "201650004": {
          "id": 201650004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Kami adanya kondektur, tidak ada pimpinan.",
          "next": [
            201650005
          ]
        },
        "201650005": {
          "id": 201650005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Oh.",
          "next": [
            201650009
          ]
        },
        "201650006": {
          "id": 201650006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Membunuh?! Kenapa aku mau membunuh kondektur?",
          "next": [
            201650007
          ]
        },
        "201650007": {
          "id": 201650007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Demi tantangan.",
          "next": [
            201650008
          ]
        },
        "201650008": {
          "id": 201650008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Baiklah.",
          "next": [
            201650009
          ]
        },
        "201650009": {
          "id": 201650009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Kelihatannya hubungan kalian sangat sederhana.",
          "next": [
            201650010
          ]
        },
        "201650010": {
          "id": 201650010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Tidak seperti kami. Ada yang saling dendam, ada yang berutang budi, ada atasan dan bawahan, dan ada teman makan siang.",
          "next": [
            201650011
          ]
        },
        "201650011": {
          "id": 201650011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Rumit amat sih.",
          "next": [
            201650012
          ]
        },
        "201650012": {
          "id": 201650012,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Aku tidak mau tahu deh.",
          "next": [
            201650013
          ]
        },
        "201650013": {
          "id": 201650013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Dalam hal balas dendam, Jenderal Feixiao mengizinkanku untuk menantangnya kapan saja.",
          "next": [
            201650014
          ]
        },
        "201650014": {
          "id": 201650014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Aku pernah mencoba menyerangnya diam-diam, menjebaknya diam-diam, serta bertarung terang-terangan, tetapi semuanya gagal.",
          "next": [
            201650015
          ]
        },
        "201650015": {
          "id": 201650015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Kalau kamu juga tidak berpengalaman, aku akan balik dan melaporkannya.",
          "next": [
            201650016
          ]
        },
        "201650016": {
          "id": 201650016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Melapor?",
          "next": [
            201650017
          ]
        },
        "201650017": {
          "id": 201650017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Melapor ke Jenderal Feixiao bahwa kamu tidak punya pengalaman membunuh atasanmu.",
          "next": [
            201650018
          ]
        },
        "201650018": {
          "id": 201650018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Hari ini aku lagi-lagi gagal membunuh Jenderal. Jenderal menyarankanku untuk mengobrol dengan orang lain dan menambah wawasan.",
          "next": [
            201650019,
            201650020
          ]
        },
        "201650019": {
          "id": 201650019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wow, ide yang luar biasa! Aku jadi terkagum-kagum sama dia.",
          "next": [
            201650021
          ]
        },
        "201650020": {
          "id": 201650020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aneh banget idenya! Kenapa kamu malah menerima saran itu?",
          "next": [
            201650021
          ]
        },
        "201650021": {
          "id": 201650021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Menurutku tidak ada salahnya banyak belajar.",
          "next": [
            201650022
          ]
        },
        "201650022": {
          "id": 201650022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Tapi teknik membunuh adalah tentang kemahiran, bukan kuantitas.",
          "next": [
            201650023
          ]
        },
        "201650023": {
          "id": 201650023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Bahkan setelah mendapat jawabanmu pun, aku masih perlu mengasahnya sendiri.",
          "next": [
            201650024
          ]
        },
        "201650024": {
          "id": 201650024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Terima kasih untuk jawabanmu. Aku akan bertanya pada orang lain.",
          "next": []
        }
      },
