        "201160001": {
          "id": 201160001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Boleh, Nyonya Xueyi butuh saran tentang apa?",
          "next": [201160002]
        },
        "201160002": {
          "id": 201160002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Terima kasih.",
          "next": [201160005]
        },
        "201160003": {
          "id": 201160003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku tidak terlalu yakin dengan saranku sendiri, jadi tolong cukup jadikan referensi saja ya.",
          "next": [201160004]
        },
        "201160004": {
          "id": 201160004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Iya, buat referensi saja sudah cukup kok.",
          "next": [201160005]
        },
        "201160005": {
          "id": 201160005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Kamu tahu adikku Hanya yang tinggal di \"dunia lain\" dan bekerja terus menerus, kan?",
          "next": [201160006]
        },
        "201160006": {
          "id": 201160006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Aku mau beri dia hadiah.",
          "next": [201160007]
        },
        "201160007": {
          "id": 201160007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Tapi aku tidak pintar dalam memilih hadiah, jadi aku mau tanya saranmu.",
          "next": [201160008, 201160011, 201160014]
        },
        "201160008": {
          "id": 201160008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nona Hanya orangnya cantik, seharusnya cocok kalau dikasih pakaian yang bagus atau produk kosmetik.",
          "next": [201160009]
        },
        "201160009": {
          "id": 201160009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Dia memang cantik.",
          "next": [201160010]
        },
        "201160010": {
          "id": 201160010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Tapi sayangnya, aku tidak pandai dalam memilih barang-barang seperti itu.",
          "next": [201160017]
        },
        "201160011": {
          "id": 201160011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Terkadang kita tidak perlu memberi barang, ucapan \"terima kasih\" saja sudah cukup.",
          "next": [201160012]
        },
        "201160012": {
          "id": 201160012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Aku sudah pernah bilang begitu ke dia berkali-kali.",
          "next": [201160013]
        },
        "201160013": {
          "id": 201160013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Itu tidak cukup.",
          "next": [201160017]
        },
        "201160014": {
          "id": 201160014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kalau mau kasih hadiah, kita harus tahu kesukaannya. Nona Hanya biasanya suka apa?",
          "next": [201160015]
        },
        "201160015": {
          "id": 201160015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Dia suka menulis novel.",
          "next": [201160016]
        },
        "201160016": {
          "id": 201160016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Tapi dia tidak kekurangan alat tulis, apalagi buku novel.",
          "next": [201160017]
        },
        "201160017": {
          "id": 201160017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Kalau cuma hadiah biasa, begitu dia terima, pasti akan langsung digeletakkan begitu saja.",
          "next": [201160018]
        },
        "201160018": {
          "id": 201160018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Apa ada hadiah yang lebih cocok untuk seseorang seperti adikku?",
          "next": [201160019, 201160020]
        },
        "201160019": {
          "id": 201160019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mungkin kedengarannya terlalu abstrak, tapi Nona Hanya butuh \"waktu\".",
          "next": [201160021]
        },
        "201160020": {
          "id": 201160020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mungkin kedengarannya terlalu abstrak, tapi Nona Hanya butuh \"ditemani\".",
          "next": [201160021]
        },
        "201160021": {
          "id": 201160021,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Kamu bisa pergi jalan-jalan bersama Nona Hanya, atau menonton drama Immersia bersama dia. Menghabiskan waktu bersama dengannya, itulah hadiah darimu untuknya.",
          "next": [201160022]
        },
        "201160022": {
          "id": 201160022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Ternyata yang seperti ini juga termasuk hadiah ya ....",
          "next": [201160023]
        },
        "201160023": {
          "id": 201160023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Aku tanya dia dulu deh.",
          "next": [201160024]
        },
        "201160024": {
          "id": 201160024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Terima kasih, {NICKNAME}!",
          "next": [201160025]
        },
        "201160025": {
          "id": 201160025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Huohuo benar, aku memang harus bertanya ke orang dari \"dunia lain\".",
          "next": [201160026]
        },
        "201160026": {
          "id": 201160026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Eh, salah tulis.",
          "next": [201160027]
        },
        "201160027": {
          "id": 201160027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Maksudnya, aku memang harus bertanya ke orang yang pernah bekerja di \"dunia lain\".",
          "next": [201160028]
        },
        "201160028": {
          "id": 201160028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Kalau ada waktu, lain kali kita mengobrol lagi ya.",
