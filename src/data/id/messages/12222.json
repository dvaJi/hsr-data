      "messages": {
        "122220000": {
          "id": 122220000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Aku sudah mendengar laporan terbaru dari Robot Asisten. Kalau mau foto bersama, lebih baik bawa orang bersangkutan juga.",
          "next": [122220001]
        },
        "122220001": {
          "id": 122220001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1007,
          "text": "Ya, Ruan Mei juga sudah kembali.",
          "next": [122220002]
        },
        "122220002": {
          "id": 122220002,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Asta mengundang {NICKNAME} masuk ke grup.",
          "next": [122220003]
        },
        "122220003": {
          "id": 122220003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Hmm ....",
          "next": [122220004]
        },
        "122220004": {
          "id": 122220004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Aku baru saja kembali dari tempat yang tidak ada sinyal.",
          "next": [122220005, 122220006]
        },
        "122220005": {
          "id": 122220005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Baguslah, Nyonya Ruan Mei, aku ada urusan mencarimu.",
          "next": [122220007]
        },
        "122220006": {
          "id": 122220006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kebetulan sekali, aku juga mau berbicara dengan Nyonya Ruan Mei. Tak disangka kamu muncul di saat yang tepat.",
          "next": [122220007]
        },
        "122220007": {
          "id": 122220007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Ada urusan apa?",
          "next": [122220008]
        },
        "122220008": {
          "id": 122220008,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Ukuran gambar yang kamu unggah terlalu besar. Tolong kompres dulu dan kirim lagi.",
          "next": [122220009]
        },
        "122220009": {
          "id": 122220009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Kamu kirim pakai mode lampiran saja, biar gambarnya lebih jelas.",
          "next": [122220010]
        },
        "122220010": {
          "id": 122220010,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Lampiran \"Kami di Stasiun Luar Angkasa Merindukanmu\" sudah diunggah.",
          "next": [122220011]
        },
        "122220011": {
          "id": 122220011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Apa ini?",
          "next": [122220012]
        },
        "122220012": {
          "id": 122220012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Coba kulihat ....",
          "next": [122220013]
        },
        "122220013": {
          "id": 122220013,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "[Minta Tolong]",
          "next": [122220014]
        },
        "122220014": {
          "id": 122220014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1007,
          "text": "Dengar-dengar makluk organik punya banyak cara untuk mengekspresikan cinta. Makhluk-makhluk ini menunjukkan kemungkinan yang tak terbatas ... sungguh menakjubkan.",
          "next": [122220015]
        },
        "122220015": {
          "id": 122220015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Aku tidak menyangka mereka bisa berkembang sampai sejauh ini. Ini hampir jadi seperti sebuah miniatur peradaban.",
          "next": [122220016]
        },
        "122220016": {
          "id": 122220016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1007,
          "text": "Mereka masih muda, kalau menghitung masa hidup kehidupan organik, mereka masih berada dalam masa kecil mereka.",
          "next": [122220017, 122220018]
        },
        "122220017": {
          "id": 122220017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ada yang ingin kamu katakan kepada mereka?",
          "next": [122220019]
        },
        "122220018": {
          "id": 122220018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Jadi kamu mau kembali melihat mereka, tidak?",
          "next": [122220019]
        },
        "122220019": {
          "id": 122220019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "....",
          "next": [122220020]
        },
        "122220020": {
          "id": 122220020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Sangat menarik ... mungkin ini adalah \"kecelakaan\" yang paling indah bagiku.",
          "next": [122220021]
        },
        "122220021": {
          "id": 122220021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Mungkin aku akan kembali berkunjung ke Stasiun Luar Angkasa dalam waktu dekat.",
          "next": [122220022]
        },
        "122220022": {
          "id": 122220022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Untuk melihat makhluk-makhluk kecil ini.",
          "next": [122220023]
        },
        "122220023": {
          "id": 122220023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Ciptaan saat ini dijaga oleh Robot Asisten dan {NICKNAME}. Kalau ada yang diinginkan atau diperlukan, silakan hubungi aku kapan saja.",
          "next": [122220024, 122220025]
        },
        "122220024": {
          "id": 122220024,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Tumpuk Tinggi]",
          "next": [122220026]
        },
        "122220025": {
          "id": 122220025,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Suguhkan Teh]",
          "next": [122220026]
        },
        "122220026": {
