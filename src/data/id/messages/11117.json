      "messages": {
        "111170000": {
          "id": 111170000,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Tuan Yang, kamu di mana? Tuan Yang~",
          "next": [111170003]
        },
        "111170003": {
          "id": 111170003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Aku selalu di sini. Ada apa, {NICKNAME}?",
          "next": [111170004, 111170005]
        },
        "111170004": {
          "id": 111170004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Apa Express kita menerima anggota baru?",
          "next": [111170006]
        },
        "111170005": {
          "id": 111170005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ada temanku yang mau bergabung ke Express. Bagaimana menurutmu, Tuan Yang?",
          "next": [111170007]
        },
        "111170006": {
          "id": 111170006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Oh? Aku tentu saja menyambut kedatangannya.",
          "next": [111170008]
        },
        "111170007": {
          "id": 111170007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Oh? Bisa bertemu teman baru adalah hal yang baik. Aku setuju dengan adanya anggota baru.",
          "next": [111170008]
        },
        "111170008": {
          "id": 111170008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Bolehkah aku bertanya sesuatu?",
          "next": [111170009]
        },
        "111170009": {
          "id": 111170009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Aku penasaran, temanmu itu seperti apa, {NICKNAME}?",
          "next": [111170010, 111170011, 111170012]
        },
        "111170010": {
          "id": 111170010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Dia adalah mekanik jenius yang teliti.",
          "next": [111170013]
        },
        "111170011": {
          "id": 111170011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Dia adalah bintang rock yang berkepribadian bebas.",
          "next": [111170014]
        },
        "111170012": {
          "id": 111170012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Dia ... tidak bisa kuungkapkan dengan kata-kata.",
          "next": [111170015]
        },
        "111170013": {
          "id": 111170013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Kurasa dia akan punya banyak kesamaan dengan Himeko. Bagus.",
          "next": [111170016]
        },
        "111170014": {
          "id": 111170014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Oh? Hobi yang bagus. Aku juga sedikit tertarik dengan hal itu.",
          "next": [111170016]
        },
        "111170015": {
          "id": 111170015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Tidak masalah. Aku percaya dengan penilaian karaktermu. Seharusnya tidak akan menyimpang terlalu jauh.",
          "next": [111170016]
        },
        "111170016": {
          "id": 111170016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Yang pasti, aku menyambut kedatangan temanmu.",
          "next": [111170017]
        },
        "111170017": {
          "id": 111170017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Tapi aku bukanlah pembuat keputusan di sini. Tapi aku akan merekomendasikannya pada Himeko.",
          "next": [111170018]
        },
        "111170018": {
          "id": 111170018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Dari pengalamanku, dia seharusnya tidak keberatan. Jadi tenang saja.",
          "next": [111170019, 111170020]
        },
        "111170019": {
          "id": 111170019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Terima kasih, Tuan Yang.",
          "next": [111170021]
        },
        "111170020": {
          "id": 111170020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hore, terima kasih Tuan Yang!",
          "next": [111170021]
        },
        "111170021": {
          "id": 111170021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Sama-sama. Ini sudah seharusnya.",
          "next": []
        }
      },
