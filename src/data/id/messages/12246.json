      "messages": {
        "122460001": {
          "id": 122460001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "{NICKNAME}, apa kamu punya waktu untuk mengobrol?",
          "next": [
            122460002,
            122460005
          ]
        },
        "122460002": {
          "id": 122460002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kebetulan lagi santai nih. Yuk ngobrol.",
          "next": [
            122460003
          ]
        },
        "122460003": {
          "id": 122460003,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1209,
          "next": [
            122460004
          ]
        },
        "122460004": {
          "id": 122460004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Baguslah, terima kasih ya.",
          "next": [
            122460008
          ]
        },
        "122460005": {
          "id": 122460005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Boleh, tapi sebentar saja ya, jangan lama-lama.",
          "next": [
            122460006
          ]
        },
        "122460006": {
          "id": 122460006,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1209,
          "next": [
            122460007
          ]
        },
        "122460007": {
          "id": 122460007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Tidak akan terlalu lama kok.",
          "next": [
            122460008
          ]
        },
        "122460008": {
          "id": 122460008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Entah karena tekanan dari Ritual Bela Diri terlalu besar, atau mungkin karena faktor lain ....",
          "next": [
            122460009
          ]
        },
        "122460009": {
          "id": 122460009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Aku belakangan sering mimpi berlatih ilmu pedang dengan berbagai lawan.",
          "next": [
            122460010
          ]
        },
        "122460010": {
          "id": 122460010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Aku tidak bisa ingat banyak hal setelah terbangun, tapi kayaknya ... aku kalah melulu.",
          "next": [
            122460011,
            122460013,
            122460015
          ]
        },
        "122460011": {
          "id": 122460011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bocah samsak Luofu, bahkan di dalam mimpi pun kamu kalah terus?",
          "next": [
            122460012
          ]
        },
        "122460012": {
          "id": 122460012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Sebenarnya mau menang atau kalah dalam mimpi, aku tidak peduli.",
          "next": [
            122460018
          ]
        },
        "122460013": {
          "id": 122460013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ini adalah perwujudan nyata dari tekanan mental yang besar!",
          "next": [
            122460014
          ]
        },
        "122460014": {
          "id": 122460014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Begitu ya.",
          "next": [
            122460018
          ]
        },
        "122460015": {
          "id": 122460015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Yanqing, kamu tahu Penacony kan? Mending kamu ke sana terus bermimpi indah mengalahkan beberapa lawan.",
          "next": [
            122460016
          ]
        },
        "122460016": {
          "id": 122460016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Kayaknya \"mengalahkan lawan di dalam mimpi\" itu tidak bisa dibilang mimpi yang indah deh ....",
          "next": [
            122460017
          ]
        },
        "122460017": {
          "id": 122460017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Tapi terima kasih untuk saranmu, {NICKNAME}!",
          "next": [
            122460018
          ]
        },
        "122460018": {
          "id": 122460018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Meski cuma mimpi, aku setiap kali menghadapi lawan yang berbeda-beda.",
          "next": [
            122460019
          ]
        },
        "122460019": {
          "id": 122460019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Aku ingat ada ujian tombak panjang, ujian pedang ringan, ujian pedang berat, ujian Devastator Glaive ... dan bahkan melawan diriku sendiri!",
          "next": [
            122460020
          ]
        },
        "122460020": {
          "id": 122460020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Tapi tidak peduli ujian pedang yang mana pun, aku selalu merasa kalau ini ada hubungannya dengan kamu, {NICKNAME} ....",
          "next": [
            122460021
          ]
        },
        "122460021": {
          "id": 122460021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Rasanya tuh seperti kamu yang mengendalikan lawan-lawan ini dan menjadikan aku samsak ....",
          "next": [
            122460022,
            122460025,
            122460028
          ]
        },
        "122460022": {
          "id": 122460022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Wah, apa maksudmu nih? Kamu mau balas dendam padaku ya?",
          "next": [
            122460023
          ]
        },
        "122460023": {
          "id": 122460023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Bukan, bukan!",
          "next": [
            122460024
          ]
        },
        "122460024": {
          "id": 122460024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Aku tidak bermaksud begitu!",
          "next": [
            122460031
          ]
        },
        "122460025": {
          "id": 122460025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku ngaku deh ... semua lawan dalam mimpimu itu memang aku yang kendalikan!",
          "next": [
            122460026
          ]
        },
        "122460026": {
          "id": 122460026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Haha, kamu memang suka bercanda ya~",
          "next": [
            122460027
          ]
        },
        "122460027": {
          "id": 122460027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Itu semua memang cuma mimpi yang agak kacau saja.",
          "next": [
            122460031
          ]
        },
        "122460028": {
          "id": 122460028,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku ... kayaknya tidak punya kemampuan seperti itu deh.",
          "next": [
            122460029
          ]
        },
        "122460029": {
          "id": 122460029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Aku sama sekali tidak bermaksud menuduh kamu kok.",
          "next": [
            122460030
          ]
        },
        "122460030": {
          "id": 122460030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Itu semua memang cuma mimpi yang agak kacau saja.",
          "next": [
            122460031
          ]
        },
        "122460031": {
          "id": 122460031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "\"Apa yang kamu pikirkan di siang hari, akan kamu mimpikan di malam hari.\"",
          "next": [
            122460032
          ]
        },
        "122460032": {
          "id": 122460032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Mungkin mimpi-mimpi ini mencerminkan keinginanku untuk berlatih denganmu, {NICKNAME}.",
          "next": [
            122460033
          ]
        },
        "122460033": {
          "id": 122460033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Jadi, {NICKNAME}, apa aku boleh ....",
          "next": [
            122460034,
            122460037
          ]
        },
        "122460034": {
          "id": 122460034,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku paham apa maumu. Kamu mau aku ajari kamu satu dua hal kan?",
          "next": [
            122460035
          ]
        },
        "122460035": {
          "id": 122460035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Betul.",
          "next": [
            122460036
          ]
        },
        "122460036": {
          "id": 122460036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Nanti kalau guru ada waktu luang, mohon bantuannya ya!",
          "next": [
            122460039
          ]
        },
        "122460037": {
          "id": 122460037,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Jangan gegabah ya dek ya, tunggu kamu ada perkembangan dulu deh baru kita coba lagi.",
          "next": [
            122460038
          ]
        },
        "122460038": {
          "id": 122460038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Hmm, baiklah.",
          "next": [
            122460039
          ]
        },
        "122460039": {
          "id": 122460039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Aku merasa jauh lebih baik setelah mengungkapkan isi hatiku. Terima kasih, lain kali kita mengobrol lagi ya.",
          "next": [
            122460040
          ]
        },
        "122460040": {
          "id": 122460040,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Oke, dadah.",
          "next": []
        }
      },
