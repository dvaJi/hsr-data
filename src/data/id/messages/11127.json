        "111270011": {
          "id": 111270011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 49,
          "text": "Kalau begitu apa kamu bisa bantu tanya ke penduduk lokal? Tolong ya, ya, ya?",
