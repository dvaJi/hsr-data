      "messages": {
        "120560000": {
          "id": 120560000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Ngomong-ngomong, tadi kamu melihatnya juga kan, penyelamatku?",
          "next": [120560001]
        },
        "120560001": {
          "id": 120560001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Saat Ebon Deer itu menyentuh tanah, cabang-cabang tumbuh dari dalam tanah.",
          "next": [120560023, 120560002]
        },
        "120560002": {
          "id": 120560002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Benda itu, seharusnya .... akar Ambrosial Arbor kan?",
          "next": [120560003]
        },
        "120560003": {
          "id": 120560003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Jejak \"Abundance\" memang luar biasa.",
          "next": [120560004]
        },
        "120560004": {
          "id": 120560004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Aku sudah menjelajahi lautan bintang, tapi baru pertama kali melihat sesuatu yang ajaib seperti ini.",
          "next": [120560005]
        },
        "120560005": {
          "id": 120560005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Aku lihat buah yang tumbuh di atas dahan ini berkilau keemasan, persis seperti yang disebutkan dalam mitologi Xianzhou.",
          "next": [120560006]
        },
        "120560006": {
          "id": 120560006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Konon di zaman kuno, para leluhur Xianzhou mendapatkan buah abadi dari Ambrosial Arbor dan berubah menjadi spesies berumur panjang.",
          "next": [120560007]
        },
        "120560007": {
          "id": 120560007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Kalau aku makan buah ini, apakah aku bisa abadi seperti orang Xianzhou ....",
          "next": [120560008]
        },
        "120560008": {
          "id": 120560008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Penyelamatku, apa kamu tidak tergoda dengan kesempatan seperti ini?",
          "next": [120560009, 120560013, 120560017]
        },
        "120560009": {
          "id": 120560009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nona Tingyun, bukankah itu akan melanggar larangan Xianzhou?",
          "next": [120560010]
        },
        "120560010": {
          "id": 120560010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Aduh",
          "next": [120560011]
        },
        "120560011": {
          "id": 120560011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Maafkan aku karena bersikap tidak sopan dan mengujimu sedikit.",
          "next": [120560012]
        },
        "120560012": {
          "id": 120560012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Seperti dugaanku, kamu memang seorang anak yang jujur dan tulus, penyelamatku.",
          "next": [120560019]
        },
        "120560013": {
          "id": 120560013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Begitu memakan buah ini, aku khawatir kamu tidak akan mendapatkan umur panjang, tapi ... dieksekusi oleh Cloud Knight.",
          "next": [120560014]
        },
        "120560014": {
          "id": 120560014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Kamu benar, penyelamatku.",
          "next": [120560015]
        },
        "120560015": {
          "id": 120560015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Tapi kalau bisa bersembunyi dari pencarian Cloud Knight ....",
          "next": [120560016]
        },
        "120560016": {
          "id": 120560016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "ini akan jadi kesempatan yang tidak boleh dilewatkan, bukan?",
          "next": [120560019]
        },
        "120560017": {
          "id": 120560017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kalau begitu ... apa Nona Tingyun ... menyimpan buah itu?",
          "next": [120560018]
        },
        "120560018": {
          "id": 120560018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Kalau aku bilang iya, apa kamu akan berbagi buah itu denganku, penyelamatku?",
          "next": [120560019]
        },
        "120560019": {
          "id": 120560019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Tapi aku sudah mengerti maksudmu, penyelamatku.",
          "next": [120560020]
        },
        "120560020": {
          "id": 120560020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Sayangnya, Ebon Deer sudah menjadi abu, buah Ambrosial Arbor juga tidak ada yang tersisa.",
          "next": [120560021]
        },
        "120560021": {
          "id": 120560021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Kesempatan seperti ini hanya sekali seumur hidup, tapi malah hilang begitu saja.",
          "next": [120560022]
        },
        "120560022": {
          "id": 120560022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Sama seperti hidup ....",
          "next": []
        },
