      "messages": {
        "101110000": {
          "id": 101110000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Terima kasih atas bantuanmu dalam mengungkap konspirator. Aku ucapkan terima kasih atas nama Asta, serahkan sisanya padaku.",
          "next": [101110001, 101110003]
        },
        "101110001": {
          "id": 101110001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Herta tidak hukum Asta, kan?",
          "next": [101110002]
        },
        "101110002": {
          "id": 101110002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Kamu tidak mengenal Nyonya Herta ya? Dia tidak akan peduli dengan hal-hal kecil ini.",
          "next": [101110005]
        },
        "101110003": {
          "id": 101110003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Asta benar-benar beli semua barang itu dengan uangnya?",
          "next": [101110004]
        },
        "101110004": {
          "id": 101110004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Kamu tidak tahu ya ....",
          "next": [101110005]
        },
        "101110005": {
          "id": 101110005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Stasiun Luar Angkasa ini memang properti milik Nyonya Herta, tapi dia tidak mengeluarkan uang sepeser pun dalam pembangunan stasiun.",
          "next": [101110006]
        },
        "101110006": {
          "id": 101110006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Nona Asta berhubungan dengan IPC, jadi dia ... sangat \"kaya\". Sebagian besar fasilitas di Stasiun Luar Angkasa pada dasarnya didanai olehnya ....",
          "next": [101110007]
        },
        "101110007": {
          "id": 101110007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Jadi biaya barang-barang yang dia beli sebenarnya tidak berarti apa-apa bagi dia.",
          "next": [101110008, 101110010, 101110013]
        },
        "101110008": {
          "id": 101110008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Apa itu \"Interastral Peace Corporation\"?",
          "next": [101110009]
        },
        "101110009": {
          "id": 101110009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Hmm ... sederhananya, organisasi terkaya di alam semesta.",
          "next": [101110015]
        },
        "101110010": {
          "id": 101110010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kalau mau menggaet Asta sekarang, sudah telat ya?",
          "next": [101110011]
        },
        "101110011": {
          "id": 101110011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Jangan bercanda seperti itu. Aku akan menganggapnya serius.",
          "next": [101110012]
        },
        "101110012": {
          "id": 101110012,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "....",
          "next": [101110015]
        },
        "101110013": {
          "id": 101110013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Apa kamu sudah putuskan Richard mau diapakan?",
          "next": [101110014]
        },
        "101110014": {
          "id": 101110014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Kamu lebih baik tidak tahu detailnya.",
          "next": [101110015]
        },
        "101110015": {
          "id": 101110015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Intinya, terima kasih sudah menolong Asta.",
