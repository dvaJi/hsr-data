        "200700002": {
          "id": 200700002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mengobrol dengan orang-orang di sekitarmu.",
          "next": [200700003]
        },
        "200700003": {
          "id": 200700003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Sebenarnya aku tidak pandai memulai percakapan ... tapi dengan peti mati di dekatku, percakapan pasti akan terjadi.",
          "next": [200700008]
        },
        "200700004": {
          "id": 200700004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Lakukan apa pun yang kamu suka. Main game, baca buku, lihat pemandangan, apa pun yang kamu inginkan.",
          "next": [200700005]
        },
        "200700005": {
          "id": 200700005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Menyelesaikan buku yang aku pinjam di Luofu, hal ini mungkin bukan ide yang buruk.",
          "next": [200700008]
        },
        "200700006": {
          "id": 200700006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Benar-benar tidak ada yang bisa dilakukan di jalan selain tidur.",
          "next": [200700007]
        },
        "200700007": {
          "id": 200700007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Tujuannya adalah titik akhir, bukan prosesnya ya?",
          "next": [200700008]
        },
        "200700008": {
          "id": 200700008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Apa itulah kegiatan yang kamu lakukan dalam perjalananmu, {NICKNAME}?",
          "next": [200700009, 200700012]
        },
        "200700009": {
          "id": 200700009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tentu saja, itulah yang kulakukan sepanjang waktu.",
          "next": [200700010]
        },
        "200700010": {
          "id": 200700010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Sepertinya banyak yang mau kamu katakan tentang hal itu.",
          "next": [200700011]
        },
        "200700011": {
          "id": 200700011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Terima kasih sudah berikan aku saran yang tulus.",
          "next": [200700014]
        },
        "200700012": {
          "id": 200700012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tentu saja tidak, aku tidak pernah melakukan hal itu.",
          "next": [200700013]
        },
        "200700013": {
          "id": 200700013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Sepertinya kamu memilih sesuatu yang menurutmu paling cocok untukku.",
          "next": [200700014]
        },
        "200700014": {
          "id": 200700014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Aku akan mencobanya.",
          "next": []
        },
        "200700015": {
          "id": 200700015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Halo, {NICKNAME}.",
          "next": [200700016]
        },
        "200700016": {
          "id": 200700016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Maaf mengganggu, tapi ada sesuatu yang aku lupa tanyakan padamu.",
          "next": [200700017]
        },
        "200700017": {
          "id": 200700017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Seperti yang pernah kubilang, perjalanan antarbintang pedagang keliling kebanyakan membosankan dan lama.",
          "next": [200700018]
        },
        "200700018": {
          "id": 200700018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "{NICKNAME}, apa kamu ada saran untuk menghabiskan waktu selama perjalanan?",
          "next": [200700002, 200700004, 200700006]
        }
