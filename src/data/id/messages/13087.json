        "130870004": {
          "id": 130870004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Aku buat film bagus tanpa pamrih.",
          "next": [
            130870005
          ]
        },
        "130870005": {
          "id": 130870005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 146,
          "text": "Kamu memang berjiwa mulia!",
