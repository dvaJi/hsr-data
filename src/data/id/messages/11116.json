        "111160005": {
          "id": 111160005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ada temanku yang mau bergabung ....",
          "next": [111160008]
        },
        "111160006": {
          "id": 111160006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ada temanku yang mau minum kopi seduhanmu!",
          "next": [111160009]
        },
        "111160007": {
          "id": 111160007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Anggota baru? Tentu saja boleh. Asalkan mau bertualang bersama, pintu Express akan selalu terbuka.",
          "next": [111160010]
        },
        "111160008": {
          "id": 111160008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Mau memperkenalkan anggota baru ya? Tentu saja boleh. Asalkan mau bertualang bersama, pintu Express akan selalu terbuka.",
          "next": [111160010]
        },
        "111160009": {
          "id": 111160009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Oh? Wah, ini hal langka. Tidak ada alasanku untuk menolak.",
          "next": [111160010]
        },
        "111160010": {
          "id": 111160010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Tapi aku ingin lebih mengenal temanmu.",
          "next": [111160011]
        },
        "111160011": {
          "id": 111160011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Menurutmu dia itu orang yang bagaimana?",
          "next": [111160012, 111160013, 111160014]
        },
        "111160012": {
          "id": 111160012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Dia adalah mekanik jenius yang teliti.",
          "next": [111160015]
        },
        "111160013": {
          "id": 111160013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Dia adalah bintang rock yang berkepribadian bebas.",
          "next": [111160016]
        },
        "111160014": {
          "id": 111160014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Dia ... tidak bisa kuungkapkan dengan kata-kata.",
          "next": [111160017]
        },
        "111160015": {
          "id": 111160015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Baik. Mungkin dia dan aku punya banyak kesamaan.",
          "next": [111160018]
        },
        "111160016": {
          "id": 111160016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Sepertinya sebuah hobi yang disukai anak-anak muda. Mungkin Express akan menjadi lebih ramai.",
          "next": [111160018]
        },
        "111160017": {
          "id": 111160017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Tidak usah cemas. Setelah menghabiskan banyak waktu bersama, semua hal akan menjadi lebih jelas.",
          "next": [111160018]
        },
        "111160018": {
          "id": 111160018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Yang pasti, aku setuju membiarkanmu membawa temanmu naik Express.",
          "next": [111160019]
        },
        "111160019": {
          "id": 111160019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Mintalah dia mempersiapkan barang-barangnya. Express selalu menyambut kedatangan penumpang baru.",
          "next": [111160020, 111160021]
        },
        "111160020": {
          "id": 111160020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Terima untuk pengertianmu, Himeko.",
