  "relatedMessages": [12151, 20064, 20065, 20066],
  "sections": [
    {
      "id": 2006500,
      "startingMessageId": [200650000],
      "messages": {
        "200650000": {
          "id": 200650000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1204,
          "text": "{NICKNAME}, aku punya satu permintaan.",
          "next": [200650001]
        },
        "200650001": {
          "id": 200650001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku mendengarkan, jenderal!",
          "next": [200650002]
        },
        "200650002": {
          "id": 200650002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1204,
          "text": "Kalau sedang senggang, bolehkah aku mengundangmu untuk berlatih dengan Yanqing?",
          "next": [200650003, 200650006, 200650024]
        },
        "200650003": {
          "id": 200650003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Yanqing berguru pada jenderal, aku pernah mendengar tentang ilmu pedang Yanqing.",
          "next": [200650004]
        },
        "200650004": {
          "id": 200650004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Aku tidak berani berlatih dengannya!",
          "next": [200650005]
        },
        "200650005": {
          "id": 200650005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1204,
          "text": "Haha, kenapa? Kamu terlalu rendah hati.",
          "next": [200650009]
        },
        "200650006": {
          "id": 200650006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sebenarnya boleh saja, tapi ....",
          "next": [200650007]
        },
        "200650007": {
          "id": 200650007,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Kudengar Yanqing dididik langsung oleh jenderal ....",
          "next": [200650008]
        },
        "200650008": {
          "id": 200650008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1204,
          "text": "Itu benar. Memangnya kenapa?",
          "next": [200650009]
        },
        "200650009": {
          "id": 200650009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kalau begitu, apa aku boleh berlatih dengan jenderal terlebih dulu?",
          "next": [200650010]
        },
        "200650010": {
          "id": 200650010,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Dengan begitu, aku bisa menguji kemampuanku",
          "next": [200650011]
        },
        "200650011": {
          "id": 200650011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1204,
          "text": "Haha, {NICKNAME}, aku sudah tahu kemampuanmu.",
          "next": [200650012, 200650016]
        },
        "200650012": {
          "id": 200650012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Gawat, aku ketahuan.",
          "next": [200650013]
        },
        "200650013": {
          "id": 200650013,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Aku ketahuan kalau aku tidak bisa menggunakan pedang",
          "next": [200650014]
        },
        "200650014": {
          "id": 200650014,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Menangis]",
          "next": [200650015]
        },
        "200650015": {
          "id": 200650015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1204,
          "text": "Karena kamu tidak tahu cara menggunakan pedang, jadi kamu tidak perlu mematuhi aturan pertarungan pedang. Ini akan jadi kesempatan bagus bagi Yanqing untuk mengasah keterampilannya.",
          "next": [200650020]
        },
        "200650016": {
          "id": 200650016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku memang sangat kuat",
          "next": [200650017]
        },
        "200650017": {
          "id": 200650017,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Aku tidak menyangka akan ketahuan secepat ini, jenderal.",
          "next": [200650018]
        },
        "200650018": {
          "id": 200650018,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Terkejut]",
          "next": [200650019]
        },
        "200650019": {
          "id": 200650019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1204,
          "text": "Wajar saja. Kekuatan tidak hanya dinilai dari fisik, keterampilan atau senjata. Melainkan juga dari kebijaksanaan dan kegigihan. Kamu harus percaya pada naluriku sebagai veteran perang.",
          "next": [200650020]
        },
        "200650020": {
          "id": 200650020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1204,
          "text": "Mengenai latihan kita, mungkin akan lebih baik kalau diatur setelah kamu dan Yanqing berlatih.",
          "next": [200650021]
        },
        "200650021": {
          "id": 200650021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1204,
          "text": "Tapi aku ada satu permintaan. Kalau {NICKNAME} tidak setuju, kita tidak bisa berlatih.",
          "next": [200650022]
        },
        "200650022": {
          "id": 200650022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku mendengarkan, jenderal!",
          "next": [200650023]
        },
        "200650023": {
          "id": 200650023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1204,
          "text": "Waktu berlatih jangan diatur pada sore hari. Itu jam-jam rawan mengantuk.",
          "next": []
        },
        "200650024": {
          "id": 200650024,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Yanqing itu siapa?",
          "next": [200650025]
        },
        "200650025": {
          "id": 200650025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1204,
          "text": "Muridku, dia juga Letnan Cloud Knight.",
          "next": [200650009]
        }
      },
      "participatingContactsIds": [1204, 8001]
    }
  ]
}
