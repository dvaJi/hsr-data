        "201070001": {
          "id": 201070001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Balapan luar angkasa ...? Apaan tuh?",
          "next": [201070003]
        },
        "201070002": {
          "id": 201070002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Apa yang bikin kamu sampai bersemangat begitu?",
          "next": [201070003]
        },
        "201070003": {
          "id": 201070003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Itu adalah balapan yang paling bergengsi di alam semesta.",
          "next": [201070004]
        },
        "201070004": {
          "id": 201070004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Demi balapan ini, setiap tahunnya aku pasti memodifikasi mesin pesawat luar angkasa \"Yang Tiada Duanya\" milikku.",
          "next": [201070005]
        },
        "201070005": {
          "id": 201070005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Semua jenis pesawat, kapal terbang, dan kendaraan luar angkasa boleh ikut balapan ini.",
          "next": [201070006]
        },
        "201070006": {
          "id": 201070006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Kalau kalian mau, aku juga bisa rekomendasikan Astral Express untuk ikut balapan.",
          "next": [201070007]
        },
        "201070007": {
          "id": 201070007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Tapi waktu balapan, suka sering ada \"badai debu\".",
          "next": [201070008]
        },
        "201070008": {
          "id": 201070008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Kalau ada badai debu, banyak debu-debu bintang yang menempel di kapal \"Yang Tiada Duanya\" milikku. Aku tidak tahan melihat kelap-kelipnya, jadi setiap kali pasti aku bersihkan.",
          "next": [201070009, 201070015]
        },
        "201070009": {
          "id": 201070009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Oh, ya, Pom-Pom juga pernah cerita soal itu.",
          "next": [201070010]
        },
        "201070010": {
          "id": 201070010,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Astral Express juga pernah kena badai debu.",
          "next": [201070011]
        },
        "201070011": {
          "id": 201070011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Debu-debu seperti itu akan bersinar kalau menempel di zirahku.",
          "next": [201070012]
        },
        "201070012": {
          "id": 201070012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Mungkin juga bisa menempel di bulu Pom-Pom dan akan sulit dibersihkan.",
          "next": [201070013]
        },
        "201070013": {
          "id": 201070013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ya, aku sudah mulai membayangkannya.",
          "next": [201070014]
        },
        "201070014": {
          "id": 201070014,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Pom-Pom yang mengkilap!",
          "next": [201070018]
        },
        "201070015": {
          "id": 201070015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku ingin lihat Pom-Pom ikut balapan pakai Astral Express.",
          "next": [201070016]
        },
        "201070016": {
          "id": 201070016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Aku yakin performa Astral Express pasti bakal luar biasa.",
          "next": [201070017]
        },
        "201070017": {
          "id": 201070017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Aku sudah tidak sabar mau balapan bersama kalian.",
          "next": [201070018]
        },
        "201070018": {
          "id": 201070018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Maaf ya, obrolan hari ini sampai di sini dulu. Aku masih harus membersihkan pesawat luar angkasa \"Yang Tiada Duanya\" milikku.",
          "next": [201070019]
        },
        "201070019": {
          "id": 201070019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Kalau ada waktu, aku mau mengajak {NICKNAME} dan kru Astral Express untuk nonton balapan luar angkasa.",
