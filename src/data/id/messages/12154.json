        "121540104": {
          "id": 121540104,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1103,
          "text": "Aku paham, aku paham. {F#Setiap gadis keren bermimpi untuk mengendarai robot, kan?}{M#Setiap laki-laki pasti menyukai robot, kan?}",
