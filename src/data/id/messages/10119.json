        },
        "101190108": {
          "id": 101190108,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Untung saja kalian menemukanku di Monitoring Room dan membawaku ke tempat yang aman, terima kasih.",
