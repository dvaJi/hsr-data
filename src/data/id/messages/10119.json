        "101190104": {
          "id": 101190104,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Kamu menemukanku di Monitoring Room dalam keadaan terluka, lalu membawaku ke tempat yang aman.",
