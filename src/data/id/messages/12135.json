      "id": 1213500,
      "startingMessageId": [121350000],
      "messages": {
        "121350000": {
          "id": 121350000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "{NICKNAME}, lagi online?",
          "next": [121350001, 121350003]
        },
        "121350001": {
          "id": 121350001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tidak, ini Pom-Pom.",
          "next": [121350002]
        },
        "121350002": {
          "id": 121350002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Ya ya, kamu mirip banget sama Pom-Pom deh. Sudahlah, berhenti bercandanya, ada hal penting nih.",
          "next": [121350005]
        },
        "121350003": {
          "id": 121350003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ya, ada apa?",
          "next": [121350005]
        },
        "121350005": {
          "id": 121350005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Aku tiba-tiba ingat ada benda yang sangat menarik perhatianku di perjalanan ke Luofu kali ini.",
          "next": [121350006]
        },
        "121350006": {
          "id": 121350006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "\"Matrix of Prescience Ultima\" punya Master Diviner itu.",
          "next": [121350007]
        },
        "121350007": {
          "id": 121350007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Master Diviner menginterogasi Kafka dengan benda itu. Dia bahkan bisa melihat masa lalunya tanpa perlu bicara.",
          "next": [121350008]
        },
        "121350008": {
          "id": 121350008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Jadi aku pikir ... apa Matrix itu juga bisa digunakan di tubuhku ...?",
          "next": [121350009, 121350010]
        },
        "121350009": {
          "id": 121350009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kenapa kamu tidak bertanya waktu di Seat of Divine Foresight?",
          "next": [121350012]
        },
        "121350010": {
          "id": 121350010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku rasa hal itu patut ditanyakan. Mungkin kita bisa menemukan ingatanmu yang hilang.",
          "next": [121350011]
        },
        "121350011": {
          "id": 121350011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Betul! Tadinya aku mau tanya waktu di Seat of Divine Foresight, tapi setelah dipikir-pikir, aku merasa tidak enak juga.",
          "next": [121350012]
        },
        "121350012": {
          "id": 121350012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Luofu baru saja mengalami masalah besar dan Jing Yuan terluka. Waktu itu Master Diviner harus mengurus semuanya.",
          "next": [121350013]
        },
        "121350013": {
          "id": 121350013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Aku tidak bisa membicarakan hal pribadi yang sepele ini.",
          "next": [121350014]
        },
        "121350014": {
          "id": 121350014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Jadi kupikir, bagaimana kalau kita pergi bersama-sama ke Divination Commission untuk memohon padanya kalau dia sudah tidak begitu sibuk?",
          "next": [121350015, 121350017]
        },
        "121350015": {
          "id": 121350015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Oke, sampai jumpa di Divination Commission.",
          "next": [121350016]
        },
        "121350016": {
          "id": 121350016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Kamu memang yang terbaik deh!",
          "next": []
        },
        "121350017": {
          "id": 121350017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Apa aku harus ikut juga? Begini, bukannya aku takut repot, tapi bagaimanapun juga ini urusan pribadimu ....",
          "next": [121350018]
        },
        "121350018": {
          "id": 121350018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Aku ... aku ... kalau aku yang memohon bantuan padanya sendirian, pasti akan agak canggung! Sudahlah, kita ketemu di Divination Commission ya!",
          "next": []
        }
      },
      "participatingContactsIds": [1001, 8001]
    }
  ]
}
