        },
        "200670015": {
          "id": 200670015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku bisa memahami keinginan orang tua, tapi anak-anak juga harus diberi kebebasan untuk memilih sendiri.",
