      "messages": {
        "201180000": {
          "id": 201180000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "<u>Tekan untuk melakukan pendaftaran [Rosetta-Rosenthal Cup]. Kami menanti kedatanganmu.</u>",
          "next": [
            201180001
          ]
        },
        "201180001": {
          "id": 201180001,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Lampiran \"Pengumuman Topik Debat Putaran 1\" sudah terkirim.",
          "next": [
            201180002
          ]
        },
        "201180002": {
          "id": 201180002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "{NICKNAME}, aku kirim ke kamu soal dari lomba debat yang belakangan ini diadakan untuk kalangan internal Intelligentsia Guild. Topik debatnya adalah \"Apakah Teknologi itu Netral?\".",
          "next": [
            201180003
          ]
        },
        "201180003": {
          "id": 201180003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Jangan salah paham ya, aku cuma meneruskan pesan saja.",
          "next": [
            201180004
          ]
        },
        "201180004": {
          "id": 201180004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Topik debat ini cocok buat pemula.",
          "next": [
            201180005,
            201180008,
            201180010
          ]
        },
        "201180005": {
          "id": 201180005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Gampang, kemampuan bicaraku sudah cukup bagus.",
          "next": [
            201180006
          ]
        },
        "201180006": {
          "id": 201180006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Kok aku tidak merasa seperti itu?",
          "next": [
            201180007
          ]
        },
        "201180007": {
          "id": 201180007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Seni bahasa itu tidak akan ada batasnya, selalu ada hal baru yang bisa dipelajari.",
          "next": [
            201180012
          ]
        },
        "201180008": {
          "id": 201180008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku sama sekali tidak paham kalau soal debat.",
          "next": [
            201180009
          ]
        },
        "201180009": {
          "id": 201180009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Berarti kamu harus belajar.",
          "next": [
            201180012
          ]
        },
        "201180010": {
          "id": 201180010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Maaf, aku orangnya ansos.",
          "next": [
            201180011
          ]
        },
        "201180011": {
          "id": 201180011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Kamu masih belum terlalu mengenal dirimu.",
          "next": [
            201180012
          ]
        },
        "201180012": {
          "id": 201180012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Mau ikut kompetisinya atau tidak, itu tidak penting.",
          "next": [
            201180013
          ]
        },
        "201180013": {
          "id": 201180013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Tapi aku mau tanya dulu, kalau menurut kamu, debat itu kegiatan yang seperti apa?",
          "next": [
            201180014,
            201180015,
            201180016
          ]
        },
        "201180014": {
          "id": 201180014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Debat itu isinya adu argumen.",
          "next": [
            201180017
          ]
        },
        "201180015": {
          "id": 201180015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Debat itu usaha untuk membuat lawan kita marah.",
          "next": [
            201180017
          ]
        },
        "201180016": {
          "id": 201180016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Debat itu isinya saling sindir.",
          "next": [
            201180017
          ]
        },
        "201180017": {
          "id": 201180017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Begini saja deh, aku jelaskan pakai topik debat ini saja.",
          "next": [
            201180018
          ]
        },
        "201180018": {
          "id": 201180018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Kamu pilih pihak yang mana?",
          "next": [
            201180019,
            201180025
          ]
        },
        "201180019": {
          "id": 201180019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Teknologi itu netral.",
          "next": [
            201180020
          ]
        },
        "201180020": {
          "id": 201180020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Kalau begitu aku jadi pihak kontra, kamu mulai duluan saja.",
          "next": [
            201180021
          ]
        },
        "201180021": {
          "id": 201180021,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Teknologi itu bagaikan pisau bermata dua. Bisa dipakai untuk berbuat baik, juga bisa dipakai untuk berbuat jahat.",
          "next": [
            201180022
          ]
        },
        "201180022": {
          "id": 201180022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Entah kamu pernah dengar tentang eksperimen atavistis Dr. Primitive atau tidak?",
          "next": [
            201180023
          ]
        },
        "201180023": {
          "id": 201180023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Untuk memancing respons stres dari subjek eksperimen, dia meneliti serangkaian teknologi untuk menyiksa makhluk hidup dan membuatnya menderita.",
          "next": [
            201180024
          ]
        },
        "201180024": {
          "id": 201180024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Teknologi seperti itu tidak layak untuk didukung.",
          "next": [
            201180030
          ]
        },
        "201180025": {
          "id": 201180025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Teknologi itu tidak netral.",
          "next": [
            201180026
          ]
        },
        "201180026": {
          "id": 201180026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Kalau begitu aku jadi pihak pro, kamu mulai dulu saja.",
          "next": [
            201180027
          ]
        },
        "201180027": {
          "id": 201180027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ada teknologi yang memang bersifat baik. Misalnya, teknologi yang memperpanjang umur.",
          "next": [
            201180028
          ]
        },
        "201180028": {
          "id": 201180028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Teknologi medis modern memang berhasil menyelamatkan banyak orang. Tapi menyembuhkan dan memperpanjang umur itu berbeda.",
          "next": [
            201180029
          ]
        },
        "201180029": {
          "id": 201180029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Bukannya kalian pernah pergi ke Xianzhou Alliance? Kamu seharusnya lebih paham tentang penderitaan orang-orang yang berumur panjang daripada aku.",
          "next": [
            201180030
          ]
        },
        "201180030": {
          "id": 201180030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Aku sudah selesai bicara.",
          "next": [
            201180031
          ]
        },
        "201180031": {
          "id": 201180031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Kamu lihat kan, ini sudah membuktikan pendapatku.",
          "next": [
            201180032,
            201180035
          ]
        },
        "201180032": {
          "id": 201180032,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku masih belum selesai bicara!",
          "next": [
            201180033
          ]
        },
        "201180033": {
          "id": 201180033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Biasanya ya, kalau orang bicaranya cepat, itu berarti kecepatan berpikirnya juga cepat.",
          "next": [
            201180034
          ]
        },
        "201180034": {
          "id": 201180034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Tapi ada kemungkinan juga, orang itu cuma asal ngomong, sebelum bicara tidak berpikir dulu.",
          "next": [
            201180038
          ]
        },
        "201180035": {
          "id": 201180035,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kamu cuma asal ngomong doang, kan?",
          "next": [
            201180036
          ]
        },
        "201180036": {
          "id": 201180036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Kalau kamu bilang begitu, berarti kamu kalah.",
          "next": [
            201180037
          ]
        },
        "201180037": {
          "id": 201180037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Tapi yang kamu bilang itu memang benar.",
          "next": [
            201180038
          ]
        },
        "201180038": {
          "id": 201180038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Jadi kesimpulannya sudah jelas. Di antara kita, tidak ada yang bisa meyakinkan pihak lawan.",
          "next": [
            201180039
          ]
        },
        "201180039": {
          "id": 201180039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Apa kamu sekarang sudah paham? Terkadang, kebenaran akan jadi makin jelas melalui debat, tapi terkadang juga tidak.",
          "next": [
            201180040
          ]
        },
        "201180040": {
          "id": 201180040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Kebanyakan kasusnya, debat itu cuma permainan kata-kata saja.",
          "next": [
            201180041
          ]
        },
        "201180041": {
          "id": 201180041,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Kalau yang lainnya ... meskipun aku tidak tertarik, tapi aku juga pernah diundang untuk mengikuti debat yang melampaui tingkatan bahasa.",
          "next": [
            201180042
          ]
        },
        "201180042": {
          "id": 201180042,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Melampaui tingkatan bahasa?",
          "next": [
            201180043
          ]
        },
        "201180043": {
          "id": 201180043,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Tubuh yang sehat pasti akan selalu berguna. Kamu harus ingat itu, {NICKNAME}.",
          "next": []
        }
