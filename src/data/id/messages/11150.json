        },
        "111500004": {
          "id": 111500004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ya.",
          "next": [111500005]
        },
        "111500005": {
          "id": 111500005,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Lama tak berjumpa, Bronya.",
          "next": [111500006]
        },
        "111500006": {
          "id": 111500006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Bronya! Kami kangen banget sama kamu!",
          "next": [111500007]
        },
        "111500007": {
          "id": 111500007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Baguslah! Aku cemas kalian tidak bisa menerima pesanku.",
          "next": [111500008]
        },
        "111500008": {
          "id": 111500008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Aku tahu kalian sangat sibuk, jadi aku akan langsung ke intinya saja.",
          "next": [111500009]
        },
        "111500009": {
          "id": 111500009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Belakangan aku sedang mempersiapkan Festival Solwarm, festival tradisional Belobog. Dulu karena ada Fragmentum dan pemblokiran jalur, perayaan dihentikan selama bertahun-tahun. Penduduk Belobog sudah lama tidak merasakan suasana perayaan festival.",
          "next": [111500010]
        },
        "111500010": {
          "id": 111500010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Kalau kalian ada waktu, datanglah ke perayaan \"Festival Solwarm\" untuk menyaksikan pencapaian kami dalam menghidupkan kembali Belobog.",
          "next": [111500011, 111500013]
        },
        "111500011": {
          "id": 111500011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku pasti akan datang!",
          "next": [111500012]
        },
        "111500012": {
          "id": 111500012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Aku juga! Festival dan perayaan tidak boleh sampai dilewatkan!",
          "next": [111500016]
        },
        "111500013": {
          "id": 111500013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Waduh, sayangnya aku lagi tidak punya waktu luang ....",
          "next": [111500014]
        },
        "111500014": {
          "id": 111500014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "@{NICKNAME} Hei, jangan pura-pura deh. Kamu pasti mau rebahan di kamar sambil main ponsel kan!",
          "next": [111500015]
        },
        "111500015": {
          "id": 111500015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Tenang saja, kami pasti datang! Festival dan perayaan tidak boleh sampai dilewatkan!",
          "next": [111500016]
        },
        "111500016": {
          "id": 111500016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Baguslah kalau kalian bisa datang. Semua orang di sini juga rindu dengan kalian.",
          "next": [111500017]
        },
        "111500017": {
          "id": 111500017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "@Dan Heng Kamu lagi sibuk ya? Dari tadi belum muncul di grup.",
          "next": [111500018]
        },
        "111500018": {
          "id": 111500018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Belakangan ini dia lagi menutup diri. Mungkin semua peralatan elektroniknya juga dimatikan.",
          "next": [111500019]
        },
        "111500019": {
          "id": 111500019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Nanti biar kami saja yang tanyakan.",
          "next": [111500020]
        },
        "111500020": {
          "id": 111500020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Terima kasih, March.",
          "next": [111500021]
        },
        "111500021": {
          "id": 111500021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Aku sudah tidak sabar untuk bertemu dengan kalian di perayaan Festival Solwarm.",
          "next": [111500022]
        },
