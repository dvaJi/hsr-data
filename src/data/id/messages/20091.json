  "sections": [
    {
      "id": 2009100,
      "startingMessageId": [200910000],
      "messages": {
        "200910000": {
          "id": 200910000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Aku belajar dari Bailu tentang bagaimana meracik obat-obatan, dan telah memperoleh pemahaman yang lebih mendalam tentang keajaiban farmakologinya.",
          "next": [200910001, 200910005]
        },
        "200910001": {
          "id": 200910001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Obat darurat ya?",
          "next": [200910002]
        },
        "200910002": {
          "id": 200910002,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Sepertinya dia pernah kasih beberapa ke Pom-Pom.",
          "next": [200910003]
        },
        "200910003": {
          "id": 200910003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Oh ya?",
          "next": [200910004]
        },
        "200910004": {
          "id": 200910004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Obat darurat dapat langsung dimakan. Sangat praktis.",
          "next": [200910007]
        },
        "200910005": {
          "id": 200910005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ada obat yang enak, tidak?",
          "next": [200910006]
        },
        "200910006": {
          "id": 200910006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Mungkin ada. Aku coba pelajari nanti kalau ada waktu.",
          "next": [200910007]
        },
        "200910007": {
          "id": 200910007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Dia pernah menyebutkan prinsip tentang \"satu orang, satu pengobatan\". Kalau aku berhasil mempelajarinya, aku bisa meracik obat berdasarkan kondisi masing-masing orang di Express.",
          "next": [200910008]
        },
        "200910008": {
          "id": 200910008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Kalau dipikir-pikir, agak disayangkan juga ya, karena aku tidak tahu teknik Cloudhymn yang bisa dipakai untuk menyembuhkan ....",
          "next": [200910009, 200910011, 200910013]
        },
        "200910009": {
          "id": 200910009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bagaimana kalau kamu tanyakan ke Omni-Synthesizer yang serba bisa?",
          "next": [200910010]
        },
        "200910010": {
          "id": 200910010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Mungkin akan lebih cepat kalau tanya ke Bailu.",
          "next": [200910015]
        },
        "200910011": {
          "id": 200910011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Astaga, ternyata Dan Heng itu tidak serba bisa ya ....",
          "next": [200910012]
        },
        "200910012": {
          "id": 200910012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Masih ada banyak hal yang perlu kupelajari.",
          "next": [200910015]
        },
        "200910013": {
          "id": 200910013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kamu tidak ingat satu pun?",
          "next": [200910014]
        },
        "200910014": {
          "id": 200910014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Aku tidak memiliki kekuatan seperti itu.",
          "next": [200910015]
        },
        "200910015": {
          "id": 200910015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Meski kekuatan Naga Terhormat berdasarkan pada warisan, tapi bagiku, kekuatan itu seperti \"pengetahuan\" dan bukan \"ingatan\".",
          "next": [200910016]
        },
        "200910016": {
          "id": 200910016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Kisah-kisah yang bisa kulihat ... kisah-kisah itu sudah lama ada sebelum aku mengetahui tentang diriku.",
          "next": [200910017, 200910023]
        },
        "200910017": {
          "id": 200910017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Karena merupakan \"pengetahuan,\" bagaimana menggunakannya berarti tergantung pada pilihanmu.",
          "next": [200910018]
        },
        "200910018": {
          "id": 200910018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Oh ya?",
          "next": [200910019]
        },
        "200910019": {
          "id": 200910019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Semoga aku membuat pilihan yang tepat.",
          "next": [200910020]
        },
        "200910020": {
          "id": 200910020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kamu sudah membantu banyak orang.",
          "next": [200910021]
        },
        "200910021": {
          "id": 200910021,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Termasuk aku, tentu saja.",
          "next": [200910022]
        },
        "200910022": {
          "id": 200910022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Baguslah kalau begitu.",
          "next": []
        },
        "200910023": {
          "id": 200910023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku juga mau memahami segala hal.",
          "next": [200910024]
        },
        "200910024": {
          "id": 200910024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Rasanya sangat menderita. Ada pemandangan dan suara yang tak terhitung jumlahnya di dalam otakmu, tapi kamu tidak pernah hadir di dalamnya.",
          "next": [200910025]
        },
        "200910025": {
          "id": 200910025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku tidak begitu paham, tapi apa mirip dengan Suara Stellaron?",
          "next": [200910026]
        },
        "200910026": {
          "id": 200910026,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Perlu waktu untuk terbiasa dengannya.",
          "next": [200910027]
        },
        "200910027": {
          "id": 200910027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Mungkin.",
          "next": [200910028]
        },
        "200910028": {
          "id": 200910028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Mungkin suatu hari aku bisa hidup bersamanya, sama seperti kamu dengan Stellaron.",
          "next": []
        }
      },
      "participatingContactsIds": [1002, 8001]
    }
  ]
}
