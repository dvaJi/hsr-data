        },
        "121900036": {
          "id": 121900036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 103,
          "text": "Teman-teman, tujuan utama kita adalah?",
          "next": [121900037]
        },
        "121900037": {
          "id": 121900037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
