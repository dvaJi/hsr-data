        "200620020": {
          "id": 200620020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Tapi melihat ke belakang dan memikirkannya, cara dia menggunakan pedang agak mirip ibuku.",
