        "201710007": {
          "id": 201710007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Tenang, ada cukup untuk semua Kru Express kok. Silakan makan sepuasmu, penyelamatku.",
