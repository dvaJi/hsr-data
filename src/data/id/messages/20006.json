  "sections": [
    {
      "id": 2000600,
      "startingMessageId": [200060000],
      "messages": {
        "200060000": {
          "id": 200060000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Aku menemukan log komunikasi Express lama di dalam Arsip, dan salah satu kategorisasinya adalah \"lelucon bapak-bapak\".",
          "next": [200060001]
        },
        "200060001": {
          "id": 200060001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Aku tidak begitu paham, tapi naluriku bilang kalau kamu seharusnya paham.",
          "next": [200060002, 200060004]
        },
        "200060002": {
          "id": 200060002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Memangnya aku terlihat setua itu?",
          "next": [200060003]
        },
        "200060003": {
          "id": 200060003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Ya.",
          "next": [200060006]
        },
        "200060004": {
          "id": 200060004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku tidak mengerti.",
          "next": [200060005]
        },
        "200060005": {
          "id": 200060005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Sepertinya kamu belum pernah melihat lognya ya ....",
          "next": [200060006]
        },
        "200060006": {
          "id": 200060006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Jadi ini yang tertulis di dalam log.",
          "next": [200060007]
        },
        "200060007": {
          "id": 200060007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "\"Waktu tugas jaga bersama Himeko, Himeko mengigau selagi tidur.\" — Pom-Pom",
          "next": [200060008]
        },
        "200060008": {
          "id": 200060008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Lalu ada entri lain satu jam kemudian.",
          "next": [200060009]
        },
        "200060009": {
          "id": 200060009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "\"Waktu tugas jaga bersama Pom-Pom, Pom-Pom mengigau selama hampir satu jam.\" — Himeko",
          "next": [200060010]
        },
        "200060010": {
          "id": 200060010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Bagaimana menurutmu, {NICKNAME}?",
          "next": [200060011, 200060013]
        },
        "200060011": {
          "id": 200060011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "....",
          "next": [200060012]
        },
        "200060012": {
          "id": 200060012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Oke, sepertinya tag kategorisasi ini boleh disimpan dulu.",
          "next": [200060015]
        },
        "200060013": {
          "id": 200060013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Menurutku, yang tadi itu sudah jelas cerita horor sih.",
          "next": [200060014]
        },
        "200060014": {
          "id": 200060014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Oh ya? Aku tidak mengerti, tapi sepertinya kita butuh tag tambahan untuk itu.",
          "next": [200060015]
        },
        "200060015": {
          "id": 200060015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Lognya berhenti sampai di situ. Aku sudah tanya ke Himeko dan Pom-Pom, tapi tidak ada satu pun dari mereka yang ingat kejadian itu.",
          "next": [200060016]
        },
        "200060016": {
          "id": 200060016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "\"Siapa yang mengigau dalam tidurnya?\" Sepertinya kita cuma bisa mengandalkan entri lain di dalam Arsip untuk memverifikasi informasi ini.",
          "next": [200060018]
        },
        "200060017": {
          "id": 200060017,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Suka]",
          "next": []
        },
        "200060018": {
          "id": 200060018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Aku mulai percaya kalau Arsip itu tahu segalanya.",
          "next": [200060017]
        }
      },
      "participatingContactsIds": [1002, 8001]
    }
  ]
}
