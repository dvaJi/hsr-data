        },
        "200630007": {
          "id": 200630007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ketemu guru rasanya hal yang baik kan?",
          "next": [200630008]
        },
        "200630008": {
          "id": 200630008,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Kenapa nasib buruk?",
          "next": [200630009]
        },
        "200630009": {
