        },
        "200630010": {
          "id": 200630010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ah, kamu tidak tahu sih!",
          "next": [200630011]
        },
        "200630011": {
          "id": 200630011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Saat aku sekolah, tuan tidak suka melihatku.",
          "next": [200630019]
        },
        "200630012": {
          "id": 200630012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Lyceum apaan? \"Tuan\" itu siapa lagi?",
          "next": [200630013]
        },
        "200630013": {
          "id": 200630013,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Aku ini alien, tidak terlalu paham.",
          "next": [200630014]
        },
        "200630014": {
          "id": 200630014,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Bingung]",
          "next": [200630015]
        },
        "200630015": {
          "id": 200630015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ah, tidak apa-apa! Aku juga mungkin tidak tahu lebih banyak darimu.",
          "next": [200630016]
        },
        "200630016": {
          "id": 200630016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Aku akan periksa bagaimana kalian mengobrol, tunggu sebentar.",
          "next": [200630017]
        },
        "200630017": {
          "id": 200630017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Oh, ada.",
          "next": [200630018]
        },
        "200630018": {
          "id": 200630018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Lyceum adalah sekolah, dan \"tuan\" adalah guru.",
          "next": [200630019]
        },
        "200630019": {
          "id": 200630019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Oh, aku benar-benar tidak ingin mengingatnya, terlalu canggung.",
          "next": [200630020]
        },
        "200630020": {
          "id": 200630020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Coba katakan, biar aku ikut merasa canggung.",
          "next": [200630021]
        },
        "200630021": {
          "id": 200630021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Aku berasal dari keluarga militer. Ibuku sudah mengejarku untuk berlatih seni bela diri sejak aku masih kecil.",
          "next": [200630022]
        },
        "200630022": {
          "id": 200630022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Aku berlatih seni bela diri sebelum matahari terbit, lalu pergi ke Lyceum untuk sekolah.",
          "next": [200630023]
        },
        "200630023": {
          "id": 200630023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Tapi aku sudah kehabisan tenaga, dengar pelajaran tidak ada yang masuk sama sekali, jadi aku tidur saja.",
          "next": [200630024]
        },
        "200630024": {
          "id": 200630024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ketika dia melihatku hari ini, dia berteriak, \"Hei! Bukankah ini Li si Muka Bantal!\"",
          "next": [200630025]
        },
        "200630025": {
          "id": 200630025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Semua orang di jalanan langsung menatapku, hal itu sangatlah canggung.",
          "next": [200630026]
        },
        "200630026": {
          "id": 200630026,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[Menangis]",
