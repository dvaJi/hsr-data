        },
        "200630015": {
          "id": 200630015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ah, tidak apa-apa kok! Aku rasa aku lebih banyak tidak tahunya darimu.",
          "next": [
            200630016
          ]
        },
        "200630016": {
          "id": 200630016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Aku akan coba jelaskan padamu, tunggu sebentar.",
