          ]
        },
        "122380009": {
          "id": 122380009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est vraiment urgent",
          "next": [
            122380010
          ]
        },
        "122380010": {
          "id": 122380010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Du calme, mon {M#ami}{F#amie}",
