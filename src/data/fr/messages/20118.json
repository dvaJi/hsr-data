  "sections": [
    {
      "id": 2011800,
      "startingMessageId": [
        201180000
      ],
      "messages": {
        "201180000": {
          "id": 201180000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "<u>Touchez pour vous inscrire à la [Coupe Rosetta-Rosenthal]. Nous avons hâte de faire votre connaissance.</u>",
          "next": [
            201180001
          ]
        },
        "201180001": {
          "id": 201180001,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Pièce jointe <i>Thème du débat du 1er tour</i> envoyée",
          "next": [
            201180002
          ]
        },
        "201180002": {
          "id": 201180002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "{NICKNAME}, je t'ai envoyé les infos concernant le dernier concours de débats de la guilde. Le thème du débat est <i>La science et la technologie sont-elles neutres ?</i>",
          "next": [
            201180003
          ]
        },
        "201180003": {
          "id": 201180003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Ne te méprends pas : je ne fais que transférer le message de quelqu'un d'autre",
          "next": [
            201180004
          ]
        },
        "201180004": {
          "id": 201180004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Le thème est accessible aux débutants",
          "next": [
            201180005,
            201180008,
            201180010
          ]
        },
        "201180005": {
          "id": 201180005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je m'exprime suffisamment bien pour m'en sortir",
          "next": [
            201180006
          ]
        },
        "201180006": {
          "id": 201180006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Non, je ne pense pas",
          "next": [
            201180007
          ]
        },
        "201180007": {
          "id": 201180007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "L'art du langage ne connaît aucune limite",
          "next": [
            201180012
          ]
        },
        "201180008": {
          "id": 201180008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mais je n'y connais rien en débats",
          "next": [
            201180009
          ]
        },
        "201180009": {
          "id": 201180009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Raison de plus pour apprendre",
          "next": [
            201180012
          ]
        },
        "201180010": {
          "id": 201180010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Pardon, mais je suis {F#antisociale}{M#antisocial}",
          "next": [
            201180011
          ]
        },
        "201180011": {
          "id": 201180011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Tu ne te connais pas suffisamment bien",
          "next": [
            201180012
          ]
        },
        "201180012": {
          "id": 201180012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Peu importe que tu participes au concours ou non",
          "next": [
            201180013
          ]
        },
        "201180013": {
          "id": 201180013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Mais j'aimerais savoir ce que tu penses du débat en tant qu'activité",
          "next": [
            201180014,
            201180015,
            201180016
          ]
        },
        "201180014": {
          "id": 201180014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Débattre, c'est se disputer avec la partie adverse",
          "next": [
            201180017
          ]
        },
        "201180015": {
          "id": 201180015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Débattre, c'est énerver la partie adverse",
          "next": [
            201180017
          ]
        },
        "201180016": {
          "id": 201180016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Débattre, c'est se narguer mutuellement",
          "next": [
            201180017
          ]
        },
        "201180017": {
          "id": 201180017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Je vais t'expliquer en prenant pour exemple le thème du débat",
          "next": [
            201180018
          ]
        },
        "201180018": {
          "id": 201180018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Quel camp choisis-tu ?",
          "next": [
            201180019,
            201180025
          ]
        },
        "201180019": {
          "id": 201180019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "La technologie est neutre",
          "next": [
            201180020
          ]
        },
        "201180020": {
          "id": 201180020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Je prendrai donc le contrepied. Je te laisse commencer",
          "next": [
            201180021
          ]
        },
        "201180021": {
          "id": 201180021,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "La technologie est comme une épée à double tranchant dans le sens où elle peut servir à faire le bien comme le mal",
          "next": [
            201180022
          ]
        },
        "201180022": {
          "id": 201180022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "As-tu déjà entendu parler de l'expérience atavique du Dr Primitif ?",
          "next": [
            201180023
          ]
        },
        "201180023": {
          "id": 201180023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Afin de provoquer une réaction de stress chez ses sujets, il a développé une série de technologies visant à les torturer et à les faire souffrir",
          "next": [
            201180024
          ]
        },
        "201180024": {
          "id": 201180024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Une telle technologie est abominable",
          "next": [
            201180030
          ]
        },
        "201180025": {
          "id": 201180025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "La technologie n'est pas neutre",
          "next": [
            201180026
          ]
        },
        "201180026": {
          "id": 201180026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Je prendrai donc le contrepied. Je te laisse commencer",
          "next": [
            201180027
          ]
        },
        "201180027": {
          "id": 201180027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Certaines technologies sont intrinsèquement bonnes, comme celles qui prolongent la durée de vie",
          "next": [
            201180028
          ]
        },
        "201180028": {
          "id": 201180028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Les technologies médicales avancées ont sauvé de nombreuses vies, mais les traitements médicaux ne rallongent pas toujours la durée de vie",
          "next": [
            201180029
          ]
        },
        "201180029": {
          "id": 201180029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Tu n'as jamais visité l'Alliance Xianzhou ? Tu devrais en savoir plus que moi sur le prix de l'immortalité",
          "next": [
            201180030
          ]
        },
        "201180030": {
          "id": 201180030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "J'ai fini",
          "next": [
            201180031
          ]
        },
        "201180031": {
          "id": 201180031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Tu vois, ça confirme ce que je disais",
          "next": [
            201180032,
            201180035
          ]
        },
        "201180032": {
          "id": 201180032,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "J'ai encore beaucoup à dire !",
          "next": [
            201180033
          ]
        },
        "201180033": {
          "id": 201180033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "La plupart du temps, la capacité à parler vite témoigne d'une grande rapidité de réflexion",
          "next": [
            201180034
          ]
        },
        "201180034": {
          "id": 201180034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Mais cela peut aussi vouloir dire que la personne ne réfléchit pas avant de parler",
          "next": [
            201180038
          ]
        },
        "201180035": {
          "id": 201180035,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Vous dites n'importe quoi",
          "next": [
            201180036
          ]
        },
        "201180036": {
          "id": 201180036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Tu perdras avec cet argument",
          "next": [
            201180037
          ]
        },
        "201180037": {
          "id": 201180037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Mais tu as raison.",
          "next": [
            201180038
          ]
        },
        "201180038": {
          "id": 201180038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Il est donc clair qu'aucun de nous n'est capable de convaincre l'autre.",
          "next": [
            201180039
          ]
        },
        "201180039": {
          "id": 201180039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Tu comprends, maintenant ? Les débats permettent parfois de voir la vérité de manière plus claire, mais pas toujours.",
          "next": [
            201180040
          ]
        },
        "201180040": {
          "id": 201180040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "La plupart des débats ne sont que l'art de jouer avec des mots.",
          "next": [
            201180041
          ]
        },
        "201180041": {
          "id": 201180041,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Aussi... J'ai beau ne pas m'y être inscrit, on m'a invité à participer à des débats qui vont au-delà du royaume du langage.",
          "next": [
            201180042
          ]
        },
        "201180042": {
          "id": 201180042,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Au-delà du royaume du langage ?",
          "next": [
            201180043
          ]
        },
        "201180043": {
          "id": 201180043,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1305,
          "text": "Il est essentiel d'avoir un corps en bonne santé à chaque instant. Souviens-t'en, {NICKNAME}.",
          "next": []
        }
      },
      "participatingContactsIds": [
        1305,
        8001
      ]
    }
  ]
}