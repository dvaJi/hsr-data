        "111540002": {
          "id": 111540002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Les agents de la CPI avaient pris le contrôle de la Grande mine. On a réussi à les déloger de là avec l'aide de Seele",
          "next": [111540003]
        },
        "111540003": {
          "id": 111540003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Je vois, vous êtes avec Seele",
          "next": [111540004]
        },
        "111540004": {
          "id": 111540004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Comment vous vous en sortez ? Vous êtes {M#blessés}{F#blessées} ?",
          "next": [111540005]
        },
        "111540005": {
          "id": 111540005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "On a la situation sous contrôle, ne t'en fais pas",
          "next": [111540006, 111540008]
        },
        "111540006": {
          "id": 111540006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "{M#Désolé}{F#Désolée} de ne pas avoir réussi à convaincre Topaz...",
          "next": [111540007]
        },
        "111540007": {
          "id": 111540007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Ne t'en fais pas, {NICKNAME} ! Ce n'était pas à toi de le faire en premier lieu.",
          "next": [111540010]
        },
        "111540008": {
          "id": 111540008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "J'ai bien peur que la CPI ne s'arrête pas là",
