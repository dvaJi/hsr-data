      "messages": {
        "121170000": {
          "id": 121170000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Notre astroskiff a dévié de sa trajectoire. Comment se présente la situation de ton côté ?",
          "next": [121170001, 121170002]
        },
        "121170001": {
          "id": 121170001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Te revoilà vivant ?",
          "next": [121170003]
        },
        "121170002": {
          "id": 121170002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tu as à nouveau du réseau ?",
          "next": [121170003]
        },
        "121170003": {
          "id": 121170003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Oui. Comment ça se passe de ton côté ?",
          "next": [121170004, 121170005]
        },
        "121170004": {
          "id": 121170004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "On a capturé Kafka, mais c'était trop facile. C'était un peu bizarre",
          "next": [121170006]
        },
        "121170005": {
          "id": 121170005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "On a capturé Kafka et on est allés à la Commission de divination",
          "next": [121170006]
        },
        "121170006": {
          "id": 121170006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Et le comparse de Kafka ? Tu as vu Blade ?",
          "next": [121170007, 121170008]
        },
        "121170007": {
          "id": 121170007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Kafka était seule quand on l'a attrapée. Pourquoi ?",
          "next": [121170009]
        },
        "121170008": {
          "id": 121170008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Il s'est enfui. On ne sait pas où il est",
          "next": [121170009]
        },
        "121170009": {
          "id": 121170009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Faites attention. Il est extrêmement dangereux.",
          "next": [121170010]
        },
        "121170010": {
          "id": 121170010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Je vais me débarrasser des Chevaliers des nuages et je vous rattraperai.",
          "next": [121170012]
        },
        "121170012": {
          "id": 121170012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Attends. Qu'est-ce qui se passe de ton côté ?",
          "next": [121170013]
        },
        "121170013": {
