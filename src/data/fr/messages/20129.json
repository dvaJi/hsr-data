        "201290031": {
          "id": 201290031,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Micha... ? Hé ? Hé oh ?",
          "next": [201290030]
        },
        "201290032": {
          "id": 201290032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1312,
          "text": "Tu m'as fichu la trouille ! Je suis tellement content que tu ailles bien ! Je cherchais un moyen de te sauver !",
          "next": [201290033]
        },
        "201290033": {
          "id": 201290033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1312,
          "text": "Attends ! Une ombre grimaçante... qui pleure... Elle est de retour...",
          "next": [201290034]
        },
        "201290034": {
          "id": 201290034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1312,
          "text": "Je vais utiliser mon balai comme batte de baseball... Ça devrait marcher aussi, pas vrai ?",
          "next": [201290035]
        },
        "201290035": {
          "id": 201290035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1312,
