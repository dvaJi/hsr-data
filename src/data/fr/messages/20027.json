  "sections": [
    {
      "id": 2002700,
      "startingMessageId": [200270000],
      "messages": {
        "200270000": {
          "id": 200270000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Quelle couleur me va le mieux, {NICKNAME} ? Le bleu ou le gris ?",
          "next": [200270001]
        },
        "200270001": {
          "id": 200270001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "En tant {M#qu'ami}{F#qu'amie}, donne-moi ton avis en fonction de ta première impression !",
          "next": [200270002, 200270005, 200270008]
        },
        "200270002": {
          "id": 200270002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Le bleu ? Comme la couleur de ton uniforme",
          "next": [200270003]
        },
        "200270003": {
          "id": 200270003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Héhé, je savais que le bleu m'allait comme un gant",
          "next": [200270004]
        },
        "200270004": {
          "id": 200270004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "On dirait qu'on a les mêmes goûts en matière de mode !",
          "next": [200270011]
        },
        "200270005": {
          "id": 200270005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Le gris ? Comme la teinte de tes cheveux",
          "next": [200270006]
        },
        "200270006": {
          "id": 200270006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Ouais, le gris c'est pas trop mal",
          "next": [200270007]
        },
        "200270007": {
          "id": 200270007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Puisque le gris me va bien selon toi, je vais prendre cette couleur pour essayer",
          "next": [200270011]
        },
        "200270008": {
          "id": 200270008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Envoie-moi un selfie pour prouver que tu ne t'es pas fait pirater. Je te trouve un peu bizarre",
          "next": [200270009]
        },
        "200270009": {
          "id": 200270009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Bizarre ? Je suis juste enthousiaste",
          "next": [200270010]
        },
        "200270010": {
          "id": 200270010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Oublie ça ! Je vais prendre les deux. On verra bien quelle couleur me va le mieux",
          "next": [200270011]
        },
        "200270011": {
          "id": 200270011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Je vais m'acheter de nouvelles chaussures de sport, alors j'aimerais avoir ton avis puisque tu es mon {M#ami}{F#amie}",
          "next": [200270012]
        },
        "200270012": {
          "id": 200270012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Je t'enverrai une photo quand je les aurai",
          "next": [200270013]
        },
        "200270013": {
          "id": 200270013,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "[J'aime]",
          "next": [200270014, 200270015]
        },
        "200270014": {
          "id": 200270014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Rendez-lui son téléphone",
          "next": [200270016]
        },
        "200270015": {
          "id": 200270015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Arrêtez de faire le clown, Asta",
          "next": [200270016]
        },
        "200270016": {
          "id": 200270016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Hein ? Mince, je suis démasquée. On aurait dit Arlan, non ?",
          "next": [200270017]
        },
        "200270017": {
          "id": 200270017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Il a modifié le message plusieurs fois sans jamais te l'envoyer",
          "next": [200270018]
        },
        "200270018": {
          "id": 200270018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Désolé, Madame Asta avait emprunté mon téléphone",
          "next": [200270019]
        },
        "200270019": {
          "id": 200270019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Pardon pour le dérangement. Ça ne se reproduira pas",
          "next": [200270020]
        },
        "200270020": {
          "id": 200270020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "On se parle plus tard. Madame Asta est en train de chercher sa clé de chiffrement pour le paiement. Il ne faut pas qu'elle la trouve",
          "next": []
        }
      },
      "participatingContactsIds": [1008, 8001]
    }
  ]
}
