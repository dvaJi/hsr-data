        },
        "130080009": {
          "id": 130080009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Au fait... Tu ne t'es pas fait avoir, j'espère ? Tu n'as pas envoyé d'argent ?",
