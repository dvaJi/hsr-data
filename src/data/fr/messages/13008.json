        },
        "130080007": {
          "id": 130080007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Ne tombe pas dans le panneau et surtout, n'envoie pas d'argent, {NICKNAME}.",
