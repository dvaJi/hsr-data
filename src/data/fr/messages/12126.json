      "messages": {
        "121260101": {
          "id": 121260101,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "Tout va bien, {NICKNAME} ?",
          "next": [
            121260102,
            121260104
          ]
        },
        "121260102": {
          "id": 121260102,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Oui. Incroyablement bien, en fait",
          "next": [
            121260103
          ]
        },
        "121260103": {
          "id": 121260103,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "C'est très bien. J'ai toujours cru en tes capacités.",
          "next": [
            121260107
          ]
        },
        "121260104": {
          "id": 121260104,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Très bien. Je fais partie des Disciples du Sanctus Medicus à présent",
          "next": [
            121260103
          ]
        },
        "121260107": {
          "id": 121260107,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "On vient de me faire un rapport... Tu as sauvé un Chevalier des nuages ?",
          "next": [
            121260108
          ]
        },
        "121260108": {
          "id": 121260108,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ce n'était rien",
          "next": [
            121260109
          ]
        },
        "121260109": {
          "id": 121260109,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Je ne qualifierais pas ça de sauvetage. Disons plutôt une collaboration fructueuse.",
          "next": [
            121260110
          ]
        },
        "121260110": {
          "id": 121260110,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "Merci infiniment pour ton aide. Ce Chevalier des nuages a transmis toutes les informations au Siège de la clairvoyance divine",
          "next": [
            121260111
          ]
        },
        "121260111": {
          "id": 121260111,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "Si tu ne l'avais pas aidé, il aurait eu du mal à s'en sortir",
          "next": [
            121260113
          ]
        },
        "121260113": {
          "id": 121260113,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "Maintenant que tu as infiltré les Disciples du Sanctus Medicus, continue de surveiller leurs activités",
          "next": [
            121260118
          ]
        },
        "121260114": {
          "id": 121260114,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "J'ai déjà dit qu'un Chevalier des nuages du nom de Zhixin était porté disparu depuis quelques mois.",
          "next": [
            121260115
          ]
        },
        "121260115": {
          "id": 121260115,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "J'aimerais savoir ce qui lui est arrivé, mort ou vivant... afin de pouvoir transmettre une réponse à sa famille.",
          "next": [
            121260116
          ]
        },
        "121260116": {
          "id": 121260116,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "Évitons d'entrer en contact à partir de maintenant. Si on te voit aller et venir au Siège de la clairvoyance divine, ces disciples vont se méfier de toi",
          "next": [
            121260117
          ]
        },
        "121260117": {
          "id": 121260117,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 60,
          "text": "Reviens en un seul morceau. Autrement, le général m'aura dans le collimateur.",
          "next": []
        },
        "121260118": {
          "id": 121260118,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Y a-t-il un point particulier sur lequel je devrais me concentrer ?",
          "next": [
            121260114
          ]
        }
      },
