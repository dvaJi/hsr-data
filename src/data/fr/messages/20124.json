        },
        "201240027": {
          "id": 201240027,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Attention ! Merci de ne pas utiliser d'application de chat externe pour effectuer des transactions afin d'éviter les risques d'escroqueries et d'activités frauduleuses !",
          "next": [
            201240028,
            201240033
          ]
        },
        "201240028": {
          "id": 201240028,
          "type": "Text",
