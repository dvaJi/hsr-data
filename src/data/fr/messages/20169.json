        },
        "201690001": {
          "id": 201690001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "Ma queue est encore toute frisée",
          "next": [
            201690002
          ]
        },
        "201690002": {
          "id": 201690002,
          "type": "Sticker",
          "sender": "NPC",
