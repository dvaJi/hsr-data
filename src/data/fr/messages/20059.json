        "200590006": {
          "id": 200590006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bien sûr",
          "next": [
            200590005
          ]
        },
        "200590007": {
          "id": 200590007,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Mais je n'ai pas entendu l'histoire d'aujourd'hui, tu crois que je vais réussir à la comprendre demain ?",
          "next": [
            200590011
          ]
        },
        "200590008": {
          "id": 200590008,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Pleure]",
