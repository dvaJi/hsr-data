          ]
        },
        "200590022": {
          "id": 200590022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Ça ne te dit rien ?",
          "next": [
            200590023
          ]
        },
        "200590023": {
          "id": 200590023,
          "type": "Text",
          "sender": "NPC",
