      "messages": {
        "200410000": {
          "id": 200410000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "{NICKNAME}, il y a une chose dont j'aimerais te parler.",
          "next": [
            200410001
          ]
        },
        "200410001": {
          "id": 200410001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Tu as le temps de discuter ?",
          "next": [
            200410002
          ]
        },
        "200410002": {
          "id": 200410002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Oui, qu'y a-t-il ?",
          "next": [
            200410003
          ]
        },
        "200410003": {
          "id": 200410003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "En fait, ma plus jeune sœur vient tout juste de rentrer d'une expédition scientifique.",
          "next": [
            200410004
          ]
        },
        "200410004": {
          "id": 200410004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "J'aimerais lui offrir un cadeau, qu'est-ce qui pourrait convenir ?",
          "next": [
            200410005,
            200410014,
            200410023
          ]
        },
        "200410005": {
          "id": 200410005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Qu'est-ce qu'elle aime ?",
          "next": [
            200410006
          ]
        },
        "200410006": {
          "id": 200410006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "J'ai un peu honte de le dire, mais je ne suis pas totalement sûr de ce qu'elle aime.",
          "next": [
            200410007
          ]
        },
        "200410007": {
          "id": 200410007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Je sais seulement qu'elle est très passionnée par les sciences et les excursions en plein air.",
          "next": [
            200410008
          ]
        },
        "200410008": {
          "id": 200410008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Oh, j'ai une idée. Et si je lui faisais cadeau d'un nouvel ensemble d'équipement d'expédition scientifique ?",
          "next": [
            200410009
          ]
        },
        "200410009": {
          "id": 200410009,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[J'aime]",
          "next": [
            200410010
          ]
        },
        "200410010": {
          "id": 200410010,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ce n'est pas une mauvaise idée",
          "next": [
            200410011
          ]
        },
        "200410011": {
          "id": 200410011,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Tant que ça lui plaît",
          "next": [
            200410012
          ]
        },
        "200410012": {
          "id": 200410012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Tu as raison. Merci de m'avoir aidé à mettre de l'ordre dans mes idées.",
          "next": [
            200410013
          ]
        },
        "200410013": {
          "id": 200410013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Je vais aller chercher des cadeaux. À bientôt.",
          "next": []
        },
        "200410014": {
          "id": 200410014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Va chez la fleuriste de l'été éternel et achète-lui un bouquet",
          "next": [
            200410015
          ]
        },
        "200410015": {
          "id": 200410015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "J'avais pensé lui offrir des fleurs, mais je viens de me souvenir qu'elle avait dit ne pas être intéressée par les plantes ordinaires.",
          "next": [
            200410016
          ]
        },
        "200410016": {
          "id": 200410016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Elle aime les fleurs des Plaines des neiges, elles sont rares en ville.",
          "next": [
            200410017
          ]
        },
        "200410017": {
          "id": 200410017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "J'ai une idée. À la prochaine patrouille dans les Plaines des neiges, je chercherai quelque chose à lui rapporter.",
          "next": [
            200410018
          ]
        },
        "200410018": {
          "id": 200410018,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[J'aime]",
          "next": [
            200410019
          ]
        },
        "200410019": {
          "id": 200410019,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ce n'est pas une mauvaise idée",
          "next": [
            200410020
          ]
        },
        "200410020": {
          "id": 200410020,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Mais fais attention",
          "next": [
            200410021
          ]
        },
        "200410021": {
          "id": 200410021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Haha, compte sur moi, merci.",
          "next": [
            200410022
          ]
        },
        "200410022": {
          "id": 200410022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Je vais trouver de quoi m'occuper maintenant, à plus tard.",
          "next": []
        },
        "200410023": {
          "id": 200410023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tu peux lui offrir de nouveaux vêtements",
          "next": [
            200410024
          ]
        },
        "200410024": {
          "id": 200410024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "J'y ai pensé, mais je n'ai pas vraiment bon goût en matière de mode, donc je n'ai pas osé me décider.",
          "next": [
            200410025
          ]
        },
        "200410025": {
          "id": 200410025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Serval lui a acheté de très beaux vêtements.",
          "next": [
            200410026
          ]
        },
        "200410026": {
          "id": 200410026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Mais je ne veux pas que Serval l'apprenne.",
          "next": [
            200410027
          ]
        },
        "200410027": {
          "id": 200410027,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Refuse]",
          "next": [
            200410028
          ]
        },
        "200410028": {
          "id": 200410028,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Pourquoi ?",
          "next": [
            200410029
          ]
        },
        "200410029": {
          "id": 200410029,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Vous êtes de la même famille, va simplement lui demander",
          "next": [
            200410030
          ]
        },
        "200410030": {
          "id": 200410030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "...",
          "next": [
