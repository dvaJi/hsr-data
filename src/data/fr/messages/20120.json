{
  "id": 20120,
  "contacts": {
    "id": 1305,
    "name": "Dr Ratio",
    "icon": "1305",
    "signature": "« Rien ne presse. »",
    "typeId": 1,
    "type": "Personnages"
  },
  "relatedMessages": [
