    {
      "id": 1302001,
      "startingMessageId": [130200101],
      "messages": {
        "130200101": {
          "id": 130200101,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Je vais vous dire comment je fais pour me détendre",
          "next": [130200102]
        },
        "130200102": {
          "id": 130200102,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Je me trouve un endroit d'où je peux voir passer les astroskiffs au Havre des astroskiffs et je me laisse aller à rêver !",
          "next": [130200103]
        },
        "130200103": {
          "id": 130200103,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "... On peut faire la même chose au Comte insomniaque",
          "next": [130200104]
        },
        "130200104": {
          "id": 130200104,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Quand on regarde les astroskiffs passer",
          "next": [130200105]
        },
        "130200105": {
          "id": 130200105,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Vous n'avez pas idée à quel point le temps passe vite",
          "next": [130200106]
        },
        "130200106": {
          "id": 130200106,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "C'est la meilleure manière de se détendre pour moi",
          "next": [130200107]
        },
        "130200107": {
          "id": 130200107,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Je ne vois pas de quoi tu parles",
          "next": [130200108]
        },
        "130200108": {
          "id": 130200108,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Je ne vois pas de quoi tu parles",
          "next": [130200109, 130200112]
        },
        "130200109": {
          "id": 130200109,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je ne vois pas de quoi vous parlez",
          "next": [130200110]
        },
        "130200110": {
          "id": 130200110,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Je me fais du souci pour ta santé mentale, Huohuo",
          "next": [130200111]
        },
        "130200111": {
          "id": 130200111,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "On va essayer, mais tu sais que tu peux tout nous dire, n'est-ce pas, Huohuo ?",
          "next": [130200117]
        },
        "130200112": {
          "id": 130200112,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est comme écouter le bruit de la pluie pour se détendre",
          "next": [130200113]
        },
        "130200113": {
          "id": 130200113,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Je vois de quoi vous parlez",
          "next": [130200114]
        },
        "130200114": {
          "id": 130200114,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "C'est ça !",
          "next": [130200115]
        },
        "130200115": {
          "id": 130200115,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Je suis un peu inquiète pour votre santé mentale à {M#tous}{F#toutes} les deux",
          "next": [130200116]
        },
        "130200116": {
          "id": 130200116,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Si vous avez quelque chose sur le cœur, vous savez que vous pouvez nous en parler, pas vrai ?",
          "next": [130200117]
        },
        "130200117": {
          "id": 130200117,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ça fait presque une demi-journée que je suis ici, je me sens vraiment détendue",
          "next": [130200118]
        },
        "130200118": {
          "id": 130200118,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ah ! Attendez !",
          "next": [130200119]
        },
        "130200119": {
          "id": 130200119,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "?",
          "next": [130200120]
        },
        "130200120": {
          "id": 130200120,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Il s'est passé quelque chose ?",
          "next": [130200121]
        },
        "130200121": {
          "id": 130200121,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[Perplexe]",
          "next": [130200122]
        },
        "130200122": {
          "id": 130200122,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Je suis de retour",
          "next": [130200123]
        },
        "130200123": {
          "id": 130200123,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Monsieur Xiyan est venu, il m'a donné un sachet plein de bonbons sans rien dire et est reparti",
          "next": [130200124]
        },
        "130200124": {
          "id": 130200124,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Q—Qu'est ce qui se passe ?",
          "next": [130200125, 130200126]
        },
        "130200125": {
          "id": 130200125,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Si c'est pas une preuve que vous êtes adorable, ça ! Si j'étais dans le coin, moi aussi, je vous aurais donné des bonbons",
          "next": [130200128]
        },
        "130200126": {
          "id": 130200126,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Huohuo, vous aviez le regard fixé sur des astroskiffs, vous deviez faire de la peine",
          "next": [130200127]
        },
        "130200127": {
          "id": 130200127,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Quiconque avec un peu de compassion voudrait faire quelque chose pour vous",
          "next": [130200128]
        },
        "130200128": {
          "id": 130200128,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "C'est un peu comme ce poisson-koï dans l'étang. Tout le monde ne peut pas s'empêcher de le nourrir, tu comprends ?",
          "next": [130200129]
        },
        "130200129": {
          "id": 130200129,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "C'est pas interdit de nourrir les poissons dans cet étang ?",
          "next": [130200130]
        },
        "130200130": {
          "id": 130200130,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Rien n'interdit de nourrir Huohuo",
          "next": [130200131]
        },
        "130200131": {
          "id": 130200131,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "C'est... pas faux",
          "next": [130200132]
        },
        "130200132": {
          "id": 130200132,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "... On s'écarte un peu du sujet là, non ?",
          "next": [130200133]
        },
        "130200133": {
          "id": 130200133,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Moi aussi j'ai des bonbons. T'es où, Huohuo ?",
          "next": [130200134]
        },
        "130200134": {
          "id": 130200134,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Moi aussi j'en ai ! Je fonce vers toi !",
          "next": [130200135]
        },
        "130200135": {
          "id": 130200135,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[Chargez !]",
          "next": [130200136]
        },
        "130200136": {
          "id": 130200136,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Inutile de venir, je suis déjà partie",
          "next": [130200137]
        },
        "130200137": {
          "id": 130200137,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ahhh",
          "next": []
        }
      },
      "participatingContactsIds": [1217, 1210, 1206, 8001]
    },
