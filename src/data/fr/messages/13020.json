        "130200010": {
          "id": 130200010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Je vois",
          "next": [130200011, 130200016, 130200026]
        },
        "130200011": {
          "id": 130200011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est le groupe de discussion de la brigade de chasse aux fantômes ici",
          "next": [130200012]
        },
        "130200012": {
          "id": 130200012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Je comprends, il faut qu'on aille ailleurs",
          "next": [130200013]
        },
        "130200013": {
          "id": 130200013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Non non, ne l'écoute pas Sushang !",
          "next": [130200014]
        },
        "130200014": {
          "id": 130200014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Pas question de parler de fantômes dans le groupe de discussion de la brigade de chasse aux fantômes, c'est évident...",
          "next": [130200015]
        },
        "130200015": {
          "id": 130200015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Vraiment ?",
          "next": [130200030]
        },
        "130200016": {
          "id": 130200016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je pense qu'elles s'ennuient tellement qu'elles font tout pour attirer l'attention. Huohuo, ne tombez pas dans le panneau",
          "next": [130200017]
        },
        "130200017": {
          "id": 130200017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "D'accord",
          "next": [130200018]
        },
        "130200018": {
          "id": 130200018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "C'est pas possible",
          "next": [130200019]
        },
        "130200019": {
          "id": 130200019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "C'est vraiment pas possible",
          "next": [130200020]
        },
        "130200020": {
          "id": 130200020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Sushang, qu'est-ce qu'on fait ?",
          "next": [130200021]
        },
        "130200021": {
          "id": 130200021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Si j'avais su, je l'aurais arrêtée (TдT)",
          "next": [130200022]
        },
        "130200022": {
          "id": 130200022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "C'est Guigui qui m'a obligée (TдT)",
          "next": [130200023]
        },
        "130200023": {
          "id": 130200023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Désolée de ne pas avoir pu déjouer ses plans (TдT)",
          "next": [130200024]
        },
        "130200024": {
          "id": 130200024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Tu es fâchée, Huohuo ? (TдT) Je comprends (TдT)",
          "next": [130200025]
        },
        "130200025": {
          "id": 130200025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "J—Je ne suis pas fâchée, ne vous en faites pas",
          "next": [130200030]
        },
        "130200026": {
          "id": 130200026,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Moi aussi, je veux discuter",
          "next": [130200027]
        },
        "130200027": {
          "id": 130200027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ouah",
          "next": [130200028]
        },
        "130200028": {
          "id": 130200028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Et toi, Huohuo ?",
          "next": [130200029]
        },
        "130200029": {
          "id": 130200029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Je ne pensais pas que la question se poserait",
          "next": [130200030]
        },
        "130200030": {
          "id": 130200030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Mais ce groupe de discussion n'a jamais été exclusivement réservé au travail",
          "next": [130200031]
        },
        "130200031": {
          "id": 130200031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Vous pouvez discuter de ce que vous voulez",
          "next": [130200032]
        },
        "130200032": {
          "id": 130200032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
