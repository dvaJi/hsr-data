      "id": 1011901,
      "startingMessageId": [101190101],
      "messages": {
        "101190101": {
          "id": 101190101,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Bonjour, {NICKNAME}",
          "next": [101190102]
        },
        "101190102": {
          "id": 101190102,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Tu aurais cinq minutes ? Je ne t'ai pas encore {M#remercié}{F#remerciée} de m'avoir aidé lors de notre première rencontre.",
          "next": [101190103, 101190106, 101190109]
        },
        "101190103": {
          "id": 101190103,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Première rencontre... De quoi parles-tu ?",
          "next": [101190104]
        },
        "101190104": {
          "id": 101190104,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Tu sais, quand vous m'avez découvert blessé dans la salle de surveillance et m'avez transporté en lieu sûr.",
          "next": [101190105]
        },
        "101190105": {
          "id": 101190105,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Merci... sans votre aide, je n'aurais pas tenu bien longtemps...",
          "next": [101190112]
        },
        "101190106": {
          "id": 101190106,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tu exagères. Ce n'était rien",
          "next": [101190107]
        },
        "101190107": {
          "id": 101190107,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Je n'aurais pas survécu longtemps si vous ne m'aviez pas trouvé.",
          "next": [101190108]
        },
        "101190108": {
          "id": 101190108,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Merci de m'avoir trouvé dans la salle de surveillance et de m'avoir conduit en lieu sûr.",
          "next": [101190112]
        },
        "101190109": {
          "id": 101190109,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tu devrais remercier Dan Heng, c'est lui qui t'a trouvé",
          "next": [101190110]
        },
        "101190110": {
          "id": 101190110,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Vous m'avez tous les deux sauvé la vie. Je n'en avais plus pour longtemps",
          "next": [101190111]
        },
        "101190111": {
          "id": 101190111,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Merci de m'avoir trouvé et de m'avoir conduit en lieu sûr.",
          "next": [101190112]
        },
        "101190112": {
          "id": 101190112,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Tu aurais le temps de passer à la Zone de base ? J'ai un cadeau que j'aimerais t'offrir.",
          "next": [101190113, 101190115, 101190118]
        },
        "101190113": {
          "id": 101190113,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bien sûr. J'arrive tout de suite",
          "next": [101190114]
        },
        "101190114": {
          "id": 101190114,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Je t'attends",
          "next": []
        },
        "101190115": {
          "id": 101190115,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Un cadeau ? Maintenant, je me sens mal à l'aise",
          "next": [101190116]
        },
        "101190116": {
          "id": 101190116,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Ne t'en fais pas, ce n'est qu'une bricole",
          "next": [101190117]
        },
        "101190117": {
          "id": 101190117,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Je t'attends",
          "next": []
        },
        "101190118": {
          "id": 101190118,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tu pourrais me l'envoyer par courrier ?",
          "next": [101190119]
        },
        "101190119": {
          "id": 101190119,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Je préfèrerais te le remettre en personne. C'est plus formel",
          "next": [101190120]
        },
        "101190120": {
          "id": 101190120,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Je t'attends",
          "next": []
        }
      },
