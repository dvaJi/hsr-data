{
  "id": 20130,
  "contacts": {
    "id": 1301,
    "name": "Gallagher",
    "icon": "1301",
    "signature": "Chien de Penacony",
    "typeId": 1,
    "type": "Personnages"
  },
  "relatedMessages": [
