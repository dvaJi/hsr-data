  "sections": [
    {
      "id": 2007000,
      "startingMessageId": [200700015],
      "messages": {
        "200700002": {
          "id": 200700002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Discuter avec les gens qui t'entourent",
          "next": [200700003]
        },
        "200700003": {
          "id": 200700003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Pour être honnête, je ne sais pas vraiment comment engager une conversation... Mais avec mon cercueil, il y a toujours matière à discuter",
          "next": [200700008]
        },
        "200700004": {
          "id": 200700004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Faire ce qu'il te plaît. Tu peux jouer, lire, admirer le paysage, tout ce que tu veux",
          "next": [200700005]
        },
        "200700005": {
          "id": 200700005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Terminer les livres que j'ai empruntés sur le Luofu... Ce n'est pas une mauvaise idée",
          "next": [200700008]
        },
        "200700006": {
          "id": 200700006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sur la route, il n'y a pas grand-chose à faire à part dormir",
          "next": [200700007]
        },
        "200700007": {
          "id": 200700007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "C'est la destination qui compte, pas le trajet, hein ?",
          "next": [200700008]
        },
        "200700008": {
          "id": 200700008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "C'est ce que tu fais pendant tes voyages, {NICKNAME} ?",
          "next": [200700009, 200700012]
        },
        "200700009": {
          "id": 200700009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bien sûr, je fais ça tout le temps",
          "next": [200700010]
        },
        "200700010": {
          "id": 200700010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "On dirait que tu t'y connais.",
          "next": [200700011]
        },
        "200700011": {
          "id": 200700011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Merci pour tes conseils avisés",
          "next": [200700014]
        },
        "200700012": {
          "id": 200700012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bien sûr que non, je ne fais jamais ça",
          "next": [200700013]
        },
        "200700013": {
          "id": 200700013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Je vois que tu as adapté tes conseils à ma personnalité",
          "next": [200700014]
        },
        "200700014": {
          "id": 200700014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Je vais essayer",
          "next": []
        },
        "200700015": {
          "id": 200700015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Salut, {NICKNAME}",
          "next": [200700016]
        },
        "200700016": {
          "id": 200700016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Désolé de te déranger, mais j'ai oublié de te poser une question",
          "next": [200700017]
        },
        "200700017": {
          "id": 200700017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Comme je te l'ai dit, la vie d'un marchand itinérant est plutôt ennuyeuse et fastidieuse",
          "next": [200700018]
        },
        "200700018": {
          "id": 200700018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Tu as des conseils à me donner sur comment passer le temps en voyage, {NICKNAME} ?",
          "next": [200700002, 200700004, 200700006]
        }
      },
      "participatingContactsIds": [1203, 8001]
    }
  ]
}
