      "messages": {
        "121240000": {
          "id": 121240000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Hé, je ne vais pas y aller par quatre chemins",
          "next": [121240001]
        },
        "121240001": {
          "id": 121240001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Est-ce que tu as eu des nouvelles de Chengjie ces derniers temps ?",
          "next": [121240002, 121240011, 121240012]
        },
        "121240002": {
          "id": 121240002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est qui Chengjie, déjà ?",
          "next": [121240003]
        },
        "121240003": {
          "id": 121240003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Waouh, tu oublies vite",
          "next": [121240004]
        },
        "121240004": {
          "id": 121240004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Laisse-moi te montrer quelques mots-clés pour te rafraîchir la mémoire",
          "next": [121240005]
        },
        "121240005": {
          "id": 121240005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Le rêve du maître",
          "next": [121240006]
        },
        "121240006": {
          "id": 121240006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Apprenti de la Commission de l'artisanat",
          "next": [121240007]
        },
        "121240007": {
          "id": 121240007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "La personne qui n'a consacré que trois jours à son rêve",
          "next": [121240008, 121240010]
        },
        "121240008": {
          "id": 121240008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "{M#Désolé}{F#Désolée}, je ne le connais pas",
          "next": [121240009]
        },
        "121240009": {
          "id": 121240009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Soit tu as une mémoire de sept secondes, soit tu ne lui as pas parlé depuis trop longtemps, soit ton enthousiasme s'est heurté à sa paresse...",
          "next": [121240013]
        },
        "121240010": {
          "id": 121240010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je me rappelle de lui maintenant. Mais on ne s'est pas parlé depuis un certain temps.",
          "next": [121240013]
        },
        "121240011": {
          "id": 121240011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "On ne s'est pas parlé depuis longtemps",
          "next": [121240013]
        },
        "121240012": {
          "id": 121240012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Chengjie m'a dit d'attendre de ses nouvelles mais il ne m'a rien envoyé",
          "next": [121240013]
        },
        "121240013": {
          "id": 121240013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Je ne suis pas son amie la plus proche, mais étant donné qu'il est mon compagnon de jeu, je suis quand même inquiète de savoir si son état mental...",
          "next": [121240014]
        },
        "121240014": {
          "id": 121240014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "... lui permet de continuer à jouer avec moi",
          "next": [121240015]
        },
        "121240015": {
          "id": 121240015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1201,
          "text": "Va le voir. Je te retrouve à la Commission de l'artisanat.",
          "next": []
        }
      },
      "participatingContactsIds": [1201, 8001]
    }
  ]
}
