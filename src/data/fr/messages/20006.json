      "messages": {
        "200060000": {
          "id": 200060000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "J'ai trouvé un vieux registre des communications de l'Express dans la banque de données, et il y avait notamment une catégorie « Blagues nulles ».",
          "next": [
            200060001
          ]
        },
        "200060001": {
          "id": 200060001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Je n'y comprends pas grand-chose, mais mon instinct me dit que ça devrait te parler.",
          "next": [
            200060002,
            200060004
          ]
        },
        "200060002": {
          "id": 200060002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tu penses que je suis ce genre de personne ?",
          "next": [
            200060003
          ]
        },
        "200060003": {
          "id": 200060003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Oui.",
          "next": [
            200060006
          ]
        },
        "200060004": {
          "id": 200060004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je ne comprends pas",
          "next": [
            200060005
          ]
        },
        "200060005": {
          "id": 200060005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Tu devrais vraiment jeter un œil au registre...",
          "next": [
            200060006
          ]
        },
        "200060006": {
          "id": 200060006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Voici un petit extrait.",
          "next": [
            200060007
          ]
        },
        "200060007": {
          "id": 200060007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "« En mission de surveillance avec Himeko. Elle parle dans son sommeil. » Pom-Pom",
          "next": [
            200060008
          ]
        },
        "200060008": {
          "id": 200060008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Il y a une autre entrée une heure plus tard.",
          "next": [
            200060009
          ]
        },
        "200060009": {
          "id": 200060009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "« En mission de surveillance avec Pom-Pom. Il parle dans son sommeil depuis près d'une heure. » Himeko",
          "next": [
            200060010
          ]
        },
        "200060010": {
          "id": 200060010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Qu'est-ce que tu en penses, {NICKNAME} ?",
          "next": [
            200060011,
            200060013
          ]
        },
        "200060011": {
          "id": 200060011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "...",
          "next": [
            200060012
          ]
        },
        "200060012": {
          "id": 200060012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Bon, on dirait que je peux garder cette catégorisation pour le moment.",
          "next": [
            200060015
          ]
        },
        "200060013": {
          "id": 200060013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est clairement une histoire d'horreur",
          "next": [
            200060014
          ]
        },
        "200060014": {
          "id": 200060014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Tu penses ? Je n'ai rien compris, mais on dirait qu'il va falloir ajouter une catégorie pour ça.",
          "next": [
            200060015
          ]
        },
        "200060015": {
          "id": 200060015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Le registre s'arrête là. J'ai interrogé Himeko et Pom-Pom. Ils ne se souviennent pas de cet incident.",
          "next": [
            200060016
          ]
        },
        "200060016": {
          "id": 200060016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Visiblement, on va devoir s'appuyer sur d'autres entrées de la banque de données pour savoir qui parle dans son sommeil.",
          "next": [
            200060018
          ]
        },
        "200060017": {
          "id": 200060017,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[J'aime]",
          "next": []
