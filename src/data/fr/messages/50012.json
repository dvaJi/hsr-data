        },
        "121190006": {
          "id": 121190006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 52,
          "text": "Hum, j'ai besoin de vous pour quelque chose",
          "next": [121190007]
        },
        "121190007": {
          "id": 121190007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 52,
          "text": "Est-ce qu'on pourrait discuter la prochaine fois que vous passerez à la Station spatiale Herta ?",
          "next": [121190008, 121190011]
        },
        "121190008": {
          "id": 121190008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Commencez par me dire pourquoi vous avez besoin de moi",
          "next": [121190009]
        },
        "121190009": {
          "id": 121190009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 52,
          "text": "C'est difficile à expliquer par écrit",
          "next": [121190010]
        },
        "121190010": {
          "id": 121190010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 52,
          "text": "Je vais vous dire l'essentiel",
          "next": [121190014]
        },
        "121190011": {
          "id": 121190011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Les gens ne se soucient de moi que quand il y a des problèmes",
