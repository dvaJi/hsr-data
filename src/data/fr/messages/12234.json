        "122340003": {
          "id": 122340003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Euh, c'est le bon numéro. Je suis bien {NICKNAME}. À qui ai-je l'honneur ?",
