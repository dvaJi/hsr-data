          ]
        },
        "121900014": {
          "id": 121900014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "Tu n'es pas au courant ?",
          "next": [
            121900015
          ]
        },
        "121900015": {
          "id": 121900015,
          "type": "Video",
          "sender": "NPC",
