  "sections": [
    {
      "id": 2007800,
      "startingMessageId": [200780000],
      "messages": {
        "200780000": {
          "id": 200780000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Salut, {NICKNAME}",
          "next": [200780001, 200780003]
        },
        "200780001": {
          "id": 200780001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Qu'est-ce qu'il se passe ? Il y a un problème ?",
          "next": [200780002]
        },
        "200780002": {
          "id": 200780002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Rien. J'étais juste en train de penser à toi",
          "next": [200780005]
        },
        "200780003": {
          "id": 200780003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est encore vous",
          "next": [200780004]
        },
        "200780004": {
          "id": 200780004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Oui, je ne suis pas la bienvenue ?",
          "next": [200780005]
        },
        "200780005": {
          "id": 200780005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Je ne m'attendais pas non plus à être la seule dans la salle de cinéma",
          "next": [200780006]
        },
        "200780006": {
          "id": 200780006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Je pensais que ce film serait plus populaire",
          "next": [200780007]
        },
        "200780007": {
          "id": 200780007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Ça s'appelle « Tout droit sorti du passé »",
          "next": [200780008, 200780011]
        },
        "200780008": {
          "id": 200780008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Vous n'avez personne pour vous accompagner ?",
          "next": [200780009]
        },
        "200780009": {
          "id": 200780009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Si tu parles des autres Chasseurs de Stellaron...",
          "next": [200780010]
        },
        "200780010": {
          "id": 200780010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Tout le monde a des goûts différents, donc je ne vois pas l'intérêt de leur demander de m'accompagner",
          "next": [200780013]
        },
        "200780011": {
          "id": 200780011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "On pourrait le regarder ensemble la prochaine fois",
          "next": [200780012]
        },
        "200780012": {
          "id": 200780012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Très bien, j'en prends bonne note",
          "next": [200780013]
        },
        "200780013": {
          "id": 200780013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Quel genre de films tu aimes, {NICKNAME} ?",
          "next": [200780014]
        },
        "200780014": {
          "id": 200780014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "C'est quoi ton style en ce moment ?",
          "next": [200780015, 200780019, 200780022]
        },
        "200780015": {
          "id": 200780015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Les films biographiques",
          "next": [200780016]
        },
        "200780016": {
          "id": 200780016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Moi aussi, j'aime me plonger dans la vie des autres à travers les livres et les films",
          "next": [200780017]
        },
        "200780017": {
          "id": 200780017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "J'espère que personne ne va faire un film sur ma vie, cependant",
          "next": [200780018]
        },
        "200780018": {
          "id": 200780018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Je frissonne rien qu'à l'idée qu'un réalisateur déforme ma vie par sa propre vision de ma personne",
          "next": [200780025]
        },
        "200780019": {
          "id": 200780019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Les films d'action",
          "next": [200780020]
        },
        "200780020": {
          "id": 200780020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Je me retrouve souvent dans des situations de films d'action",
          "next": [200780021]
        },
        "200780021": {
          "id": 200780021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Alors, ces films ont beau plaire aux autres spectateurs, mais pour moi, ça manque de réalisme",
          "next": [200780025]
        },
        "200780022": {
          "id": 200780022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Les films de science-fiction",
          "next": [200780023]
        },
        "200780023": {
          "id": 200780023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Même s'il s'agit d'histoires qui se passent dans le futur",
          "next": [200780024]
        },
        "200780024": {
          "id": 200780024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Les événements passés finissent souvent par se reproduire",
          "next": [200780025]
        },
        "200780025": {
          "id": 200780025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Bon, le film va commencer",
          "next": [200780026]
        },
        "200780026": {
          "id": 200780026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "À bientôt",
          "next": [200780027, 200780028]
        },
        "200780027": {
          "id": 200780027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bon film",
          "next": [200780029]
        },
        "200780028": {
          "id": 200780028,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "À plus",
          "next": [200780029]
        },
        "200780029": {
          "id": 200780029,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Erreur : Le destinataire n'existe pas",
          "next": []
        }
      },
