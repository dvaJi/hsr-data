      ],
      "messages": {
        "200710000": {
          "id": 200710000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "{NICKNAME}, en arrivant au Luofu, où as-tu logé ?",
          "next": [
            200710001
          ]
        },
        "200710001": {
          "id": 200710001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Attends, laisse-moi deviner",
          "next": [
            200710002
          ]
        },
        "200710002": {
          "id": 200710002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "L'Auberge de Petrichor ?",
          "next": [
            200710003,
            200710007
          ]
        },
        "200710003": {
          "id": 200710003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Exact, j'étais à l'Auberge de Petrichor",
          "next": [
            200710004
          ]
        },
        "200710004": {
          "id": 200710004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "La suite premium n'est pas mal",
          "next": [
            200710005
          ]
        },
        "200710005": {
          "id": 200710005,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[J'aime]",
          "next": [
            200710006
          ]
        },
        "200710006": {
          "id": 200710006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "La suite premium n'est pas facile à réserver. La plupart des voyageurs y vont pour admirer la vue sur l'avenue des Observateurs d'étoiles",
          "next": [
            200710012
          ]
        },
        "200710007": {
          "id": 200710007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Exact, j'étais à l'Auberge de Petrichor",
          "next": [
            200710008
          ]
        },
        "200710008": {
          "id": 200710008,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Honnêtement, il n'y a pas grand-chose à en dire",
          "next": [
            200710009
          ]
        },
        "200710009": {
          "id": 200710009,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Mais, chaque fois que je vais à l'hôtel, j'ai peur qu'il arrive un malheur, donc je n'ai pas le temps de penser à autre chose",
          "next": [
            200710010
          ]
        },
        "200710010": {
          "id": 200710010,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Pleure]",
          "next": [
            200710011
          ]
        },
        "200710011": {
          "id": 200710011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "On dirait que tu n'as pas eu une vie facile",
          "next": [
            200710012
          ]
        },
        "200710012": {
          "id": 200710012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Je reste souvent à l'Auberge de Petrichor. Après tout, elle est très bien située",
          "next": [
            200710013
          ]
        },
        "200710013": {
          "id": 200710013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "En tant que marchand itinérant, je ne profite pas vraiment de mes voyages. De toute façon, je m'ennuie rapidement",
          "next": [
            200710014
          ]
        },
        "200710014": {
          "id": 200710014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Il y a des locations à courte durée dans le quartier des Pavillons célestes, donc j'y suis allé",
          "next": [
            200710015
          ]
        },
        "200710015": {
          "id": 200710015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "C'est là que les résidents de Xianzhou séjournent habituellement",
          "next": [
            200710016,
            200710020
          ]
        },
        "200710016": {
          "id": 200710016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ça n'a pas l'air très pratique, et tout le monde doit te dévisager.",
          "next": [
            200710017
          ]
        },
        "200710017": {
          "id": 200710017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Ça dépend de la façon dont on voit les choses",
          "next": [
            200710018
          ]
        },
        "200710018": {
          "id": 200710018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "La vie à Xianzhou n'est pas si fermée. Les locaux sont amicaux. J'adore observer et adopter leur mode de vie...",
          "next": [
            200710019
          ]
        },
        "200710019": {
          "id": 200710019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Mais pour être honnête, la plupart du temps, c'est moi qui suis observé.",
          "next": [
            200710023
          ]
        },
        "200710020": {
          "id": 200710020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "On dirait que tu es devenu un vrai résident de Xianzhou",
          "next": [
            200710021
          ]
        },
        "200710021": {
          "id": 200710021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Héhé, tu me connais",
          "next": [
            200710022
          ]
        },
        "200710022": {
          "id": 200710022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Ouais, grâce à ça, j'ai pu me fondre dans la population du Luofu",
          "next": [
            200710023
          ]
        },
        "200710023": {
          "id": 200710023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Je suis un nomade, je n'ai pas vraiment de foyer. Rester quelque temps dans le quartier de Pavillons célestes m'aide à oublier ma condition de marchand itinérant",
          "next": [
            200710024
          ]
        },
        "200710024": {
          "id": 200710024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "C'est plutôt agréable",
          "next": [
            200710025,
            200710027
          ]
        },
        "200710025": {
          "id": 200710025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "J'essaierai la prochaine fois",
          "next": [
            200710026
          ]
        },
        "200710026": {
          "id": 200710026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "D'accord, on sera peut-être voisins si on a de la chance",
          "next": []
        },
        "200710027": {
          "id": 200710027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je ne pense pas loger là-bas",
          "next": [
            200710028
          ]
        },
        "200710028": {
          "id": 200710028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Je peux te demander pourquoi ?",
          "next": [
            200710029,
            200710030,
            200710031
          ]
        },
        "200710029": {
          "id": 200710029,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ça me fait peur !",
          "next": [
            200710032
          ]
        },
        "200710030": {
          "id": 200710030,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "J'aime trop mon lit !",
          "next": [
            200710032
          ]
        },
        "200710031": {
          "id": 200710031,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je suis {M#un}{F#une} maniaque de la propreté !",
          "next": [
            200710032
          ]
        },
        "200710032": {
          "id": 200710032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Je vois",
          "next": [
            200710033
          ]
        },
        "200710033": {
          "id": 200710033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Je comprends, et je respecte ta décision",
          "next": [
            200710034
          ]
        },
        "200710034": {
          "id": 200710034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Si un jour tu changes d'avis, on pourra en discuter",
          "next": []
        }
      },
      "participatingContactsIds": [
        1203,
        8001
      ]
    }
  ]
}