        "200820007": {
          "id": 200820007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je suis {M#désolé}{F#désolée}, je suis {M#occupé}{F#occupée}. Je ne peux pas venir",
