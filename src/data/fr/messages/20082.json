        },
        "200820019": {
          "id": 200820019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1111,
          "text": "Honnêtement, j'ai bien envie, moi aussi, d'installer des missiles sur mon bras robotique",
