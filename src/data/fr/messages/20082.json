        },
        "200820022": {
          "id": 200820022,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Un combattant ne devrait compter que sur sa propre force !",
          "next": [200820023]
        },
        "200820023": {
          "id": 200820023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1111,
          "text": "Exactement !!!",
