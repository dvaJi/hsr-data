      "messages": {
        "127010001": {
          "id": 127010001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 63,
          "text": "Le but de ce questionnaire est de connaître votre avis sur la version bêta de l'Hexanexus. Nous espérons que vous ferez preuve de la plus grande sincérité. Les personnes ayant répondu honnêtement seront récompensées.\n\nVos réponses ne seront pas divulguées. Merci de soutenir la société secrète de l'Hexanexus !",
          "next": [127010002]
        },
        "127010002": {
          "id": 127010002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 63,
          "text": "Êtes-vous...",
          "next": [127010003, 127010004, 127010005]
        },
        "127010003": {
          "id": 127010003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Un homme",
          "next": [127010006]
        },
        "127010004": {
          "id": 127010004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Une femme",
          "next": [127010006]
        },
        "127010005": {
          "id": 127010005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je ne m'identifie ni comme un homme ni comme une femme.",
          "next": [127010006]
        },
        "127010006": {
          "id": 127010006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 63,
          "text": "Quel âge avez-vous ?",
          "next": [127010007, 127010008, 127010009]
        },
        "127010007": {
          "id": 127010007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Entre 1 et 100 ans",
          "next": [127010010]
        },
        "127010008": {
          "id": 127010008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Entre 101 et 500 ans",
          "next": [127010010]
        },
        "127010009": {
          "id": 127010009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Plus de 500 ans",
          "next": [127010010]
        },
        "127010010": {
          "id": 127010010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 63,
          "text": "Savez-vous ce qu'est l'Hexanexus ?",
          "next": [127010011, 127010012, 127010014]
        },
        "127010011": {
          "id": 127010011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je sais ce qu'est l'Hexanexus.",
          "next": [127010016]
        },
        "127010012": {
          "id": 127010012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je ne sais pas ce qu'est l'Hexanexus.",
          "next": [127010013]
        },
        "127010013": {
          "id": 127010013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 63,
          "text": "L'Hexanexus est l'appareil avec lequel vous avez joué.",
          "next": [127010016]
        },
        "127010014": {
          "id": 127010014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "L'Hexa... Je n'arrive même pas à le prononcer.",
          "next": [127010015]
        },
        "127010015": {
          "id": 127010015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 63,
          "text": "L'Hexanexus. C'est une structure connectée à six faces.",
          "next": [127010016]
        },
        "127010016": {
          "id": 127010016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 63,
          "text": "Comment avez-vous connu l'Hexanexus ?",
          "next": [127010017, 127010018, 127010019]
        },
        "127010017": {
          "id": 127010017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "J'en ai vu sur le bord de la route, tout simplement.",
          "next": [127010024]
        },
        "127010018": {
          "id": 127010018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est un ami qui m'en a parlé.",
          "next": [127010024]
        },
        "127010019": {
          "id": 127010019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "J'ai entendu parler de l'Hexanexus sur votre compte officiel.",
          "next": [127010020]
        },
        "127010020": {
          "id": 127010020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 63,
          "text": "Euh... Nous n'avons pas de compte officiel.",
          "next": [127010021, 127010022]
        },
        "127010021": {
          "id": 127010021,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est peut-être un compte promotionnel qui essaie de tirer parti de votre popularité.",
          "next": [127010023]
        },
        "127010022": {
          "id": 127010022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est peut-être un faux compte, qui usurpe votre identité.",
          "next": [127010023]
        },
        "127010023": {
          "id": 127010023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 63,
          "text": "Si nous étions si populaires que ça, nous ne laisserions pas autant d'appareils sur le bord de la route.",
          "next": [127010024]
        },
        "127010024": {
          "id": 127010024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 63,
          "text": "Comment estimez-vous la difficulté de l'Hexanexus ?",
          "next": [127010025, 127010026, 127010027]
        },
        "127010025": {
          "id": 127010025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Très difficile",
          "next": [127010028]
        },
        "127010026": {
          "id": 127010026,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Moyennement difficile",
          "next": [127010028]
        },
        "127010027": {
          "id": 127010027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Très simple",
          "next": [127010028]
        },
        "127010028": {
          "id": 127010028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 63,
          "text": "Avez-vous autre chose à ajouter sur l'Hexanexus ?",
          "next": [127010029, 127010030, 127010031]
        },
        "127010029": {
          "id": 127010029,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est un chef-d'œuvre, il relève du génie.",
          "next": [127010032]
        },
        "127010030": {
          "id": 127010030,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est intrigant. C'est tout ce que j'ai à dire.",
          "next": [127010032]
        },
        "127010031": {
          "id": 127010031,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ça me fait perdre un temps précieux.",
          "next": [127010032]
        },
        "127010032": {
          "id": 127010032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 63,
          "text": "Merci d'avoir répondu à notre questionnaire.\nContinuez à soutenir l'Hexanexus !",
          "next": []
        }
