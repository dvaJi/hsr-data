      "id": 1111700,
      "startingMessageId": [111170000],
      "messages": {
        "111170000": {
          "id": 111170000,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Monsieur Yang, vous êtes là ? Monsieur Yaaaang~",
          "next": [111170003]
        },
        "111170003": {
          "id": 111170003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Je n'ai pas bougé depuis la dernière fois. Que se passe-t-il {NICKNAME}?",
          "next": [111170004, 111170005]
        },
        "111170004": {
          "id": 111170004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Est-ce que notre Astral Express peut accueillir de nouveaux passagers ?",
          "next": [111170006]
        },
        "111170005": {
          "id": 111170005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Une amie souhaiterait monter à bord de l'Astral Express, qu'en pensez-vous, Monsieur Yang ?",
          "next": [111170007]
        },
        "111170006": {
          "id": 111170006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Oh ? Bien sûr que je me réjouirais de son arrivée",
          "next": [111170008]
        },
        "111170007": {
          "id": 111170007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Oh ? C'est bien que tu noues de nouvelles amitiés. Je me réjouis toujours de l'arrivée des nouveaux membres",
          "next": [111170008]
        },
        "111170008": {
          "id": 111170008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Cependant, est-ce que je peux te poser quelques questions ?",
          "next": [111170009]
        },
        "111170009": {
          "id": 111170009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Je suis intrigué, quel genre de personne est ta nouvelle amie, {NICKNAME} ?",
          "next": [111170010, 111170011, 111170012]
        },
        "111170010": {
          "id": 111170010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est une prodigieuse mécanicienne avec un esprit minutieux",
          "next": [111170013]
        },
        "111170011": {
          "id": 111170011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est une rock star extravertie et désinhibée",
          "next": [111170014]
        },
        "111170012": {
          "id": 111170012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Elle est... Ben je ne sais pas trop exactement",
          "next": [111170015]
        },
        "111170013": {
          "id": 111170013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "J'ai l'impression qu'elle aura beaucoup d'atomes crochus avec Himeko. Ce n'est pas plus mal",
          "next": [111170016]
        },
        "111170014": {
          "id": 111170014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Oh ? Pas mal comme loisir. Je dois avouer que je m'y intéresse aussi",
          "next": [111170016]
        },
        "111170015": {
          "id": 111170015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Peu importe, j'ai confiance en toi. Son caractère ne devrait pas trop s'éloigner de ta description",
          "next": [111170016]
        },
        "111170016": {
          "id": 111170016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Quoi qu'il en soit, j'aimerais accueillir ton amie",
          "next": [111170017]
        },
        "111170017": {
          "id": 111170017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Cependant, ce n'est pas moi qui prends les décisions ici. J'en toucherai deux mots à Himeko",
          "next": [111170018]
        },
        "111170018": {
          "id": 111170018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "D'après mon expérience, elle n'y verra aucune objection, alors ne te fais pas soucis",
          "next": [111170019, 111170020]
        },
        "111170019": {
          "id": 111170019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Merci, Monsieur Yang",
          "next": [111170021]
        },
        "111170020": {
          "id": 111170020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Youpi ! Merci, Monsieur Yang !",
          "next": [111170021]
        },
        "111170021": {
          "id": 111170021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Inutile de me remercier. C'est la moindre des choses",
          "next": []
        }
      },
      "participatingContactsIds": [8001, 1004]
    }
  ]
}
