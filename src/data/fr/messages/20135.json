          ]
        },
        "201350026": {
          "id": 201350026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Je leur ai dit de te l'envoyer",
          "next": [
            201350027
          ]
        },
        "201350027": {
          "id": 201350027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Je suis sûr que ça t'ira super bien. Tu m'enverras une photo !",
          "next": []
        },
        "201350028": {
          "id": 201350028,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
