        },
        "201350029": {
          "id": 201350029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Cool ! C'est ça l'amitié !",
          "next": [
            201350030
          ]
        },
        "201350030": {
          "id": 201350030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "J'ai tout pris",
          "next": [
            201350031
          ]
        },
        "201350031": {
          "id": 201350031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Ça te servira pendant tous tes voyages",
