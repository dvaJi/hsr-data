  "sections": [
    {
      "id": 2009800,
      "startingMessageId": [
        200980000
      ],
      "messages": {
        "200980000": {
          "id": 200980000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Au fait, comment ça se passe, la vie pour toi à Xianzhou ? Tu t'en sors ?",
          "next": [
            200980001,
            200980003,
            200980005
          ]
        },
        "200980001": {
          "id": 200980001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tout se passe bien, merci.",
          "next": [
            200980002
          ]
        },
        "200980002": {
          "id": 200980002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ravie de l'entendre. Je suis contente pour toi.",
          "next": [
            200980008
          ]
        },
        "200980003": {
          "id": 200980003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "J'avoue que j'ai eu quelques difficultés.",
          "next": [
            200980004
          ]
        },
        "200980004": {
          "id": 200980004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Eh oui, je comprends. Je suis passée par là.",
          "next": [
            200980008
          ]
        },
        "200980005": {
          "id": 200980005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je n'y fais pas attention. Je ne compte pas rester éternellement, de toute façon.",
          "next": [
            200980006
          ]
        },
        "200980006": {
          "id": 200980006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Hmm, tu as peut-être raison...",
          "next": [
            200980007
          ]
        },
        "200980007": {
          "id": 200980007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Tu ne resteras pas forcément ici longtemps, c'est vrai, mais en tant que Sans Nom, tu devras sans doute interagir souvent avec Xianzhou.",
          "next": [
            200980008
          ]
        },
        "200980008": {
          "id": 200980008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "En fait, tu sais quoi ? Ce n'est pas important. J'étais un peu perdue dans mes pensées.",
          "next": [
            200980009
          ]
        },
        "200980009": {
          "id": 200980009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Quand je suis arrivée ici, j'étais une gamine qui ne comprenait rien à rien.",
          "next": [
            200980010
          ]
        },
        "200980010": {
          "id": 200980010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Une jeunette dans un environnement inconnu, une société dont je ne savais rien.",
          "next": [
            200980011
          ]
        },
        "200980011": {
          "id": 200980011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Quand je me suis remémoré ma crainte, mon angoisse de l'époque, j'ai pensé à toi.",
          "next": [
            200980012
          ]
        },
        "200980012": {
          "id": 200980012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "J'ai eu peur que tu traverses la même chose.",
          "next": [
            200980013,
            200980015
          ]
        },
        "200980013": {
          "id": 200980013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Merci de ta sollicitude, Guigui. Mais je me suis vite {F#habituée}{M#habitué}.",
          "next": [
            200980014
          ]
        },
        "200980014": {
          "id": 200980014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Eh oui, j'imagine. Tu as connu beaucoup plus de choses que moi.",
          "next": [
            200980017
          ]
        },
        "200980015": {
          "id": 200980015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ça n'a pas dû être facile pour toi. J'en ai la larme à l'œil.",
          "next": [
            200980016
          ]
        },
        "200980016": {
          "id": 200980016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Haha, t'en fais pas pour moi. Prends plutôt soin de toi ! Sérieusement, n'oublie pas de faire des pauses de temps en temps, même si tu as mille choses à faire.",
          "next": [
            200980017
          ]
        },
        "200980017": {
          "id": 200980017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Permets-moi de le répéter...",
          "next": [
            200980018
          ]
        },
        "200980018": {
          "id": 200980018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Si tu as besoin d'un coup de main, tu sais que tu peux toujours compter sur Guigui.",
          "next": [
            200980019
          ]
        },
        "200980019": {
          "id": 200980019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Et même si je ne peux rien faire personnellement, j'ai des contacts chez les Chevaliers des nuages et à la Commission des dix seigneurs !",
          "next": [
            200980020
          ]
        },
        "200980020": {
          "id": 200980020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Haha !",
          "next": []
        }
      },
      "participatingContactsIds": [
        1210,
        8001
      ]
    }
  ]
}