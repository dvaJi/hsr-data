      "messages": {
        "145090000": {
          "id": 145090000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 190,
          "text": "... {NICKNAME}, c'est toi ?",
          "next": [
            145090001
          ]
        },
        "145090001": {
          "id": 145090001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 190,
          "text": "Je ne voulais pas te déranger, mais pourrais-tu te libérer pour que nous puissions discuter ?",
          "next": [
            145090002,
            145090004
          ]
        },
        "145090002": {
          "id": 145090002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bien sûr, j'adore bavarder",
          "next": [
            145090003
          ]
        },
        "145090003": {
          "id": 145090003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 190,
          "text": "Merci, jeune {F#fille}{M#homme}",
          "next": [
            145090006
          ]
        },
        "145090004": {
          "id": 145090004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "{F#Navrée}{M#Navré}, mais je suis un peu {F#occupée}{M#occupé}",
          "next": [
            145090005
          ]
        },
        "145090005": {
          "id": 145090005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 190,
          "text": "Oh, pas de souci, ne te sens pas {F#obligée}{M#obligé} de répondre. Je peux parler toute seule...",
          "next": [
            145090006
          ]
        },
        "145090006": {
          "id": 145090006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 190,
          "text": "J'aimerais discuter du cas de Perséphone et d'Hadès...",
          "next": [
            145090007
          ]
        },
        "145090007": {
          "id": 145090007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 190,
          "text": "À l'époque, je désapprouvais totalement leur relation",
          "next": [
            145090008
          ]
        },
        "145090008": {
          "id": 145090008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 190,
          "text": "Perséphone est un cœur simple. Si on est aimable avec elle, alors elle rendra la pareille",
          "next": [
            145090009
          ]
        },
        "145090009": {
          "id": 145090009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 190,
          "text": "Mais ce Hadès... Je l'ai toujours trouvé trop calculateur",
          "next": [
            145090010
          ]
        },
        "145090010": {
          "id": 145090010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 190,
          "text": "Perséphone l'aimait, donc j'ai respecté sa décision et leur ai souhaité tous mes vœux de bonheur",
          "next": [
            145090011
          ]
        },
        "145090011": {
          "id": 145090011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 190,
          "text": "Mais une part de moi regrette de n'avoir pas été une mère assez stricte et ferme",
          "next": [
            145090012
          ]
        },
        "145090012": {
          "id": 145090012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 190,
          "text": "...",
          "next": [
            145090013
          ]
        },
        "145090013": {
          "id": 145090013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 190,
          "text": "Merci pour ton aide dans cette enquête, je vais poursuivre les recherches",
          "next": [
            145090014
          ]
        },
        "145090014": {
          "id": 145090014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 190,
          "text": "Et merci d'avoir supporté mes élucubrations pendant tout ce temps",
          "next": [
            145090015
          ]
        },
        "145090015": {
          "id": 145090015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 190,
          "text": "Un grand merci pour ton aide, {NICKNAME}",
