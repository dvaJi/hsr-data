  "relatedMessages": [20070, 20071, 20072],
  "sections": [
    {
      "id": 2007200,
      "startingMessageId": [200720000],
      "messages": {
        "200720000": {
          "id": 200720000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "{NICKNAME}, je n'ai pas fermé l'œil de la nuit",
          "next": [200720001]
        },
        "200720001": {
          "id": 200720001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Je suis peut-être trop naïf. Sans clients fidèles, c'est dur de faire des affaires la nuit tombée.",
          "next": [200720004]
        },
        "200720004": {
          "id": 200720004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Je suis allé dans l'allée des Aurums, une vieille rue célèbre pour son marché nocturne",
          "next": [200720005]
        },
        "200720005": {
          "id": 200720005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "En tant que marchand itinérant, j'étais obligé d'y faire un tour ?",
          "next": [200720006, 200720010]
        },
        "200720006": {
          "id": 200720006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tu y as fait des ventes ?",
          "next": [200720007]
        },
        "200720007": {
          "id": 200720007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Oui, et non. Le commerce, c'est la réciprocité de l'offre et de la demande",
          "next": [200720008]
        },
        "200720008": {
          "id": 200720008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "J'ai des babioles inédites à vendre, mais je ne trouve pas toujours acheteur",
          "next": [200720009]
        },
        "200720009": {
          "id": 200720009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Quoi qu'il en soit, les habitants de Xianzhou sont très amicaux. Même s'ils ne m'achètent rien, ils prennent toujours le temps de discuter avec moi",
          "next": [200720014]
        },
        "200720010": {
          "id": 200720010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tu as fait des achats ?",
          "next": [200720011]
        },
        "200720011": {
          "id": 200720011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Oui, et non. Le commerce, c'est la réciprocité de l'offre et de la demande",
          "next": [200720012]
        },
        "200720012": {
          "id": 200720012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Beaucoup de boutiques proposent des objets intéressants, mais ce n'est pas comme si tout m'intéressait",
          "next": [200720013]
        },
        "200720013": {
          "id": 200720013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Quoi qu'il en soit, les habitants de Xianzhou sont très amicaux. Même si je n'achète rien, ils prennent toujours le temps de discuter avec moi",
          "next": [200720014]
        },
        "200720014": {
          "id": 200720014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Je ne pensais pas que ces conversations dureraient jusqu'à ce que les marchands commencent à remballer",
          "next": [200720015]
        },
        "200720015": {
          "id": 200720015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "J'ai vraiment passé une bonne soirée, mais avec le recul, qu'est-ce que j'y ai gagné ?",
          "next": [200720016, 200720019, 200720022]
        },
        "200720016": {
          "id": 200720016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ne dis pas ça",
          "next": [200720017]
        },
        "200720017": {
          "id": 200720017,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Tu as glané des informations toutes fraiches sur le commerce à Xianzhou !",
          "next": [200720018]
        },
        "200720018": {
          "id": 200720018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Haha, à tes yeux je suis un véritable marchand itinérant",
          "next": [200720025]
        },
        "200720019": {
          "id": 200720019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ne dis pas ça",
          "next": [200720020]
        },
        "200720020": {
          "id": 200720020,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Tu t'es fait des amis parmi les habitants de Xianzhou !",
          "next": [200720021]
        },
        "200720021": {
          "id": 200720021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "C'est vrai, y a-t-il quelque chose de plus précieux que l'amitié ?",
          "next": [200720025]
        },
        "200720022": {
          "id": 200720022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ne dis pas ça",
          "next": [200720023]
        },
        "200720023": {
          "id": 200720023,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Au moins tu as gagné de jolis cernes !",
          "next": [200720024]
        },
        "200720024": {
          "id": 200720024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Raté, d'habitude ça ne se voit pas chez moi, même si je veille toute la nuit.",
          "next": [200720025]
        },
        "200720025": {
          "id": 200720025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Si tu en as l'occasion, va faire un tour à l'allée des Aurums",
          "next": [200720026]
        },
        "200720026": {
          "id": 200720026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Et si je suis toujours à Xianzhou, appelle-moi",
          "next": []
        }
      },
      "participatingContactsIds": [1203, 8001]
    }
  ]
}
