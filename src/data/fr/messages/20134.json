          "next": [
            201340024
          ]
        },
        "201340024": {
          "id": 201340024,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "J'ai gagné, je veux savoir maintenant",
          "next": [
            201340025
          ]
        },
        "201340025": {
          "id": 201340025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Après avoir subi la frappe de l'épée de l'Émanatrice, je me suis retrouvé au milieu d'une rivière sans savoir pourquoi",
          "next": [
            201340026
          ]
        },
        "201340026": {
          "id": 201340026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "J'ai cru que c'était la fin pour moi",
          "next": [
            201340027
          ]
        },
        "201340027": {
          "id": 201340027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1304,
          "text": "Mais comme tu peux le voir, je suis en vie, et j'en suis plutôt ravi",
