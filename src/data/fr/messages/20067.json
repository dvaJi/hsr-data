      "messages": {
        "200670000": {
          "id": 200670000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "J'espère que ma question ne va pas te gêner, {NICKNAME}",
          "next": [200670001]
        },
        "200670001": {
          "id": 200670001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Tu aimes vraiment ce que tu fais ? Est-ce ce dont tu rêvais ?",
          "next": [200670002, 200670003, 200670004]
        },
        "200670002": {
          "id": 200670002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Plutôt oui, j'aime bien ma situation actuelle",
          "next": [200670005]
        },
        "200670003": {
          "id": 200670003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ce n'est pas vraiment ce que j'espérais, mais ce n'est pas mal",
          "next": [200670005]
        },
        "200670004": {
          "id": 200670004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je ne sais pas, je ne m'en souviens pas très bien",
          "next": [200670005]
        },
        "200670005": {
          "id": 200670005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Honnêtement, je n'ai jamais envisagé une autre carrière, que celle de pilote",
          "next": [200670006]
        },
        "200670006": {
          "id": 200670006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Si mon père me voyait, il me hurlerait dessus pour avoir lâchement fui le champ de bataille",
          "next": [200670007, 200670008]
        },
        "200670007": {
          "id": 200670007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Un travail de bureau, c'est un peu un champ de bataille. Ça n'a pas dû être simple d'arriver jusqu'ici",
          "next": [200670009]
        },
        "200670008": {
          "id": 200670008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est ta vie et tes choix. L'important, c'est que tu sois épanouie",
          "next": [200670009]
        },
        "200670009": {
          "id": 200670009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Ouais... Je ne suis pas comme mon père...",
          "next": [200670010]
        },
        "200670010": {
          "id": 200670010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Je ne souhaite qu'une chose pour mon enfant...",
          "next": [200670011]
        },
        "200670011": {
          "id": 200670011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Ça m'est égal si elle n'excelle pas en tout, tant qu'elle est en bonne santé",
          "next": [200670012, 200670015]
        },
        "200670012": {
          "id": 200670012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je n'ai pas d'enfant, je n'ai aucun moyen de savoir...",
          "next": [200670013]
        },
        "200670013": {
          "id": 200670013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "C'est vrai, il est peut-être un peu tôt pour te parler de ça",
          "next": [200670014]
        },
        "200670014": {
          "id": 200670014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Tu es encore si jeune après tout",
          "next": [200670017]
        },
        "200670015": {
          "id": 200670015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est important d'écouter ses parents, mais les enfants ont aussi leur mot à dire",
          "next": [200670016]
        },
        "200670016": {
          "id": 200670016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Ils sont encore jeunes, après tout",
          "next": [200670017]
        },
        "200670017": {
          "id": 200670017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Certaines choses ne peuvent être comprises que si on les vit comme je les ai vécues",
          "next": [200670018]
        },
        "200670018": {
          "id": 200670018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "Tant d'années ont passé...",
          "next": []
        }
      },
      "participatingContactsIds": [1207, 8001]
    }
  ]
