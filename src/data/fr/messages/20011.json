      "messages": {
        "200110000": {
          "id": 200110000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "{NICKNAME}, aujourd'hui est un nouveau jour.",
          "next": [200110001]
        },
        "200110001": {
          "id": 200110001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Monter dans l'Express et suivre les traces des Sans Noms signifie que nous allons finir par nous habituer à des choses qui ne font pas partie de l'ordinaire.",
          "next": [200110002]
        },
        "200110002": {
          "id": 200110002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Sans même t'en rendre compte, tu seras {M#amené}{F#amenée} à repousser tes limites.",
          "next": [200110003]
        },
        "200110003": {
          "id": 200110003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "{NICKNAME}, t'es-tu déjà {M#retrouvé}{F#retrouvée} dans une telle situation ?",
          "next": [200110004, 200110005]
        },
        "200110004": {
          "id": 200110004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "En effet, j'ai commencé à recevoir d'innombrables messages tout à coup",
          "next": [200110006]
        },
        "200110005": {
          "id": 200110005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "En effet, j'ai commencé à recevoir plein de nouvelles affectations tout à coup",
          "next": [200110009]
        },
        "200110006": {
          "id": 200110006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Les messages représentent les liens qui unissent les gens.",
          "next": [200110007]
        },
        "200110007": {
          "id": 200110007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Si tu reçois des messages tous les jours, cela signifie que tu as développé des liens profonds avec les autres au cours de ton voyage de {M#pionnier}{F#pionnière}.",
          "next": [200110008]
        },
        "200110008": {
          "id": 200110008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Mais ces liens doivent être entretenus. Alors, fais bon usage de ton temps.",
          "next": [200110012, 200110013]
        },
        "200110009": {
          "id": 200110009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Tu ne peux pas t'estimer digne des Sans Noms si tu n'es pas capable de faire face à plusieurs affectations en même temps.",
          "next": [200110010]
        },
        "200110010": {
          "id": 200110010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Tu dois planifier ton itinéraire de manière efficace afin d'aller au plus court et au plus rapide. Tu éviteras ainsi de faire des efforts en vain.",
          "next": [200110011]
        },
        "200110011": {
          "id": 200110011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Tu verras que ta mémoire musculaire t'aidera très vite à prendre le pli.",
          "next": [200110012, 200110013]
        },
        "200110012": {
          "id": 200110012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Vous avez tout l'air d'être un vétéran chevronné, Monsieur Yang !",
          "next": [200110014]
        },
        "200110013": {
          "id": 200110013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est un prix que je suis {M#prêt}{F#prête} à payer pour suivre la Voie du Pionnier !",
          "next": [200110016]
        },
        "200110014": {
          "id": 200110014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "L'expérience des Sans Noms n'est autre qu'une leçon de sacrifice et de sang versé.",
          "next": [200110015]
        },
        "200110015": {
          "id": 200110015,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "[D'accord]",
          "next": []
        },
        "200110016": {
          "id": 200110016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Si jeune et déjà si mature.",
          "next": [200110017]
        },
        "200110017": {
          "id": 200110017,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "[J'aime]",
