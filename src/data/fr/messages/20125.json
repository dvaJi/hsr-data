      "messages": {
        "201250000": {
          "id": 201250000,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Pom-Pom vous a envoyé une demande par message.",
          "next": [
            201250001
          ]
        },
        "201250001": {
          "id": 201250001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "{NICKNAME} ! À l'aide !",
          "next": [
            201250002
          ]
        },
        "201250002": {
          "id": 201250002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Un truc bizarre s'est glissé à bord de l'Express !!!",
          "next": [
            201250003
          ]
        },
        "201250003": {
          "id": 201250003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Je l'ai poursuivi jusqu'à la salle des machines !!!",
          "next": [
            201250004
          ]
        },
        "201250004": {
          "id": 201250004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Viens m'aider !!!",
          "next": [
            201250005,
            201250006
          ]
        },
        "201250005": {
          "id": 201250005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "T'es qui, déjà ?",
          "next": [
            201250007
          ]
        },
        "201250006": {
          "id": 201250006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sparkle ? Encore ?",
          "next": [
            201250007
          ]
        },
        "201250007": {
          "id": 201250007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Qu'est-ce que tu racontes ? C'est moi, Pom-Pom, le conducteur de l'Express !!!",
          "next": [
            201250008
          ]
        },
        "201250008": {
          "id": 201250008,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1000,
          "next": [
            201250009
          ]
        },
        "201250009": {
          "id": 201250009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "J'ai dû passer sur un compte temporaire, car j'ai cassé le dispositif de communications pendant une inspection de routine...",
          "next": [
            201250010,
            201250022
          ]
        },
        "201250010": {
          "id": 201250010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "J'ai toujours l'impression que tu es Sparkle.",
          "next": [
            201250011
          ]
        },
        "201250011": {
          "id": 201250011,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Car...",
          "next": [
            201250012,
            201250015,
            201250018
          ]
        },
        "201250012": {
          "id": 201250012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Pom-Pom ne parlerait jamais de la sorte !!!",
          "next": [
            201250013
          ]
        },
        "201250013": {
          "id": 201250013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Ohh... Je vois...",
          "next": [
            201250014
          ]
        },
        "201250014": {
          "id": 201250014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Je n'ai pas réussi à incarner mon personnage jusqu'au bout, désolée...",
          "next": [
            201250045
          ]
        },
        "201250015": {
          "id": 201250015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Pom-Pom est du genre à punir quiconque pénètre la salle des machines !",
          "next": [
            201250016
          ]
        },
        "201250016": {
          "id": 201250016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Ouah",
          "next": [
            201250017
          ]
        },
        "201250017": {
          "id": 201250017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Je ne savais pas que l'autre peluche était si dangereuse. Impressionnant...",
          "next": [
            201250045
          ]
        },
        "201250018": {
          "id": 201250018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Personne ne peut monter à bord de l'Express en dehors des Sans Noms et des invités !",
          "next": [
            201250019
          ]
        },
        "201250019": {
          "id": 201250019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Oh ?",
          "next": [
            201250020
          ]
        },
        "201250020": {
          "id": 201250020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Je n'y crois pas. De nombreux auto-stoppeurs sont déjà montés à bord.",
          "next": [
            201250021
          ]
        },
        "201250021": {
          "id": 201250021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Tu crois qu'il n'y a pas de Fou masqué parmi eux ?",
          "next": [
            201250045
          ]
        },
        "201250022": {
          "id": 201250022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est toi ! Le grand conducteur de l'Express !",
          "next": [
            201250023
          ]
        },
        "201250023": {
          "id": 201250023,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Tu utilises un compte temporaire suspect... Ce n'est un secret pour personne !",
          "next": [
            201250024
          ]
        },
        "201250024": {
          "id": 201250024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "?",
          "next": [
            201250025
          ]
        },
        "201250025": {
          "id": 201250025,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Pardon... j'ai toujours connu ton secret... le secret de l'Express...",
          "next": [
            201250026
          ]
        },
        "201250026": {
          "id": 201250026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "?",
          "next": [
            201250027,
            201250032,
            201250037
          ]
        },
        "201250027": {
          "id": 201250027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Le secret qui se cache dans la salle des machines est...",
          "next": [
            201250028
          ]
        },
        "201250028": {
          "id": 201250028,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Le précédent conducteur de l'Express !",
          "next": [
            201250029
          ]
        },
        "201250029": {
          "id": 201250029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "?",
          "next": [
            201250030
          ]
        },
        "201250030": {
          "id": 201250030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Hein ? C'est vrai ?",
          "next": [
            201250031
          ]
        },
        "201250031": {
          "id": 201250031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Vous seriez prêts à aller aussi loin ?",
          "next": [
            201250042
          ]
        },
        "201250032": {
          "id": 201250032,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tu es donc un Fou masqué...",
          "next": [
            201250033
          ]
        },
        "201250033": {
          "id": 201250033,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Pom-Pom et Aha sont de la même famille !",
          "next": [
            201250034
          ]
        },
        "201250034": {
          "id": 201250034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "?",
          "next": [
            201250035
          ]
        },
        "201250035": {
          "id": 201250035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Hein ? Comment ça se fait que je l'ignorais ?",
          "next": [
            201250036
          ]
        },
        "201250036": {
          "id": 201250036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Les Sans Noms seraient prêts à aller aussi loin ?",
          "next": [
            201250042
          ]
        },
        "201250037": {
          "id": 201250037,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tous les passagers de l'Express ont été créés par son conducteur !",
          "next": [
            201250038
          ]
        },
        "201250038": {
          "id": 201250038,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ce sont tous des Pom-Pom !",
          "next": [
            201250039
          ]
        },
        "201250039": {
          "id": 201250039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "?",
          "next": [
            201250040
          ]
        },
        "201250040": {
          "id": 201250040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Hein ? Même moi je n'inventerais pas un truc pareil...",
          "next": [
            201250041
          ]
        },
        "201250041": {
          "id": 201250041,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Franchement, tu pars trop loin !",
          "next": [
            201250042
          ]
        },
        "201250042": {
          "id": 201250042,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sparkle, t'es grillée.",
          "next": [
            201250043
          ]
        },
        "201250043": {
          "id": 201250043,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Ah, j'étais persuadé que c'était vrai... Sous le coup de l'émotion, j'ai oublié...",
          "next": [
            201250044
          ]
        },
        "201250044": {
          "id": 201250044,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Tu as un sacré culot pour inventer un tel mensonge.",
          "next": [
            201250045
          ]
        },
        "201250045": {
          "id": 201250045,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "C'est gênant d'être si vite reconnue...",
          "next": [
            201250046
          ]
        },
        "201250046": {
          "id": 201250046,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Je dois me préparer pour notre prochaine conversation. Je reviendrai quand j'aurai quelque chose d'intéressant.",
          "next": [
            201250047,
            201250048
          ]
        },
        "201250047": {
          "id": 201250047,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je t'attends de pied ferme. On verra bien qui piègera l'autre, la prochaine fois.",
          "next": [
            201250049
          ]
        },
        "201250048": {
          "id": 201250048,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ne reviens pas ! Les Fous n'apportent que des problèmes...",
          "next": [
            201250049
          ]
        },
        "201250049": {
          "id": 201250049,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Ooooh...",
          "next": [
            201250050
          ]
        },
        "201250050": {
          "id": 201250050,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Tu commences vraiment à radoter...",
          "next": [
            201250051
          ]
        },
        "201250051": {
          "id": 201250051,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "C'est nul de dire que s'amuser crée des problèmes...",
          "next": [
            201250052
          ]
        },
        "201250052": {
          "id": 201250052,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Tu n'as jamais entendu le dicton : « Il suffit de s'amuser un rien, pour que le Fou masqué reste loin. »",
          "next": [
            201250053
          ]
        },
        "201250053": {
          "id": 201250053,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Les Fous masqués préfèrent pimenter les choses banales de la vie pour les rendre plus fantastiques !",
          "next": [
            201250054
          ]
        },
        "201250054": {
          "id": 201250054,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Bon, j'ai des trucs à faire. Si tu ne veux pas de moi, je ne reviendrai pas.",
          "next": [
            201250055
          ]
        },
        "201250055": {
          "id": 201250055,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "Fais-moi savoir la prochaine fois que tu as un truc amusant de prévu.",
          "next": [
            201250056
          ]
        },
        "201250056": {
          "id": 201250056,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1000,
          "next": [
            201250057,
            201250058
          ]
