      "messages": {
        "122080000": {
          "id": 122080000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Le Seigneur de la foudre du général est vraiment stylé",
          "next": [122080001]
        },
        "122080001": {
          "id": 122080001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Tellement cool",
          "next": [122080002]
        },
        "122080002": {
          "id": 122080002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ouais. C'est la première fois que je le vois",
          "next": [122080003, 122080007]
        },
        "122080003": {
          "id": 122080003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "À chaque fois que je vois apparaître le Seigneur de la foudre, je suis {F#émerveillée}{M#émerveillé}",
          "next": [122080004]
        },
        "122080004": {
          "id": 122080004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Tu m'étonnes !",
          "next": [122080011]
        },
        "122080007": {
          "id": 122080007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je l'ai vu tellement de fois que ça ne me fait plus rien",
          "next": [122080008]
        },
        "122080008": {
          "id": 122080008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Vraiment ?",
          "next": [122080011]
        },
        "122080011": {
          "id": 122080011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Revenons à nos chèvres-globes. Déjà, hourra pour Huohuo qui a retrouvé Plumeau !",
          "next": [122080012]
        },
        "122080012": {
          "id": 122080012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Oui ! Qui aurait cru que Plumeau était avec Hanya ?",
          "next": [122080013]
        },
        "122080013": {
          "id": 122080013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Il paraît qu'il a fallu du temps à Plumeau pour se remettre. Ça n'a pas dû être facile.",
          "next": [122080014]
        },
        "122080014": {
          "id": 122080014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Attendez, je viens de penser à un truc.",
          "next": [122080015]
        },
        "122080015": {
          "id": 122080015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Tu veux prendre une photo de Plumeau ?",
          "next": [122080016]
        },
        "122080016": {
          "id": 122080016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Que... Comment tu le sais ?",
          "next": [122080017]
        },
        "122080017": {
          "id": 122080017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Ah ça...",
          "next": [122080018, 122080020]
        },
        "122080018": {
          "id": 122080018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ne te force pas, Huohuo. Si tu n'es pas d'accord, tu peux le dire.",
          "next": [122080019]
        },
        "122080019": {
          "id": 122080019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Merci, {NICKNAME}.",
          "next": [122080022]
        },
        "122080020": {
          "id": 122080020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Pourquoi ne pas demander à Plumeau ce qu'il en pense ?",
          "next": [122080021]
        },
        "122080021": {
          "id": 122080021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1217,
          "text": "Oh, monsieur Plumeau dit que ça ne l'intéresse pas.",
          "next": [122080022]
        },
        "122080022": {
          "id": 122080022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Bon, si Plumeau n'est pas intéressé, tant pis. On a plein d'autres choses dont on peut parler.",
          "next": [122080023]
        },
        "122080023": {
          "id": 122080023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Commençons par le commencement et vérifions notre compte.",
          "next": [122080024]
        },
