  "relatedMessages": [20115, 20116, 20117],
  "sections": [
    {
      "id": 2011500,
      "startingMessageId": [201150000],
      "messages": {
        "201150000": {
          "id": 201150000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Tu as le temps de discuter ?",
          "next": [201150001, 201150003]
        },
        "201150001": {
          "id": 201150001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bien sûr. De quoi voulez-vous parler, Madame Xueyi ?",
          "next": [201150002]
        },
        "201150002": {
          "id": 201150002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Tu as l'air disponible, c'est parfait",
          "next": [201150006]
        },
        "201150003": {
          "id": 201150003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je ne vous savais pas aussi loquace, vous qui êtes toujours si réservée",
          "next": [201150004]
        },
        "201150004": {
          "id": 201150004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Je sais que je parais réservée",
          "next": [201150005]
        },
        "201150005": {
          "id": 201150005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Mais ça m'arrive de parler",
          "next": [201150006]
        },
        "201150006": {
          "id": 201150006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "J'ai quelques questions à propos de la mission qui vient de se terminer. J'espère que tu pourras y répondre",
          "next": [201150007]
        },
        "201150007": {
          "id": 201150007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "La Commission des dix seigneurs a été informée qu'une personne postait la phrase « Je suis frappé par le mara » en boucle sur internet",
          "next": [201150008]
        },
        "201150008": {
          "id": 201150008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Il y a plein de posts de ce genre",
          "next": [201150009]
        },
        "201150009": {
          "id": 201150009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Ça a l'air d'être un appel à l'aide",
          "next": [201150010]
        },
        "201150010": {
          "id": 201150010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "J'ai donc mené mon enquête",
          "next": [201150011, 201150014]
        },
        "201150011": {
          "id": 201150011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Il n'est pas réellement frappé par le mara, si ?",
          "next": [201150012]
        },
        "201150012": {
          "id": 201150012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Non",
          "next": [201150013]
        },
        "201150013": {
          "id": 201150013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Il n'en présente même pas les premiers symptômes",
          "next": [201150017]
        },
        "201150014": {
          "id": 201150014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je pense qu'il doit juste trouver ça drôle",
          "next": [201150015]
        },
        "201150015": {
          "id": 201150015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "On dirait que ce n'est pas la première fois que tu as affaire à ce genre d'individu",
          "next": [201150016]
        },
        "201150016": {
          "id": 201150016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Il n'est pas frappé par le mara, il n'en présente même pas les premiers symptômes",
          "next": [201150017]
        },
        "201150017": {
          "id": 201150017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Maintenant que je me suis assurée qu'il n'était pas frappé par le mara, je suis plus tranquille",
          "next": [201150018]
        },
        "201150018": {
          "id": 201150018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Mais pourquoi dire à tout le monde qu'il est frappé par le mara ? Je ne comprends pas",
          "next": [201150019, 201150023]
        },
        "201150019": {
          "id": 201150019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Peut-être que ça lui sert de prétexte pour faire ce qui lui chante sans avoir à en subir les conséquences",
          "next": [201150020]
        },
        "201150020": {
          "id": 201150020,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "« Ne faites pas le malin avec moi parce que je suis frappé par le mara. »... quelque chose dans le genre",
          "next": [201150021]
        },
        "201150021": {
          "id": 201150021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Je vois. Je comprends mieux",
          "next": [201150022]
        },
        "201150022": {
          "id": 201150022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "En d'autres termes, il ne veut pas assumer les conséquences de ses propos et de ses actes",
          "next": [201150027]
        },
        "201150023": {
          "id": 201150023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "C'est peut-être juste de l'autodérision, il essaie de faire de l'humour. J'ai déjà vu ça",
          "next": [201150024]
        },
        "201150024": {
          "id": 201150024,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "« J'ai réussi à être frappé par le mara tout seul, comme un grand ! »... quelque chose dans le genre",
          "next": [201150025]
        },
        "201150025": {
          "id": 201150025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Et ça fait rire les gens ?",
          "next": [201150026]
        },
        "201150026": {
          "id": 201150026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Je ne suis pas sûre d'avoir le même sens de l'humour",
          "next": [201150027]
        },
        "201150027": {
          "id": 201150027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Pour moi, le mara n'a rien d'amusant",
          "next": [201150028]
        },
        "201150028": {
          "id": 201150028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "Merci. Cette conversation m'a beaucoup appris",
          "next": []
        }
      },
      "participatingContactsIds": [1214, 8001]
    }
  ]
}
