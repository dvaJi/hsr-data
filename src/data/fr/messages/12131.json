  "sections": [
    {
      "id": 1213101,
      "startingMessageId": [121310101],
      "messages": {
        "121310101": {
          "id": 121310101,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Merci, {NICKNAME}. Le Héron express a repris contact avec les mécagrues disparues.",
          "next": [121310102]
        },
        "121310102": {
          "id": 121310102,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Au fait, j'ai demandé à mon collègue : il reste 5 mécagrues en détresse.",
          "next": [121310103]
        },
        "121310103": {
          "id": 121310103,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Si tu les vois, aide-les, je t'en prie.",
          "next": []
        }
      },
      "participatingContactsIds": [55]
    },
    {
      "id": 1213102,
      "startingMessageId": [121310201],
      "messages": {
        "121310201": {
          "id": 121310201,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Merci beaucoup, {NICKNAME}. Un collègue vient de m'informer que toutes les mécagrues disparues avaient été localisées.",
          "next": [121310202]
        },
        "121310202": {
          "id": 121310202,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Il ne reste plus que 4 mécagrues à retrouver. Garde l'œil ouvert, s'il te plaît.",
          "next": [121310203, 121310206]
        },
        "121310203": {
          "id": 121310203,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Il n'y a que moi qui les cherche ? Personne d'autre n'y met du sien ?",
          "next": [121310204]
        },
        "121310204": {
          "id": 121310204,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Bien sûr qu'on les cherche !",
          "next": [121310205]
        },
        "121310205": {
          "id": 121310205,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Tu peux me croire !",
          "next": []
        },
        "121310206": {
          "id": 121310206,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Laissez-moi faire. Je vais vous aider à retrouver ces mécagrues.",
          "next": [121310207]
        },
        "121310207": {
          "id": 121310207,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "D'accord, je compte sur toi.",
          "next": []
        }
      },
      "participatingContactsIds": [55, 8001]
    },
    {
      "id": 1213103,
      "startingMessageId": [121310301],
      "messages": {
        "121310301": {
          "id": 121310301,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Tu sais ce que je m'apprête à dire.",
          "next": [121310302]
        },
        "121310302": {
          "id": 121310302,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Grâce à toi, on a retrouvé une autre mécagrue perdue. Il n'en reste plus que 3.",
          "next": [121310303]
        },
        "121310303": {
          "id": 121310303,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Tu sais comment on t'appelle ici ?",
          "next": [121310304]
        },
        "121310304": {
          "id": 121310304,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "« {M#Le}{F#La} prodige de la chasse aux mécagrues » !",
          "next": [121310305]
        },
        "121310305": {
          "id": 121310305,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Je t'attends avec d'autres bonnes nouvelles, j'espère.",
          "next": []
        }
      },
      "participatingContactsIds": [55]
    },
    {
      "id": 1213104,
      "startingMessageId": [121310401],
      "messages": {
        "121310401": {
          "id": 121310401,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Bonne nouvelle ! On a relocalisé une nouvelle mécagrue en détresse. Il n'en reste plus que 2.",
          "next": [121310402]
        },
        "121310402": {
          "id": 121310402,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "{M#Le}{F#La} prodige de la chasse aux mécagrues ! Tu es à deux doigts d'avoir retrouvé toutes les mécagrues disparues.",
          "next": [121310403, 121310406]
        },
        "121310403": {
          "id": 121310403,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Vous devriez peut-être vous interroger. Vous faites vraiment de votre mieux pour les trouver ?",
          "next": [121310405]
        },
        "121310405": {
          "id": 121310405,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Tu peux me croire !",
          "next": []
        },
        "121310406": {
          "id": 121310406,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Laissez-moi faire. Je vais vous aider à les retrouver.",
          "next": [121310408]
        },
        "121310408": {
          "id": 121310408,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Je t'attends avec d'autres bonnes nouvelles, j'espère.",
          "next": []
        }
      },
      "participatingContactsIds": [55, 8001]
    },
    {
      "id": 1213105,
      "startingMessageId": [121310501],
      "messages": {
        "121310501": {
          "id": 121310501,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Une autre mécagrue est revenue. Il n'en reste plus qu'une.",
          "next": [121310502]
        },
        "121310502": {
          "id": 121310502,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Plus qu'une mécagrue !",
          "next": [121310503]
        },
        "121310503": {
          "id": 121310503,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Merci infiniment !",
          "next": []
        }
      },
      "participatingContactsIds": [55]
    },
    {
      "id": 1213106,
      "startingMessageId": [121310601],
      "messages": {
        "121310601": {
          "id": 121310601,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Elles sont toutes de retour !",
          "next": [121310602]
        },
        "121310602": {
          "id": 121310602,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Toutes les mécagrues disparues ont été retrouvées !",
          "next": [121310603]
        },
        "121310603": {
          "id": 121310603,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "Une autre bonne nouvelle pour toi : les ingénieurs ont analysé l'itinéraire des mécagrues disparues et ont trouvé des indices.",
          "next": [121310604]
        },
        "121310604": {
          "id": 121310604,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 55,
          "text": "On t'a aussi préparé des récompenses. Tu sais où me trouver. Reviens me voir plus tard.",
          "next": []
        }
      },
      "participatingContactsIds": [55]
    }
  ]
