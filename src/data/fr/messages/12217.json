        "122170004": {
          "id": 122170004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Vous voulez savoir comment je vais ? Plutôt pas mal",
          "next": [122170005]
        },
        "122170005": {
          "id": 122170005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Hm...",
