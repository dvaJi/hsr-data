        },
        "122170007": {
          "id": 122170007,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Elles se plaisent à la station spatiale. Leur seul problème est que vous leur manquez",
          "next": [122170008]
        },
        "122170008": {
          "id": 122170008,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1303,
