      "messages": {
        "145220001": {
          "id": 145220001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 181,
          "text": "Il y a eu un retournement de situation inattendu.",
          "next": [
            145220002
          ]
        },
        "145220002": {
          "id": 145220002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 181,
          "text": "Un des membres du Conseil des anciens digne de confiance (ce qui ne court pas les rues) m'a demandé de vous transmettre une invitation.",
          "next": [
            145220003
          ]
        },
        "145220003": {
          "id": 145220003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 181,
          "text": "« Comme c'est un intérêt personnel, j'ai développé une compréhension théorique du Bouc émissaire doré, mais rien du côté pratique. »",
          "next": [
            145220004
          ]
        },
        "145220004": {
          "id": 145220004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 181,
          "text": "« J'ai entendu parler de votre expertise en la matière. Pourriez-vous me consulter pour trouver un moment et en discuter ? »",
          "next": [
            145220005
          ]
        },
        "145220005": {
          "id": 145220005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 181,
          "text": "« J'ai préparé un mur pour exposer vos trophées. Passez-y quand vous en aurez l'occasion. »",
          "next": [
            145220006
          ]
        },
        "145220006": {
          "id": 145220006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 181,
          "text": "« Un Antikythérien est prêt à vous aider. »",
          "next": [
            145220007
          ]
        },
        "145220007": {
          "id": 145220007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 181,
          "text": "C'est à peu près tout. Choisissez une heure qui vous convient et nous nous retrouverons à Néphauros.",
          "next": [
            145220008
          ]
        },
        "145220008": {
          "id": 145220008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 181,
          "text": "Avilius est impatient d'y aller, alors on le laisse nous accompagner.",
          "next": []
        }
      },
