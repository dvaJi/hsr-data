        "200360001": {
          "id": 200360001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Qu'est-ce que tu préfères : la forêt, la montagne, ou la mer ?",
          "next": [200360002, 200360005, 200360008]
        },
        "200360002": {
          "id": 200360002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "J'aime bien la forêt",
