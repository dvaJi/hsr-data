  "relatedMessages": [10125, 20106, 20107, 20108],
  "sections": [
    {
      "id": 2010700,
      "startingMessageId": [201070000],
      "messages": {
        "201070000": {
          "id": 201070000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Savez-vous, {NICKNAME}, que j'ai effectué une course spatiale au cours de mon voyage.",
          "next": [201070001, 201070002]
        },
        "201070001": {
          "id": 201070001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Une course spatiale ? Qu'est-ce que c'est ?",
          "next": [201070003]
        },
        "201070002": {
          "id": 201070002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Qu'est-ce qui vous réjouit tant ?",
          "next": [201070003]
        },
        "201070003": {
          "id": 201070003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Oh trois fois rien, c'est juste le plus haut niveau de course qui existe dans le cosmos.",
          "next": [201070004]
        },
        "201070004": {
          "id": 201070004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "C'est pour cela que chaque année, j'apporte des modifications au moteur de la Perle du cosmos.",
          "next": [201070005]
        },
        "201070005": {
          "id": 201070005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Les aéronefs, vaisseaux et navettes de tous types sont bienvenus dans la course.",
          "next": [201070006]
        },
        "201070006": {
          "id": 201070006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Si vous êtes {F#intéressée}{M#intéressé}, je peux vous présenter pour que l'Astral Express participe à la course.",
          "next": [201070007]
        },
        "201070007": {
          "id": 201070007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Notez cependant qu'on croise parfois des tempêtes de poussière pendant la course.",
          "next": [201070008]
        },
        "201070008": {
          "id": 201070008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Ces tempêtes charrient des débris interplanétaires qui s'accrochent à la coque de la Perle du cosmos. Elle brille alors si fort que je dois me faire violence pour la nettoyer.",
          "next": [201070009, 201070015]
        },
        "201070009": {
          "id": 201070009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ah, oui. Pom-Pom a parlé de ça, une fois.",
          "next": [201070010]
        },
        "201070010": {
          "id": 201070010,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "On a été pris dans une tempête de poussière, nous aussi.",
          "next": [201070011]
        },
        "201070011": {
          "id": 201070011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Cette substance luit et s'accroche à mon armure.",
          "next": [201070012]
        },
        "201070012": {
          "id": 201070012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Elle pourrait se coller dans le pelage de Pom-Pom. Un cauchemar à nettoyer.",
          "next": [201070013]
        },
        "201070013": {
          "id": 201070013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Je vois ça d'ici.",
          "next": [201070014]
        },
        "201070014": {
          "id": 201070014,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Un Pom-Pom tout brillant !",
          "next": [201070018]
        },
        "201070015": {
          "id": 201070015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "J'aimerais bien voir Pom-Pom conduire l'Astral Express dans cette course.",
          "next": [201070016]
        },
        "201070016": {
          "id": 201070016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Je suis persuadé que l'Astral Express ferait une grosse impression.",
          "next": [201070017]
        },
        "201070017": {
          "id": 201070017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "J'ai hâte de me mesurer à vous.",
          "next": [201070018]
        },
        "201070018": {
          "id": 201070018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Bon, assez papoté pour aujourd'hui. Je dois nettoyer ma Perle du cosmos.",
          "next": [201070019]
        },
        "201070019": {
          "id": 201070019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Quand l'occasion se présentera, j'aimerais vous inviter {NICKNAME}, vous et l'équipage à participer à la course.",
          "next": [201070020]
        },
        "201070020": {
          "id": 201070020,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1302,
          "next": []
        }
      },
