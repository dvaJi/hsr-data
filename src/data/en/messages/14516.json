        "145160030": {
          "id": 145160030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1408,
          "text": "That's true",
          "next": [
            145160031
          ]
        },
        "145160031": {
          "id": 145160031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1408,
          "text": "But the price they paid isn't really worth it, right? Just look at Aglaea and Tribbie",
          "next": [
            145160036
          ]
        },
        "145160032": {
          "id": 145160032,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "This is only my theory, so it could be wrong",
