        "145160028": {
          "id": 145160028,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Uh, for power and status?",
          "next": [
            145160029
          ]
        },
        "145160029": {
          "id": 145160029,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Everyone wants to climb above others. That's human nature",
