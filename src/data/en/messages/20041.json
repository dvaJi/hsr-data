  "relatedMessages": [11114, 11801, 20021, 20041, 20042],
  "sections": [
    {
      "id": 2004100,
      "startingMessageId": [200410000],
      "messages": {
        "200410000": {
          "id": 200410000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "{NICKNAME}, there's something I'd like to discuss with you.",
          "next": [200410001]
        },
        "200410001": {
          "id": 200410001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Do you have time?",
          "next": [200410002]
        },
        "200410002": {
          "id": 200410002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "I do, what is it?",
          "next": [200410003]
        },
        "200410003": {
          "id": 200410003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Actually, my youngest sister just came home from a scientific expedition.",
          "next": [200410004]
        },
        "200410004": {
          "id": 200410004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "I want to gift her something, what do you think is a suitable gift?",
          "next": [200410005, 200410014, 200410023]
        },
        "200410005": {
          "id": 200410005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "What does she like?",
          "next": [200410006]
        },
        "200410006": {
          "id": 200410006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "I feel bad for saying this, but I'm not quite sure what she likes.",
          "next": [200410007]
        },
        "200410007": {
          "id": 200410007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "I only know that she's very passionate about science and outdoor expeditions.",
          "next": [200410008]
        },
        "200410008": {
          "id": 200410008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Oh, I have an idea. What if I gift her a new set of scientific expedition gear?",
          "next": [200410009]
        },
        "200410009": {
          "id": 200410009,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Thumbs Up]",
          "next": [200410010]
        },
        "200410010": {
          "id": 200410010,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "That's not a bad idea",
          "next": [200410011]
        },
        "200410011": {
          "id": 200410011,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "As long as she likes it",
          "next": [200410012]
        },
        "200410012": {
          "id": 200410012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "You're right. Thank you for helping me sort out my thoughts.",
          "next": [200410013]
        },
        "200410013": {
          "id": 200410013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "I'm going ahead to look for gifts. See you soon.",
          "next": []
        },
        "200410014": {
          "id": 200410014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "You can buy her some flowers",
          "next": [200410015]
        },
        "200410015": {
          "id": 200410015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "I thought about gifting her flowers, but I remember her saying she's not interested in run-of-the-mill plants.",
          "next": [200410016]
        },
        "200410016": {
          "id": 200410016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "She likes the ones in the Snow Plains, those that are rare in the city.",
          "next": [200410017]
        },
        "200410017": {
          "id": 200410017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "I have an idea. Next time I'm patrolling the Snow Plains, I'll keep an eye on anything interesting.",
          "next": [200410018]
        },
        "200410018": {
          "id": 200410018,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Thumbs Up]",
          "next": [200410019]
        },
        "200410019": {
          "id": 200410019,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "That's not a bad idea",
          "next": [200410020]
        },
        "200410020": {
          "id": 200410020,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "But be safe",
          "next": [200410021]
        },
        "200410021": {
          "id": 200410021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Haha, I will, thank you.",
          "next": [200410022]
        },
        "200410022": {
          "id": 200410022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "I'll make myself busy now, see you soon.",
          "next": []
        },
        "200410023": {
          "id": 200410023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "You can buy her some new clothes",
          "next": [200410024]
        },
        "200410024": {
          "id": 200410024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "I thought about that, but I don't have good taste when it comes to fashion, so I dared not make a choice.",
          "next": [200410025]
        },
        "200410025": {
          "id": 200410025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Serval did buy her some really nice clothes.",
          "next": [200410026]
        },
        "200410026": {
          "id": 200410026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "But I don't want Serval to know.",
          "next": [200410027]
        },
        "200410027": {
          "id": 200410027,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[decline]",
          "next": [200410028]
        },
        "200410028": {
          "id": 200410028,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Why so shy?",
          "next": [200410029]
        },
        "200410029": {
          "id": 200410029,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "You're family, just go and ask",
          "next": [200410030]
        },
        "200410030": {
          "id": 200410030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "...",
          "next": [200410031]
        },
        "200410031": {
          "id": 200410031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "You're right, I should just ask Serval just to avoid buying the wrong things.",
          "next": [200410032]
        },
        "200410032": {
          "id": 200410032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Thank you for your suggestion, I'll look for Serval now, see you.",
          "next": []
        }
      },
      "participatingContactsIds": [1104, 8001]
    }
  ]
}
