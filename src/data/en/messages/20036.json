        },
        "200360015": {
          "id": 200360015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Unfortunately, they are all covered in snow now",
          "next": [200360016, 200360018]
        },
        "200360016": {
          "id": 200360016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Everything will get better",
          "next": [200360017]
        },
        "200360017": {
          "id": 200360017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "I hope so",
          "next": [200360020]
        },
        "200360018": {
          "id": 200360018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "There are many people who have never seen snow in their life",
          "next": [200360019]
        },
        "200360019": {
          "id": 200360019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "You're pretty good at comforting people",
