  "sections": [
    {
      "id": 2003500,
      "startingMessageId": [
        200350000
      ],
      "messages": {
        "200350000": {
          "id": 200350000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "{NICKNAME}, have you ever felt that...",
          "next": [
            200350001
          ]
        },
        "200350001": {
          "id": 200350001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Everyone around is afraid of you and is reluctant to tell you the truth",
          "next": [
            200350002,
            200350004
          ]
        },
        "200350002": {
          "id": 200350002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "I have never been a leader",
          "next": [
            200350003
          ]
        },
        "200350003": {
          "id": 200350003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Sorry, I just don't know who else to ask",
          "next": [
            200350006
          ]
        },
        "200350004": {
          "id": 200350004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "You need to learn to fit in",
          "next": [
            200350005
          ]
        },
        "200350005": {
          "id": 200350005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "I know, but it's hard to actually do it",
          "next": [
            200350006
          ]
        },
        "200350006": {
          "id": 200350006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "My mother said the life of a Guardian is doomed to be lonely",
          "next": [
            200350007
          ]
        },
        "200350007": {
          "id": 200350007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "I guess I need some more time to adapt",
          "next": [
            200350008
          ]
        },
        "200350008": {
          "id": 200350008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "I also talked to Seele, but she looked at me, bewildered, and said she couldn't understand what those people were thinking",
          "next": [
            200350009,
            200350013
          ]
        },
        "200350009": {
          "id": 200350009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Is it possible that...",
          "next": [
            200350010
          ]
        },
        "200350010": {
          "id": 200350010,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Seele is different because she is not afraid of you?",
          "next": [
            200350011
          ]
        },
        "200350011": {
          "id": 200350011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Is that so...",
          "next": [
            200350012
          ]
        },
        "200350012": {
          "id": 200350012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Now I think about it, Seele really is not afraid of anyone",
          "next": [
            200350017
          ]
        },
        "200350013": {
          "id": 200350013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "How dare she do this to you!",
          "next": [
            200350014
          ]
        },
        "200350014": {
          "id": 200350014,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "You are the Supreme Guardian",
          "next": [
            200350015
          ]
        },
        "200350015": {
          "id": 200350015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "You mean she is not afraid of me?",
          "next": [
            200350016
          ]
        },
        "200350016": {
          "id": 200350016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "I think you are right",
          "next": [
            200350017
          ]
        },
        "200350017": {
          "id": 200350017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Maybe I can find my own path as a leader...",
          "next": [
            200350018
          ]
        },
        "200350018": {
          "id": 200350018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "I'll do my best!",
          "next": []
        }
      },
