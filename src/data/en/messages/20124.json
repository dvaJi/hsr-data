        "201240006": {
          "id": 201240006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 124,
          "text": "It's me!",
          "next": [
            201240007
          ]
        },
        "201240007": {
          "id": 201240007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 124,
          "text": "I know you're busy, but I need your help with something",
