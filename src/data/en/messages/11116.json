      "messages": {
        "111160000": {
          "id": 111160000,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Miss Himeko, I have a question...",
          "next": [111160003]
        },
        "111160003": {
          "id": 111160003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Ask away, {NICKNAME}",
          "next": [111160004, 111160005, 111160006]
        },
        "111160004": {
          "id": 111160004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Does our Express welcome new members?",
          "next": [111160007]
        },
        "111160005": {
          "id": 111160005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "It's just... I have a friend who wants to join...",
          "next": [111160008]
        },
        "111160006": {
          "id": 111160006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "I have a friend who wants to have the coffee you brew!",
          "next": [111160009]
        },
        "111160007": {
          "id": 111160007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "New members? Of course. So long as they are willing to travel with us, the Express's door will always be open",
          "next": [111160010]
        },
        "111160008": {
          "id": 111160008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "A potential new crew mate? Of course. So long as they are willing to travel with us, the Express's door will always be open",
          "next": [111160010]
        },
        "111160009": {
          "id": 111160009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Oh? This is new, there's no reason for me to refuse",
          "next": [111160010]
        },
        "111160010": {
          "id": 111160010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "But, I would like to know more about your friend",
          "next": [111160011]
        },
        "111160011": {
          "id": 111160011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "In your experience, what kind of person are they?",
          "next": [111160012, 111160013, 111160014]
        },
        "111160012": {
          "id": 111160012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "She's a mechanical prodigy with a meticulous mind",
          "next": [111160015]
        },
        "111160013": {
          "id": 111160013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "She's a rock star with an uninhibited personality",
          "next": [111160016]
        },
        "111160014": {
          "id": 111160014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "She's... I can't say for sure",
          "next": [111160017]
        },
        "111160015": {
          "id": 111160015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "That's pretty good, maybe she and I will have a lot in common",
          "next": [111160018]
        },
        "111160016": {
          "id": 111160016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Sounds like a hobby young people would like. Maybe the Express will become livelier now",
          "next": [111160018]
        },
        "111160017": {
          "id": 111160017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "No worries. Everything will become clearer after spending some time together",
          "next": [111160018]
        },
        "111160018": {
          "id": 111160018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "What I'm saying is, I agree with letting your friend on the Express",
          "next": [111160019]
        },
        "111160019": {
          "id": 111160019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Ask her to take her time packing her stuff. Our Express will always welcome new passengers",
          "next": [111160020, 111160021]
        },
        "111160020": {
          "id": 111160020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Thank you for your time, Miss Himeko",
          "next": [111160022]
        },
        "111160021": {
          "id": 111160021,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "I understand, thanks Himeko!",
          "next": [111160022]
        },
        "111160022": {
          "id": 111160022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Don't worry about it. If there are other issues along the way, feel free to let me know",
          "next": []
        }
      },
      "participatingContactsIds": [8001, 1003]
    }
  ]
