        "201690001": {
          "id": 201690001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1202,
          "text": "My tail is frizzy again",
          "next": [
            201690002
          ]
        },
        "201690002": {
          "id": 201690002,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1202,
          "next": [
