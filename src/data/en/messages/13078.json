        "130780001": {
          "id": 130780001,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Master Gongshu, there's something I want to talk with you about",
          "next": [
            130780002
          ]
        },
        "130780002": {
          "id": 130780002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "General Huaiyan gave the Luofu a sword called Guyun",
          "next": [
            130780003
          ]
        },
        "130780003": {
          "id": 130780003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 147,
          "text": "Don't you worry. General Huaiyan has already conferred his instructions.",
          "next": [
            130780004
          ]
        },
        "130780004": {
          "id": 130780004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 147,
          "text": "Right, I heard from my apprentices that a young lady with a sword barged into the Artisanship Commission.",
          "next": [
            130780005
          ]
        },
        "130780005": {
          "id": 130780005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 147,
          "text": "That's General Huaiyan's granddaughter, right? Is she here to chat or kidnap me?",
          "next": [
            130780006
          ]
        },
        "130780006": {
          "id": 130780006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "She already promised me that she wouldn't take Guyun until we've confirmed the situation.",
          "next": [
            130780007
          ]
        },
        "130780007": {
          "id": 130780007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 147,
          "text": "What? She's coming to steal the sword?",
          "next": [
            130780008
          ]
        },
        "130780008": {
          "id": 130780008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 147,
          "text": "My nerves can't take much more of this!",
          "next": [
            130780009
          ]
        },
        "130780009": {
          "id": 130780009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 147,
          "text": "The Knight of Beauty just escorted the sword to the Artisanship Commission. It is currently in the armory without a chance for compromise or mishaps. You just relax.",
          "next": [
            130780010
          ]
        },
        "130780010": {
          "id": 130780010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 147,
          "text": "When I have time, I want to hear that girl tell my why she tried to steal the blade!",
          "next": []
        }
