  "sections": [
    {
      "id": 1111900,
      "startingMessageId": [111190000],
      "messages": {
        "111190000": {
          "id": 111190000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "{F#Big sister}{M#Big brother}, are you free now?",
          "next": [111190001]
        },
        "111190001": {
          "id": 111190001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "I want to talk about Pascal...",
          "next": [111190002, 111190003]
        },
        "111190002": {
          "id": 111190002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Are you still sad...?",
          "next": [111190004]
        },
        "111190003": {
          "id": 111190003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Okay. What do you want to talk about?",
          "next": [111190005]
        },
        "111190004": {
          "id": 111190004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "No... I've calmed down",
          "next": [111190005]
        },
        "111190005": {
          "id": 111190005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Actually, I've thought a lot about it",
          "next": [111190006]
        },
        "111190006": {
          "id": 111190006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "I just don't understand... What is Pascal?",
          "next": [111190007]
        },
        "111190007": {
          "id": 111190007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Other robots think Pascal is a Fragmentum creation",
          "next": [111190008]
        },
        "111190008": {
          "id": 111190008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Mr. Svarog thinks Pascal is a normal robot",
          "next": [111190009]
        },
        "111190009": {
          "id": 111190009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Pascal doesn't even know what it is itself",
          "next": [111190010]
        },
        "111190010": {
          "id": 111190010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "I think Pascal could even be a human",
          "next": [111190011]
        },
        "111190011": {
          "id": 111190011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "{F#Big sister}{M#Big brother}, what do you think?",
          "next": [111190012, 111190013, 111190014]
        },
        "111190012": {
          "id": 111190012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "I think Pascal is just a robot.",
          "next": [111190015]
        },
        "111190013": {
          "id": 111190013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Pascal might be a Fragmentum creature...",
          "next": [111190017]
        },
        "111190014": {
          "id": 111190014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Pascal is a living human being.",
          "next": [111190019]
        },
        "111190015": {
          "id": 111190015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "You think so, {F#Big sister}{M#Big brother}?",
          "next": [111190016]
        },
        "111190016": {
          "id": 111190016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Pascal does look like a regular robot, but...",
          "next": [111190021]
        },
        "111190017": {
          "id": 111190017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "You think so, {F#Big sister}{M#Big brother}?",
          "next": [111190018]
        },
        "111190018": {
          "id": 111190018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "But doesn't Pascal just look like a normal little robot? Also...",
          "next": [111190021]
        },
        "111190019": {
          "id": 111190019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "You think so, {F#Big sister}{M#Big brother}?",
          "next": [111190020]
        },
        "111190020": {
          "id": 111190020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "I think so too, but...",
          "next": [111190021]
        },
        "111190021": {
          "id": 111190021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Fragmentum creature, robot, human",
          "next": [111190022]
        },
        "111190022": {
          "id": 111190022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "What is the difference between these?",
          "next": [111190023]
        },
        "111190023": {
          "id": 111190023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Some Fragmentum creatures can speak and behave like humans",
          "next": [111190024]
        },
        "111190024": {
          "id": 111190024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "While Mr. Svarog looks like a robot, he also can speak, think, and even express \"emotions\" like us",
          "next": [111190025]
        },
        "111190025": {
          "id": 111190025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "So how do we tell what is \"us\" and what is \"them?\"",
          "next": [111190026, 111190027, 111190028]
        },
        "111190026": {
          "id": 111190026,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Only humans have the ability to identify and empathize with other types of creatures",
          "next": [111190029]
        },
        "111190027": {
          "id": 111190027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Simply put, our biological makeup is completely different",
          "next": [111190032]
        },
        "111190028": {
          "id": 111190028,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "There is no difference between humans and any other types of creatures in the world",
          "next": [111190034, 111190035]
        },
        "111190029": {
          "id": 111190029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "So that's how it is",
          "next": [111190030]
        },
        "111190030": {
          "id": 111190030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "So maybe, Pascal and Mr. Svarog...",
          "next": [111190031]
        },
        "111190031": {
          "id": 111190031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Maybe they're all living, breathing humans too",
          "next": [111190038]
        },
        "111190032": {
          "id": 111190032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "So that's how it is",
          "next": [111190033]
        },
        "111190033": {
          "id": 111190033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "That's probably the only way... we can draw a clear distinction between us and other things",
          "next": [111190038]
        },
        "111190034": {
          "id": 111190034,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "We were born in the same universe, and we are heading toward the same demise",
          "next": [111190036]
        },
        "111190035": {
          "id": 111190035,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "We're all just different manifestations of the same basic particles",
          "next": [111190036]
        },
        "111190036": {
          "id": 111190036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "So that's how it is",
          "next": [111190037]
        },
        "111190037": {
          "id": 111190037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "It sounds complicated, but I will try and understand it",
          "next": [111190038]
        },
        "111190038": {
          "id": 111190038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "I don't feel so lost anymore",
          "next": [111190039]
        },
        "111190039": {
          "id": 111190039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1107,
          "text": "Thank you, {F#big sister}{M#big brother}~",
          "next": [111190040]
        },
