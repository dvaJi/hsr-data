  "sections": [
    {
      "id": 1212000,
      "startingMessageId": [121200000],
      "messages": {
        "121200000": {
          "id": 121200000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 54,
          "text": "Hi, {NICKNAME}. I got your contact details from Miss Qingque",
          "next": [121200001]
        },
        "121200001": {
          "id": 121200001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 54,
          "text": "I'm trying to decipher my master's plans. By the Reignbow, I feel like I'm overflowing with inspiration!",
          "next": [121200002]
        },
        "121200002": {
          "id": 121200002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 54,
          "text": "I want to talk to someone about my ideas. Do you have the time?",
          "next": [121200003, 121200007]
        },
        "121200003": {
          "id": 121200003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "I won't be able to understand what you say...",
          "next": [121200004]
        },
        "121200004": {
          "id": 121200004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 54,
          "text": "Hehehe, I just want someone to chat with. Just treat it as listening to a new friend's rants.",
          "next": [121200005, 121200006]
        },
        "121200005": {
          "id": 121200005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "I charge a lot for counseling!",
          "next": [121200015]
        },
        "121200006": {
          "id": 121200006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "The user you have messaged is currently unavailable",
          "next": [121200015]
        },
        "121200007": {
          "id": 121200007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Have you talked to Qingque about your inspirations?",
          "next": [121200008]
        },
        "121200008": {
          "id": 121200008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 54,
          "text": "I have. She said she's too busy and doesn't have time, and asked me to find you",
          "next": [121200009, 121200012]
        },
        "121200009": {
          "id": 121200009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Qingque and \"being busy\" are like fire and ice",
          "next": [121200010]
        },
        "121200010": {
          "id": 121200010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 54,
          "text": "What does that mean? Is that some new proverb?",
          "next": [121200011]
        },
        "121200011": {
          "id": 121200011,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Fire and ice are so unrelated, they're practically incompatible",
          "next": [121200015]
        },
        "121200012": {
          "id": 121200012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Um, I actually don't have time either",
          "next": [121200013]
        },
        "121200013": {
          "id": 121200013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 54,
          "text": "But Miss Qingque said you're in Xianzhou as a tourist, and must have plenty of free time. She also said the secret faction you belong to loves to help people. Why else would you help the general and save us at the Artisanship Commission?",
          "next": [121200014]
        },
        "121200014": {
          "id": 121200014,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "What did I do to offend Qingque?",
          "next": [121200015]
        },
        "121200015": {
          "id": 121200015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 54,
          "text": "Please, I'm begging you, {NICKNAME}",
          "next": [121200016]
        },
        "121200016": {
          "id": 121200016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 54,
          "text": "I really need someone to supervise me. I'll definitely pay you for it!",
          "next": []
        }
      },
      "participatingContactsIds": [54, 8001]
    }
  ]
}
