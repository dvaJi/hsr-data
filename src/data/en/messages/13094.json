      "messages": {
        "130940000": {
          "id": 130940000,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "The Message Doll has initiated a temporary conversation.",
          "next": [
            130940001
          ]
        },
        "130940001": {
          "id": 130940001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Cool",
          "next": [
            130940002
          ]
        },
        "130940002": {
          "id": 130940002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "That feels so cool, don't you think?",
          "next": [
            130940003,
            130940008,
            130940013
          ]
        },
        "130940003": {
          "id": 130940003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Huh? More bombs to defuse?",
          "next": [
            130940004
          ]
        },
        "130940004": {
          "id": 130940004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Just give me the coordinates, and I'll defuse the bomb",
          "next": [
            130940005
          ]
        },
        "130940005": {
          "id": 130940005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Well, there are only 1,000 bombs. Miss Sparkle never breaks her words~",
          "next": [
            130940006
          ]
        },
        "130940006": {
          "id": 130940006,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "[image] Crying mask sticker",
          "next": [
            130940007
          ]
        },
        "130940007": {
          "id": 130940007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "I'm just here to spill the beans on Miss Sparkle's motive, got it?",
          "next": [
            130940017,
            130940018
          ]
        },
        "130940008": {
          "id": 130940008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Come on!",
          "next": [
            130940009
          ]
        },
        "130940009": {
          "id": 130940009,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Don't text me anymore. I don't want Pom-Pom to get the wrong idea",
          "next": [
            130940010
          ]
        },
        "130940010": {
          "id": 130940010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Think you can mess with me? Well, you just stepped on chewing gum. I'll stick to you forever!",
          "next": [
            130940011
          ]
        },
        "130940011": {
          "id": 130940011,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "[image] Naughty smile sticker",
          "next": [
            130940012
          ]
        },
        "130940012": {
          "id": 130940012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "By the way, Miss Sparkle hasn't even revealed her motive yet! Aren't you curious?",
          "next": [
            130940017,
            130940018
          ]
        },
        "130940013": {
          "id": 130940013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Fireworks! Cool!",
          "next": [
            130940014
          ]
        },
        "130940014": {
          "id": 130940014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Yeah! You're getting the hang of it, aren't you?",
          "next": [
            130940015
          ]
        },
        "130940015": {
          "id": 130940015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "So, are you curious about why Miss Sparkle is doing all this?",
          "next": [
            130940016
          ]
        },
        "130940016": {
          "id": 130940016,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "[image] Lighter sticker",
          "next": [
            130940017,
            130940018
          ]
        },
        "130940017": {
          "id": 130940017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Explain",
          "next": [
            130940021
          ]
        },
        "130940018": {
          "id": 130940018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "I'm going offline, catch you later",
          "next": [
            130940019
          ]
        },
        "130940019": {
          "id": 130940019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "No, don't go offline! Please! Please!",
          "next": [
            130940020
          ]
        },
        "130940020": {
          "id": 130940020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Heh, just kidding~ Let's get back to business!",
          "next": [
            130940021
          ]
        },
        "130940021": {
          "id": 130940021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Do you remember the prophesied \"three deaths\"?",
          "next": [
            130940022
          ]
        },
        "130940022": {
          "id": 130940022,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "What's that got to do with you?",
          "next": [
            130940023
          ]
        },
        "130940023": {
          "id": 130940023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Who knows? All I can say is that someone paid a hefty sum to hire Miss Sparkle as the director at Penacony.",
          "next": [
            130940024
          ]
        },
        "130940024": {
          "id": 130940024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Seems like it's a friend she made while gaming. The client told Miss Sparkle to make sure the \"three deaths\" happen in the safest way possible",
          "next": [
            130940025
          ]
        },
        "130940025": {
          "id": 130940025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "In return, they shared their \"script\" with Miss Sparkle",
          "next": [
            130940026
          ]
        },
        "130940026": {
          "id": 130940026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "And what happened next... Well, a smart cookie like you can surely guess, Gray Hair!",
          "next": [
            130940027
          ]
        },
        "130940027": {
          "id": 130940027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "However, Miss Sparkle isn't some control freak. She let the actors decide how the story would unfold for themselves",
          "next": [
            130940028
          ]
        },
        "130940028": {
          "id": 130940028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Surprise question! Did you press the \"Mutually Assured Destruction Button\" that Miss Sparkle gave you?",
          "next": [
            130940029,
            130940031,
            130940034
          ]
        },
        "130940029": {
          "id": 130940029,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Yeah, I literally mashed it",
          "next": [
            130940030
          ]
        },
        "130940030": {
          "id": 130940030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Well, congratulations! You're the one who reshaped the third death!",
          "next": [
            130940037
          ]
        },
        "130940031": {
          "id": 130940031,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "No, that button seems way too suspicious",
          "next": [
            130940032
          ]
        },
        "130940032": {
          "id": 130940032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Hmhm, you're a cautious one, huh?",
          "next": [
            130940033
          ]
        },
        "130940033": {
          "id": 130940033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "But no worries. Since more than half of the actors pressed the button, the third death went exactly as planned!",
          "next": [
            130940037
          ]
        },
        "130940034": {
          "id": 130940034,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "I didn't even get the button!",
          "next": [
            130940035
          ]
        },
        "130940035": {
          "id": 130940035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Oh, really? Well, good luck scoring a ticket next time then!",
          "next": [
            130940036
          ]
        },
        "130940036": {
          "id": 130940036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "But no worries. Since more than half of the actors pressed the button, the third death went exactly as planned!",
          "next": [
            130940037
          ]
        },
        "130940037": {
          "id": 130940037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "She soared up in the sky and set off the most epic fireworks Penacony had ever seen, giving a grand finale to this beautiful dream",
          "next": [
            130940038
          ]
        },
        "130940038": {
          "id": 130940038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "And she herself was safe and sound! Happiest ending ever!",
          "next": [
            130940039
          ]
        },
        "130940039": {
          "id": 130940039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "So, congrats, Gray Hair. After all, you're the one who made the call and actually saved that firefly girl",
          "next": [
            130940040
          ]
        },
        "130940040": {
          "id": 130940040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "And that means Miss Sparkle will get paid as promised!",
          "next": [
            130940041
          ]
        },
        "130940041": {
          "id": 130940041,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Oh",
          "next": [
            130940042
          ]
        },
        "130940042": {
          "id": 130940042,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "By the way, to celebrate the successful grand finale, Miss Sparkle has decided to share another secret with you, Gray Hair",
          "next": [
            130940043,
            130940044
          ]
        },
        "130940043": {
          "id": 130940043,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "What is it?",
          "next": [
            130940045
          ]
        },
        "130940044": {
          "id": 130940044,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Alright, I'm all ears!",
          "next": [
            130940045
          ]
        },
        "130940045": {
          "id": 130940045,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "You know what?",
          "next": [
            130940046
          ]
        },
        "130940046": {
          "id": 130940046,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Actually...",
          "next": [
            130940047
          ]
        },
        "130940047": {
          "id": 130940047,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Dr. Primitive is a huge fan of bananas",
          "next": [
            130940048,
            130940049,
            130940050
          ]
        },
        "130940048": {
          "id": 130940048,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Huh?",
          "next": [
            130940051
          ]
        },
        "130940049": {
          "id": 130940049,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "What do you mean?",
          "next": [
            130940051
          ]
        },
        "130940050": {
          "id": 130940050,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Go on.",
          "next": [
