        "145220001": {
          "id": 145220001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 181,
          "text": "There's been an unexpected turn of events",
          "next": [
            145220002
          ]
        },
        "145220002": {
          "id": 145220002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 181,
          "text": "A trustworthy elder from the Council of Elders (and there aren't many) asked me to forward you an invitation",
          "next": [
            145220003
          ]
        },
        "145220003": {
          "id": 145220003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 181,
          "text": "\"As it's a personal interest, I've developed a theoretical understanding of the Golden Scapegoat, nothing practical\"",
          "next": [
            145220004
          ]
        },
        "145220004": {
          "id": 145220004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 181,
          "text": "\"I've heard of your expertise in this matter. Would it be possible for you to find a moment to discuss it with me?\"",
          "next": [
            145220005
          ]
        },
        "145220005": {
          "id": 145220005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 181,
          "text": "\"I've prepared a wall to display your trophies. Do drop by whenever it is convenient for you\"",
          "next": [
            145220006
          ]
        },
        "145220006": {
          "id": 145220006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 181,
          "text": "\"An Antikytheran stands ready to help\"",
          "next": [
            145220007
          ]
        },
        "145220007": {
          "id": 145220007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 181,
          "text": "That's about it. Just pick a time that works and we'll meet at Dawncloud",
          "next": [
            145220008
          ]
        },
        "145220008": {
          "id": 145220008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 181,
          "text": "Avilius is raring to come, so we'll let him tag along",
          "next": []
        }
      },
