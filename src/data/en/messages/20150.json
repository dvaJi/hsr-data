          ]
        },
        "201500001": {
          "id": 201500001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1308,
          "text": "As I was passing by the COLORS store, the store clerk asked for my autograph",
          "next": [
            201500002,
            201500005,
            201500007
          ]
        },
        "201500002": {
          "id": 201500002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Well, you're totally a celebrity now with everything you've done!",
          "next": [
            201500003
          ]
        },
        "201500003": {
          "id": 201500003,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "next": [
