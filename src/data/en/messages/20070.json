  "sections": [
    {
      "id": 2007000,
      "startingMessageId": [200700015],
      "messages": {
        "200700002": {
          "id": 200700002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Chat with the people around you",
          "next": [200700003]
        },
        "200700003": {
          "id": 200700003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "To be honest, I'm not great at opening a conversation... But with that coffin around me, a conversation is bound to happen",
          "next": [200700008]
        },
        "200700004": {
          "id": 200700004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Do whatever pleases you. Play games, read books, watch the scenery, anything you want",
          "next": [200700005]
        },
        "200700005": {
          "id": 200700005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Finish the books I've borrowed on the Luofu, this might not be a bad idea",
          "next": [200700008]
        },
        "200700006": {
          "id": 200700006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "There's really nothing to do on the road except sleeping",
          "next": [200700007]
        },
        "200700007": {
          "id": 200700007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "The purpose is the end point, not the process, huh?",
          "next": [200700008]
        },
        "200700008": {
          "id": 200700008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Is that what you've been doing in your journeys, {NICKNAME}?",
          "next": [200700009, 200700012]
        },
        "200700009": {
          "id": 200700009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Of course, that's what I do all the time",
          "next": [200700010]
        },
        "200700010": {
          "id": 200700010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Sounds like you've got some insights",
          "next": [200700011]
        },
        "200700011": {
          "id": 200700011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Thank you for giving me a sincere suggestion",
          "next": [200700014]
        },
        "200700012": {
          "id": 200700012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Of course not, I never do that",
          "next": [200700013]
        },
        "200700013": {
          "id": 200700013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Sounds like you chose something that you think suits me best",
          "next": [200700014]
        },
        "200700014": {
          "id": 200700014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "I'll try it",
          "next": []
        },
        "200700015": {
          "id": 200700015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Hey, {NICKNAME}",
          "next": [200700016]
        },
        "200700016": {
          "id": 200700016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Sorry to disturb you, but there's something I forgot to ask",
          "next": [200700017]
        },
        "200700017": {
          "id": 200700017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Like I said before, the journey of a traveling merchant is mostly drawn-out and boring",
          "next": [200700018]
        },
        "200700018": {
          "id": 200700018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Do you have any advice for passing time when traveling, {NICKNAME}?",
          "next": [200700002, 200700004, 200700006]
        }
      },
      "participatingContactsIds": [1203, 8001]
    }
  ]
}
