        },
        "500100002": {
          "id": 500100002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "I pack light when I travel",
          "next": [500100003]
        },
        "500100003": {
          "id": 500100003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "That may be true, but don't underestimate the Express' capacity! We can carry as much luggage as you want!",
          "next": [500100006]
        },
        "500100004": {
          "id": 500100004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "I'm poor. Can't afford luggage",
          "next": [500100005]
        },
        "500100005": {
          "id": 500100005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "...Seriously? Every passenger should have their personal possessions",
