  "relatedMessages": [20049, 20050, 20051],
  "sections": [
    {
      "id": 2005100,
      "startingMessageId": [200510000],
      "messages": {
        "200510000": {
          "id": 200510000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "{NICKNAME}, SOS!",
          "next": [200510001]
        },
        "200510001": {
          "id": 200510001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "It's raining and I saw a litter of white kittens without their mom, what should I do?",
          "next": [200510002, 200510005, 200510008]
        },
        "200510002": {
          "id": 200510002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Maybe wait for their mom to return? She could've been out to hunt food",
          "next": [200510003]
        },
        "200510003": {
          "id": 200510003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "I thought so too, so I've been holding an oil-paper umbrella for them",
          "next": [200510004]
        },
        "200510004": {
          "id": 200510004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "I don't dare to move them, it would be bad if their mom came back and couldn't find them",
          "next": [200510011]
        },
        "200510005": {
          "id": 200510005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Move them somewhere with cover, they'll get sick at this rate",
          "next": [200510006]
        },
        "200510006": {
          "id": 200510006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "I don't dare to move them, it would be bad if their mom came back and couldn't find them",
          "next": [200510007]
        },
        "200510007": {
          "id": 200510007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Luckily, I bought a paper umbrella, I can keep them under it for a while",
          "next": [200510011]
        },
        "200510008": {
          "id": 200510008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Take them home and become a cat dad",
          "next": [200510009]
        },
        "200510009": {
          "id": 200510009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "I'll wait for their mom to return, she could've gone away to hunt food. It would be bad if she can't find the kitten when she comes back",
          "next": [200510010]
        },
        "200510010": {
          "id": 200510010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Luckily, I bought a paper umbrella, I can keep them under it for a while",
          "next": [200510011]
        },
        "200510011": {
          "id": 200510011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Looking at them, sigh...",
          "next": [200510012]
        },
        "200510012": {
          "id": 200510012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Something the matter?",
          "next": [200510013]
        },
        "200510013": {
          "id": 200510013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "I just thought how I never saw my birth parents. I don't even know if they didn't want me, or if they're already gone...",
          "next": [200510014]
        },
        "200510014": {
          "id": 200510014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "But then again, I don't have many memories anyway, so... I'm okay",
          "next": [200510015]
        },
        "200510015": {
          "id": 200510015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "My earliest memory is the general teaching me",
          "next": [200510016, 200510018]
        },
        "200510016": {
          "id": 200510016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "It must have been difficult for you",
          "next": [200510017]
        },
        "200510017": {
          "id": 200510017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "I'm okay, the general had it harder, I think",
          "next": [200510020]
        },
        "200510018": {
          "id": 200510018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "He has his work cut out for him",
          "next": [200510019]
        },
        "200510019": {
          "id": 200510019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Yeah, he must've put in so much effort",
          "next": [200510020]
        },
        "200510020": {
          "id": 200510020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Oh, cat mommy is back",
          "next": [200510021]
        },
        "200510021": {
          "id": 200510021,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "That's good!",
          "next": [200510022]
        },
        "200510022": {
          "id": 200510022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "I'm going now, thank you for listening",
          "next": [200510023]
        },
        "200510023": {
          "id": 200510023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Remember to keep it a secret! I'm a big boy now, so no pinkie swears are needed",
          "next": []
        }
      },
      "participatingContactsIds": [1209, 8001]
    }
  ]
}
