        },
        "120050002": {
          "id": 120050002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 34,
          "text": "Your friend request has been approved. You may now start chatting.",
          "next": [
            120050003
          ]
        },
        "120050003": {
          "id": 120050003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 34,
          "text": "Welcome, lonesome traveler.",
          "next": [
            120050004
          ]
        },
        "120050004": {
          "id": 120050004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 34,
          "text": "I've heard what happened. Our meeting in this sea of strangers could be a type of fated encounter.",
          "next": [
            120050005
          ]
        },
        "120050005": {
          "id": 120050005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 34,
          "text": "From today on, I am your friend in which you can confide your secrets. I will not break confidence and reveal anything you share with another person. I hope you will do the same.",
          "next": [
            120050006,
            120050008
          ]
        },
        "120050006": {
          "id": 120050006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Really? You're not some kind of high-quality imitation of the real one?",
          "next": [
            120050007
          ]
        },
        "120050007": {
          "id": 120050007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 34,
          "text": "Am I that unworthy of trust? That is a little disappointing, but believe what you will.",
          "next": [
            120050010
          ]
        },
        "120050008": {
          "id": 120050008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Don't worry, miss. I will not expose you",
          "next": [
            120050009
          ]
        },
        "120050009": {
          "id": 120050009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 34,
          "text": "Good, let that be a promise between us.",
          "next": [
            120050010
          ]
        },
        "120050010": {
          "id": 120050010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 34,
          "text": "This picture is for you, as my meeting gift to you. I hope our communication will soon be more in-depth.",
          "next": [
            120050011
          ]
        },
        "120050011": {
          "id": 120050011,
          "type": "Image",
          "sender": "NPC",
          "senderContactId": 34,
          "image": "PhoneMessagePic10006",
          "text": "[Image]",
          "next": []
        }
      },
