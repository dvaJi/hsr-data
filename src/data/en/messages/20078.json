  "sections": [
    {
      "id": 2007800,
      "startingMessageId": [200780000],
      "messages": {
        "200780000": {
          "id": 200780000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Hi, {NICKNAME}",
          "next": [200780001, 200780003]
        },
        "200780001": {
          "id": 200780001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "What's wrong? Did something happen?",
          "next": [200780002]
        },
        "200780002": {
          "id": 200780002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Nothing. I just thought of you",
          "next": [200780005]
        },
        "200780003": {
          "id": 200780003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "It's you again",
          "next": [200780004]
        },
        "200780004": {
          "id": 200780004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Yup, am I not welcome?",
          "next": [200780005]
        },
        "200780005": {
          "id": 200780005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "I didn't expect to be the only one in this movie theater, either",
          "next": [200780006]
        },
        "200780006": {
          "id": 200780006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "I thought this movie would have more fans",
          "next": [200780007]
        },
        "200780007": {
          "id": 200780007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "This movie called Out of the Past",
          "next": [200780008, 200780011]
        },
        "200780008": {
          "id": 200780008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Not watching the movie with anyone else?",
          "next": [200780009]
        },
        "200780009": {
          "id": 200780009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "If you mean the other Stellaron Hunters...",
          "next": [200780010]
        },
        "200780010": {
          "id": 200780010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Everyone's preferences differ to some extent, so there's no need to insist on them coming",
          "next": [200780013]
        },
        "200780011": {
          "id": 200780011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Let's watch this movie together next time",
          "next": [200780012]
        },
        "200780012": {
          "id": 200780012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Alright, I'll hold you to your promise",
          "next": [200780013]
        },
        "200780013": {
          "id": 200780013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "What kind of movies do you like, {NICKNAME}?",
          "next": [200780014]
        },
        "200780014": {
          "id": 200780014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "What do you like right now",
          "next": [200780015, 200780019, 200780022]
        },
        "200780015": {
          "id": 200780015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Biographical films",
          "next": [200780016]
        },
        "200780016": {
          "id": 200780016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "I also enjoy the feeling of reading about and watching others' lives",
          "next": [200780017]
        },
        "200780017": {
          "id": 200780017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "I hope no one turns my life into a movie, though",
          "next": [200780018]
        },
        "200780018": {
          "id": 200780018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "I don't like the idea of some director showing the world their own interpretation of me",
          "next": [200780025]
        },
        "200780019": {
          "id": 200780019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Action films",
          "next": [200780020]
        },
        "200780020": {
          "id": 200780020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "I often find myself in scenarios similar to the ones in those movies",
          "next": [200780021]
        },
        "200780021": {
          "id": 200780021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "It might be exciting for other moviegoers, but I find it lacks immersion",
          "next": [200780025]
        },
        "200780022": {
          "id": 200780022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sci-Fi movies",
          "next": [200780023]
        },
        "200780023": {
          "id": 200780023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Even though they are stories set in the future",
          "next": [200780024]
        },
        "200780024": {
          "id": 200780024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Things that happened in the past will often be repeated in the future",
          "next": [200780025]
        },
        "200780025": {
          "id": 200780025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Okay, the movie's about to start",
          "next": [200780026]
        },
        "200780026": {
          "id": 200780026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1005,
          "text": "Talk soon, bye",
          "next": [200780027, 200780028]
        },
        "200780027": {
          "id": 200780027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Enjoy your movie",
          "next": [200780029]
        },
        "200780028": {
          "id": 200780028,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bye",
          "next": [200780029]
        },
