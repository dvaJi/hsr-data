        "200630020": {
          "id": 200630020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "说来听听，让我也尴尬一下",
          "next": [200630021]
        },
        "200630021": {
          "id": 200630021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "就我身在军人世家嘛，我娘从小就狠抓我习武",
          "next": [200630022]
        },
        "200630022": {
          "id": 200630022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "每天大清早练武，练完了还得去黉学上课",
