        "201440023": {
          "id": 201440023,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1310,
          "next": [
            201440028
          ]
        },
        "201440024": {
          "id": 201440024,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
