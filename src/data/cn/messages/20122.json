{
  "id": 20122,
  "contacts": {
    "id": 1307,
    "name": "黑天鹅",
    "icon": "1307",
    "signature": "记忆是柔软的琥珀",
    "typeId": 1,
    "type": "角色"
  },
  "relatedMessages": [
    13042,
    20121,
