          ]
        },
        "122330036": {
          "id": 122330036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "喂喂，你要干嘛？！",
          "next": [
            122330037
          ]
        },
        "122330037": {
          "id": 122330037,
          "type": "Text",
          "sender": "NPC",
