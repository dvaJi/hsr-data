  "sections": [
    {
      "id": 2011500,
      "startingMessageId": [201150000],
      "messages": {
        "201150000": {
          "id": 201150000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "方便聊天吗？",
          "next": [201150001, 201150003]
        },
        "201150001": {
          "id": 201150001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "好啊，雪衣大人想聊些什么？",
          "next": [201150002]
        },
        "201150002": {
          "id": 201150002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "看来你也有空，那就好",
          "next": [201150006]
        },
        "201150003": {
          "id": 201150003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "还以为判官大人您平时比较酷，不说话",
          "next": [201150004]
        },
        "201150004": {
          "id": 201150004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "平时确实比较酷",
          "next": [201150005]
        },
        "201150005": {
          "id": 201150005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "但是会说话",
          "next": [201150006]
        },
        "201150006": {
          "id": 201150006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "任务刚结束，有些疑惑，想问问汝",
          "next": [201150007]
        },
        "201150007": {
          "id": 201150007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "十王司接到消息，有人在网络上不断发布「我有魔阴身」",
          "next": [201150008]
        },
        "201150008": {
          "id": 201150008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "消息数量过多",
          "next": [201150009]
        },
        "201150009": {
          "id": 201150009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "很可能是求救信号",
          "next": [201150010]
        },
        "201150010": {
          "id": 201150010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "所以吾去调查了一下",
          "next": [201150011, 201150014]
        },
        "201150011": {
          "id": 201150011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "不会是真有魔阴身吧？",
          "next": [201150012]
        },
        "201150012": {
          "id": 201150012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "他没有",
          "next": [201150013]
        },
        "201150013": {
          "id": 201150013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "甚至没有魔阴身预兆",
          "next": [201150017]
        },
        "201150014": {
          "id": 201150014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "我猜，这只是在网络上随口说说罢了",
          "next": [201150015]
        },
        "201150015": {
          "id": 201150015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "看来汝见过很多信口开河之人",
          "next": [201150016]
        },
        "201150016": {
          "id": 201150016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "他没有魔阴身，甚至没有预兆",
          "next": [201150017]
        },
        "201150017": {
          "id": 201150017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "确认没有魔阴身后，现在空闲了一会",
          "next": [201150018]
        },
        "201150018": {
          "id": 201150018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "所以，他为什么要四处宣称自己有魔阴身呢？吾不理解",
          "next": [201150019, 201150023]
        },
        "201150019": {
          "id": 201150019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "也许只要声称了自己有魔阴身，就可以为所欲为了",
          "next": [201150020]
        },
        "201150020": {
          "id": 201150020,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "「你要当心，不要惹我，我有魔阴身」这样",
          "next": [201150021]
        },
        "201150021": {
          "id": 201150021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "原来是这样，吾理解了",
          "next": [201150022]
        },
        "201150022": {
          "id": 201150022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "换句话说，就是想对自己的言行不负责任",
          "next": [201150027]
        },
        "201150023": {
          "id": 201150023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "只是想说些自嘲性质的俏皮话吧，我看过类似的",
          "next": [201150024]
        },
        "201150024": {
          "id": 201150024,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "「谢谢你，本来要堕入魔阴身的，看完直接痊愈了」这样",
          "next": [201150025]
        },
        "201150025": {
          "id": 201150025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "原来也有人视它为一种幽默感吗？",
          "next": [201150026]
        },
        "201150026": {
          "id": 201150026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "看来是吾不够幽默了",
          "next": [201150027]
        },
        "201150027": {
          "id": 201150027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "但对吾来说，「魔阴身」并非轻松的词语",
          "next": [201150028]
        },
        "201150028": {
          "id": 201150028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1214,
          "text": "感谢，与汝简单交谈，受益良多",
          "next": []
        }
      },
      "participatingContactsIds": [1214, 8001]
    }
  ]
}
