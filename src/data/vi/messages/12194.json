      "messages": {
        "121940000": {
          "id": 121940000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Ifrit này không đơn giản.",
          "next": [121940001]
        },
        "121940001": {
          "id": 121940001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Chuyện thế nào?",
          "next": [121940002]
        },
        "121940002": {
          "id": 121940002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Bản chất Quỷ Lửa của Fetora vô cùng hung bạo, nhưng Ifrit lại dễ dàng trở thành lãnh đạo của bọn chúng.",
          "next": [121940003]
        },
        "121940003": {
          "id": 121940003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Trong số các Quỷ Lửa, bản chất lửa của hắn là một loại đặc biệt.",
          "next": [121940004]
        },
        "121940004": {
          "id": 121940004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nói vậy là sao?",
          "next": [121940005]
        },
        "121940005": {
          "id": 121940005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Trong những sự kiện thảm khốc xảy ra ở Dinh Thự Ever-Flame, tôi phát hiện một báo cáo được tận mắt chứng kiến có nội dung tương tự: \"Ác Đồ Xuất Hiện Từ Trong Lửa\"",
          "next": [121940006]
        },
        "121940006": {
          "id": 121940006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Chỉ là suy đoán của tôi, ngọn lửa của Ifrit có lẽ kế thừa một sức mạnh nào đó liên quan đến nguồn gốc của hắn, ngoài sự thiêu đốt.",
          "next": [121940007]
        },
        "121940007": {
          "id": 121940007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Tôi đã từng nghe nói đến một loại ngọn lửa khác có tính chất tương tự, bởi vì nó liên tục chuyển đổi giữa các pha và rất khó bắt được.",
          "next": [121940008]
        },
        "121940008": {
          "id": 121940008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Tài liệu liên quan đến nó không nhiều, chỉ có một câu khiến người ta chú ý \"thiêu cháy các giới, để lại vô số hậu duệ lửa\".",
          "next": [121940009]
        },
        "121940009": {
          "id": 121940009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Ifrit... có lẽ là một trong những hậu duệ của nó.",
          "next": [121940010]
        },
        "121940010": {
          "id": 121940010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Xuyên qua pha sóng...",
          "next": [121940011]
        },
        "121940011": {
          "id": 121940011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Đại khái là như thế.",
          "next": [121940012]
        },
        "121940012": {
          "id": 121940012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Số lượng ghi chép liên quan có hạn, không biết có giúp được gì cho {F#cô}{M#anh} không.",
          "next": [121940013]
        },
        "121940013": {
          "id": 121940013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Đủ rồi, cảm ơn.",
          "next": [121940014]
        },
        "121940014": {
          "id": 121940014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1002,
          "text": "Thế thì tốt.",
          "next": []
        }
      },
