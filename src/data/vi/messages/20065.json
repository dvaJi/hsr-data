        },
        "200650005": {
          "id": 200650005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1204,
          "text": "Haha, vậy sao? {F#Cô}{M#Anh} khiêm tốn quá rồi.",
          "next": [200650009]
        },
        "200650006": {
          "id": 200650006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Được thì cũng được, nhưng mà...",
