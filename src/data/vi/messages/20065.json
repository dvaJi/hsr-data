        },
        "200650016": {
          "id": 200650016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Đúng là... tôi rất mạnh",
          "next": [200650017]
        },
        "200650017": {
          "id": 200650017,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Không ngờ tướng quân lại nhìn thấu nhanh như vậy",
