        "200650011": {
          "id": 200650011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1204,
          "text": "Haha, {NICKNAME}, khả năng của {F#cô}{M#anh} ta sớm đã nắm rõ rồi.",
