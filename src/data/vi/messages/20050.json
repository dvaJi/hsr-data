        "200500011": {
          "id": 200500011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Giống như thống lĩnh Vân Kỵ Quân của Luofu vậy",
          "next": [
            200500012
          ]
        },
        "200500012": {
          "id": 200500012,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Nắm đấm của anh ấy cứng như băng lạnh, vũ khí cũng khác với bình thường, là chiếc khiên cải tạo từ hộp đàn",
          "next": [
            200500013
          ]
        },
        "200500013": {
          "id": 200500013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Hộp đàn!",
          "next": [
            200500014
          ]
        },
        "200500014": {
          "id": 200500014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1209,
          "text": "Quả nhiên trong tay kẻ mạnh, cỏ cây sỏi đá đều có thể làm vũ khí",
