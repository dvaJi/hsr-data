        "120050114": {
          "id": 120050114,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "A, xin lỗi, vừa nãy điện thoại bị mèo cướp mất, chúng ta tiếp tục nhé.",
          "next": [
            120050105
          ]
        },
        "120050115": {
          "id": 120050115,
          "type": "Text",
          "sender": "PlayerAuto",
