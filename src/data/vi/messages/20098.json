        "200980001": {
          "id": 200980001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Vẫn tốt, không gặp trở ngại gì cả",
          "next": [200980002]
        },
        "200980002": {
          "id": 200980002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Vậy thì tốt, {F#cô}{M#anh} nói vậy là tôi yên tâm rồi",
