        },
        "200980003": {
          "id": 200980003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Đúng là cảm thấy có một số trở ngại",
          "next": [200980004]
        },
        "200980004": {
          "id": 200980004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ừm, cũng dễ hiểu thôi, ban đầu tôi cũng như vậy",
          "next": [200980008]
        },
        "200980005": {
          "id": 200980005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Không sao đâu, dù sao cũng không sống ở đây mãi mãi",
          "next": [200980006]
        },
        "200980006": {
          "id": 200980006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Điều đó cũng không đúng...",
          "next": [200980007]
        },
        "200980007": {
          "id": 200980007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "{F#Cô}{M#Anh} sẽ không ở lâu, nhưng những vị khách vô danh như các {F#cô}{M#anh} chắc chắn phải thường xuyên đến Xianzhou làm việc",
          "next": [200980008]
        },
        "200980008": {
          "id": 200980008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Thực ra cũng không có chuyện gì, chỉ là bỗng nhiên nhớ lại",
          "next": [200980009]
        },
        "200980009": {
          "id": 200980009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ban đầu lúc tôi mới đến Xianzhou vẫn là một đứa trẻ nhút nhát",
          "next": [200980010]
        },
        "200980010": {
          "id": 200980010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Cảnh sắc xung quanh đều xa lạ, một nền văn hóa mà tôi hoàn toàn không hiểu",
          "next": [200980011]
        },
        "200980011": {
          "id": 200980011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Nghĩ đến nỗi sợ hãi và lo lắng lúc đó, bỗng nhiên lại nhớ đến {F#cô}{M#anh}",
          "next": [200980012]
        },
        "200980012": {
          "id": 200980012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Lo lắng liệu rằng {F#cô}{M#anh} có cảm nhận tương tự hay không",
          "next": [200980013, 200980015]
        },
        "200980013": {
          "id": 200980013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Cảm ơn Gui Bé Nhỏ đã lo lắng, tôi sớm đã quen rồi",
          "next": [200980014]
        },
        "200980014": {
          "id": 200980014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Ừm, có lẽ là vậy, những gì {F#cô}{M#anh} trải qua thăng trầm hơn tôi rất nhiều",
          "next": [200980017]
        },
        "200980015": {
          "id": 200980015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hồi nhỏ Gui Bé Nhỏ rất đáng thương, tôi nghe cảm thấy đau lòng",
          "next": [200980016]
        },
        "200980016": {
          "id": 200980016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Đừng đau lòng nữa! Hãy thương cho bản thân hơn đi! Dù bận rộn, mệt mỏi đến đâu cũng phải chú ý đến sức khỏe",
          "next": [200980017]
        },
        "200980017": {
          "id": 200980017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Vẫn là câu nói đó",
          "next": [200980018]
        },
        "200980018": {
          "id": 200980018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Nếu gặp phải phiền phức, nhớ đến tìm Gui Bé Nhỏ~",
          "next": [200980019]
