        "200980000": {
          "id": 200980000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "À mà, {F#cô}{M#anh} đã quen với cuộc sống ở Xianzhou chưa? Có gặp trở ngại gì không?",
          "next": [200980001, 200980003, 200980005]
