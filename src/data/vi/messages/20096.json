        },
        "200960006": {
          "id": 200960006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1112,
          "text": "Qua quan sát của tôi thì {F#cô}{M#anh} rất có tiềm năng để vào Bộ Đầu Tư Chiến Lược.",
          "next": [200960007]
        },
        "200960007": {
          "id": 200960007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1112,
          "text": "Quy mô của Công Ty Hành Tinh Hòa Bình thì tôi không cần nói nhiều. Với khả năng của {F#cô}{M#anh} thì làm vài nhiệm vụ là có thể tăng đến P20 ngay.",
          "next": [200960008]
        },
        "200960008": {
          "id": 200960008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1112,
          "text": "Để tôi nói thêm, trong vòng 10 năm được thăng chức giám đốc chi nhánh cũng không phải là không thể!",
          "next": [200960009]
        },
        "200960009": {
          "id": 200960009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1112,
          "text": "Suy nghĩ thêm?",
          "next": [200960010, 200960015]
        },
        "200960010": {
          "id": 200960010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Cơm văn phòng có món gì?",
          "next": [200960011]
        },
        "200960011": {
          "id": 200960011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1112,
          "text": "{F#Cô}{M#Anh} nên hỏi là không có món gì. Muốn ăn gì thì đặt, hệ thống đặt món sẽ đáp ứng bất cứ yêu cầu nào của {F#cô}{M#anh}, món gì cũng có.",
          "next": [200960012]
        },
        "200960012": {
          "id": 200960012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1112,
          "text": "Có muốn thử không?",
          "next": [200960013]
        },
        "200960013": {
          "id": 200960013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Không phải nhân viên cũng được sao?",
          "next": [200960014]
        },
        "200960014": {
          "id": 200960014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1112,
          "text": "Chuyện nhỏ. Quẹt thẻ của tôi đi.",
          "next": [200960022]
        },
        "200960015": {
          "id": 200960015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Không ai \"săn\" được tôi đâu",
          "next": [200960016]
        },
        "200960016": {
          "id": 200960016,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Tôi toàn tâm toàn ý với Đội Tàu Astral!",
          "next": [200960017]
        },
        "200960017": {
          "id": 200960017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1112,
          "text": "Vậy sao? Vậy thì thật tiếc.",
          "next": [200960018]
        },
        "200960018": {
          "id": 200960018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1112,
          "text": "Nhưng cô Himeko cũng là một cấp trên tốt, làm việc chung với cô ấy chắc sẽ rất vui.",
          "next": [200960019]
        },
        "200960019": {
          "id": 200960019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1112,
          "text": "Hôm khác ăn bữa cơm nhé, coi như duy trì mối quan hệ.",
          "next": [200960020]
        },
        "200960020": {
          "id": 200960020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Như vậy được không? Cảm thấy sẽ rất tốn kém...",
          "next": [200960021]
        },
        "200960021": {
          "id": 200960021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1112,
          "text": "Chuyện nhỏ. Quẹt thẻ của tôi đi.",
          "next": [200960022]
        },
        "200960022": {
          "id": 200960022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Còn phúc lợi khác không?",
          "next": [200960023]
        },
        "200960023": {
          "id": 200960023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1112,
          "text": "Đương nhiên là có, như... được đem thú cưng tới văn phòng!",
          "next": [200960024]
        },
        "200960024": {
          "id": 200960024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1112,
          "text": "Tôi nuôi rất nhiều thú nhỏ, chúng siêu... dễ thương!",
          "next": [200960025]
        },
        "200960025": {
          "id": 200960025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1112,
          "text": "Chỉ cần trở thành nhân viên công ty, chúng có thể ở cạnh {F#cô}{M#anh} mỗi ngày. Tuyệt đúng không?",
          "next": [200960026]
        },
        "200960026": {
          "id": 200960026,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
