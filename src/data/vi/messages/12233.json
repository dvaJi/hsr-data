        "122330015": {
          "id": 122330015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1003,
          "text": "Một cái máy mát xa thôi mà, {NICKNAME} muốn thì lên đơn mua thêm một cái nữa đi",
