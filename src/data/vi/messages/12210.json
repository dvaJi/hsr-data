        "122100005": {
          "id": 122100005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "Bộ phận an ninh cũng không biết chuyện này, tin tức nhanh nhạy quá nhỉ.",
          "next": [122100006]
        },
        "122100006": {
          "id": 122100006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 103,
          "text": "Khụ khụ, tôi vừa chuyển tiếp tin từ nhóm khác qua thôi.",
          "next": [122100007]
        },
        "122100007": {
          "id": 122100007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 25,
          "text": "Hình như có hơi gượng ép, nhưng nếu đây là thật...",
          "next": [122100008]
        },
        "122100008": {
          "id": 122100008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "Tôi thấy có mùi của một âm mưu nào đó.",
          "next": [122100009]
        },
        "122100009": {
          "id": 122100009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 103,
          "text": "Màn mất tích tự biên tự diễn này thực ra là để loại trừ những người bất đồng quan điểm.",
          "next": [122100010]
        },
        "122100010": {
          "id": 122100010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 103,
          "text": "Kaka, trò này hay nhỉ. Tiếc là, chính nghĩa tuy chậm nhưng sẽ đến thôi, các cấp quản lý phải từ chức!",
          "next": [122100011]
        },
        "122100011": {
          "id": 122100011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "Cấp quản lý phải từ chức!",
