        },
        "122100004": {
          "id": 122100004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "\"Các nhà nghiên cứu mất tích đều là những người dẫn dắt dư luận trên web ẩn ở Trạm Không Gian...\"",
          "next": [122100005]
