        "145100002": {
          "id": 145100002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nghe có vẻ như là tác phẩm của Asta, cô ấy giàu có như vậy, không biết bên trong có gì nhỉ?",
          "next": [
            145100004
          ]
