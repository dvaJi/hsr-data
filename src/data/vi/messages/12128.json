      "messages": {
        "121280101": {
          "id": 121280101,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "{TEXTJOIN#24} tìm thấy tất cả Chim Robot à?",
          "next": [
            121280102,
            121280103
          ]
        },
        "121280102": {
          "id": 121280102,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Đúng vậy, là tôi tìm thấy tất cả Chim Robot",
          "next": [
            121280111
          ]
        },
        "121280103": {
          "id": 121280103,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nhầm người rồi, tôi không biết chuyện {TEXTJOIN#24} là gì",
          "next": [
            121280104
          ]
        },
        "121280104": {
          "id": 121280104,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Yên tâm, tôi không tìm nhầm người, tôi đã tra được dữ liệu backend của Chuyển Phát Nhanh Hạc Vận",
          "next": [
            121280105
          ]
        },
        "121280105": {
          "id": 121280105,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Nhưng {F#cô}{M#anh} cũng có thể tiếp tục giả vờ mình không phải {TEXTJOIN#24}",
          "next": [
            121280106
          ]
        },
        "121280106": {
          "id": 121280106,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Coi như tôi tự lẩm bẩm một mình đi",
          "next": [
            121280111
          ]
        },
        "121280111": {
          "id": 121280111,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Tôi chính là người thêm trí thông minh cho Chim Robot",
          "next": [
            121280112
          ]
        },
        "121280112": {
          "id": 121280112,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Tôi nhận được một gói hàng, người gửi... quá nửa là {F#cô}{M#anh}, nhưng xem nội dung bưu kiện thì chắc là do Chuyển Phát Nhanh Hạc Vận gửi",
          "next": [
            121280113
          ]
        },
        "121280113": {
          "id": 121280113,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Xin lỗi, đã làm phiền rồi",
          "next": [
            121280114,
            121280115
          ]
        },
        "121280114": {
          "id": 121280114,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tại sao phải thêm trí thông minh cho Chim Robot?",
          "next": [
            121280118
          ]
        },
        "121280115": {
          "id": 121280115,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Vì anh từ chối liên lạc với họ, nên họ mới tốn nhiều công sức như vậy nhờ tôi liên lạc với anh",
          "next": [
            121280116
          ]
        },
        "121280116": {
          "id": 121280116,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "...Đó là vì, họ muốn đến tận nơi để nói chuyện, hoặc là hẹn ở nơi nào đó",
          "next": [
            121280117
          ]
        },
        "121280117": {
          "id": 121280117,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Mà tôi đã mấy trăm năm không ra khỏi nhà rồi",
          "next": [
            121280118
          ]
        },
        "121280118": {
          "id": 121280118,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Hỏi tôi tại sao phải thêm trí thông minh cho Chim Robot ư... Có lẽ mọi người chưa hiểu trọng điểm rồi",
          "next": [
            121280119
          ]
        },
        "121280119": {
          "id": 121280119,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Vì tôi luôn ru rú trong nhà, đã mấy trăm năm không nói chuyện trực tiếp với ai khác",
          "next": [
            121280120
          ]
        },
        "121280120": {
          "id": 121280120,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Tôi nghĩ, nói chuyện trực tiếp với người khác là chuyện rất khó, nhưng ngày nào tôi cũng gặp Chim Robot",
          "next": [
            121280121
          ]
        },
        "121280121": {
          "id": 121280121,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Nên tôi nghĩ, liệu có thể thử nói chuyện với Chim Robot không, có lẽ như vậy sau này tôi sẽ nói chuyện được với người khác",
          "next": [
            121280122,
            121280123
          ]
        },
        "121280122": {
          "id": 121280122,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nên trí thông minh không phải trọng điểm, có thể nói chuyện mới là trọng điểm",
          "next": [
            121280124
          ]
        },
        "121280123": {
          "id": 121280123,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Vậy thật ra anh là người ghét tương tác xã hội, đột nhiên hào hứng muốn nói chuyện với người khác, nên mới làm như vậy",
          "next": [
            121280124
          ]
        },
        "121280124": {
          "id": 121280124,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Đúng vậy, trí thông minh với nhiều nhân cách khác nhau này đều là để giúp tôi nói chuyện đúng cách",
          "next": [
            121280125
          ]
        },
        "121280125": {
          "id": 121280125,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Công việc của tôi có thể hoàn thành trên mạng, thu nhập cũng có thể nhận qua mạng, có đồ chuyển phát nhanh, đặt đồ ăn, tôi chỉ cần dọn rác định kỳ, hoàn toàn không cần gặp gỡ bất cứ ai",
          "next": [
            121280126
          ]
        },
        "121280126": {
          "id": 121280126,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Sự thật là không cần gặp gỡ bất cứ ai, điều này khiến tôi cảm thấy rất an toàn",
          "next": [
            121280127,
            121280130
          ]
        },
        "121280127": {
          "id": 121280127,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Thế nên hiện giờ anh vẫn không thể đối diện với người khác, không thể ra khỏi nhà à?",
          "next": [
            121280128
          ]
        },
        "121280128": {
          "id": 121280128,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Đúng vậy",
          "next": [
            121280129
          ]
        },
        "121280129": {
          "id": 121280129,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Ban đầu cứ tưởng mình có thể nói chuyện với Chim Robot, nhưng tôi đã đánh giá cao bản thân rồi, tôi quá căng thẳng",
          "next": [
            121280133
          ]
        },
        "121280130": {
          "id": 121280130,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mấy trăm năm không gặp mặt bất cứ ai, có chút khó khăn khi mở lời",
          "next": [
            121280131
          ]
        },
        "121280131": {
          "id": 121280131,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Đúng vậy, tôi cũng không ngờ đã lâu như vậy rồi",
          "next": [
            121280132
          ]
        },
        "121280132": {
          "id": 121280132,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Mấy trăm năm nay cũng không phải sống phí, tôi đã bắt đầu thử nói chuyện với Chim Robot",
          "next": [
            121280133
          ]
        },
        "121280133": {
          "id": 121280133,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "...Trong quá khứ tôi từng có những trải nghiệm đau buồn khó lòng nói ra, đả kích thực sự quá lớn",
          "next": [
            121280134
          ]
        },
        "121280134": {
          "id": 121280134,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Nhưng tôi không hề muốn cắt đứt việc giao lưu, xem này, tôi gõ chữ trên bàn phím cũng rất bình thường",
          "next": [
            121280135
          ]
        },
        "121280135": {
          "id": 121280135,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Hahahaha",
          "next": [
            121280136
          ]
        },
        "121280136": {
          "id": 121280136,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Xin lỗi, có thể chuyện này không buồn cười",
          "next": [
            121280137,
            121280141
          ]
        },
        "121280137": {
          "id": 121280137,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mỗi người đều có chỗ khó của mình, cho dù bây giờ không thể ra khỏi nhà, thì chỉ cần sống tốt là được",
          "next": [
            121280138
          ]
        },
        "121280138": {
          "id": 121280138,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Cảm ơn, tôi sẽ sống thật tốt",
          "next": [
            121280139
          ]
        },
        "121280139": {
          "id": 121280139,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Đang cố gắng vì mục tiêu \"nói chuyện với Chim Robot\"",
          "next": [
            121280140
          ]
        },
        "121280140": {
          "id": 121280140,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Có lẽ thêm một thời gian nữa, mấy trăm năm? Tôi sẽ có thể ra ngoài",
          "next": [
            121280145
          ]
        },
        "121280141": {
          "id": 121280141,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Không cần ép bản thân, dùng cách gõ chữ để giao lưu cũng rất tốt",
          "next": [
            121280142
          ]
        },
        "121280142": {
          "id": 121280142,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Cảm ơn, {F#cô}{M#anh} thật biết an ủi người khác",
          "next": [
            121280143
          ]
        },
        "121280143": {
          "id": 121280143,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Có điều, trước nay tôi không hề miễn cưỡng bản thân, muốn luyện nói chuyện cũng là kết quả mà tôi nỗ lực đạt được",
          "next": [
            121280144
          ]
        },
        "121280144": {
          "id": 121280144,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "\"Nói chuyện với Chim Robot\" chính là mục tiêu hiện tại của tôi",
          "next": [
            121280145
          ]
        },
        "121280145": {
          "id": 121280145,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Tôi cũng sẽ xin lỗi Chuyển Phát Nhanh Hạc Vận",
          "next": [
            121280146
          ]
        },
        "121280146": {
          "id": 121280146,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Ngoài ra, tôi gửi món quà cho {F#cô}{M#anh}, nhưng tôi không biết địa chỉ nhận hàng của {F#cô}{M#anh}, nên gửi cho Ziqiao trước rồi",
          "next": [
            121280147
          ]
        },
        "121280147": {
          "id": 121280147,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 65,
          "text": "Cảm ơn",
          "next": []
        }
      },
