      "messages": {
        "121030000": {
          "id": 121030000,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Xin hỏi có phải là họ hàng của Mei không?",
          "next": [
            121030001
          ]
        },
        "121030001": {
          "id": 121030001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 32,
          "text": "Tôi là chủ thuê của cô ấy, Mei xảy ra chuyện gì vậy?",
          "next": [
            121030002,
            121030004
          ]
        },
        "121030002": {
          "id": 121030002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bị Thuyền Sao chạy với tốc độ cao đụng phía sau, chị Mei bị văng ra khỏi Thuyền Sao.",
          "next": [
            121030003
          ]
        },
        "121030003": {
          "id": 121030003,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Hiện tại xem ra vẫn chưa gặp trở ngại lớn, đội thầy thuốc của Sở Đan Đỉnh sắp đến đây rồi.",
          "next": [
            121030006
          ]
        },
        "121030004": {
          "id": 121030004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Chị Mei gặp phải chút tai nạn giao thông, hiện đang chờ trợ giúp.",
          "next": [
            121030005
          ]
        },
        "121030005": {
          "id": 121030005,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Hiện tại xem ra vẫn chưa gặp trở ngại lớn, đội thầy thuốc của Sở Đan Đỉnh sắp đến đây rồi.",
          "next": [
            121030006
          ]
        },
        "121030006": {
          "id": 121030006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 32,
          "text": "Tai nạn giao thông? Tôi hiểu rồi, ở đâu vậy, tôi qua đó ngay",
          "next": [
            121030007
          ]
        },
        "121030007": {
          "id": 121030007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 32,
          "text": "...Nhưng Ảo Cảnh Bươm Bướm khá bận rộn... có thể sẽ không tới kịp, tóm lại tôi qua đó trước nhé. Xin lỗi đã làm phiền, tôi nên xưng hô thế nào nhỉ?",
          "next": [
            121030008,
            121030010,
            121030012
          ]
        },
        "121030008": {
          "id": 121030008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tôi chỉ là nhân viên kiểm tra tốc độ tạm thời do Sở Thiên Bách phái đến mà thôi.",
          "next": [
            121030009
          ]
        },
        "121030009": {
          "id": 121030009,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Gọi tôi {NICKNAME} là được",
          "next": [
            121030013
          ]
        },
        "121030010": {
          "id": 121030010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tôi chỉ là Nhà Khai Phá qua đường mà thôi, khỏi cần bận tâm.",
          "next": [
            121030011
          ]
        },
        "121030011": {
          "id": 121030011,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Gọi tôi {NICKNAME} là được",
          "next": [
            121030013
          ]
        },
        "121030012": {
          "id": 121030012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tôi tên là {NICKNAME}.",
          "next": [
