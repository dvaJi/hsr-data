      "messages": {
        "200410000": {
          "id": 200410000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "{NICKNAME}, có một việc muốn thảo luận với {F#cô}{M#anh}.",
          "next": [200410001]
        },
        "200410001": {
          "id": 200410001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "{F#Cô}{M#Anh} có rảnh không?",
          "next": [200410002]
        },
        "200410002": {
          "id": 200410002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Rảnh, anh nói đi",
          "next": [200410003]
        },
        "200410003": {
          "id": 200410003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Thật ra em gái của nhà Landau chúng tôi sắp trở về sau chuyến nghiên cứu khoa học.",
          "next": [200410004]
        },
        "200410004": {
          "id": 200410004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Tôi muốn tặng cho em ấy một món quà, {F#cô}{M#anh} cảm thấy tặng gì thì được?",
          "next": [200410005, 200410014, 200410023]
        },
        "200410005": {
          "id": 200410005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Cô ấy thích gì?",
          "next": [200410006]
        },
        "200410006": {
          "id": 200410006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Nói ra thật xấu hổ, tôi cũng không rõ sở thích của em ấy.",
          "next": [200410007]
        },
        "200410007": {
          "id": 200410007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Tôi chỉ biết em ấy rất thích việc nghiên cứu khoa học.",
          "next": [200410008]
        },
        "200410008": {
          "id": 200410008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "A, tôi có ý này. Không biết tặng em ấy một trang bị nghiên cứu khoa học loại mới thì sao nhỉ.",
          "next": [200410009]
        },
        "200410009": {
          "id": 200410009,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "(Thích)",
          "next": [200410010]
        },
        "200410010": {
          "id": 200410010,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Nghe hay đấy",
          "next": [200410011]
        },
        "200410011": {
          "id": 200410011,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Chỉ cần em ấy thích là được",
          "next": [200410012]
        },
        "200410012": {
          "id": 200410012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "{F#Cô}{M#Anh} nói phải. Cảm ơn nhé, giúp tôi suy nghĩ thông suốt.",
          "next": [200410013]
        },
        "200410013": {
          "id": 200410013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Bây giờ tôi đi chọn quà đây, gặp sau nhé.",
          "next": []
        },
        "200410014": {
          "id": 200410014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Có thể thử mua cho cô ấy một bó hoa",
          "next": [200410015]
        },
        "200410015": {
          "id": 200410015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Tôi cũng nghĩ tới việc tặng hoa. Nhưng tôi nhớ em ấy không hứng thú với thực vật bình thường.",
          "next": [200410016]
        },
        "200410016": {
          "id": 200410016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Em ấy thích thực vật trên Cánh Đồng Tuyết, mà trong thành không có.",
          "next": [200410017]
        },
        "200410017": {
          "id": 200410017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Có rồi. Lần sau khi tôi ra ngoài tuần tra, sẽ để ý một chút.",
          "next": [200410018]
        },
        "200410018": {
          "id": 200410018,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "(Thích)",
          "next": [200410019]
        },
        "200410019": {
          "id": 200410019,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Nghe hay đấy",
          "next": [200410020]
        },
        "200410020": {
          "id": 200410020,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Nhưng hãy chú ý an toàn",
          "next": [200410021]
        },
        "200410021": {
          "id": 200410021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Haha, cảm ơn. Tôi sẽ cẩn thận.",
          "next": [200410022]
        },
        "200410022": {
          "id": 200410022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Vậy tôi đi làm việc đây, gặp sau nhé.",
          "next": []
        },
        "200410023": {
          "id": 200410023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Có thể mua quần áo mới cho cô ấy",
          "next": [200410024]
        },
        "200410024": {
          "id": 200410024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Tôi cũng từng nghĩ sẽ mua quần áo mới cho em ấy, tiếc là mắt nhìn của tôi không tốt, không dám mua bừa.",
          "next": [200410025]
        },
        "200410025": {
          "id": 200410025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Chị gái thường mua cho em ấy những bộ quần áo rất đẹp.",
          "next": [200410026]
        },
        "200410026": {
          "id": 200410026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Nhưng tôi không muốn để Serval biết chuyện này.",
          "next": [200410027]
        },
        "200410027": {
          "id": 200410027,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "(Từ chối)",
          "next": [200410028]
        },
        "200410028": {
          "id": 200410028,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Sao phải rườm rà thế",
          "next": [200410029]
        },
        "200410029": {
          "id": 200410029,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Đều là người nhà cả, đi hỏi là được mà",
          "next": [200410030]
        },
        "200410030": {
          "id": 200410030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "...",
          "next": [200410031]
        },
        "200410031": {
          "id": 200410031,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "{F#Cô}{M#Anh} nói phải, đi hỏi thử xem sao, tránh việc mua nhầm.",
          "next": [200410032]
        },
        "200410032": {
          "id": 200410032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1104,
          "text": "Cảm ơn đã cho tôi lời khuyên. Bây giờ tôi đi tìm Serval, gặp sau nhé.",
          "next": []
        }
      },
