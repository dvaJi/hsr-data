        "145280000": {
          "id": 145280000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 193,
          "text": "Liên quan đến Nhộng Tiên Sáng Thế, có một việc cần nói rõ với quý khách.",
          "next": [
            145280001
          ]
        },
        "145280001": {
          "id": 145280001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 193,
          "text": "Vị Thú Đại Địa mặc trang phục lộng lẫy lần trước đến gặp ngài Aglaea kia, trước khi rời đi đã đi ngang qua đây và chỉ dạy một chút.",
          "next": [
            145280002,
            145280003
          ]
        },
        "145280002": {
          "id": 145280002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hắn vẫn thích chỉ chỉ trỏ trỏ như vậy.",
          "next": [
            145280004
          ]
        },
        "145280003": {
          "id": 145280003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hắn lại có cao kiến gì nữa?",
          "next": [
            145280004
          ]
        },
        "145280004": {
          "id": 145280004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 193,
          "text": "Phải công nhận rằng, những gì người đó nói cũng có lý.",
          "next": [
            145280005
          ]
        },
        "145280005": {
          "id": 145280005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 193,
          "text": "\"Hừm, tâm trí của các người thật mù quáng, đến nỗi không nhìn thấy câu trả lời ngay trước mắt.\"",
          "next": [
            145280006,
            145280007
          ]
        },
        "145280006": {
          "id": 145280006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mấy câu nói kiểu này thì không cần nhắc lại đâu.",
          "next": [
            145280008
          ]
        },
        "145280007": {
          "id": 145280007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hãy nhanh chóng đi vào chủ đề chính thôi.",
          "next": [
            145280008
          ]
        },
        "145280008": {
          "id": 145280008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 193,
          "text": "\"Chỉ cần truyền Sức Mạnh Titan Thời Gian vào tơ vàng, rồi buộc vào chân xúc giác của nhộng tiên, bất kể nó ở đâu và khi nào, đều có thể triệu hồi nó về.\"",
          "next": [
            145280009
          ]
        },
        "145280009": {
          "id": 145280009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 193,
          "text": "Tôi đã thử nghiệm với nhộng tiên mà {F#cô}{M#anh} chạm vào lúc trước, đúng là có hiệu quả.",
          "next": [
            145280010
          ]
        },
        "145280010": {
          "id": 145280010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 193,
          "text": "Sau này, chỉ cần là cùng một nhộng tiên, sau khi {F#cô}{M#anh} chạm vào thì sẽ không sợ lại bị lạc nữa.",
          "next": [
            145280011
          ]
        },
        "145280011": {
          "id": 145280011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 193,
          "text": "{F#Cô}{M#Anh} có thể đến chỗ tôi để triệu hồi chúng về, khi cần hãy ghé qua xem thử nhé.",
          "next": []
        }
