        "111540007": {
          "id": 111540007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Không cần xin lỗi, {NICKNAME}! Đây vốn dĩ không phải trách nhiệm của các vị",
