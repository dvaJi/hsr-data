        "121150002": {
          "id": 121150002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Phải, tôi đang ở Bến Cảng Sao Xoay, có chút việc phải đến đây",
          "next": [
            121150003
          ]
        },
        "121150003": {
          "id": 121150003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 38,
          "text": "Tốt quá",
          "next": [
            121150006
          ]
        },
        "121150004": {
          "id": 121150004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sao biết thế! Chắc không phải chị Xikui đang theo dõi tôi đó chứ!",
          "next": [
            121150005
          ]
        },
        "121150005": {
          "id": 121150005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 38,
          "text": "Xin lỗi, bây giờ không phải lúc để đùa",
          "next": [
            121150006
          ]
        },
        "121150006": {
          "id": 121150006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 38,
          "text": "Tôi nhìn thấy em ở gần đó thông qua hệ thống Chim Robot",
          "next": [
            121150007
          ]
        },
        "121150007": {
          "id": 121150007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 38,
          "text": "Bây giờ có một việc rất gấp muốn nhờ em giúp đỡ, vô cùng khẩn cấp",
          "next": [
            121150008
          ]
        },
        "121150008": {
          "id": 121150008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 38,
          "text": "Nửa giờ trước, bầu trời phía trên Trường Lạc Thiên gặp sự cố, chắc là do âm dương tương cảm do Cây Kiến Tạo sinh sôi gây ra",
          "next": [
            121150009
          ]
        },
        "121150009": {
          "id": 121150009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 38,
          "text": "Chi tiết kỹ thuật rất phức tạp, nên sẽ không đề cập nhiều ở đây. Nói tóm lại là có một khu vực xảy ra vấn đề nghiêm trọng, chắc sắp nổ tung rồi",
          "next": [
            121150010
          ]
        },
        "121150010": {
          "id": 121150010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 38,
          "text": "Bây giờ Luofu vẫn chưa hoàn toàn hồi phục sau hỗn loạn, thực sự không thể san sẻ nhân lực cho việc giải cứu. May là có một nhân viên văn phòng của Sở Thiên Bách đã nhảy lên Thuyền Sao, bay lên trời tiến hành sửa chữa khẩn cấp",
          "next": [
            121150011
          ]
        },
        "121150011": {
          "id": 121150011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 38,
          "text": "Nhưng chiếc Thuyền Sao đó cũng gặp sự cố trong quá trình giải cứu (chắc cũng do ảnh hưởng của âm dương tương cảm), mất liên lạc với Bến Cảng Sao Xoay, không chừng đã phải hạ cánh khẩn cấp hoặc rơi xuống đâu đó rồi",
          "next": [
            121150012
          ]
        },
        "121150012": {
          "id": 121150012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 38,
          "text": "Tôi đã nhanh chóng báo cho Vân Kỵ đến tiếp viện rồi",
          "next": [
            121150013
          ]
        },
        "121150013": {
          "id": 121150013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 38,
          "text": "Nhưng với sự cố này, chậm một giây là mất một giây khả năng sống sót, không biết em và mọi người có thể đi tìm trước không?",
          "next": [
            121150014,
            121150017
          ]
        },
        "121150014": {
          "id": 121150014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "\"Mọi người\"? Còn ai khác đang ở đây hả?",
          "next": [
            121150015
          ]
        },
        "121150015": {
          "id": 121150015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 38,
          "text": "Ừm, ngài Welt cũng đang ở gần đây",
          "next": [
            121150016
          ]
        },
        "121150016": {
          "id": 121150016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 38,
          "text": "Tôi cũng đã liên lạc với ngài Welt rồi",
          "next": [
            121150020
          ]
        },
        "121150017": {
          "id": 121150017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Chiếc Thuyền Sao này rơi xuống ở đâu?",
          "next": [
            121150018
          ]
        },
        "121150018": {
          "id": 121150018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 38,
          "text": "Tôi vẫn đang tìm kiếm",
          "next": [
            121150019
          ]
        },
        "121150019": {
          "id": 121150019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 38,
          "text": "Ngài Welt cũng đang ở gần đây. Tôi cũng đã liên lạc với anh ấy rồi",
          "next": [
            121150020
          ]
        },
        "121150020": {
          "id": 121150020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 38,
          "text": "Tóm lại, đến Bến Cảng Sao Xoay tìm một nơi thích hợp rồi dùng Chim Robot tìm thử nhé. Làm phiền mọi người",
          "next": [
            121150021
          ]
        },
        "121150021": {
          "id": 121150021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 38,
          "text": "Còn nữa... Người nhân viên này có thân phận khá đặc biệt",
          "next": [
            121150022
          ]
        },
        "121150022": {
          "id": 121150022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 38,
          "text": "Mong mọi người dùng hết khả năng để bảo toàn tính mạng của cô ấy",
