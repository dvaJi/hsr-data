        "200900010": {
          "id": 200900010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Yên tâm đi, Nhà Chiêm Tinh, tôi không nhìn thấy tin nhắn của cô",
          "next": [
