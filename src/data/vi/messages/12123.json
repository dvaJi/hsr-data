      "messages": {
        "121230000": {
          "id": 121230000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 54,
          "text": "Xin lỗi, rõ ràng mọi chuyện đều do tôi cầu xin bắt đầu, nhưng giữa đường lại rút lui...",
          "next": [121230001, 121230004, 121230007]
        },
        "121230001": {
          "id": 121230001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tôi cuối cùng cũng hiểu sao anh lại không xuất sư được tận hai trăm năm rồi",
          "next": [121230002]
        },
        "121230002": {
          "id": 121230002,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Thật là, không ngờ lại phí thời gian vào người như anh",
          "next": [121230003]
        },
        "121230003": {
          "id": 121230003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 54,
          "text": "...Thật xin lỗi, để {NICKNAME} chê cười rồi",
          "next": [121230012]
        },
        "121230004": {
          "id": 121230004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Không cần xin lỗi, muốn hoàn thành công việc nào đó đều sẽ khó như vậy đấy",
          "next": [121230005]
        },
        "121230005": {
          "id": 121230005,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Chuyện bỏ cuộc giữa chừng thì trong vũ trụ này đâu đâu cũng có... Vì vậy những người kiên trì đến cùng mới là xuất chúng",
          "next": [121230006]
        },
        "121230006": {
          "id": 121230006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 54,
          "text": "...Cảm ơn {NICKNAME}. Cảm ơn vì đã kiên nhẫn với tôi.",
          "next": [121230012]
        },
        "121230007": {
          "id": 121230007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Thực ra anh cũng không cần phải ép mình như vậy đâu",
          "next": [121230008]
        },
        "121230008": {
          "id": 121230008,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Một chiếc thuyền, cho dù làm thành thì thay đổi được gì chứ? Tôi hiểu điều này có ý nghĩa đặc biệt với anh, nhưng nếu chúng ta nhìn xa hơn chút...",
          "next": [121230009]
        },
        "121230009": {
          "id": 121230009,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Đa phần thứ mà con người làm đều vô nghĩa, mấy trăm năm sau ai mà nhớ đến sự tủi hổ và phấn đấu của một học trò nhỏ nhoi ở Sở Công Nghiệp chứ?",
          "next": [121230010]
        },
        "121230010": {
          "id": 121230010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 54,
          "text": "...Tôi",
          "next": [121230011]
        },
        "121230011": {
          "id": 121230011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 54,
          "text": "...Tự bản thân tôi sẽ nhớ.",
          "next": [121230012]
        },
        "121230012": {
          "id": 121230012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 54,
          "text": "Tôi sẽ hoàn thành công việc còn lại. Đến lúc đó tôi sẽ mời {F#cô}{M#anh} đến xem thử... Cứ đợi tin của tôi nhé",
          "next": []
        }
      },
