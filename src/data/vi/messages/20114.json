        },
        "201140006": {
          "id": 201140006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Tôi có chiếc khác dự phòng, nên không cần gấp",
          "next": [201140007]
        },
        "201140007": {
          "id": 201140007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "{F#Cô}{M#Anh} thấy thì cầm giúp tôi nhé, cảm ơn",
          "next": [201140008]
        },
        "201140008": {
          "id": 201140008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Nếu Herta có hỏi thì nói tôi để quên lúc ngắm tranh chân dung của cô ấy, cô ấy sẽ không hỏi nhiều đâu",
          "next": [201140009, 201140010]
        },
        "201140009": {
          "id": 201140009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mấy lời này giả tạo quá",
          "next": [201140011]
        },
        "201140010": {
          "id": 201140010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mấy lời này lọt tai đó",
