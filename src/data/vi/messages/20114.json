        },
        "201140012": {
          "id": 201140012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Vẻ đẹp và tính cáu kỉnh của Herta giống nhau y hệt",
          "next": [201140013]
        },
        "201140013": {
          "id": 201140013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Lần nào đến Trạm Không Gian tôi cũng mê mẩn thành quả nghiên cứu, vì vậy viện cớ này để chống chế cho việc rơi đồ là quá hợp lý",
          "next": [201140014]
        },
        "201140014": {
          "id": 201140014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Cô còn rơi gì nữa sao?",
          "next": [201140015]
        },
        "201140015": {
          "id": 201140015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Ừm, một ít",
          "next": [201140016]
        },
        "201140016": {
          "id": 201140016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Một tấm vải thêu, một cuốn sổ tay nuôi dưỡng sinh mệnh, Linh Hỏa Pha Sóng của thành viên #29 Sserkal...",
          "next": [201140017, 201140021, 201140025]
        },
        "201140017": {
          "id": 201140017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Thêu... Sở thích của cô đó hả?",
          "next": [201140018]
        },
        "201140018": {
          "id": 201140018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Ừm. Khi đó tôi ở gần Sân Ga, thêu tấm Hành Tinh Xanh",
          "next": [201140019]
        },
        "201140019": {
          "id": 201140019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Nhưng tôi không tìm lại được, chắc bị nhà nghiên cứu khác nhặt rồi",
          "next": [201140020]
        },
        "201140020": {
          "id": 201140020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Hi vọng là rơi vào tay người nào thích nó",
          "next": []
        },
        "201140021": {
          "id": 201140021,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Sổ tay nuôi dưỡng sinh mệnh? Hình như tôi đã từng nhìn thấy...",
          "next": [201140022]
        },
        "201140022": {
          "id": 201140022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Có một số nhà nghiên cứu muốn sao chép thành quả của tôi, bọn họ sẽ bán mấy công thức nuôi dưỡng cấp thấp này ở chợ đen",
          "next": [201140023]
        },
        "201140023": {
          "id": 201140023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Nhưng điều quan trọng nhất của nuôi dưỡng sinh mệnh không phải nguyên liệu",
          "next": [201140024]
        },
        "201140024": {
          "id": 201140024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "Mà là ý tưởng viết nên các công thức đó",
          "next": []
        },
        "201140025": {
          "id": 201140025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Linh Hỏa Pha Sóng? Herta biết không?",
