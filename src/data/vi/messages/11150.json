        "111500007": {
          "id": 111500007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Hay quá, tôi còn lo mọi người không nhận được tin nhắn",
          "next": [111500008]
        },
        "111500008": {
          "id": 111500008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Tôi biết mọi người rất bận, nên sẽ cố gắng nói ngắn gọn",
          "next": [111500009]
        },
        "111500009": {
          "id": 111500009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Thời gian này tôi đang xúc tiến công tác chuẩn bị cho Lễ Hội Mặt Trời. Lễ Hội Mặt Trời là ngày lễ truyền thống của Belobog, trước đó bị Rãnh Nứt và lệnh phong tỏa ảnh hưởng, hoạt động lễ hội thường quy đã ngừng nhiều năm, người Belobog đã lâu lắm rồi không được cảm nhận không khí lễ hội sôi động",
          "next": [111500010]
        },
        "111500010": {
          "id": 111500010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Nếu Đội Tàu Astral có thời gian, tôi muốn mời mọi người đến tham gia \"Lễ Hội Mặt Trời\", xem thành quả giai đoạn của kế hoạch phục hưng Belobog",
          "next": [111500011, 111500013]
        },
        "111500011": {
          "id": 111500011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tôi nhất định đến!",
          "next": [111500012]
        },
        "111500012": {
          "id": 111500012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Tôi nữa! Ngày lễ, lễ hội, chính là chỗ dành cho tôi đấy!",
          "next": [111500016]
        },
        "111500013": {
          "id": 111500013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Xin lỗi, lần này có thể không rảnh...",
          "next": [111500014]
        },
        "111500014": {
          "id": 111500014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "@{NICKNAME}, xạo đi, {F#cô}{M#anh} muốn trốn trong phòng lướt điện thoại chứ gì!",
          "next": [111500015]
        },
        "111500015": {
          "id": 111500015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1001,
          "text": "Yên tâm, chúng tôi nhất định sẽ đến tham gia! Ngày lễ, lễ hội, chính là chỗ dành cho tôi đấy!",
          "next": [111500016]
        },
        "111500016": {
          "id": 111500016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "Đến được thì tuyệt quá, mọi người ở đây đều nhớ các vị lắm",
          "next": [111500017]
        },
        "111500017": {
          "id": 111500017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "@DanHeng đang bận hả? Nãy giờ không thấy anh ấy nói gì",
