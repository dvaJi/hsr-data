        },
        "200120005": {
          "id": 200120005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Có thể nghe thử suy nghĩ của tôi trước.",
          "next": [200120006]
        },
        "200120006": {
          "id": 200120006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Đương nhiên, đây có lẽ là chuyện rất lâu sau này",
          "next": [200120007]
        },
        "200120007": {
          "id": 200120007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Đồng thời xét theo hiện thực, cuối cùng chúng ta nhiều khả năng phải sửa phần lớn tình tiết... Nhưng những điều này đều là việc sau này.",
          "next": [200120008]
        },
        "200120008": {
          "id": 200120008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Tôi cũng theo ngành này khá lâu rồi, nói thật, tôi rất muốn nghe thử suy nghĩ của những người trẻ tuổi như mọi người.",
          "next": [200120009]
        },
        "200120009": {
          "id": 200120009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Ví dụ, {F#cô}{M#cậu} cảm thấy điểm hấp dẫn nhất của câu chuyện này là gì?",
          "next": [200120010, 200120011, 200120012]
        },
        "200120010": {
          "id": 200120010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Chắc là hành trình phiêu lưu đầy chông gai nhỉ",
          "next": [200120013]
        },
        "200120011": {
          "id": 200120011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Chắc là trận chiến hào sảng nhỉ",
          "next": [200120014]
        },
        "200120012": {
          "id": 200120012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Thực ra tôi chỉ muốn nâng cấp tối đa trang bị",
          "next": [200120015]
        },
        "200120013": {
          "id": 200120013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Phong cảnh độc đáo, sự va chạm của vận mệnh, sẽ không thể làm nếu thiếu một trong hai thứ đó.",
          "next": [200120016]
        },
        "200120014": {
          "id": 200120014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Bạn đồng hành tương trợ, kẻ địch hùng mạnh, sẽ không thể làm nếu thiếu một trong hai thứ đó.",
          "next": [200120016]
        },
        "200120015": {
          "id": 200120015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Có nghĩa là nhân vật chính không ngừng nâng cấp để mạnh mẽ hơn nhỉ.",
          "next": [200120016]
        },
        "200120016": {
          "id": 200120016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Câu hỏi tiếp theo, {F#cô}{M#cậu} cảm thấy khi nào câu chuyện này sẽ kết thúc?",
          "next": [200120017, 200120018, 200120019]
        },
        "200120017": {
          "id": 200120017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tôi hy vọng nó sẽ không kết thúc",
          "next": [200120020]
        },
        "200120018": {
          "id": 200120018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Khi ước nguyện của mọi người thành hiện thực",
          "next": [200120021]
        },
        "200120019": {
          "id": 200120019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Khi trang bị của mọi người được nâng cấp tối đa",
          "next": [200120022]
        },
        "200120020": {
          "id": 200120020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Muốn đăng nhiều kỳ ấy à... Quả thực cũng có tác phẩm như vậy",
          "next": [200120023]
        },
        "200120021": {
          "id": 200120021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Sau khi có kết cục viên mãn thì có thể kết thúc... Quả thực cũng có tác phẩm như vậy.",
          "next": [200120023]
        },
        "200120022": {
          "id": 200120022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Sau khi nâng cấp tối đa trang bị mà câu chuyện vẫn miễn cưỡng tiếp diễn, thì chiến lực có thể sẽ không cân bằng.",
          "next": [200120023]
        },
        "200120023": {
          "id": 200120023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Thực ra tôi rất vui, {F#cô}{M#cậu} có thể trân trọng hành trình của chúng ta như vậy.",
          "next": [200120024]
        },
        "200120024": {
          "id": 200120024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Xem ra lần sau khi tôi đã về hưu, nhất định phải thực hiện ước mơ này...",
          "next": [200120025]
        },
