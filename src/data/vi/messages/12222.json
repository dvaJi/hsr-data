        },
        "122220005": {
          "id": 122220005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hay quá, quý cô Ruan Mei, vừa hay tôi có chuyện muốn tìm cô đây",
