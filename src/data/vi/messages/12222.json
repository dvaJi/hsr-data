        "122220008": {
          "id": 122220008,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Ảnh tải lên vượt quá kích cỡ cho phép, vui lòng nén rồi gửi lại.",
          "next": [122220009]
        },
        "122220009": {
          "id": 122220009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Gửi tệp đính kèm là được rồi, vậy sẽ rõ nét hơn",
