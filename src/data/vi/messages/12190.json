        "121900008": {
          "id": 121900008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nói chứ... ID Rich Biết Tỏng nhìn có vẻ quen, chẳng lẽ có liên quan đến Richard Biết Tuốt?",
          "next": [121900009]
        },
        "121900009": {
          "id": 121900009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 103,
          "text": "...Quả nhiên nhìn ra dấu tích cũ trước lúc chuyển sinh của tôi, xem ra cũng dày dặn kinh nghiệm đấy.",
          "next": [121900010]
        },
        "121900010": {
          "id": 121900010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "Được rồi, được rồi, quay về việc chính đi, việc này giờ tính sao?",
          "next": [121900011]
        },
        "121900011": {
          "id": 121900011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 103,
          "text": "Truy cứu trách nhiệm! Nhất quyết phải truy cứu trách nhiệm!",
          "next": [121900012, 121900013]
        },
        "121900012": {
          "id": 121900012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mấy người đang bực tức chuyện gì?",
          "next": [121900014]
        },
        "121900013": {
          "id": 121900013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Truy trách nhiệm gì chứ?",
