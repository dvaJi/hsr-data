        },
        "201520005": {
          "id": 201520005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Đúng vậy, bởi vì tôi đang thực hiện thử thách cay biến thái",
          "next": [
            201520006
          ]
        },
        "201520006": {
          "id": 201520006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1218,
          "text": "Thì ra là vậy",
