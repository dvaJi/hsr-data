        },
        "201520007": {
          "id": 201520007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1218,
          "text": "Một số đồng nghiệp Yaoqing gần đây thường gặp nạn trong bữa ăn",
          "next": [
            201520008
          ]
        },
        "201520008": {
