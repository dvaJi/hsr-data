        },
        "121650003": {
          "id": 121650003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 83,
          "text": "Phải không? Thuận lợi đó",
          "next": [121650007]
        },
        "121650004": {
          "id": 121650004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Đừng có giục, giục nữa là tôi nghỉ làm đó",
          "next": [121650005]
        },
        "121650005": {
          "id": 121650005,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "(Hình ảnh)",
          "next": [121650006]
        },
        "121650006": {
          "id": 121650006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 83,
          "text": "Giục một cái thôi mà, có mất miếng thịt nào đâu",
          "next": [121650007]
        },
        "121650007": {
          "id": 121650007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 83,
          "text": "...Thực ra tôi rất ít bạn bè, muốn hỏi thử {F#cô}{M#anh} về vấn đề tình cảm, được không?",
          "next": [121650008, 121650009]
        },
        "121650008": {
          "id": 121650008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tôi rất sành về chủ đề yêu đương đó, cứ hỏi tự nhiên nhé",
          "next": [121650010]
        },
        "121650009": {
          "id": 121650009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tôi không giỏi về chủ đề yêu đương, hay là anh hỏi người khác đi nhỉ?",
          "next": [121650010]
        },
        "121650010": {
          "id": 121650010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 83,
          "text": "Haiz, con gái không phải rất thích những món quà quý giá mang ý nghĩa kỷ niệm sao?",
          "next": [121650011]
        },
        "121650011": {
          "id": 121650011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 83,
          "text": "Vì sao Jingzhai lại khác biệt thế, cô ấy đã vứt quà của tôi đi",
          "next": [121650012, 121650014]
        },
        "121650012": {
          "id": 121650012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Rốt cuộc anh đã tặng gì thế?",
          "next": [121650013]
        },
        "121650013": {
          "id": 121650013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 83,
          "text": "Khóa Đồng Tâm đó!",
          "next": [121650016]
        },
        "121650014": {
          "id": 121650014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Có thể là Jingzhai không muốn nhận món quà quý giá đến như vậy",
          "next": [121650015]
        },
        "121650015": {
          "id": 121650015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 83,
          "text": "Tôi có tặng gì đâu, chỉ là, khóa đồng tâm thôi mà, lỗi thời lắm sao?",
          "next": [121650016]
        },
        "121650016": {
          "id": 121650016,
          "type": "Image",
          "sender": "NPC",
          "senderContactId": 83,
          "image": "PhoneMessagePic_10014",
          "text": "(Hình ảnh)",
          "next": [121650017]
        },
        "121650017": {
          "id": 121650017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 83,
          "text": "Haiz, tôi hiểu rồi, đúng là món quà này có hơi lỗi thời. Hơn nữa, cái gì mà ngụ ý \"khóa lại trái tim người thương\" chứ, có vẻ không theo kịp gu yêu đương của người thời nay rồi",
          "next": [121650018]
        },
        "121650018": {
          "id": 121650018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 83,
          "text": "Có điều, dù sao thì chiếc khóa này cũng mạ vàng khảm ngọc, ít nhiều gì cũng rất có giá trị. Nó có thể khiến con gái chê đến mức này hay sao?",
          "next": [121650019, 121650020]
        },
        "121650019": {
          "id": 121650019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Thứ này mua ở đâu thế...",
          "next": [121650021]
        },
        "121650020": {
          "id": 121650020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Gu mua đồ của anh làm tim tôi tan nát...",
          "next": [121650021]
        },
        "121650021": {
          "id": 121650021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 83,
          "text": "Đây là... là báu vật truyền từ đời trước đấy, không phải tôi đi mua đâu! {F#Cô}{M#Anh} không hiểu gì về giá trị của nó cả",
          "next": [121650022]
        },
        "121650022": {
          "id": 121650022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 83,
          "text": "Khi tôi thoát xác chuyển sinh thì trong tay đã cầm nó rồi, các trưởng lão đều nói đây là \"duyên\", nên đã đưa tôi đến Sở Công Nghiệp để đào tạo",
          "next": [121650023]
        },
        "121650023": {
          "id": 121650023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 83,
          "text": "Tôi cứ nghĩ món đồ này sẽ khiến các cô gái yêu thích. Haiz, không ngờ, Jingzhai lại là người coi châu báu như cỏ rác",
          "next": [121650024]
        },
        "121650024": {
          "id": 121650024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 83,
          "text": "Thôi được rồi, để tôi suy nghĩ xem nên tặng quà gì mới có thể lấy lòng cô ấy",
          "next": []
        }
