            101090035
          ]
        },
        "101090033": {
          "id": 101090033,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Mua trên danh nghĩa cá nhân à?",
          "next": [
            101090034
          ]
        },
        "101090034": {
          "id": 101090034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Chắc không phải danh nghĩa cá nhân chứ? Điền Trạm Không Gian là phải báo hoá đơn đó, nghĩ trạm trưởng ngốc à",
          "next": [
            101090035
          ]
        },
        "101090035": {
          "id": 101090035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Chắc là tự mình mua nhỉ",
          "next": [
            101090036
          ]
        },
        "101090036": {
          "id": 101090036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Mấy thứ đó ngoài cô Herta ra thì ai mua nổi chứ",
