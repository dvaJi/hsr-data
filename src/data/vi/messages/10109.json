            101090025
          ]
        },
        "101090025": {
          "id": 101090025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Chuyện lấy của công làm của riêng ấy mà",
          "next": [
            101090026
          ]
        },
        "101090026": {
          "id": 101090026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Có nhìn thấy chồng kiện hàng chất cao như núi ở Sảnh Tiếp Tân không? Người nhận đều là vị trạm trưởng trẻ tuổi này của chúng ta đấy",
          "next": [
            101090027
          ]
        },
        "101090027": {
          "id": 101090027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "Hay lắm, tôi đã nói mà, ngân sách thì keo kiệt từng đồng, tiền tiêu đi đâu hết rồi",
          "next": [
            101090028
          ]
        },
        "101090028": {
          "id": 101090028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "Chuyện này không thể nói bừa",
