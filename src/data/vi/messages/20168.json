        "201680021": {
          "id": 201680021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Đầu gậy được chạm khắc một lỗ rỗng hình ngọn lửa, chất lỏng màu chứa trong đó chỉ chờ được phun ra để hóa thành cầu vồng",
          "next": [
            201680022
          ]
