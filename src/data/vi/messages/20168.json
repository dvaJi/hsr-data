        "201680004": {
          "id": 201680004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Không ngờ cô còn có kỹ năng này!",
          "next": [
            201680005
          ]
        },
        "201680005": {
          "id": 201680005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Cứ yên tâm, Nhẫn Cụ của tại hạ do chính tay tại hạ tự chế tạo, nên khi sử dụng không có rủi ro gì đâu.",
          "next": [
            201680007
          ]
        },
        "201680006": {
          "id": 201680006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ninja Gậy Bóng Chày tới đây!",
          "next": [
            201680007
          ]
        },
        "201680007": {
          "id": 201680007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Khi Ninja đi khắp thế giới, phải nhìn khắp mọi hướng, thân, vật, lời nói và tâm trí phải hợp thành một",
          "next": [
            201680008
          ]
        },
        "201680008": {
          "id": 201680008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Nhẫn Cụ cũng phải được rèn bằng nhẫn pháp, truyền Nhẫn Hồn vào và dùng Nhẫn Thuật điều khiển",
          "next": [
            201680009
          ]
        },
        "201680009": {
          "id": 201680009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Nhẫn Ngôn-Phá Vỡ Quy Tắc của các hạ khiến tại hạ ấn tượng vô cùng",
          "next": [
            201680010
          ]
        },
        "201680010": {
          "id": 201680010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1317,
          "text": "Cảm hứng chế tạo công cụ thường nảy ra vào ban đêm",
          "next": [
            201680011,
            201680013
          ]
        },
        "201680011": {
          "id": 201680011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Nhẫn Ngôn-Phá Vỡ Quy Tắc? Ý là câu \"Quy tắc sinh ra là để phá vỡ\"?",
