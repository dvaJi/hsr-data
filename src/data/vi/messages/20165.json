        "201650022": {
          "id": 201650022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1223,
          "text": "Nhưng chiêu thức ám sát chú trọng vào tinh hoa, chứ không phải số lượng nhiều hay ít.",
