          "next": [200710002]
        },
        "200710002": {
          "id": 200710002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Nhà Trọ Petrichor?",
          "next": [200710003, 200710007]
        },
        "200710003": {
          "id": 200710003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Đúng rồi, tôi thật sự đã ở Nhà Trọ Petrichor",
          "next": [200710004]
        },
        "200710004": {
          "id": 200710004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Phòng cao cấp, đúng là rất tốt",
          "next": [200710005]
        },
        "200710005": {
          "id": 200710005,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "(Thích)",
          "next": [200710006]
        },
        "200710006": {
          "id": 200710006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Rất khó đặt phòng cao cấp, rất nhiều người đều muốn đến đó để ngắm cảnh đêm của Đại Lộ Thiên Văn",
          "next": [200710012]
        },
        "200710007": {
          "id": 200710007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Đúng rồi, tôi thật sự đã ở Nhà Trọ Petrichor",
          "next": [200710008]
        },
        "200710008": {
          "id": 200710008,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Nhưng mà nói thật là, cũng chỉ có vậy thôi",
          "next": [200710009]
        },
        "200710009": {
          "id": 200710009,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Dù gì thì mỗi lần ở khách sạn tôi đều lo sẽ xảy ra chuyện, cho nên không có thời gian nghĩ về những thứ khác",
          "next": [200710010]
        },
        "200710010": {
          "id": 200710010,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "(Khóc)",
          "next": [200710011]
        },
        "200710011": {
          "id": 200710011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Xem ra {F#cô}{M#anh} cũng trải qua không ít sóng gió",
          "next": [200710012]
        },
        "200710012": {
          "id": 200710012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Tôi trước đây cũng thường ở lại Nhà Trọ Petrichor, dù sao thì cũng khá tiện lợi",
          "next": [200710013]
        },
        "200710013": {
          "id": 200710013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Mặc dù thương nhân không chú trọng đến chuyến hành trình có thú vị hay không, nhưng ở lâu thì cũng cảm thấy chán",
          "next": [200710014]
        },
        "200710014": {
          "id": 200710014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Gần đây Đăng Vân Quần Các có cho thuê phòng ngắn hạn, nên tôi đến đó",
          "next": [200710015]
        },
        "200710015": {
          "id": 200710015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Là nơi mà người Xianzhou thường ở",
          "next": [200710016, 200710020]
        },
        "200710016": {
          "id": 200710016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Cảm thấy không tiện lắm, lúc nào cũng bị người khác dòm ngó",
          "next": [200710017]
        },
        "200710017": {
          "id": 200710017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Còn tùy vào cách nhìn của {F#cô}{M#anh} đối với việc này như thế nào",
          "next": [200710018]
        },
        "200710018": {
          "id": 200710018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Cuộc sống của Xianzhou không phải vòng tròn khép kín, mọi người cũng rất thân tình, trải nghiệm và quan sát cuộc sống của họ cũng là niềm vui của tôi...",
          "next": [200710019]
        },
        "200710019": {
          "id": 200710019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Nhưng nói như vậy không thỏa đáng lắm, phần lớn thời gian, tôi đều là người bị quan sát",
          "next": [200710023]
        },
        "200710020": {
          "id": 200710020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Chẳng mấy chốc anh cũng trở thành người Xianzhou đấy",
          "next": [200710021]
        },
        "200710021": {
          "id": 200710021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Haha... {F#Cô}{M#Anh} hiểu tôi đấy.",
          "next": [200710022]
        },
        "200710022": {
          "id": 200710022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Đúng vậy, cứ như vậy, tôi dần hòa nhập vào cuộc sống của Luofu",
          "next": [200710023]
        },
        "200710023": {
          "id": 200710023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Tôi đi khắp nơi, bốn bể là nhà, có thể trú tại Đăng Vân Quần Các là khoảng thời gian khó quên trong cuộc đời thương nhân của tôi rồi.",
          "next": [200710024]
        },
        "200710024": {
          "id": 200710024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Cho nên, tôi phải tranh thủ tận hưởng chứ.",
          "next": [200710025, 200710027]
        },
        "200710025": {
          "id": 200710025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Lần sau tôi cũng muốn ở thử xem sao",
          "next": [200710026]
        },
        "200710026": {
          "id": 200710026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Được, không chừng lúc đó còn có thể trở thành hàng xóm",
          "next": []
        },
        "200710027": {
          "id": 200710027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tôi e rằng không ở đó được",
          "next": [200710028]
        },
        "200710028": {
          "id": 200710028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Có thể cho tôi biết lí do không?",
          "next": [200710029, 200710030, 200710031]
        },
        "200710029": {
          "id": 200710029,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tôi sợ!",
          "next": [200710032]
        },
        "200710030": {
          "id": 200710030,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tôi lạ giường!",
          "next": [200710032]
        },
        "200710031": {
          "id": 200710031,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tôi bị dị ứng!",
          "next": [200710032]
        },
        "200710032": {
          "id": 200710032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
