        "111500007": {
          "id": 111500007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "ดีจัง ฉันยังกลัวอยู่เลยว่า ทุกคนจะไม่ได้รับข้อความ",
