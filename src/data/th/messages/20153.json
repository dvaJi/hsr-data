        },
        "201530010": {
          "id": 201530010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "แทะจนขาโต๊ะหัก? เจ้าของร้านให้ Diting ชดใช้มั้ยเนี่ย",
          "next": [
            201530011
