        "127040014": {
          "id": 127040014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "ฉันไม่รู้ว่า \"Hexanexus: Remake\" คืออะไร",
          "next": [
            127040015
