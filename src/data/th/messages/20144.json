        },
        "201440008": {
          "id": 201440008,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "ว่าแต่ เธอก็ตื่นเช้าขนาดนี้เลยเหรอ?",
