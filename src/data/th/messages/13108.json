        },
        "131080004": {
          "id": 131080004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "น่าจะไม่ค่อยสะดวกเท่าไหร่ มีเรื่องสำคัญอะไรเหรอ?",
          "next": [
            131080005
