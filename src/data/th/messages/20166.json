        "201660007": {
          "id": 201660007,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "เดี๋ยวนะ ทำไมยังมีเจ้าจ๋ออยู่อีกล่ะ!",
