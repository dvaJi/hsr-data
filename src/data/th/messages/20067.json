        },
        "200670005": {
          "id": 200670005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1207,
          "text": "อันที่จริง ฉันไม่เคยคิดจะทำงานอย่างอื่น นอกจากเป็นนักบินเลย",
          "next": [
            200670006
          ]
