        "200110009": {
          "id": 200110009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Você não pode se considerar {M#digno}{F#digna} dos Inominados se não consegue lidar com várias atribuições ao mesmo tempo",
