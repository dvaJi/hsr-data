        "200110007": {
          "id": 200110007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Se você recebe mensagens todos os dias, a conexão entre você e as outras pessoas ficou mais forte durante sua Jornada de Desbravamento",
          "next": [200110008]
        },
        "200110008": {
          "id": 200110008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Mas conexões também precisam ser mantidas. Tente gerenciar seu tempo com sabedoria",
