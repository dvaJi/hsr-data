        },
        "101250017": {
          "id": 101250017,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Talvez também seja super assustador",
          "next": [
            101250018
          ]
        },
        "101250018": {
          "id": 101250018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Quando encontrei Velite, ele disse que se vislumbrou comandando a Única e Singular.",
          "next": [
            101250019
          ]
        },
        "101250019": {
          "id": 101250019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1302,
          "text": "Auxiliei-o na derrocada do chefe inseto, no entanto, ele demonstrou uma certa perplexidade após o término da ilusão.",
