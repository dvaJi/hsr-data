        },
        "200700009": {
          "id": 200700009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Claro, é isso que eu faço o tempo todo",
          "next": [200700010]
        },
        "200700010": {
          "id": 200700010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1203,
          "text": "Parece que você tem algumas ideias",
