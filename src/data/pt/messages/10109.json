        "101090036": {
          "id": 101090036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Além da madame Herta, quem mais pode bancar tudo aquilo?",
          "next": [101090037]
        },
        "101090037": {
          "id": 101090037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Vocês conhecem o Arlan, do Departamento de Segurança? Eu vi ele agindo todo esquisito e suspeito na recepção. Se não fosse pela cara de culpa dele, eu não ia nem ver",
          "next": [101090038]
        },
        "101090038": {
          "id": 101090038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Hm, agora que você falou nisso... eu acho que eu também vi o Arlan meio esquisito recebendo uns pacotes",
          "next": [101090039]
        },
        "101090039": {
          "id": 101090039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Eu falei",
          "next": [101090040]
        },
        "101090040": {
          "id": 101090040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
