        "101090032": {
          "id": 101090032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "Eu só vi rapidinho, e é tudo supercaro!",
          "next": [101090035]
        },
        "101090033": {
          "id": 101090033,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "E ela comprou tudo no nome dela?",
          "next": [101090034]
        },
        "101090034": {
          "id": 101090034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "E tem outro jeito? Se ela tivesse comprado no nome da Estação Espacial, ela teria que ter declarado. Nossa pesquisadora-chefe não ia comer essa bola, né?",
          "next": [101090035]
        },
        "101090035": {
          "id": 101090035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "Vai que o dinheiro era dela mesmo?",
