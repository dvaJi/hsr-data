        },
        "200650006": {
          "id": 200650006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Eu posso, mas...",
          "next": [200650007]
        },
        "200650007": {
          "id": 200650007,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Ouvi dizer que você é o professor dele, general...",
          "next": [200650008]
        },
        "200650008": {
          "id": 200650008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1204,
          "text": "Isso é verdade. Mas e daí?",
          "next": [200650009]
        },
        "200650009": {
          "id": 200650009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Então, posso lutar com você primeiro?",
