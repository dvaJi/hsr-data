        "130940012": {
          "id": 130940012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Aliás, a Srta. Sparkle ainda nem revelou os motivos dela! Você não está {F#curiosa}{M#curioso}?",
