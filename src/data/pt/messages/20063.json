      "messages": {
        "200630000": {
          "id": 200630000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Que dia esquisito...",
          "next": [200630001]
        },
        "200630001": {
          "id": 200630001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Eu queria ir ao Chá Insone ouvir as histórias, e adivinha.",
          "next": [200630003]
        },
        "200630002": {
          "id": 200630002,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[confusa]",
          "next": [200630004]
        },
        "200630003": {
          "id": 200630003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "O quê?",
          "next": [200630002]
        },
        "200630004": {
          "id": 200630004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Encontrei o velho alfabeto que me deu aula no liceu!",
          "next": [200630005]
        },
        "200630005": {
          "id": 200630005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Eu não sabia ele estava no Luofu, que sorte a minha...",
          "next": [200630006]
        },
        "200630006": {
          "id": 200630006,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[choro]",
          "next": [200630007, 200630012]
        },
        "200630007": {
          "id": 200630007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Achei que encontrar velhos professores fosse uma coisa boa, não?",
          "next": [200630008]
        },
        "200630008": {
          "id": 200630008,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Por que \"que sorte\"?",
          "next": [200630009]
        },
        "200630009": {
          "id": 200630009,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[confusa]",
          "next": [200630010]
        },
        "200630010": {
          "id": 200630010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ah, nem te conto!",
          "next": [200630011]
        },
        "200630011": {
          "id": 200630011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Quando eu ainda estudava ele nem me notava.",
          "next": [200630019]
        },
        "200630012": {
          "id": 200630012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "O que é um liceu? E o que é um alfabeto?",
          "next": [200630013]
        },
        "200630013": {
          "id": 200630013,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Eu sou alienígena, não conheço essas palavras.",
          "next": [200630014]
        },
        "200630014": {
          "id": 200630014,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[confusa]",
          "next": [200630015]
        },
        "200630015": {
          "id": 200630015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ah, não é nada. Eu não devo saber muito mais que você mesmo...",
          "next": [200630016]
        },
        "200630016": {
          "id": 200630016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Deixa eu pensar em como te explicar, peraí.",
          "next": [200630017]
        },
        "200630017": {
          "id": 200630017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Tá bom!",
          "next": [200630018]
        },
        "200630018": {
          "id": 200630018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Liceu é uma escola, e alfabeto é um professor.",
          "next": [200630019]
        },
        "200630019": {
          "id": 200630019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ai, eu realmente não queria lembrar disso. É tão constrangedor.",
          "next": [200630020]
        },
        "200630020": {
          "id": 200630020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Me conte, por que constrangedor?",
          "next": [200630021]
        },
        "200630021": {
          "id": 200630021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Então, eu fui criada numa família militar, e a minha mãe era muito rígida com os treinos das minhas habilidades marciais.",
          "next": [200630022]
        },
        "200630022": {
          "id": 200630022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Então eu tinha que acordar para lutar com ela antes do amanhecer, e depois disso eu ia para o liceu.",
          "next": [200630023]
        },
        "200630023": {
          "id": 200630023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Só que naquela altura eu já estava tão exausta que não conseguia manter os olhos abertos nas aulas do alfabeto.",
          "next": [200630024]
        },
        "200630024": {
          "id": 200630024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ainda hoje, quando ele me viu, gritou \"Olha só, é a Sushang Capotada\"!",
          "next": [200630025]
        },
        "200630025": {
          "id": 200630025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Na hora, todo mundo que estava na rua olhou para mim, fiquei morrendo de vergonha.",
          "next": [200630026]
        },
        "200630026": {
          "id": 200630026,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "[choro]",
          "next": [200630027]
        },
        "200630027": {
          "id": 200630027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "É bem vergonhoso mesmo.",
          "next": [200630028]
        },
        "200630028": {
          "id": 200630028,
          "type": "Sticker",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "[Choque]",
          "next": [200630029]
        },
        "200630029": {
          "id": 200630029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1206,
          "text": "Ai, espero nunca mais dar de cara com ele...",
          "next": []
        }
      },
