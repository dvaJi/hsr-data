        "130410007": {
          "id": 130410007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Vou ser bem direta: aquela Tola chamada \"Sparkle\" está procurando aquela Bolha de Sonhos por todo canto.",
