        "127020005": {
          "id": 127020005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Se eu não continuasse, vocês iam me denunciar pra a Comissão de Manutenção do Reino. Fiquei com medo",
