        },
        "111490008": {
          "id": 111490008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Eu sou Wilder, do Departamento de Investimento Estratégico da Corporação da Paz Interastral",
          "next": [111490009]
        },
        "111490009": {
          "id": 111490009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 86,
          "text": "Estou entrando em contato para recomendar um videogame",
          "next": [111490010, 111490012, 111490015]
        },
        "111490010": {
          "id": 111490010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Um jogo? Maravilha! Que jogo?",
