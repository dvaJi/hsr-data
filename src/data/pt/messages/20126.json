        },
        "201260015": {
          "id": 201260015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 126,
          "text": "Eu sei lá o que é sparkel?! Eu sou só uma...",
          "next": [
            201260016
          ]
        },
        "201260016": {
          "id": 201260016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 127,
          "text": "...Uma lata de lixo com uma paixão ardente!",
