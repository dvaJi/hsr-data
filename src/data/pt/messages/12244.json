        "122440029": {
          "id": 122440029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1111,
          "text": "É, tem razão",
          "next": [
            122440030
          ]
        },
        "122440030": {
          "id": 122440030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 163,
          "text": "Muito bem. Eu entendo",
          "next": [
            122440031
          ]
        },
        "122440031": {
