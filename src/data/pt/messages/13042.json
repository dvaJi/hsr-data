        },
        "130420014": {
          "id": 130420014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Quer que eu continue?",
          "next": [
            130420015,
            130420016
          ]
        },
        "130420015": {
          "id": 130420015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Acho que já chega",
          "next": [
