        },
        "130420016": {
          "id": 130420016,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Acho que eu preciso de mais informações",
          "next": [
            130420017
          ]
        },
        "130420017": {
          "id": 130420017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1307,
          "text": "Não posso ser a única a falar. Pela sua lógica, você também tem que provar que não é a Sparkle, {NICKNAME}.",
          "next": [
            130420018
          ]
        },
        "130420018": {
          "id": 130420018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Hã?",
