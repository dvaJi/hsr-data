        "111240001": {
          "id": 111240001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Tenho uma pergunta para você.\nA Março, o Dan Heng e você viram alguma coisa fora do comum nas montanhas ao norte de Belobog?",
          "next": [
            111240002,
            111240006,
            111240010
          ]
