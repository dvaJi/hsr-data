        "111240002": {
          "id": 111240002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Tinha um robô gigante chamado Motor da Criação",
          "next": [
            111240003
          ]
        },
        "111240003": {
          "id": 111240003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Talvez seja exatamente isso que procuramos. Seria legal se eu pudesse ver com os meus próprios olhos.\nTalvez seja possível reativá-lo",
          "next": [
            111240004
          ]
        },
        "111240004": {
          "id": 111240004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Se for possível, eu gostaria de tentar...",
