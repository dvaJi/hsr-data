      "messages": {
        "200260000": {
          "id": 200260000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "{NICKNAME}, você sabe por onde anda o Peppy?",
          "next": [200260001, 200260002]
        },
        "200260001": {
          "id": 200260001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Desculpe, eu estava {M#ocupado}{F#ocupada} e não prestei muita atenção no Peppy.",
          "next": [200260003]
        },
        "200260002": {
          "id": 200260002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "O Peppy? Acho que ele ainda está vagando pela Estação Espacial. Aonde mais ele iria?",
          "next": [200260003]
        },
        "200260003": {
          "id": 200260003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "O que houve?",
          "next": [200260004]
        },
        "200260004": {
          "id": 200260004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Estou esperando o Departamento de Medicina enviar os resultados do exame físico do Peppy.",
          "next": [200260005]
        },
        "200260005": {
          "id": 200260005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Ele está mancando com a patinha da frente, mas não parece ter nenhum machucado.",
          "next": [200260006]
        },
        "200260006": {
          "id": 200260006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Eu verifiquei as câmeras de segurança da Estação Espacial e não vi como ele machucou a perna.",
          "next": [200260007, 200260009]
        },
        "200260007": {
          "id": 200260007,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Bom, animais aprontam tanto quanto crianças. Será que ele não está fingindo que está mancando?",
          "next": [200260008]
        },
        "200260008": {
          "id": 200260008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Não sei... O Peppy não apronta.",
          "next": [200260011]
        },
        "200260009": {
          "id": 200260009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "De acordo com a sua descrição, talvez o Peppy possa ter machucado a mãozinha igual o Arlan, não?",
          "next": [200260010]
        },
        "200260010": {
          "id": 200260010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Isso não faz sentido. O Peppy não tem nenhuma ferida.",
          "next": [200260011]
        },
        "200260011": {
          "id": 200260011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "...",
          "next": [200260012]
        },
        "200260012": {
          "id": 200260012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Estou com os resultados dos exames do Peppy.",
          "next": [200260013]
        },
        "200260013": {
          "id": 200260013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Aqui diz que ele está perfeitamente saudável.",
          "next": [200260014]
        },
        "200260014": {
          "id": 200260014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Diz que ele não está ferido.",
          "next": [200260015]
        },
        "200260015": {
          "id": 200260015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Não há sinais de fraturas.",
          "next": [200260016]
        },
        "200260016": {
          "id": 200260016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Então por que o Peppy fingiria estar mancando? O que ele ganharia com isso?",
          "next": [200260017]
        },
        "200260017": {
          "id": 200260017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Não faço ideia.",
          "next": [200260018, 200260022]
        },
        "200260018": {
          "id": 200260018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Você tem trabalhado muito. Será que não brincou pouco com ele?",
          "next": [200260019]
        },
        "200260019": {
          "id": 200260019,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Então ele fingiu estar mancando de propósito e queria que você passasse mais tempo com ele.",
          "next": [200260020]
        },
        "200260020": {
          "id": 200260020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "É possível. Faz um tempo que não levo o Peppy para passear.",
          "next": [200260021]
        },
        "200260021": {
          "id": 200260021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Vou precisar comprar mais dos biscoitos favoritos dele.",
          "next": [200260026]
        },
        "200260022": {
          "id": 200260022,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Talvez ele esteja imitando como o Arlan estava antes sem nem perceber.",
          "next": [200260023]
        },
        "200260023": {
          "id": 200260023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "O Arlan machucou a mão durante a invasão da Legião.",
          "next": [200260024]
        },
        "200260024": {
          "id": 200260024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "O Peppy passou toda essa época do lado dele.",
          "next": [200260025]
        },
        "200260025": {
          "id": 200260025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Você tem razão. Ele deve estar imitando o comportamento das pessoas por perto...",
          "next": [200260026]
        },
        "200260026": {
          "id": 200260026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "A boa notícia é que o Peppy não está ferido.",
          "next": [200260027]
        },
        "200260027": {
          "id": 200260027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Esse carinha é muito esperto...",
          "next": [200260028]
        },
        "200260028": {
          "id": 200260028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "O que posso dizer?",
          "next": [200260029]
        },
        "200260029": {
          "id": 200260029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "Tal dona, tal pet.",
          "next": []
        }
