        "121900004": {
          "id": 121900004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "거물이 왔으니 내 자리가 위태해지겠어",
          "next": [
            121900005
          ]
        },
        "121900005": {
          "id": 121900005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "내 자리도",
