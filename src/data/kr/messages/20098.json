        "200980001": {
          "id": 200980001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "그럭저럭 괜찮아. 불편한 거 없어",
          "next": [
            200980002
          ]
        },
        "200980002": {
          "id": 200980002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "그렇다니 마음이 놓이네요",
