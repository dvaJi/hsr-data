        "145090002": {
          "id": 145090002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "물론이죠. 저 얘기 하는 거 좋아해요",
          "next": [
            145090003
          ]
        },
        "145090003": {
          "id": 145090003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 190,
          "text": "하하, 고맙구나",
