        "200360007": {
          "id": 200360007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "넌 구름 위로 우뚝 솟은 장엄한 풍경을 좋아하는구나, 그렇지?",
          "next": [200360011]
        },
        "200360008": {
