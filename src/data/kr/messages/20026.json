        },
        "200260026": {
          "id": 200260026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
          "text": "어쨌든 페피가 다친 게 아니라 다행이야",
          "next": [
            200260027
          ]
        },
        "200260027": {
          "id": 200260027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1009,
