  "sections": [
    {
      "id": 1011100,
      "startingMessageId": [101110000],
      "messages": {
        "101110000": {
          "id": 101110000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "범인을 찾는 걸 도와줘서 고마워. 아가씨를 대신해서 감사를 표할게. 이제 뒷일은 내게 맡겨",
          "next": [101110001, 101110003]
        },
        "101110001": {
          "id": 101110001,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "헤르타 씨가 아스타를 처벌하진 않았지?",
          "next": [101110002]
        },
        "101110002": {
          "id": 101110002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "헤르타 님에 대해서 잘 모르나 보네——그분은 여기에 신경도 안 쓰셔",
          "next": [101110005]
        },
        "101110003": {
          "id": 101110003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "진짜 아스타가 사비를 들인 거야?",
          "next": [101110004]
        },
        "101110004": {
          "id": 101110004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "정말 잘 모르는구나……",
          "next": [101110005]
        },
        "101110005": {
          "id": 101110005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "이 우주정거장이 헤르타 님의 개인 자산인 건 맞아. 하지만 헤르타 님은 이걸 지을 때 한 푼도 안 내셨어",
          "next": [101110006]
        },
        "101110006": {
          "id": 101110006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "아가씨 집안은 「컴퍼니」와 관계가 있어서… 아주 부유하시지. 이 우주정거장의 대부분의 시설은 아가씨 사비로 지은 거라 보면 돼……",
          "next": [101110007]
        },
        "101110007": {
          "id": 101110007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "그러니까 이 물품들을 구입한 비용은… 아가씨한테는 별거 아니야",
          "next": [101110008, 101110010, 101110013]
        },
        "101110008": {
          "id": 101110008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "「스타피스 컴퍼니」가 대체 뭐야?",
          "next": [101110009]
        },
        "101110009": {
          "id": 101110009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "이 우주에서… 가장… 가장 부유한 조직이야",
          "next": [101110015]
        },
        "101110010": {
          "id": 101110010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "이제부터라도 아스타한테 잘 보여야겠네",
          "next": [101110011]
        },
        "101110011": {
          "id": 101110011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "…그런 농담하지 마. 난 진지하게 받아들인다고",
          "next": [101110012]
        },
        "101110012": {
          "id": 101110012,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "…",
          "next": [101110015]
        },
        "101110013": {
          "id": 101110013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "리차드 씨는 어떻게 처리할 거야?",
          "next": [101110014]
        },
        "101110014": {
          "id": 101110014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "…별로 알고 싶지 않을 텐데",
          "next": [101110015]
        },
        "101110015": {
          "id": 101110015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "어쨌든 아가씨 일을 도와줘서 고마워",
          "next": []
        }
      },
      "participatingContactsIds": [1008, 8001]
    }
  ]
}
