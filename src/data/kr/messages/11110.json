        "111100001": {
          "id": 111100001,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1101,
          "text": "(중년이 쓸법한 촌스러운 이모티콘 하나)",
          "next": [
            111100002,
            111100003,
