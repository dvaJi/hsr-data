  "sections": [
    {
      "id": 1010900,
      "startingMessageId": [101090000],
      "messages": {
        "101090000": {
          "id": 101090000,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "「무명인사」님이 「얼굴로 방어」님을 단톡방에 초대하고 최근 10건의 채팅 기록을 공유했습니다",
          "next": [101090001]
        },
        "101090001": {
          "id": 101090001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 16,
          "text": "걔는 예전부터 문제 있는 것 같았다니까",
          "next": [101090002]
        },
        "101090002": {
          "id": 101090002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "자세히 말해줘",
          "next": [101090003]
        },
        "101090003": {
          "id": 101090003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "자세히 말해줘",
          "next": [101090004]
        },
        "101090004": {
          "id": 101090004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "이렇게 큰 우주정거장에 돈 나올 구멍이 얼마나 많겠어? 잘 생각해 봐",
          "next": [101090005]
        },
        "101090005": {
          "id": 101090005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "함부로 말하면 안 돼. 그래도 헤르타 님 제자인데",
          "next": [101090006]
        },
        "101090006": {
          "id": 101090006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "제자면 뭐? 제자면 뭐라고 말도 못 해?",
          "next": [101090007]
        },
        "101090007": {
          "id": 101090007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "그녀 말이지",
          "next": [101090008]
        },
        "101090008": {
          "id": 101090008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "그녀에 관한 안 좋은 이야기가 꽤 많아",
          "next": [101090009]
        },
        "101090009": {
          "id": 101090009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "자세히 말해줘",
          "next": [101090010]
        },
        "101090010": {
          "id": 101090010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "그건 곤란해",
          "next": [101090011]
        },
        "101090011": {
          "id": 101090011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "그럼 증거를 보여줘!!",
          "next": [101090012, 101090014, 101090018]
        },
        "101090012": {
          "id": 101090012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "여긴 뭐하는 단톡방이야?",
          "next": [101090013]
        },
        "101090013": {
          "id": 101090013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "@얼굴로 방어 단톡방 제목을 봐",
          "next": [101090020]
        },
        "101090014": {
          "id": 101090014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "지금 누구 얘기하는 거야?",
          "next": [101090015]
        },
        "101090015": {
          "id": 101090015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "뉴비는 규칙도 모르나?",
          "next": [101090016]
        },
        "101090016": {
          "id": 101090016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "아스타",
          "next": [101090017]
        },
        "101090017": {
          "id": 101090017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "아스타 그 계집애",
          "next": [101090020]
        },
        "101090018": {
          "id": 101090018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "누가 배경 설명 좀 해줄래?",
          "next": [101090019]
        },
        "101090019": {
          "id": 101090019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "알아서 찾아봐",
          "next": [101090020]
        },
        "101090020": {
          "id": 101090020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "끼어들지 마. @안다니 똥파리 리차드 얼른 불어",
          "next": [101090021]
        },
        "101090021": {
          "id": 101090021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "됐다, 됐어",
          "next": [101090022]
        },
        "101090022": {
          "id": 101090022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 16,
          "text": "익명인데 왜 말을 하다 말아, 예의가 없네",
          "next": [101090023]
        },
        "101090023": {
          "id": 101090023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "다들 모르는 것도 아니고……",
          "next": [101090024]
        },
        "101090024": {
          "id": 101090024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "헛소리 말고, 말해!",
          "next": [101090025]
        },
        "101090025": {
          "id": 101090025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "공금 횡령 건이지, 뭐",
          "next": [101090026]
        },
        "101090026": {
          "id": 101090026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "로비에 쌓여있는 택배 더미 못 봤어? 수취인이 전부 우리의 젊은 정거장 책임자님이시라고!",
          "next": [101090027]
        },
        "101090027": {
          "id": 101090027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "세상에, 어쩐지 예산이 빡빡하다 싶었는데",
          "next": [101090028]
        },
        "101090028": {
          "id": 101090028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "그건 좀 아니다",
          "next": [101090029]
        },
        "101090029": {
          "id": 101090029,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "말만 하지 말고 증거를 보여줘",
          "next": [101090030]
        },
        "101090030": {
          "id": 101090030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "증거가 필요해? 로비에 쌓여있는 택배 더미 못 봤어? 수취인이 전부 우리의 젊은 정거장 책임자님이시라고!",
          "next": [101090031, 101090033]
        },
        "101090031": {
          "id": 101090031,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "택배로 온 게 뭐지?",
          "next": [101090032]
        },
        "101090032": {
          "id": 101090032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "슬쩍 봤는데, 다 엄청 비싼 것들이었어",
          "next": [101090035]
        },
        "101090033": {
          "id": 101090033,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "개인 명의로 구매한 건가?",
          "next": [101090034]
        },
        "101090034": {
          "id": 101090034,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "개인 명의지. 우주정거장 물품을 구매하려면 보고를 올려야 한다고. 책임자가 바보인 줄 아나",
          "next": [101090035]
        },
        "101090035": {
          "id": 101090035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "사비로 구매한 걸 수도 있잖아",
          "next": [101090036]
        },
        "101090036": {
          "id": 101090036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "헤르타 님 말고 그런 걸 살 능력이 있는 사람이 어딨어?",
          "next": [101090037]
        },
        "101090037": {
          "id": 101090037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "방위과의 그 아를란이 프런트에 수상하게 얼쩡거리더라고. 그 녀석이 아니었다면 나도 몰랐을 거야",
          "next": [101090038]
        },
        "101090038": {
          "id": 101090038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "흠, 그러고 보니… 나도 로비에서 아를란이 잔뜩 긴장해서 택배 받는 걸 본 것 같아",
          "next": [101090039]
        },
        "101090039": {
          "id": 101090039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "이거 봐 봐",
          "next": [101090040]
        },
        "101090040": {
          "id": 101090040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "딱 맞아떨어지네",
          "next": [101090041]
        },
        "101090041": {
          "id": 101090041,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "딱 맞아떨어지네",
          "next": [101090042, 101090044, 101090047]
        },
        "101090042": {
          "id": 101090042,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "그러니까 아를란이랑 아스타가 손잡고 공금을 횡령했다는 거야?",
          "next": [101090043]
        },
        "101090043": {
          "id": 101090043,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "난 몰라. @안다니 똥파리 리차드가 말한 거지, 내가 한 말 아냐",
          "next": [101090051]
        },
        "101090044": {
          "id": 101090044,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "그냥 일방적인 추측인 것 같은데, 증거도 없고……",
          "next": [101090045]
        },
        "101090045": {
          "id": 101090045,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "믿든가 말든가",
          "next": [101090046]
        },
        "101090046": {
          "id": 101090046,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "웃겨 죽겠네, 뻔하잖아. 아를란이 그 계집애 측근이라는 걸 모르는 사람도 있나?",
          "next": [101090051]
        },
        "101090047": {
          "id": 101090047,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "이런 엄청난 정보를… 공짜로 들어도 되나?",
          "next": [101090048]
        },
        "101090048": {
          "id": 101090048,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "난 지금까지의 채팅 내용과 무관해!",
          "next": [101090049]
        },
        "101090049": {
          "id": 101090049,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 20,
          "text": "난 지금까지의 채팅 내용과 무관해!",
          "next": [101090050]
        },
        "101090050": {
          "id": 101090050,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "난 무조건 헤르타 님의 지시에 따를 거야!",
          "next": [101090051]
        },
        "101090051": {
          "id": 101090051,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 16,
          "text": "…뭐야, 대박 사건인 줄 알고 한참을 기다렸는데, 겨우 이게 다야?",
          "next": [101090052]
        },
        "101090052": {
          "id": 101090052,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "그러게, 한참 떠들었지만 증거는 없잖아",
          "next": [101090053]
        },
        "101090053": {
          "id": 101090053,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 18,
          "text": "@마스 로봇 너도 아를란이 수상하다고 했잖아",
          "next": [101090054]
        },
        "101090054": {
          "id": 101090054,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 17,
          "text": "난 딱 그거만 말했어",
          "next": [101090055]
        },
        "101090055": {
          "id": 101090055,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "사실 그 큰 돈이 어디서 났는지 몰래 조사 중이야. 아직 터트릴 시기는 아니지만",
          "next": [101090056]
        },
        "101090056": {
          "id": 101090056,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 19,
          "text": "다들 기다려. 곧 우주정거장이 뒤집어질 뉴스가 터질 테니!",
          "next": [101090057]
        },
        "101090057": {
          "id": 101090057,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 16,
          "text": "그러길 바라야지",
          "next": []
        }
      },
      "participatingContactsIds": [8001, 16, 17, 20, 18, 19]
    }
  ]
}
