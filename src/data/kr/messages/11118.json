  "sections": [
    {
      "id": 1111800,
      "startingMessageId": [111180000],
      "messages": {
        "111180000": {
          "id": 111180000,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "차장님, 나 좀 살려줘——",
          "next": [111180002]
        },
        "111180002": {
          "id": 111180002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "[자동 회신] 현재 열차 운전 중입니다. 용건이 있으면 메시지를 남겨주세요!",
          "next": [111180003, 111180004]
        },
        "111180003": {
          "id": 111180003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "입력 중인 거 다 봤거든…",
          "next": [111180005]
        },
        "111180004": {
          "id": 111180004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "왜 출발하려는 거야? 나 아직 안 탔어!",
          "next": [111180006]
        },
        "111180005": {
          "id": 111180005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "이런, 들켰네!",
          "next": [111180007]
        },
        "111180006": {
          "id": 111180006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "하! 속았구나, {NICKNAME}!",
          "next": [111180007]
        },
        "111180007": {
          "id": 111180007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "좋아, 장난은 여기까지. 무슨 일이야, {NICKNAME}?",
          "next": [111180008, 111180009]
        },
        "111180008": {
          "id": 111180008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "혹시 열차에서 신규 회원을 받아주기도 해?",
          "next": [111180010]
        },
        "111180009": {
          "id": 111180009,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "실은, 내 친구가…",
          "next": [111180010]
        },
        "111180010": {
          "id": 111180010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "오, 친구를 열차에 태우고 싶다고?",
          "next": [111180011]
        },
        "111180011": {
          "id": 111180011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "우선 이것부터 말해봐. 그 친구는 어떤 사람이야?",
          "next": [111180012]
        },
        "111180012": {
          "id": 111180012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "대답하기 전에 생각을 정리하도록 해. 폼폼은 진지한 답변을 원하니까!",
          "next": [111180013, 111180014, 111180015]
        },
        "111180013": {
          "id": 111180013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "굉장히 치밀한 성격의 천재 엔지니어예요",
          "next": [111180016]
        },
        "111180014": {
          "id": 111180014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "자유분방한 성격의 록스타예요",
          "next": [111180017]
        },
        "111180015": {
          "id": 111180015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "음… 뭐라고 설명을 할 수가 없어",
          "next": [111180018]
        },
        "111180016": {
          "id": 111180016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "오! 그럼 히메코가 좋아하겠네!",
          "next": [111180019]
        },
        "111180017": {
          "id": 111180017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "록스타라고? 그럼 폼폼도 음반 한 장 받을 수 있으려나?",
          "next": [111180019]
        },
        "111180018": {
          "id": 111180018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "그럼 됐어! 사실 폼폼도 그냥 물어본 것뿐이니까!",
          "next": [111180019]
        },
        "111180019": {
          "id": 111180019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "어쨌든 폼폼은 문제 없을 거라고 생각해! 그래도——",
          "next": [111180020]
        },
        "111180020": {
          "id": 111180020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "엔진실에는 들어오면 안 돼!",
          "next": [111180021]
        },
        "111180021": {
          "id": 111180021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "어떤 것도 부수면 안 돼!",
          "next": [111180022]
        },
        "111180022": {
          "id": 111180022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "그래줄 수 있으려나?",
          "next": [111180023, 111180024, 111180025]
        },
        "111180023": {
          "id": 111180023,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "문제 없어!",
          "next": [111180026]
        },
        "111180024": {
          "id": 111180024,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "최대한 설득해볼게…",
          "next": [111180026]
        },
        "111180025": {
          "id": 111180025,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "아마 통제 불능일 것 같은데",
          "next": [111180026]
        },
        "111180026": {
          "id": 111180026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "뭐가 어쨌든 그런 행위는 허용할 수 없어! 그것만 지켜진다면 폼폼은 반대하지 않을 거야!",
          "next": [111180027, 111180028]
        },
        "111180027": {
          "id": 111180027,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "알겠어. 고마워, 차장님",
          "next": [111180029]
        },
        "111180028": {
          "id": 111180028,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "좋았어! 차장님 최고——",
          "next": [111180029]
        },
        "111180029": {
          "id": 111180029,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "(만능 이모티콘을 보낸다)",
          "next": []
        }
      },
      "participatingContactsIds": [8001, 1000]
    }
  ]
}
