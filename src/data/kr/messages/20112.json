        "201120001": {
          "id": 201120001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "스티븐이 실종됐어. 스크루룸과 헤르타도 그를 찾지 못했어. 분명 어디에서 게으름을 피우고 있을 거야",
          "next": [
            201120002
          ]
        },
        "201120002": {
          "id": 201120002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "잠든 건 아니었으면 좋겠는데",
          "next": [
            201120003
          ]
        },
        "201120003": {
          "id": 201120003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "근데 전 스티븐이 어떻게 생겼는지 모르는데…",
          "next": [
            201120004
          ]
        },
        "201120004": {
          "id": 201120004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "앗, 잠깐만 기다려 봐",
          "next": [
            201120005
          ]
        },
        "201120005": {
          "id": 201120005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "걸려들었다, 찾았어",
          "next": [
            201120006
          ]
        },
        "201120006": {
          "id": 201120006,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "벌써요?!",
          "next": [
            201120007
          ]
        },
        "201120007": {
          "id": 201120007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "이게 다 「스티븐 미끼」 덕분이야",
          "next": [
            201120008
          ]
        },
        "201120008": {
          "id": 201120008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "이름은 거창하지만 원리는 간단해",
          "next": [
            201120009
          ]
        },
        "201120009": {
          "id": 201120009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "과일 디저트 1인분",
          "next": [
            201120010
          ]
        },
        "201120010": {
          "id": 201120010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1303,
          "text": "미끼는 수량이 아니라 맛이 중요하거든",
          "next": [
            201120011,
            201120013
          ]
        },
        "201120011": {
          "id": 201120011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "「헤르타 미끼」는 없어요?",
