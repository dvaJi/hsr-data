        "111270004": {
          "id": 111270004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "음. 안 사요. 감사합니다",
          "next": [
            111270007
          ]
        },
        "111270005": {
          "id": 111270005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "드디어 링크를 찾으셨나요?",
