  "sections": [
    {
      "id": 2002700,
      "startingMessageId": [200270000],
      "messages": {
        "200270000": {
          "id": 200270000,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "{NICKNAME}, как ты думаешь, какой цвет мне больше подходит, синий или серый?",
          "next": [200270001]
        },
        "200270001": {
          "id": 200270001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Пожалуйста, выбери по первому впечатлению!",
          "next": [200270002, 200270005, 200270008]
        },
        "200270002": {
          "id": 200270002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Синий. Как твоя форма.",
          "next": [200270003]
        },
        "200270003": {
          "id": 200270003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Хе-хе, так и знал, что он мне подходит.",
          "next": [200270004]
        },
        "200270004": {
          "id": 200270004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Кажется, у нас с тобой похожие вкусы.",
          "next": [200270011]
        },
        "200270005": {
          "id": 200270005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Серый? Как твои волосы.",
          "next": [200270006]
        },
        "200270006": {
          "id": 200270006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Серый... пойдёт!",
          "next": [200270007]
        },
        "200270007": {
          "id": 200270007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Раз ты считаешь, что он мне подходит, куплю его.",
          "next": [200270011]
        },
        "200270008": {
          "id": 200270008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Отправь мне селфи, чтобы доказать, что тебя не взломали. Ты ведёшь себя странно.",
          "next": [200270009]
        },
        "200270009": {
          "id": 200270009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Странно? Меня просто переполняют эмоции.",
          "next": [200270010]
        },
        "200270010": {
          "id": 200270010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Забудь! Куплю оба цвета, тогда и пойму, какой подходит лучше.",
          "next": [200270011]
        },
        "200270011": {
          "id": 200270011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Я собираюсь купить новую тренировочную обувь, поэтому спрашиваю мнение {F#подруги}{M#друга}.",
          "next": [200270012]
        },
        "200270012": {
          "id": 200270012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Я сфоткаю тебе, когда она придёт.",
          "next": [200270013]
        },
        "200270013": {
          "id": 200270013,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "[Одобрение]",
          "next": [200270014, 200270015]
        },
        "200270014": {
          "id": 200270014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Верни ему смартфон.",
          "next": [200270016]
        },
        "200270015": {
          "id": 200270015,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Аста, хватит прикалываться.",
          "next": [200270016]
        },
        "200270016": {
          "id": 200270016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Ну вот, я попалась. Я не похожа на Арлана?",
          "next": [200270017]
        },
        "200270017": {
          "id": 200270017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Он несколько раз отредактировал сообщение для тебя, но так и не отправил.",
          "next": [200270018]
        },
        "200270018": {
          "id": 200270018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Прости, госпожа Аста взяла мой смартфон.",
          "next": [200270019]
        },
        "200270019": {
          "id": 200270019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Прошу прощения за беспокойство. Больше это не повторится.",
          "next": [200270020]
        },
        "200270020": {
          "id": 200270020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1008,
          "text": "Поговорим позже. Госпожа Аста ищет ключ шифрования для оплаты, нельзя, чтобы она его нашла.",
          "next": []
        }
      },
      "participatingContactsIds": [1008, 8001]
    }
  ]
}
