        "500100001": {
          "id": 500100001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1000,
          "text": "{NICKNAME}, а у тебя ведь багажа нет, так? Или это Пом-Пом его не нашла?",
          "next": [500100002, 500100004]
        },
        "500100002": {
          "id": 500100002,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Люблю путешествовать налегке.",
