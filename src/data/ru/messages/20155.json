        },
        "201550021": {
          "id": 201550021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "Эмоции у него нестабильные...",
          "next": [
            201550022
          ]
        },
        "201550022": {
          "id": 201550022,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1221,
          "text": "«Фу, воняет!»",
