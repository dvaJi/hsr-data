{
  "id": 20120,
  "contacts": {
    "id": 1305,
    "name": "Доктор Рацио",
    "icon": "1305",
    "signature": "«Не срочно».",
    "typeId": 1,
    "type": "Персонаж"
  },
  "relatedMessages": [
    20118,
    20119,
