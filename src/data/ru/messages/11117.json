      "id": 1111700,
      "startingMessageId": [111170000],
      "messages": {
        "111170000": {
          "id": 111170000,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Господин Янг, вы здесь? Господин Янг!",
          "next": [111170003]
        },
        "111170003": {
          "id": 111170003,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Я всё время был здесь. Что случилось, {NICKNAME}?",
          "next": [111170004, 111170005]
        },
        "111170004": {
          "id": 111170004,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Экспресс принимает новеньких?",
          "next": [111170006]
        },
        "111170005": {
          "id": 111170005,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Одна моя подруга хочет присоединиться к команде экспресса, что скажете?",
          "next": [111170007]
        },
        "111170006": {
          "id": 111170006,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "А? Конечно.",
          "next": [111170008]
        },
        "111170007": {
          "id": 111170007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "А? Друзья - это хорошо, я приветствую новых членов.",
          "next": [111170008]
        },
        "111170008": {
          "id": 111170008,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Но могу я задать вопрос?",
          "next": [111170009]
        },
        "111170009": {
          "id": 111170009,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Можешь описать свою подругу, {NICKNAME}?",
          "next": [111170010, 111170011, 111170012]
        },
        "111170010": {
          "id": 111170010,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Она умный и талантливый механик.",
          "next": [111170013]
        },
        "111170011": {
          "id": 111170011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Она рок-звезда со свободолюбивым характером.",
          "next": [111170014]
        },
        "111170012": {
          "id": 111170012,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Она... Я не могу точно описать.",
          "next": [111170015]
        },
        "111170013": {
          "id": 111170013,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Кажется, у неё будет много общего с Химеко, неплохо.",
          "next": [111170016]
        },
        "111170014": {
          "id": 111170014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Отличное хобби, я бы и сам таким занялся.",
          "next": [111170016]
        },
        "111170015": {
          "id": 111170015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Неважно. Я доверяю твоему мнению.",
          "next": [111170016]
        },
        "111170016": {
          "id": 111170016,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Я с радостью приветствую твою подругу.",
          "next": [111170017]
        },
        "111170017": {
          "id": 111170017,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Не я здесь принимаю решения, но я замолвлю словечко перед Химеко.",
          "next": [111170018]
        },
        "111170018": {
          "id": 111170018,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Не думаю, что она будет против, так что не переживай.",
          "next": [111170019, 111170020]
        },
        "111170019": {
          "id": 111170019,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Спасибо, господин Вельт.",
          "next": [111170021]
        },
        "111170020": {
          "id": 111170020,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ура! Спасибо, господин Вельт!",
          "next": [111170021]
        },
        "111170021": {
          "id": 111170021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1004,
          "text": "Не за что.",
          "next": []
        }
      },
      "participatingContactsIds": [8001, 1004]
    }
  ]
}
