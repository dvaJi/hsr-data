        },
        "121820011": {
          "id": 121820011,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "@Хохо",
          "next": [121820012]
        },
        "121820012": {
          "id": 121820012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1210,
          "text": "Хватит отмечать её, лучше приходи в Сад безмятежности.",
