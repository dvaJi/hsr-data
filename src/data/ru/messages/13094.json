      "messages": {
        "130940000": {
          "id": 130940000,
          "type": "Text",
          "sender": "System",
          "senderContactId": 8001,
          "text": "Кукла «Сообщение» начинает временный чат.",
          "next": [
            130940001
          ]
        },
        "130940001": {
          "id": 130940001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Круть",
          "next": [
            130940002
          ]
        },
        "130940002": {
          "id": 130940002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Мне так классно, а тебе?",
          "next": [
            130940003,
            130940008,
            130940013
          ]
        },
        "130940003": {
          "id": 130940003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "А? Опять бомбы искать?",
          "next": [
            130940004
          ]
        },
        "130940004": {
          "id": 130940004,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Давай координаты, разминирую.",
          "next": [
            130940005
          ]
        },
        "130940005": {
          "id": 130940005,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Бомб всего 1000. Госпожа Искорка держит своё слово~",
          "next": [
            130940006
          ]
        },
        "130940006": {
          "id": 130940006,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "(Стикер «Плачущая маска»)",
          "next": [
            130940007
          ]
        },
        "130940007": {
          "id": 130940007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Я хочу рассказать о мотивах госпожи Искорки, ясно?",
          "next": [
            130940017,
            130940018
          ]
        },
        "130940008": {
          "id": 130940008,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ну хватит!",
          "next": [
            130940009
          ]
        },
        "130940009": {
          "id": 130940009,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Не пиши мне больше. Я не хочу, чтобы Пом-Пом что-то не то подумала.",
          "next": [
            130940010
          ]
        },
        "130940010": {
          "id": 130940010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Нарываешься? Ты наступил{F#а} на жвачку. Я приклеилась к тебе навечно!",
          "next": [
            130940011
          ]
        },
        "130940011": {
          "id": 130940011,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "(Стикер «Ухмылка»)",
          "next": [
            130940012
          ]
        },
        "130940012": {
          "id": 130940012,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Кстати, госпожа Искорка ещё не рассказала о своих мотивах! Тебе разве не интересно?",
          "next": [
            130940017,
            130940018
          ]
        },
        "130940013": {
          "id": 130940013,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Фейерверки! Крутота!",
          "next": [
            130940014
          ]
        },
        "130940014": {
          "id": 130940014,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Да! Ты понимаешь~",
          "next": [
            130940015
          ]
        },
        "130940015": {
          "id": 130940015,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Скажи, тебе интересно, почему госпожа Искорка всё это затеяла?",
          "next": [
            130940016
          ]
        },
        "130940016": {
          "id": 130940016,
          "type": "Sticker",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "(Стикер «Зажигалка»)",
          "next": [
            130940017,
            130940018
          ]
        },
        "130940017": {
          "id": 130940017,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Расскажи.",
          "next": [
            130940021
          ]
        },
        "130940018": {
          "id": 130940018,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Я отключаюсь, поговорим в другой раз.",
          "next": [
            130940019
          ]
        },
        "130940019": {
          "id": 130940019,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Нет, не уходи! Ну пожалуйста!",
          "next": [
            130940020
          ]
        },
        "130940020": {
          "id": 130940020,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Я же шучу~ Давай вернёмся к делу.",
          "next": [
            130940021
          ]
        },
        "130940021": {
          "id": 130940021,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Помнишь о «трёх смертях»?",
          "next": [
            130940022
          ]
        },
        "130940022": {
          "id": 130940022,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "А к тебе это какое имеет отношение?",
          "next": [
            130940023
          ]
        },
        "130940023": {
          "id": 130940023,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Кто знает? Я могу только сказать, что кое-кто заплатил кругленькую сумму, чтобы нанять госпожу Искорку на место режиссёра на Пенаконии.",
          "next": [
            130940024
          ]
        },
        "130940024": {
          "id": 130940024,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Кажется, это кто-то, с кем она познакомилась в игре. Этот человек попросил госпожу Искорку, чтобы «три смерти» прошли максимально безопасно.",
          "next": [
            130940025
          ]
        },
        "130940025": {
          "id": 130940025,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Взамен этот человек поделился своим сценарием с госпожой Искоркой.",
          "next": [
            130940026
          ]
        },
        "130940026": {
          "id": 130940026,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "А что произошло дальше... Ну, ты наверняка сможешь догадаться самостоятельно!",
          "next": [
            130940027
          ]
        },
        "130940027": {
          "id": 130940027,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Однако госпожа Искорка не какой-нибудь там одержимый режиссёр-манипулятор. Она решила, что актёры сами способны выбрать, как развернётся история.",
          "next": [
            130940028
          ]
        },
        "130940028": {
          "id": 130940028,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Внезапный вопрос! Ты нажал{F#а} на кнопку гарантированного взаимного уничтожения?",
          "next": [
            130940029,
            130940031,
            130940034
          ]
        },
        "130940029": {
          "id": 130940029,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Да, со всей силы.",
          "next": [
            130940030
          ]
        },
        "130940030": {
          "id": 130940030,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Поздравляю! Именно ты и изменил{F#а} третью смерть!",
          "next": [
            130940037
          ]
        },
        "130940031": {
          "id": 130940031,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Нет, подозрительно как-то, я такое не нажимаю.",
          "next": [
            130940032
          ]
        },
        "130940032": {
          "id": 130940032,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Хм, а ты из осторожных.",
          "next": [
            130940033
          ]
        },
        "130940033": {
          "id": 130940033,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Не волнуйся. Благодаря тому, что большинство актёров нажало кнопку, третья смерть случилась так, как было запланировано!",
          "next": [
            130940037
          ]
        },
        "130940034": {
          "id": 130940034,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "У меня и кнопки-то не было!",
          "next": [
            130940035
          ]
        },
        "130940035": {
          "id": 130940035,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Правда? Ну, в следующий раз тебе наверняка повезёт.",
          "next": [
            130940036
          ]
        },
        "130940036": {
          "id": 130940036,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Не волнуйся. Благодаря тому, что большинство актёров нажало кнопку, третья смерть случилась так, как было запланировано!",
          "next": [
            130940037
          ]
        },
        "130940037": {
          "id": 130940037,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Она взлетела высоко-высоко и запустила самый яркий фейерверк за всю историю Пенаконии, который ознаменовал счастливый финал этой красивой истории.",
          "next": [
            130940038
          ]
        },
        "130940038": {
          "id": 130940038,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Сама она осталась целой и невредимой! Счастливый конец!",
          "next": [
            130940039
          ]
        },
        "130940039": {
          "id": 130940039,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Так что поздравляю, {F#серовласка}{M#серовласый} Ведь именно благодаря тебе Светлячок смогла спастись.",
          "next": [
            130940040
          ]
        },
        "130940040": {
          "id": 130940040,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "И благодаря этому госпожа Искорка получит обещанное вознаграждение!",
          "next": [
            130940041
          ]
        },
        "130940041": {
          "id": 130940041,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "А!",
          "next": [
            130940042
          ]
        },
        "130940042": {
          "id": 130940042,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Кстати, в честь счастливого финала госпожа Искорка решила поделиться с тобой ещё одним секретом.",
          "next": [
            130940043,
            130940044
          ]
        },
        "130940043": {
          "id": 130940043,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Каким?",
          "next": [
            130940045
          ]
        },
        "130940044": {
          "id": 130940044,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Давай, рассказывай!",
          "next": [
            130940045
          ]
        },
        "130940045": {
          "id": 130940045,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Знаешь, что?",
          "next": [
            130940046
          ]
        },
        "130940046": {
          "id": 130940046,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "Оказывается",
          "next": [
            130940047
          ]
        },
        "130940047": {
          "id": 130940047,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 156,
          "text": "доктор Примитив без ума от бананаов.",
          "next": [
            130940048,
            130940049,
            130940050
          ]
        },
        "130940048": {
          "id": 130940048,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Чего?",
          "next": [
            130940051
          ]
        },
        "130940049": {
          "id": 130940049,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Ты о чём?",
          "next": [
            130940051
          ]
        },
        "130940050": {
          "id": 130940050,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "А дальше?",
          "next": [
            130940051
          ]
        },
