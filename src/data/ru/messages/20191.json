        },
        "201910014": {
          "id": 201910014,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Мой друг, мы видим свет, который ты зажёг для всего мира.",
          "next": [
            201910015
          ]
        },
        "201910015": {
          "id": 201910015,
          "type": "Text",
          "sender": "PlayerAuto",
