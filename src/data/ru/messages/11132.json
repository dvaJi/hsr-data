        },
        "111320002": {
          "id": 111320002,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 40,
          "text": "Значит, мы в одной лодке.",
          "next": [111320007]
        },
        "111320003": {
          "id": 111320003,
          "type": "Text",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "Хотя бы Среброгривые Стражи больше меня не преследуют.",
          "next": [111320004]
        },
        "111320004": {
          "id": 111320004,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 40,
          "text": "Ха-ха, да.",
