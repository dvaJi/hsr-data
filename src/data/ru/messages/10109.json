        "101090001": {
          "id": 101090001,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 16,
          "text": "Я давно думал, что с ней что-то не так.",
          "next": [
            101090002
          ]
        },
        "101090002": {
          "id": 101090002,
          "type": "Text",
