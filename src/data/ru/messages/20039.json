        },
        "200390007": {
          "id": 200390007,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1108,
          "text": "Ты разве не собираешься спросить, что это за новый вид мошенничества?",
          "next": [200390008]
        },
        "200390008": {
          "id": 200390008,
          "type": "Sticker",
          "sender": "Player",
          "senderContactId": 8001,
          "text": "[Недоумение]",
          "next": [200390009]
        },
        "200390009": {
          "id": 200390009,
          "type": "Text",
          "sender": "PlayerAuto",
          "senderContactId": 8001,
          "text": "Боишься?",
          "next": [200390010]
        },
        "200390010": {
          "id": 200390010,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1108,
          "text": "Я напуган.",
          "next": [200390011]
        },
        "200390011": {
          "id": 200390011,
          "type": "Text",
          "sender": "NPC",
          "senderContactId": 1108,
          "text": "Я больше не буду так делать.",
